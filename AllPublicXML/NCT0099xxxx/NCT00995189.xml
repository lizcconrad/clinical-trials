  <brief_title>Clinical Evaluation of the Causes of Contact Lens Related Dry Eye</brief_title>
  <official_title>A Clinical Evaluation of the Causes of Soft Contact Lens Related Dry Eye</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alcon Research</agency>
      <agency_class>Industry</agency_class>
