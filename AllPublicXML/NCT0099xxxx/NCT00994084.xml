  <brief_title>Healthy Kids-Houston: A Community Childhood Obesity Intervention Program</brief_title>
  <acronym>HKH</acronym>
  <official_title>Healthy Kids-Houston: An Integrated Community Program for the Prevention of Obesity Among Minority Children</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Baylor College of Medicine</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
