  </study_design_info>
  <primary_outcome>
    <measure>Change in Fasting Plasma Glucose (FPG) levels</measure>
    <time_frame>3 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change in glycosylated hemoglobin (HbA1c) levels</measure>
    <time_frame>3 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Effect on glycated albumin, insulin, C-peptide, glucagon and lipid parameters</measure>
    <time_frame>3 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Safety (biochemistry, hematology, urinalysis and adrenal-related markers) and tolerability (adverse events)</measure>
    <time_frame>3 months</time_frame>
  </secondary_outcome>
