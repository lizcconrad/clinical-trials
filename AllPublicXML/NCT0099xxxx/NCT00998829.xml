    <arm_group_label>Study population</arm_group_label>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        Specialist dermatology clinics
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Diagnosis of plaque psoriasis

          -  &gt;= 18 years of age

          -  Able to complete English-language questionnaires

        Exclusion Criteria:

          -  Participation in an interventional clinical trial in previous 3 months

          -  Known rheumatologist-confirmed psoriatic arthritis
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
