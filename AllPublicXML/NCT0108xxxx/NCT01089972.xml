  </study_design_info>
  <primary_outcome>
    <measure>Radial artery diameter and blood flow before and after cannulation</measure>
    <time_frame>30 min</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Ulnar artery diameter and blood flow before and after cannulation</measure>
    <time_frame>30 min</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>The differences of radial artery diameter and blood flow after cannulation between catheter gauge(20- and 22-G)</measure>
    <time_frame>30 min</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>The differences of ulnar artery diameter and blood flow after cannulation between catheter gauge(20- and 22-G)</measure>
    <time_frame>30 min</time_frame>
  </secondary_outcome>
