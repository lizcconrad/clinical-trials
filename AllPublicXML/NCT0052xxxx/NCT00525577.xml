  <brief_title>ANDES-AGI-1067 as a Novel Antidiabetic Agent Evaluation Study</brief_title>
  <acronym>ANDES</acronym>
  <official_title>AGI-1067 as a Novel Antidiabetic Agent Evaluation Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AtheroGenics</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
