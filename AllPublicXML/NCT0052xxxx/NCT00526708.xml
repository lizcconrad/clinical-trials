  <brief_summary>
    <textblock>
      To evaluate the efficacy of rosuvastatin in Korean dyslipidemia patients with hypertension.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>May 2007</start_date>
  <completion_date type="Actual">November 2007</completion_date>
