  </study_design_info>
  <primary_outcome>
    <measure>Overall survival</measure>
    <time_frame>12 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Complete blood counts (CBC), Transfusion Requirements, Infections or Fevers of Unknown Origin Requiring Hospitalization or IV Antibiotics</measure>
    <time_frame>13 months</time_frame>
  </secondary_outcome>
