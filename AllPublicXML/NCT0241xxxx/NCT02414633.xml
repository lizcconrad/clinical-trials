    <arm_group_label>Humira</arm_group_label>
    <description>Subjects with an exposure</description>
  </arm_group>
  <eligibility>
    <study_pop>
      <textblock>
        This is a single-arm, multi-center study with a prospective cohort.
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Paid workers (including part-time) with Psoriatic Arthritis, who have never
             administered adalimumab, and are diagnosed by ClASsification of Psoriatic ARthritis
             (CASPAR) criteria

        Exclusion Criteria:

          -  Subjects showing decreased basic activities of daily life such as hospitalization and
             bedridden

          -  Subjects with contraindications to adalimumab
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>16 Years</minimum_age>
    <maximum_age>99 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
