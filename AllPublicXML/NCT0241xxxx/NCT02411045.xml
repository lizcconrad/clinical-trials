  <brief_title>Dress for Success: Can Just Dressing for the Gym Help Going to the Gym?</brief_title>
  <official_title>Dress for Success: Can Just Dressing for the Gym Help Going to the Gym?</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Pennsylvania</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>Harvard University</agency>
      <agency_class>Other</agency_class>
    </collaborator>
