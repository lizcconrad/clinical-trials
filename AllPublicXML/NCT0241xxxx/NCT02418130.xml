    <intervention_type>Drug</intervention_type>
    <intervention_name>Placebo</intervention_name>
    <arm_group_label>Placebo of UDCA004</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  CIS greater than or equal to 76, HADS less than or equal to 10

        Exclusion Criteria:

          -  Subjects who have diseases that can cause fatigue

          -  Subjects who are taking medication that can cause fatigue
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>19 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
