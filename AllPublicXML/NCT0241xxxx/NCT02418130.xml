  </study_design_info>
  <primary_outcome>
    <measure>Rate of subjects whose CIS score has improved under 76 at week 4</measure>
    <time_frame>4 weeks</time_frame>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">166</enrollment>
