  <brief_summary>
    <textblock>
      The study is designed to collect data on LV latency in CRT-D patients by the CRT-D and
      compare measurements to 12-lead ECG data
    </textblock>
  </brief_summary>
  <overall_status>Recruiting</overall_status>
  <start_date type="Actual">February 26, 2018</start_date>
  <completion_date type="Anticipated">August 2018</completion_date>
  <primary_completion_date type="Anticipated">August 2018</primary_completion_date>
  <phase>N/A</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <intervention_model>Single Group Assignment</intervention_model>
    <primary_purpose>Diagnostic</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Rate of patients with LV latency &gt;/= 40ms (stimulus to QRS onset) with respect to the total number of screened patients</measure>
    <time_frame>1 day</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Device-base measured LV latency in the LV IEGM channel and Far Field channel</measure>
    <time_frame>1 day</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>12-lead ECG-measured LV latency in the channel with earliest QRS onset</measure>
    <time_frame>1 day</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Device-based measured latency intervals (stimulus to electrogram peak and end) in the LV IEGM channel and the Far Field channel</measure>
    <time_frame>1 day</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>12-lead ECG-measured latency intervals</measure>
    <time_frame>1 day</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
