  <brief_title>A Study to Evaluate the Effect of BMS-955176 on Pharmacokinetics of Dolutegravir and the Effect of Dolutegravir on the Pharmacokinetics of BMS-955176</brief_title>
  <official_title>A Two-way Drug Interaction Study to Evaluate the Effect of BMS-955176 on the Pharmacokinetics of Dolutegravir and the Effect of Dolutegravir on the Pharmacokinetics of BMS-955176</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>ViiV Healthcare</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>GlaxoSmithKline</agency>
      <agency_class>Industry</agency_class>
    </collaborator>
  </sponsors>
