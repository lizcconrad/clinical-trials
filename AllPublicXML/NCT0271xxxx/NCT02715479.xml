  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to evaluate how BMS955176 affects pharmacokinetics (PK) of
      Dolutegravir (DTG) and also how DTG administration affects the PK of BMS955176
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>April 2016</start_date>
  <completion_date type="Actual">May 2016</completion_date>
  <primary_completion_date type="Actual">May 2016</primary_completion_date>
  <phase>Phase 1</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
