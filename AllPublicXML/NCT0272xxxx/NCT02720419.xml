  <brief_title>IRIS-Synergy Cohort in the IRIS-DES Registry</brief_title>
  <acronym>IRIS Synergy</acronym>
  <official_title>Evaluation of Effectiveness and Safety of Synergy™ Stent in Routine Clinical Practice; A Multicenter, Prospective Observational Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Jung-min Ahn</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
