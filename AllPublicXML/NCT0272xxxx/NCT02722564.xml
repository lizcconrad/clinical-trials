  <brief_title>Accuracy of Self-estimation of Blood Alcohol Concentration Compared to Object Values</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>St. Luke's Hospital and Health Network, Pennsylvania</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
