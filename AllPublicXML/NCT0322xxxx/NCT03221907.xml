    <intervention_name>GLA5PR GLARS-NF1 placebo</intervention_name>
    <description>tablet manufactured to mimic GLA5PR GLARS-NF1</description>
    <arm_group_label>GLA5PR GLARS-NF1 placebo &amp; Pregabalin</arm_group_label>
    <other_name>placebo (for GLA5PR GLARS-NF1)</other_name>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Pregabalin</intervention_name>
    <description>Pregabalin capsule</description>
    <arm_group_label>GLA5PR GLARS-NF1 placebo &amp; Pregabalin</arm_group_label>
    <other_name>Lyrica</other_name>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Pregabalin placebo</intervention_name>
    <description>tablet manufactured to mimic pregabalin capsule</description>
    <arm_group_label>GLA5PR GLARS-NF1 &amp; Pregabalin placebo</arm_group_label>
    <other_name>placebo (for pregablin)</other_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  both male and female who are over 19-year-old

          -  patient whose pain scale is 4~9

          -  patient who has pain of mononeuropathy

        Exclusion Criteria:

          -  patient whose CCr is &lt;60ml/min

          -  patient who has other pain which is not from mononeuropathy

          -  patient who is going to have an operation during this study

          -  patient who has abnormal ECG
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>19 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
