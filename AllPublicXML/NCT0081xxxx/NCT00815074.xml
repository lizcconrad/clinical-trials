  <brief_title>Military Women's Health and Illness Behaviors in Deployed Settings</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Wilford Hall Medical Center</agency>
      <agency_class>U.S. Fed</agency_class>
    </lead_sponsor>
  </sponsors>
