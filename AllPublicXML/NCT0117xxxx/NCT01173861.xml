  <intervention>
    <intervention_type>Behavioral</intervention_type>
    <intervention_name>Physical activity counseling</intervention_name>
    <description>20-30 minutes of face-to-face physical activity counseling using motivational interviewing followed by 3 20 minute telephone counseling sessions.</description>
    <arm_group_label>Physical activity counseling</arm_group_label>
    <arm_group_label>Manual</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
