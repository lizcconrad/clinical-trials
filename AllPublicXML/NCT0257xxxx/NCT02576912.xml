  <brief_title>Testing the Interactive Effects of Delta-9-Tetrahydrocannabinol and Pregnenolone</brief_title>
  <acronym>THC-Preg</acronym>
  <official_title>Testing the Interactive Effects of Delta-9-Tetrahydrocannabinol and Pregnenolone</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Yale University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
