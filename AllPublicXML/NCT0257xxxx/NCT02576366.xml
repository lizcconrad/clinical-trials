  <brief_title>Phenotypic Drug Probes as Predictors of Drug-drug Interactions With Tacrolimus</brief_title>
  <official_title>Phenotypic Drug Probes as Predictors of Drug-drug Interactions With Tacrolimus</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Universitaire Ziekenhuizen Leuven</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
