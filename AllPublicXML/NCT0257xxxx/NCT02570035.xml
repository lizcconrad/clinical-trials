  </study_design_info>
  <primary_outcome>
    <measure>Changes in electrocardiogram parameters pre- and post- administration of mirabegron</measure>
    <time_frame>Baseline and up to four weeks</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Incidence rate of cardiovascular system adverse reactions</measure>
    <time_frame>Up to four weeks</time_frame>
  </primary_outcome>
  <primary_outcome>
