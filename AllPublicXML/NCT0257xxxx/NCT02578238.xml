  <primary_outcome>
    <measure>Number of subjects with adverse events</measure>
    <time_frame>From Day 0 (informed consent) to up to 70 days following the last administration of Humira®</time_frame>
    <description>Adverse event information is collected to assess prevalence of safety.</description>
  </primary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Actual">147</enrollment>
  <condition>Pediatric Crohn's Disease</condition>
  <arm_group>
