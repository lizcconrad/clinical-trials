  <brief_title>Assessing Circulating Tumor Cell as a Biomarker for Prostate Cancer Detection in Patients With Gray Zone PSA Level</brief_title>
  <official_title>Assessing Circulating Tumor Cell as a Biomarker for Prostate Cancer Detection in Patients With Gray Zone PSA Level</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>CellMaxLife</agency>
      <agency_class>Industry</agency_class>
