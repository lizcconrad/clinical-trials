  <primary_outcome>
    <measure>Tmax of Venetoclax</measure>
    <time_frame>Approximately 29 days after first dose of study drug</time_frame>
    <description>Time to maximum plasma concentration (Tmax) of venetoclax.</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Recommended Phase 2 Dose (RPTD) of co-administered Dinaciclib and Venetoclax</measure>
    <time_frame>Minimum first cycle of dosing (21 days)</time_frame>
