  </study_design_info>
  <primary_outcome>
    <measure>The rate of skin delayed type hypersensitivity to metals among patients with restenosis and without restenosis</measure>
  </primary_outcome>
  <enrollment>150</enrollment>
  <condition>Coronary Stent Restenosis</condition>
  <intervention>
    <intervention_type>Procedure</intervention_type>
    <intervention_name>skin sensitivity test</intervention_name>
  </intervention>
