  <primary_outcome>
    <measure>Safety assessment of the drug</measure>
    <time_frame>12-18 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Efficacy (reduction of pelvic pain)</measure>
    <time_frame>12-18 months</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">168</enrollment>
