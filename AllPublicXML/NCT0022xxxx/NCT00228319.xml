  <brief_title>Treatment of Newly Diagnosed Ovarian Cancer With Antioxidants</brief_title>
  <official_title>Antioxidant Effects on the Outcome of Ovarian Cancer</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Jeanne Drisko, MD, CNS, FACN</agency>
      <agency_class>Other</agency_class>
