  <eligibility>
    <study_pop>
      <textblock>
        healthy volunteers
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  healthy subjects, non-smokers, physical fitness,

        Exclusion Criteria:

          -  ocular pathologies, ascent above 2000m 2 weeks prior to study and 2 weeks before
             post-examination
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>65 Years</maximum_age>
