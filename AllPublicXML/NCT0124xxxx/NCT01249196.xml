  </study_design_info>
  <primary_outcome>
    <measure>ADAS-cog (Alzheimer's Disease Assessment Scale-cognitive subscale)</measure>
    <time_frame>30 minutes</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>CIBIC-Plus (Clinician's Interview-Based Impression of Change-Plus caregiver input)</measure>
    <time_frame>45 minutes</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>MMSE (Mini Mental State Examination)</measure>
    <time_frame>10 minutes</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>CDR-SB (Clinical Dementia Rating Sum of Box)</measure>
    <time_frame>20 minutes</time_frame>
  </secondary_outcome>
