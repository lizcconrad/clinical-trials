  <brief_title>Trends in Breastfeeding Disparity in U.S. Infants by WIC Eligibility and Participation</brief_title>
  <acronym>WIC breastfeed</acronym>
  <official_title>Trends in Breastfeeding Disparity in U.S. Infants by WIC Eligibility and Participation: Evidence From NHANES 1999-2014</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Old Dominion University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
