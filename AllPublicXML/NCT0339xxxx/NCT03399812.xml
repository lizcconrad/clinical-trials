  <brief_title>Breakfast and Metabolism in Men</brief_title>
  <official_title>The Effect of Breakfast on Energy Metabolism, Appetite, and Food Intake in Men</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Arkansas, Fayetteville</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
