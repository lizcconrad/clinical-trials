  <primary_outcome>
    <measure>Cmax</measure>
    <time_frame>0~48hours after medication</time_frame>
    <description>Maximum Concentration of Fimasartan, Amlodipine, Hydrochlorothiazide in plasma</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>AUCt</measure>
    <time_frame>0~48hours after medication</time_frame>
    <description>AUCt(Area under the curve from the dosing time to the last measurable concentration) of Fimasartan, Amlodipine, Hydrochlorothiazide</description>
  </secondary_outcome>
