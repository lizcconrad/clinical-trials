  <brief_title>Clinical Evaluation of the Aptima® Combo 2® Assay Using the Panther® System in Female Urine Samples</brief_title>
  <official_title>Clinical Evaluation of the Aptima® Combo 2® Assay Using the Panther® System in Female Urine Samples</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Hologic, Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
