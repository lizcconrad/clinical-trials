  <eligibility>
    <study_pop>
      <textblock>
        patients with tandem lesion evaluated by FFR
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Tandem lesion evaluated by FFR

          -  Written consent

        Exclusion Criteria:

          -  TIMI flow 〈 3

          -  Grafted vessel

          -  Left ventricular ejection fraction 〈 30%

          -  Severe calcification and/or severe tortuosity

          -  Uncontrolled coronary spasm

          -  Life expectancy 〈 2 years

          -  Planned high risk surgery

          -  Pregnancy or breast-feeding
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>19 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
