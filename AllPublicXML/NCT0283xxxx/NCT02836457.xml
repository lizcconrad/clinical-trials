  <primary_outcome>
    <measure>The incidence of all treatment-related adverse events ( AE)</measure>
    <time_frame>52 Weeks</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>The incidence of serious adverse events (SAE)</measure>
    <time_frame>52 Weeks</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>The incidence of unexpected treatment-related adverse events</measure>
    <time_frame>52 Weeks</time_frame>
