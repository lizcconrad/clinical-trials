  <brief_summary>
    <textblock>
      Aim of this study is to investigate the effects of intake of sea buckthorn oil on mucous
      membranes
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>October 2012</start_date>
  <completion_date type="Actual">May 2013</completion_date>
