  <brief_title>Cognitive-behavioral Weight Loss Treatment</brief_title>
  <official_title>Effects of a Cognitive-behavioral Weight Loss Treatment</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>YMCA of Metropolitan Atlanta</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
