  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>comparing 2 drugs on their effect on the bowel preparation for colonoscopy</intervention_name>
    <description>the effects of the drug on the colon preparation for colonoscopy</description>
    <arm_group_label>miralax</arm_group_label>
    <arm_group_label>moviprep split dose</arm_group_label>
  </intervention>
