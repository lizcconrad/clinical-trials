  <primary_outcome>
    <measure>bowel prep efficacy</measure>
    <time_frame>8 months</time_frame>
    <description>To assess the bowel prep efficacy in cleansing the colon for colonoscopy</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>assess change in renal function pre and post bowel prep</measure>
    <time_frame>8 months</time_frame>
    <description>changes in electrolytes pre and post bowel preparation administration</description>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
