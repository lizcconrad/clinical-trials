  <primary_outcome>
    <measure>Change in Baseline Hemoglobin</measure>
    <time_frame>8 Weeks</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Change in Baseline Hemoglobin</measure>
    <time_frame>16 Weeks</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Change in Week 8 Hemoglobin</measure>
    <time_frame>16 Weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pharmacodynamic response as measured by actual values and change from baseline in hemoglobin, hematocrit, red blood cell count, and reticulocyte count.</measure>
    <time_frame>16 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of subjects that require transfusion and/or ESA (erythropoiesis stimulating agent) rescue.</measure>
    <time_frame>16 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Safety and tolerability measures to include assessments of adverse events, vital signs, electrocardiograms, and laboratory assay results.</measure>
    <time_frame>16 weeks of therapy and 4 weeks of follow-up</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Concentration measurements of investigational product and its metabolites pre- and post-dialysis.</measure>
    <time_frame>2 and16 weeks</time_frame>
  </secondary_outcome>
  <other_outcome>
    <measure>Iron metabolism to include actual values and change from baseline in iron indices.</measure>
    <time_frame>16 weeks</time_frame>
  </other_outcome>
  <other_outcome>
    <measure>Intravenous iron utilization.</measure>
    <time_frame>16 weeks</time_frame>
  </other_outcome>
