    <arm_group_label>Replenine®-VF</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Patients under six years of age at the time of entry with severe Haemophilia B at the
             time of diagnosis without inhibitors to FIX and requiring Factor IX therapy.

        Exclusion Criteria:
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>N/A</minimum_age>
    <maximum_age>6 Years</maximum_age>
