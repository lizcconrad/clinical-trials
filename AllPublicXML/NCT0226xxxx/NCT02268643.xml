  <brief_title>Ultrasound-based Breast Cancer Screening in Chinese Women</brief_title>
  <official_title>Ultrasound-based Breast Cancer Screening in Chinese Women</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Peking Union Medical College Hospital</agency>
