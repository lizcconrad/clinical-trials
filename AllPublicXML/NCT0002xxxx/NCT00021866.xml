  <brief_title>Developmental Effects On Children Of Women Who Take Antiepileptic Drugs During Pregnancy</brief_title>
  <official_title>Neurodevelopmental Effects of Antiepileptic Drugs II: the NEAD Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Emory University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
