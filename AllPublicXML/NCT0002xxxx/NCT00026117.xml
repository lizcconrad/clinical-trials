  <brief_title>Shark Cartilage in Treating Patients With Advanced Colorectal or Breast Cancer</brief_title>
  <official_title>Phase III Evaluation Of Benefin Shark Cartilage In Patients With Advanced Cancer</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alliance for Clinical Trials in Oncology</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
