  <brief_title>Screening Tests in Detecting Colorectal Cancer</brief_title>
  <official_title>Colorectal Cancer Screening: Fecal Blood vs. DNA</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alliance for Clinical Trials in Oncology</agency>
      <agency_class>Other</agency_class>
