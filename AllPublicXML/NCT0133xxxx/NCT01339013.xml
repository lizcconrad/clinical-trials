  <brief_title>Rebreathing of Carbon Dioxide With a Device Used for Giving Inhalational Anaesthesia</brief_title>
  <official_title>Dead Space Effect of an Anaesthesia Gas Reflector (AnaConDa)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Region Skane</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
