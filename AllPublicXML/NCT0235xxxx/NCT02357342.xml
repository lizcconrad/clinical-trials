  <brief_title>Sirolimus Versus Anti-Vascular Endothelial Growth Factor (antiVEGF) for Wet AMD</brief_title>
  <official_title>Sirolimus Versus AntiVEGF for Wet AMD</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Maturi, Raj K., M.D., P.C.</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
