  <brief_title>Safety and Efficacy of Zylet vs Lotemax, Tobramycin and Vehicle in Pediatric Blepharoconjunctivitis</brief_title>
  <official_title>Safety and Efficacy of Zylet vs Lotemax, Tobramycin and Vehicle in Pediatric Blepharoconjunctivitis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Bausch &amp; Lomb Incorporated</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
