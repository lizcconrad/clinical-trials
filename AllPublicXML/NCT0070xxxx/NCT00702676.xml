  <primary_outcome>
    <measure>Visual Analog Scale (VAS) 1 hour post dose at Day 1</measure>
    <time_frame>1 hour after dose administration at the first dosing day (i.e. Day 1) of each period</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Area under the VAS-time curve</measure>
    <time_frame>Calculated daily from the 13 assessments for 5 days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pharmacodynamic relationship between maximum VAS and PK concentration</measure>
    <time_frame>On last day of period (Day 5)</time_frame>
  </secondary_outcome>
