    <intervention_type>Drug</intervention_type>
    <intervention_name>Testim</intervention_name>
    <description>50 mg/dose7day</description>
    <arm_group_label>1</arm_group_label>
    <other_name>testosteron</other_name>
  </intervention>
  <intervention>
    <intervention_type>Behavioral</intervention_type>
    <intervention_name>Strength training</intervention_name>
    <description>three times a week</description>
    <arm_group_label>3</arm_group_label>
    <other_name>training</other_name>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>placebo</intervention_name>
    <description>placebo</description>
    <arm_group_label>2</arm_group_label>
    <other_name>gel</other_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Male 65-78

          -  Bioavailable testosterone &lt; 7.3 nmol/L

          -  Waist &gt; 94 cm

        Exclusion Criteria:

          -  Haematocrit &gt; 0.50

          -  Known c. prostate or PSA &gt; 3 Myg/L

          -  Known malignant disease

          -  Alcohol or drug abuse

          -  BMI &gt; 40 kg/m2

          -  Use of 5 alpha reductance inhibitors

          -  Plan to father children
      </textblock>
    </criteria>
    <gender>Male</gender>
    <minimum_age>65 Years</minimum_age>
    <maximum_age>78 Years</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
