    <masking>Single (Outcomes Assessor)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Equivalence in ongoing pregnancy per embryo transfer between the two groups</measure>
    <time_frame>week 5 after embryo tranfer</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Convenience for the patient</measure>
    <time_frame>the treatment period (from the day of embryo transfer and 19 days ahead)</time_frame>
  </secondary_outcome>
