  <brief_title>Brain Markers of Treatment Response in Post-Traumatic Stress Disorder (PTSD)</brief_title>
  <official_title>Neurofunctional Markers of SSRI Treatment Response in PTSD</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>VA Office of Research and Development</agency>
      <agency_class>U.S. Fed</agency_class>
    </lead_sponsor>
  </sponsors>
