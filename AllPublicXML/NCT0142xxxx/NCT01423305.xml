  <brief_title>Phase I Pharmacokinetic Study (BF-BLOCK)</brief_title>
  <acronym>BF-BLOCK</acronym>
  <official_title>Effect of Charcoal on Gastrointestinal Absorption of Budesonide and Formoterol</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Orion Corporation, Orion Pharma</agency>
      <agency_class>Industry</agency_class>
