  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Product 33525</intervention_name>
    <description>Daily dosing for 2 weeks each recurrence for patients with tinea pedis and Daily dosing for 1 week each recurrence for patients with tinea cruris and tinea corporis</description>
    <arm_group_label>Safety Population</arm_group_label>
  </intervention>
