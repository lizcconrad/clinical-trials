  <brief_title>Improved Outcomes Associated With Inositol Dietary Supplementation in Women With Gestational Diabetes Mellitus</brief_title>
  <acronym>inositol</acronym>
  <official_title>Improved Maternal and Fetal Outcomes Associated With Inositol Dietary Supplementation in Women With Early Gestational Diabetes Mellitus</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>G. d'Annunzio University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
