    <observational_model>Cohort</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>pharyngeal measurements</measure>
  </primary_outcome>
  <secondary_outcome>
    <measure>anatomic structures of the upper airway</measure>
    <description>distances, volumes and cross sectional areas of the tongue, soft palat, pharyngeal structures as well as oral airspace</description>
  </secondary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Actual">20</enrollment>
