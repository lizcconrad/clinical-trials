  <brief_title>Electromyographic (EMG) on the Anaesthesia Monitor</brief_title>
  <official_title>The Appearance of Electromyography on the Anaesthesia Monitor.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Tampere University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
