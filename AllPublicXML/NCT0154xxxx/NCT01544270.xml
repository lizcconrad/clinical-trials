    <primary_purpose>Basic Science</primary_purpose>
    <masking>Quadruple (Participant, Care Provider, Investigator, Outcomes Assessor)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Subjective well-being</measure>
    <time_frame>baseline and 4 weeks</time_frame>
    <description>Change in stress and mood states measured by State-trait anxiety inventory (STAI) and modified Profile of Mood States (POMS) at baseline and after 4 wk intervention</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Physiological measurements</measure>
    <time_frame>baseline and 4 weeks</time_frame>
    <description>Change in salivary cortisol, blood pressure, heart beat variability, sleep and inflammatory markers.</description>
  </secondary_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment type="Actual">101</enrollment>
  <condition>Chronic Stress</condition>
  <arm_group>
    <arm_group_label>Study product containing milk proteins</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>Yoghurt-like milk-based product</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Study product containing probiotics</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>Yoghurt-like milk-based product</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Control product</arm_group_label>
    <arm_group_type>Placebo Comparator</arm_group_type>
    <description>Yoghurt-like milk-based product without supplemented nutrients</description>
  </arm_group>
  <intervention>
    <intervention_type>Dietary Supplement</intervention_type>
    <intervention_name>Yogurt-like snacks</intervention_name>
    <description>twice a day 120 g for four weeks</description>
    <arm_group_label>Study product containing milk proteins</arm_group_label>
    <arm_group_label>Study product containing probiotics</arm_group_label>
    <arm_group_label>Control product</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  STAI points 40-65

          -  BMI 18-30 kg/m2

          -  healthy

        Exclusion Criteria:

          -  Gastrointestinal disease, diabetes or any other significant major medical morbidity

          -  milk allergy

          -  smoking

          -  excess use of alcohol

          -  pregnancy and breast feeding

          -  waist circumference males &gt;102 cm, females &gt;88 cm

          -  no medication or dietary supplements influencing measured responses
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>65 Years</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
