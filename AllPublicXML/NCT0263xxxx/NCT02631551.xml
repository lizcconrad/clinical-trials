  <primary_outcome>
    <measure>Change in average AM and PM subject-reported 12-hour reflective Total Nasal Symptoms Score (rTNSS) from baseline to end of treatment.</measure>
    <time_frame>14 days</time_frame>
    <description>Average AM and PM scores</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change from baseline in average AM and PM subject-reported 12-hour instantaneous Total Nasal Symptoms Score (iTNSS)</measure>
    <time_frame>14 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change from baseline in 12-hour reflective Total Ocular Symptom Score (rTOSS)</measure>
    <time_frame>14 days</time_frame>
  </secondary_outcome>
