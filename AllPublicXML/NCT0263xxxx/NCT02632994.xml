  <brief_summary>
    <textblock>
      This study is for patients who have participated in a previous study and who continue to
      receive benefit to have continued access to study drug and/or treatment.
    </textblock>
  </brief_summary>
  <overall_status>Available</overall_status>
  <study_type>Expanded Access</study_type>
  <condition>Cancer</condition>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>LY2503029</intervention_name>
    <description>Administered orally.</description>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  To be eligible patients must be currently receiving benefit in a Lilly study that has
             met its endpoints or has otherwise stopped.

        Exclusion Criteria:

          -  Patients must not be concurrently enrolled in any other type of medical research
             judged not to be scientifically or medically compatible with this study.
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
  </eligibility>
