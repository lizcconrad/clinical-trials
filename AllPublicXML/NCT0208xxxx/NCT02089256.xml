  <brief_title>Effect of Repeated Administration of Liraglutide on Insulinogenic Indices</brief_title>
  <official_title>Effect of Repeated Administration of Liraglutide on Insulinogenic Indices</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Tartu</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
