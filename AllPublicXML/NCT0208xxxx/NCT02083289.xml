  </study_design_info>
  <primary_outcome>
    <measure>Mean change in diurnal IOP (average of four time points) from baseline</measure>
    <time_frame>28 days</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Evaluating the safety parameters per protocol</measure>
    <time_frame>28 days</time_frame>
