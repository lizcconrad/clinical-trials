  <brief_title>AGN-229666 Ophthalmic Solution for the Treatment of Seasonal or Perennial Allergic Conjunctivitis</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Allergan</agency>
      <agency_class>Industry</agency_class>
