  <primary_outcome>
    <measure>Area under the plasma concentration versus time curve (AUC) of tralokinumab</measure>
    <time_frame>Day 1,2,4,6,8,10,15,22,36,57</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pain intensity, measured using 100mm Visual Analogue Scale (VAS)</measure>
    <time_frame>Day 1 to 57</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of injection site reactions</measure>
    <time_frame>Day 1 to 57</time_frame>
  </secondary_outcome>
