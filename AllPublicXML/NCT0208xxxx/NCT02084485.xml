  <primary_outcome>
    <measure>Sputum Cell Counts</measure>
    <time_frame>6 hours post LPS challenge</time_frame>
    <description>Neutrophil &amp; Macrophage count (total and differential)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Sputum Cytokines</measure>
    <time_frame>6 hours post LPS challenge</time_frame>
    <description>TNF-α, MIP-1β, IL-6, MPO, MCP-1, GRO-α</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Systemic Inflammatory Indices</measure>
    <time_frame>1, 4, 6 and 7 hours post LPS</time_frame>
    <description>Neutrophil count, GRO-α, TNF-α, MIP-1β, IL-6, MPO, MCP-1, CRP</description>
  </secondary_outcome>
