  <brief_title>The Effects of L-arabinose in a Meal in Healthy Men</brief_title>
  <official_title>The Effects of L-arabinose in a Meal on Glucose Metabolism in Healthy Men</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Copenhagen</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
