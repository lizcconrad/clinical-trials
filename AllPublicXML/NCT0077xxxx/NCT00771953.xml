          <group group_id="O1">
            <title>Apricoxib Plus Docetaxel</title>
            <description>Apricoxib 400mg qd plus docetaxel 75mg/m2 q21 days</description>
          </group>
          <group group_id="O2">
            <title>Placebo Plus Docetaxel</title>
            <description>Placebo plus docetaxel 75mg/m2 q21 days</description>
          </group>
          <group group_id="O3">
            <title>Apricoxib Plus Pemetrexed</title>
            <description>Apricoxib 400mg qd plus pemetrexed 500mg/m2 q21 days</description>
          </group>
