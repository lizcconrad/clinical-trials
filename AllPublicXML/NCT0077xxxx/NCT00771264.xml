  <brief_title>Study of Urgent PC Versus Sham Effectiveness in Treatment of Overactive Bladder Symptoms</brief_title>
  <acronym>SUmiT</acronym>
  <sponsors>
    <lead_sponsor>
      <agency>Uroplasty, Inc</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
