    <observational_model>Case Control</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Sensory function (Semmens-Weinstein), motor function (Abductor Pollicis Brevis (APB)-strength, grip strength)</measure>
    <time_frame>Baseline, 6 weeks, 12 weeks, 52 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pillar pain, cold intolerance, and patient satisfaction.</measure>
    <time_frame>Baseline, 6 weeks, 12 weeks, 52 weeks</time_frame>
  </secondary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Actual">67</enrollment>
