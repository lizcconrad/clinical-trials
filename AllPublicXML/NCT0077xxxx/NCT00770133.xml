  <primary_outcome>
    <measure>Ocular itching</measure>
    <time_frame>3, 5 and 7 minutes post-challenge</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Conjunctival redness</measure>
    <time_frame>7, 15 and 20 minutes post-challenge</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Ciliary and episcleral redness, chemosis, lid swelling, tearing and ocular mucus discharge.</measure>
    <time_frame>7, 15 and 20 minutes post challenge</time_frame>
  </secondary_outcome>
