  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to determine if low level magnetic fields may help to relieve
      symptoms of Parkinson's disease.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>August 2007</start_date>
  <completion_date type="Actual">June 2009</completion_date>
  <primary_completion_date type="Actual">June 2009</primary_completion_date>
  <phase>Phase 2</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
