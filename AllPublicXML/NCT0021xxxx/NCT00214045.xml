  <brief_title>Rigid Versus Flexible Cystoscopy in Women</brief_title>
  <official_title>Rigid Versus Flexible Cystoscopy in Women in Outpatient Clinic</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Wisconsin, Madison</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
