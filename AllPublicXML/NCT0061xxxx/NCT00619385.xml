  <brief_title>A Safety and Pharmacokinetic Study of Proellex®</brief_title>
  <acronym>PK</acronym>
  <official_title>A Safety and Pharmacokinetic Study of 100 mg, 150 mg and 200 mg of Proellex® Taken for Seven Days by Healthy Adult Female Subjects</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Repros Therapeutics Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
