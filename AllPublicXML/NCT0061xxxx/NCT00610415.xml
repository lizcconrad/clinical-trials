  <brief_title>Biopsy of Human Tumors for Cancer Stem Cell Characterization: a Feasibility Study</brief_title>
  <official_title>Biopsy of Human Tumors for Cancer Stem Cell Characterization: a Feasibility Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Stanford University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Stanford University</source>
  <brief_summary>
    <textblock>
      To see if a limited sampling of tumor tissue from human subjects is a feasible way to gather
      adequate tissue for cancer stem cell quantification.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>December 2007</start_date>
  <primary_completion_date type="Actual">October 2012</primary_completion_date>
