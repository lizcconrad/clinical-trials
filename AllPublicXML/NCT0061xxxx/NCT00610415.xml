  </study_design_info>
  <primary_outcome>
    <measure>Quantification of the cancer stem cell population by flow cytometry</measure>
    <time_frame>1 month</time_frame>
  </primary_outcome>
  <enrollment type="Actual">40</enrollment>
  <condition>Head and Neck Cancer</condition>
  <condition>Head and Neck Cancers</condition>
  <intervention>
    <intervention_type>Procedure</intervention_type>
    <intervention_name>tumor biopsy</intervention_name>
  </intervention>
