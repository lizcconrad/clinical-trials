  <enrollment type="Anticipated">150</enrollment>
  <condition>Healthy</condition>
  <arm_group>
    <arm_group_label>1</arm_group_label>
    <description>women without contraction at 24-34 weeks of gestation</description>
  </arm_group>
  <arm_group>
    <arm_group_label>2</arm_group_label>
    <description>women without contractions between 24-34 weeks of gestation with a history of preterm labor</description>
  </arm_group>
  <arm_group>
    <arm_group_label>3</arm_group_label>
    <description>women with preterm contractions 24-34 weeks of gestation</description>
  </arm_group>
