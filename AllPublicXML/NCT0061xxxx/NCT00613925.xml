  <brief_title>Endometrial Biopsy Instrument Comparison Study</brief_title>
  <official_title>Comparison of Pain Experienced by Patients Undergoing Endometrial Biopsy by Pipelle and Explora Curette</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Oregon Health and Science University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
