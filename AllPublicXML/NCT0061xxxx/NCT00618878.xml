    <arm_group_label>2</arm_group_label>
    <other_name>USA Laser</other_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  people with chronic neck pain, participants with a VAS level between 3 and 10 will be
             included in the study

        Exclusion Criteria:

          -  history of cancer, tuberculosis, pacemaker usage, pregnancy, hemophilia,
             cardiovascular disease, osteoporosis
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>70 Years</maximum_age>
