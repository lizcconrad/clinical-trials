  <brief_title>Comparison of NICOM and Innocor for Non-Invasive Determination of Cardiac Output</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Universitätsmedizin Mannheim</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
