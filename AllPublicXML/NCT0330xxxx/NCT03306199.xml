  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Cohort</observational_model>
    <time_perspective>Retrospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Freedom from target lesion revascularization (TLR)</measure>
    <time_frame>1 year</time_frame>
  </primary_outcome>
  <enrollment type="Anticipated">90</enrollment>
  <condition>Peripheral Arterial Disease</condition>
  <intervention>
    <intervention_type>Device</intervention_type>
    <intervention_name>Laser Atherectomy</intervention_name>
    <description>The Turbo-Power is a laser atherectomy catheter designed for treatment of de novo or restenotic lesions in native infrainguinal arteries and for the treatment of femoropopliteal artery in-stent restenosis (ISR) in bare nitinol stents with adjunctive percutaneous transluminal angioplasty (PTA).</description>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        Patients are recruited from the investigator's general patient population, and they must
        meet the inclusion/exclusion criteria.
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  All patients treated for femoropopliteal de novo/restenotic lesions or in-stent
             restenosis using Turbo Power laser atherectomy plus drug coated balloon angioplasty

        Exclusion Criteria:

          -  Any incomplete data on procedural approach and treatment
      </textblock>
    </criteria>
    <gender>All</gender>
