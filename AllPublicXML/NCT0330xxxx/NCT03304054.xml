      <agency>Catalyst Pharmaceuticals, Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Catalyst Pharmaceuticals, Inc.</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
