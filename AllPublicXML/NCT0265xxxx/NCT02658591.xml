  <brief_title>Effects of Faba Bean Fractions as Ingredients in Novel Food Products on Glycemia, Appetite and Metabolic Control</brief_title>
  <official_title>Effects of Faba Bean Fractions as Ingredients in Novel Food Products on Glycemia, Appetite and Metabolic Control</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Toronto</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
