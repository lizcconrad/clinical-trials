  </study_design_info>
  <primary_outcome>
    <measure>Cutout rate</measure>
    <time_frame>3 months after intervention</time_frame>
    <description>Differences in the Cutout rate of the femoral head screw with/without U-Blade antirational screw</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Mobility</measure>
    <time_frame>3 months after intervention</time_frame>
    <description>Mobility of the patients 3 months after intervention measured with the Parker mobility score</description>
  </secondary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Actual">135</enrollment>
