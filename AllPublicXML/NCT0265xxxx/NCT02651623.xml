    <intervention_model>Crossover Assignment</intervention_model>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Postdose QTcF (Fridericia's correction) intervals</measure>
    <time_frame>0 to 72 hours</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Averse events, vital signs, physical examinations and abnormal laboratory for safety assessments (safety and tolerability)</measure>
    <time_frame>Through study completion, an average of 3 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Relationship between QTc prolongation and measured sertraline/metabolite plasma concentrations</measure>
    <time_frame>0 to 72 hours</time_frame>
  </secondary_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment type="Actual">54</enrollment>
  <condition>Healthy</condition>
