  <brief_title>Nasal Naloxone for Narcotic Overdose</brief_title>
  <official_title>Nasal Naloxone for Narcotic Overdose</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Judith Feinberg</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
