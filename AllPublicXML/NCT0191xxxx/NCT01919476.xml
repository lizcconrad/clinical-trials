  <brief_title>Postprandial Response to Almond Consumption in Overweight Hispanic Pregnant Women</brief_title>
  <official_title>Postprandial Response to Almond Consumption in Overweight Hispanic Pregnant Women</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Children's Hospital &amp; Research Center Oakland</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
