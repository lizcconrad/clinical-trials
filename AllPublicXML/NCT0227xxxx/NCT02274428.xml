  <primary_outcome>
    <measure>unsuspected death or anaphylactic shock</measure>
    <time_frame>within 6 h after pneumostem transplantation</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Death or hydrocephalus required shunt operation</measure>
    <time_frame>First discharge home, maximum time frame : 1 year</time_frame>
    <description>outcome measurement time frame would not exceed 1 year after birth</description>
  </secondary_outcome>
