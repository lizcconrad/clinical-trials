  <brief_title>Neurophysiological Assessment of Healthy and Impaired Human Lower Urinary Tract Function</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ulrich Mehnert</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
