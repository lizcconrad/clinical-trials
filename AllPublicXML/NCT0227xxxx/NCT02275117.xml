    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Change in migraine days from baseline to week 12.</measure>
    <time_frame>12 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Evaluate safety of ALD403: laboratory variables, ECG and adverse events</measure>
    <time_frame>49 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Cmax - Peak plasma concentration of ALD403</measure>
    <time_frame>49 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Tmax - Time to achieve peak plasma concentration of ALD403</measure>
    <time_frame>49 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>AUC - Area under the plasma concentration vs.time curve of ALD403</measure>
    <time_frame>49 weeks</time_frame>
  </secondary_outcome>
