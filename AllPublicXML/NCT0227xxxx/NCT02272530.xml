    <description>Determination with reductive chemiluminescence</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Nitric oxid and reactive oxygen species</measure>
    <time_frame>baseline</time_frame>
    <description>Determination with fluorescence coloring in flow cytometry and fluorescence microscopy</description>
  </secondary_outcome>
