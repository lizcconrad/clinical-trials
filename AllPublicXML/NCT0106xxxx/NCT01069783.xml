    <primary_purpose>Treatment</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Change from baseline in low-density lipoprotein (LDL) cholesterol</measure>
    <time_frame>eight weeks</time_frame>
  </primary_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment type="Actual">36</enrollment>
