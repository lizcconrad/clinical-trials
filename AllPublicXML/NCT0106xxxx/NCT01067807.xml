  <brief_title>Proellex Pharmacokinetics Bridging Study</brief_title>
  <official_title>Proellex Pharmacokinetics Bridging Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Repros Therapeutics Inc.</agency>
