  <primary_outcome>
    <measure>Proellex peak plasma concentration (Cmax)</measure>
    <time_frame>24 hours</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Proellex time to peak plasma concentration (tmax).</measure>
    <time_frame>24 hours</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>The area under the plasma concentration over time curve from zero to the last measurable Proellex plasma concentration (AUC0-24) was calculated.</measure>
    <time_frame>24 hours</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>The area under the plasma concentration over time curve from zero to infinity (AUC 0-infinity).</measure>
