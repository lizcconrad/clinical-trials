  <brief_title>Evaluation of an Investigational Lubricant Eye Drop on Lipid Layer Thickness</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alcon Research</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
