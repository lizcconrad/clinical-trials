  <brief_title>Rabies Immune Plasma Booster Study</brief_title>
  <official_title>Rabies Immune Plasma Booster Program - A Clinical Study Conducted as an Investigational New Drug Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>CSL Behring</agency>
      <agency_class>Industry</agency_class>
