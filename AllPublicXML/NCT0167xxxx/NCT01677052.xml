  <brief_title>THERMOCOOL® SMARTTOUCH™ Registry</brief_title>
  <official_title>THERMOCOOL® SMARTTOUCH™ Catheter for the Treatment of Symptomatic Atrial Fibrillation- Registry</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Biosense Webster, Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
