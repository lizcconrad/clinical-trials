  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>SGN-75</intervention_name>
    <description>1-2 mg/kg IV every 21 days</description>
    <arm_group_label>Arm 1</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>everolimus</intervention_name>
    <description>10 mg PO daily</description>
    <arm_group_label>Arm 1</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Diagnosis of metastatic renal cell carcinoma with confirmed CD70 expression

          -  Previously treated with at least 1 tyrosine kinase inhibitor (TKI)

          -  Measurable disease

          -  Eastern Cooperative Oncology Group performance status 0 or 1

          -  Adequate lung and renal function

        Exclusion Criteria:

          -  Prior treatment with anti-CD70-directed therapy

          -  Received more than one prior treatment with an mTOR inhibitor
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
