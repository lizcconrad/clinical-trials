    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>PK AUC determination of Lu AA21004 (Vortioxetine)</measure>
    <time_frame>11 days</time_frame>
    <description>Area under the curve (AUC)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>PK Cmax determination of Lu AA21004 (Vortioxetine)</measure>
    <time_frame>11 days</time_frame>
    <description>Maximum observed concentration (Cmax)</description>
  </primary_outcome>
  <other_outcome>
    <measure>PK MR determination of metabolites Lu AA34443 and Lu AA39835</measure>
    <time_frame>11 days</time_frame>
    <description>Metabolic ratio (MR)</description>
  </other_outcome>
  <other_outcome>
    <measure>Number of patients with adverse events as a measure of safety and tolerability</measure>
    <time_frame>11 days</time_frame>
  </other_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">16</enrollment>
  <condition>Healthy</condition>
