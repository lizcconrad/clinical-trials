  <brief_title>Protein Source, Nutrition Messaging, and Food Intake</brief_title>
  <official_title>Evaluating How Breakfast Protein Source Affects Satiety, Glucose and Food Preference</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Arkansas, Fayetteville</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
