  <brief_title>Standard PET/CT vs. Digital PET/CT</brief_title>
  <official_title>Comparison of Standard PET/CT vs. Digital PET/CT</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Stanford University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
