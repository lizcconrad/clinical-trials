  <brief_title>Comparison of Clinical Outcomes of Toric Intraocular Lens</brief_title>
  <official_title>Comparison of Clinical Outcomes of Toric Intraocular Lens</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Samsung Medical Center</agency>
