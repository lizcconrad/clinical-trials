  <brief_title>Retrospective Study of Reoperation After Primary Augmentation With NATRELLE® INSPIRA® Breast Implants</brief_title>
  <acronym>RANBI-I</acronym>
  <sponsors>
    <lead_sponsor>
      <agency>Allergan</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
