  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      To evaluate the effect of idalopirdine (120 and 360 mg) on cardiac repolarisation in healthy
      men.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>May 2015</start_date>
  <primary_completion_date type="Actual">August 2015</primary_completion_date>
