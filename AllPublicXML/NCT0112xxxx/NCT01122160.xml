  <brief_title>Gastric pH and Anthocyanin Absorption</brief_title>
  <official_title>Gastric pH as a Possible Determinant of Anthocyanin Absorption</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>USDA Beltsville Human Nutrition Research Center</agency>
      <agency_class>U.S. Fed</agency_class>
    </lead_sponsor>
