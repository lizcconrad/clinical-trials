  <brief_title>Comparison of Fenofibrate and Niacin in Patients With Hypertriglyceridemia and Low High-Density Lipoprotein (HDL)-Cholesterol</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Yonsei University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
