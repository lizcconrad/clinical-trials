  <primary_outcome>
    <measure>Safety and tolerability as measured by the frequency of drug-related clinical adverse events.</measure>
    <time_frame>Up to 14 days after infusion of CSL112</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Safety and tolerability as measured by liver function tests.</measure>
    <time_frame>Up to 14 days after infusion of CSL112</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetics of lipoprotein.</measure>
    <time_frame>Up to 10 days after infusion of CSL112</time_frame>
    <description>Plasma levels of lipoprotein.</description>
  </secondary_outcome>
