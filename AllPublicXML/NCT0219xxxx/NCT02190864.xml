  <brief_title>Patient-Defined Treatment Success and Preferences in Stage IV Lung Cancer Patients</brief_title>
  <official_title>Patient-Defined Treatment Success and Preferences in Stage IV Lung Cancer Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Nebraska</agency>
      <agency_class>Other</agency_class>
