    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Change in pain level during kayakergometer test</measure>
    <time_frame>participants will be followed for the duration of kayakergometer test, an expected average of 30 minutes</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>change in the time of appearance of the symptoms during kayakergometer test.</measure>
    <time_frame>participants will be followed for the duration of kayakergometer test, an expected average of 30 minutes</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
