  <brief_title>Pharmacokinetic Evaluation of EXPAREL in Adults Undergoing Tonsillectomy</brief_title>
  <official_title>Pharmacokinetic Evaluation of EXPAREL in Adults Undergoing Tonsillectomy</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Pacira Pharmaceuticals, Inc</agency>
