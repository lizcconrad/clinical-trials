  <brief_title>The WEB-IT Clinical Study</brief_title>
  <acronym>WEB-IT</acronym>
  <official_title>The WEB® Intrasaccular Therapy Study (WEB-IT)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Sequent Medical, Inc</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
