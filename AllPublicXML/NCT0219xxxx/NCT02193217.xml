  <brief_title>A Phase 1 Study to Explore the Cardiac Pharmacodynamics of MT-1303</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mitsubishi Tanabe Pharma Corporation</agency>
      <agency_class>Industry</agency_class>
