  <brief_title>Effect of Eplerenone on Postprandial Inflammatory Response in Healthy Adults</brief_title>
  <official_title>Effect of Eplerenone on Postprandial Inflammatory Response in Healthy Adults</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Brigham and Women's Hospital</agency>
