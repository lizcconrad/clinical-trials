  <brief_title>Influence of Exercise on Anemia in MDS Patients</brief_title>
  <official_title>The Influence of Exercise on Anemia in MDS Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Rochester</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
