  </study_design_info>
  <primary_outcome>
    <measure>Plasma concentrations of parent and metabolites</measure>
    <time_frame>8 Days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Safety and tolerability</measure>
    <time_frame>8 days</time_frame>
    <description>Assessments to include Physical Examination (PE), Labs, ECGs, and Adverse Events (AEs)</description>
  </secondary_outcome>
