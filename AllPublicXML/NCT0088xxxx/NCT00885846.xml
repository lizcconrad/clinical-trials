  <brief_title>Effects of Qigong on Type 2 Diabetic Patients</brief_title>
  <official_title>A Pilot Study: The Effects of Qigong Therapy on Type 2 Diabetic Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Bastyr University</agency>
      <agency_class>Other</agency_class>
