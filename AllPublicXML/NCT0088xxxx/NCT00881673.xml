  <primary_outcome>
    <measure>The primary efficacy variable is mean change in tear eosinophil level at Visit 7 post-CAC from Visit 2 post-CAC.</measure>
    <time_frame>15 days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Mean change in relative ratio of tear eosinophil and neutrophils at Visit 7 post-CAC from Visit 2 post-CAC</measure>
    <time_frame>15 days</time_frame>
  </secondary_outcome>
