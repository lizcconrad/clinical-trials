    <primary_purpose>Treatment</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Safety: incidence of adverse events</measure>
    <time_frame>from subject enrollment to study discharge</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>PK parameters: Cmax, Tmax, AUC(0-24h), AUC(INF) and T-HALF</measure>
    <time_frame>from pre-dose to 24 hrs post-dose</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>PD Measures: Fasting and postprandial serum glucose (AUC), serum insulin, and plasma glucagon concentrations. Acetaminophen plasma concentrations will be measured after a single dose of acetaminophen</measure>
    <time_frame>from pre-dose to 9 hrs post-dose</time_frame>
  </secondary_outcome>
  <number_of_arms>3</number_of_arms>
