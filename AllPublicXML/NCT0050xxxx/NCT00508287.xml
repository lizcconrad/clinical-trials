    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to evaluate the safety, pharmacokinetics and pharmacodynamics of
      single doses of BMS-686117
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>August 2007</start_date>
  <completion_date type="Actual">November 2007</completion_date>
  <primary_completion_date type="Actual">November 2007</primary_completion_date>
  <phase>Phase 1</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
