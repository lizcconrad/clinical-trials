  <brief_title>Symbicort Turbuhaler 30/60 Specific Clinical Experience Investigation</brief_title>
  <acronym>ENSURE</acronym>
  <official_title>Symbicort Turbuhaler 30/60 Specific Clinical Experience Investigation for Long-term Use</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AstraZeneca</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
