  <brief_title>Efficacy of a Propolis-based Denture Cleanser</brief_title>
  <official_title>Efficacy of a Propolis Solution for Cleaning Complete Dentures: a Randomized Crossover Trial</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Raphael Freitas de Souza</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
