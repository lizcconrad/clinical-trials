  <brief_title>Intubation of Morbidly Obese Patients. A Clinical Trial, Comparing Glide Scope ® With Fastrach TM</brief_title>
  <official_title>Intubation of Morbidly Obese Patients. A Randomized Clinical Trial, Comparing GlideScope ® With Fastrach TM.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Glostrup University Hospital, Copenhagen</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
