  <brief_title>Flaxseed Mucilage and Satiety</brief_title>
  <acronym>MUCISAT</acronym>
  <official_title>Effect of Flaxseed Mucilage Drinks and Tablets on Satiety and Energy Intake</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Copenhagen</agency>
      <agency_class>Other</agency_class>
