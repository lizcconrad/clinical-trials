    <masking>Triple (Participant, Care Provider, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Assess number of participants with adverse events as a measure of safety and tolerability</measure>
    <time_frame>312 hours postdose</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Assess pharmacokinetics to determine study drug half-life, maximum concentration time, elimination time, and area under the curve</measure>
    <time_frame>Predose to 312 hours postdose</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Assess pharmacodynamics to determine study drug levels to signal pJAK2, pSTAT3, and pSTAT5</measure>
    <time_frame>Predose, 6 and 24 hours postdose</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Assess food effects on pharmacokinetics</measure>
    <time_frame>Predose to 312 hours postdose</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Determine recommended dose</measure>
    <time_frame>March 2011</time_frame>
  </secondary_outcome>
