  <brief_title>Esmolol Pretreatment on Pain During Injection of Propofol</brief_title>
  <official_title>Esmolol Pretreatment on Pain During Injection of Propofol</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Kyungpook National University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
