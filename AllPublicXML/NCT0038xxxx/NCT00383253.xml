  <brief_summary>
    <textblock>
      Methods study to characterize early indicators of weight loss in people.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>October 2006</start_date>
  <completion_date type="Actual">March 2008</completion_date>
