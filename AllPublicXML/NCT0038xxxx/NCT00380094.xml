  </study_design_info>
  <enrollment type="Anticipated">1500</enrollment>
  <condition>Acute Coronary Syndrome</condition>
  <condition>Myocardial Infarction</condition>
  <eligibility>
    <study_pop>
      <textblock>
        Patients presenting with chest pain and ACE in differential diagnosis.
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Chest pain and ACS in differential diagnosis

          -  Age&gt; 18 years

          -  Ability to give informed consent

        Exclusion Criteria:

          -  Trauma

          -  Vulnerable populations

          -  Known inflammatory condition
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>85 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
