    <arm_group_label>Epratuzumab</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>Placebo</intervention_name>
    <description>Intravenous</description>
    <arm_group_label>Placebo</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  +ANA at screening

          -  BILAG Index B Level Activity in at least 2 body systems/organs

          -  Has SLE by ACR revised criteria (meets,&lt;4 criteria)

        Exclusion Criteria:

          -  Active Severe Lupus as defined by BILAG Index Level A in any body system or organ.

          -  Allergy to human antibodies or Murine.
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
