  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>phenoxodiol</intervention_name>
    <description>400mg phenoxodiol three times daily in 28 day cycles.</description>
    <arm_group_label>1</arm_group_label>
  </intervention>
  <intervention>
