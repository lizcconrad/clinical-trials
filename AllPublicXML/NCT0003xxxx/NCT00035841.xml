  <brief_summary>
    <textblock>
      The purpose of this study is to determine the effectiveness of TLK286 in treatment of
      metastatic breast cancer.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>April 2002</start_date>
  <completion_date type="Actual">July 2004</completion_date>
