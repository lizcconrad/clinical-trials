  <brief_title>Observational Study To Evaluate Statins Patterns Of Use In Spain</brief_title>
  <official_title>Statins Use Patterns in Spain</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AstraZeneca</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>Fundació Institut Català de Farmacologia</agency>
