    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>safety and tolerability of single doses of MK8266 measured by number of clinical and laboratory adverse experiences</measure>
    <time_frame>14 days after administration of last dose of study drug</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Change in Aortic Augmentation index</measure>
    <time_frame>predose, 1, 3, 6, 12 and 24 hours postdose</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>change in heart rate</measure>
    <time_frame>predose through 12 hours postdose</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">25</enrollment>
  <condition>Hypertension</condition>
