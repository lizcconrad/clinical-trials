  <primary_outcome>
    <measure>To compare the effect of phenylephrine hydrochloride 10mg + acetaminophen 500mg + dimethindene maleate 1mg and phenylephrine hydrochloride 10mg alone on vital signs</measure>
    <time_frame>15 days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Electrocardiographic (ECG) monitoring and evaluation of heart rhythm following dosing</measure>
    <time_frame>15 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>To report and evaluate adverse events</measure>
    <time_frame>15 days</time_frame>
  </secondary_outcome>
