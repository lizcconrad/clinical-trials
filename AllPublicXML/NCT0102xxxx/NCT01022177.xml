  <brief_title>Influence of Acupuncture to Ocular Microcirculation in Patients With Diabetes Mellitus</brief_title>
  <official_title>Influence of Acupuncture to Ocular Microcirculation in Patients With Diabetes Mellitus</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Technische Universität Dresden</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
