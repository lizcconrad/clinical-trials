    <masking>Single (Outcomes Assessor)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Constant total score as a mean and by category</measure>
    <time_frame>at 3 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Constant total score</measure>
    <time_frame>at 12 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Constant subscores for pain, activity, mobility and strength, and self-assessed disability on a 0- to 100-point visual analog scale, and analgesic and nonsteroidal anti-inflammatory drug (NSAID) use</measure>
    <time_frame>at 3 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Constant subscores for pain, activity, mobility and strength, and self-assessed disability on a 0- to 100-point visual analog scale, and analgesic and nonsteroidal anti-inflammatory drug (NSAID) use</measure>
    <time_frame>at 12 months</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
