  <brief_title>Comparative Effectiveness Feasibility Trial for Insomnia Among Breast Cancer Survivors</brief_title>
  <official_title>Comparative Effectiveness Feasibility Trial for Insomnia Among Breast Cancer Survivors</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>George Washington University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
