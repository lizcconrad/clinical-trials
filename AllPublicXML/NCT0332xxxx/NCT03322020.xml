  <brief_title>Androgen Deprivation Therapy, External Beam Radiotherapy and Stereotactic Radiosurgery Boost for Prostate Cancer</brief_title>
  <acronym>ADEBAR</acronym>
  <official_title>Androgen Deprivation Therapy, External Beam Radiotherapy and Stereotactic Radiosurgery Boost for Men With Intermediate- or High-risk Prostate Cancer</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Asan Medical Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
