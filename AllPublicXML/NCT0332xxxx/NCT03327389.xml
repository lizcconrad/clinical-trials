  </secondary_outcome>
  <secondary_outcome>
    <measure>Neutrophil gelatinase-associated lipocalin (NGAL)</measure>
    <time_frame>maximum 30 days before surgery, 1 hour after surgery, 1 day, 2 day, 3 day, 5 day, 7 day after surgery</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>urine output</measure>
    <time_frame>during surgery, 1 day, 2 day, 3 day, 4 day, 5 day, 6 day, 7 day after surgery</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>fluid intake(intraoperative intake and output measured by the amount of fluid(crystalloid/colloid)(mL) and blood administered(mL))</measure>
    <time_frame>during surgery, 1 day, 2 day, 3 day, 4 day, 5 day, 6 day, 7 day after surgery</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>kidney ultrasound</measure>
    <time_frame>1 week, 1 month after surgery, if possible</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>inflammatory mediator (IL-18, Il-1b) level</measure>
    <time_frame>immediately after induction, immediately after vessel clamp, immediately after reperfusion, 1hour after surgery</time_frame>
  </secondary_outcome>
