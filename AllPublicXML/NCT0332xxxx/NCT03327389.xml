  </primary_outcome>
  <secondary_outcome>
    <measure>incidence of delayed graft function</measure>
    <time_frame>7 days after surgery</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>level of serum creatinine</measure>
    <time_frame>maximum 30 days before surgery, 1 hour after surgery, 1 day, 2 day, 3 day, 5 day, 7 day after surgery</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>blood urea nitrogen (BUN)</measure>
    <time_frame>maximum 30 days before surgery, 1 hour after surgery, 1 day, 2 day, 3 day, 5 day, 7 day after surgery</time_frame>
  </secondary_outcome>
  <secondary_outcome>
