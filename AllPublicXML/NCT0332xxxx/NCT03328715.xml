  <brief_title>iOCT for Patients With Diabetic Macular Edema</brief_title>
  <official_title>iOCT in Patients With Diabetic Macular Edema Scheduled for Phakoemulsification - a Pilot Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Vienna Institute for Research in Ocular Surgery</agency>
      <agency_class>Other</agency_class>
