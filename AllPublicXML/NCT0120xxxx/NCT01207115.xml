  <brief_title>A Study of ABT-652 in Adults With Osteoarthritis Pain of the Knee</brief_title>
  <official_title>A Global Multicenter, Randomized, Double-Blind, Placebo- and Active-Controlled Study Comparing the Analgesic Efficacy and Safety of ABT-652 to Placebo in Subjects With Osteoarthritis Pain of the Knee</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AbbVie (prior sponsor, Abbott)</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>AbbVie</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      To compare the analgesic efficacy and safety of ABT-652 administered twice daily (BID) to
      placebo in subjects with osteoarthritis (OA) of the knee.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>September 2010</start_date>
  <completion_date type="Actual">May 2011</completion_date>
  <primary_completion_date type="Actual">May 2011</primary_completion_date>
  <phase>Phase 2</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>24-hour average pain score measured by Visual Analog Scale</measure>
    <time_frame>8 weeks</time_frame>
    <description>Subject reported pain intensity measured by Visual Analog Scale (0-100)</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Western Ontario and McMaster (WOMAC™ ) Osteoarthritis subscales, Pain, Physical Function and Stiffness scores</measure>
    <time_frame>8 weeks</time_frame>
    <description>Self administered, patient-centered, health status questionnaire</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Subject's Global Assessment of Arthritis Status</measure>
    <time_frame>8 weeks</time_frame>
    <description>Subject reported assessment of arthritis status measured by Visual Analogue Scale (0-100)</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Brief Pain Inventory</measure>
    <time_frame>8 weeks</time_frame>
    <description>Subject reported pain intensity and intereference (scale 0-10)</description>
  </secondary_outcome>
