  <brief_title>Prasugrel Re-load Strategies</brief_title>
  <official_title>Impact of Prasugrel Re-load on Platelet Aggregation in Patients on Chronic Prasugrel Therapy</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Florida</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
