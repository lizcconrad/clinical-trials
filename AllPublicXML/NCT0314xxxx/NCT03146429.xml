  <brief_title>Prediction of Future Weight Change With Dopamine Transporter</brief_title>
  <official_title>Prediction of Future Weight Change With Dopamine Transporter</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Pusan National University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Pusan National University Hospital</source>
  <oversight_info>
    <is_fda_regulated_drug>No</is_fda_regulated_drug>
    <is_fda_regulated_device>No</is_fda_regulated_device>
  </oversight_info>
  <brief_summary>
    <textblock>
      This is the re-analyzing study of observational study by Parkinson's Progression Markers
      Initiative
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>January 1, 2017</start_date>
  <completion_date type="Actual">April 15, 2017</completion_date>
  <primary_completion_date type="Actual">April 15, 2017</primary_completion_date>
  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Cohort</observational_model>
    <time_perspective>Retrospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Weight change</measure>
    <time_frame>1, 2, 3, 4, 5 year after SPECT</time_frame>
    <description>weight change per year until 5 years after SPECT</description>
  </primary_outcome>
  <enrollment type="Actual">200</enrollment>
  <condition>Weight Change, Body</condition>
  <intervention>
    <intervention_type>Diagnostic Test</intervention_type>
    <intervention_name>I-123 FP-CIT Single photon emission tomography</intervention_name>
    <description>Brain scan for dopamine transporter</description>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        The study population consisted of 192 healthy controls with screening 123I-FP-CIT SPECT.
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  males or females with their age of 30 years or older at screening

        Exclusion Criteria:

          -  subjects with a neurological disorder, a first degree relative with idiopathic PD,
             Montreal Cognitive Assessment score of 26 or less, medications that might interfere
             with DAT SPECT scans, anticoagulants that might preclude safe completion of the lumbar
             puncture, or investigational drugs, and a condition that precludes the safe
             performance of routine lumbar puncture
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>30 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
