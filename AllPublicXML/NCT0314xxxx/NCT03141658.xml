  <oversight_info>
    <has_dmc>No</has_dmc>
    <is_fda_regulated_drug>Yes</is_fda_regulated_drug>
    <is_fda_regulated_device>No</is_fda_regulated_device>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to evaluate the effects of TS-134 on ketamine-induced BOLD
      signals in ROIs in resting fMRI in healthy adult subjects.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date type="Actual">June 28, 2017</start_date>
  <completion_date type="Actual">April 10, 2018</completion_date>
