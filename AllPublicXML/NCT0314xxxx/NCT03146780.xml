  <brief_title>Digital vs Conventional Impressions Study</brief_title>
  <official_title>Clinical Evaluation of Digital Versus Conventional Impressions Part I: Marginal and Internal Fit</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Rochester</agency>
      <agency_class>Other</agency_class>
