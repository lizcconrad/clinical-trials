  <primary_outcome>
    <measure>Safety and tolerability of single and multiple doses of APX001 as measured by adverse events (AEs), physical examinations (PE), vital signs (VS), laboratory safety tests, urinalysis and 12-lead electrocardiograms (ECG).</measure>
    <time_frame>21 days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetics of single and multiple doses of APX001 as measured by maximum observed concentration (Cmax).</measure>
    <time_frame>21 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetics of single and multiple doses of APX001 as measured by area under the curve (AUC).</measure>
    <time_frame>21 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetics of single and multiple doses of APX001 as measured by terminal phase half-life (t1/2).</measure>
    <time_frame>21 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetics of single and multiple doses of APX001 as measured by volume of distribution (Vd).</measure>
    <time_frame>21 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetics of single and multiple doses of APX001 as measured by elimination rate constant (Kel).</measure>
    <time_frame>21 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetics of single and multiple dose of APX001 as measured by accumulation ratio.</measure>
    <time_frame>21 days</time_frame>
  </secondary_outcome>
