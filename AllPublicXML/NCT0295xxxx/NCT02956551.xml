  <brief_title>Personalized DC Vaccine for Lung Cancer</brief_title>
  <acronym>SKLB1608</acronym>
  <official_title>Neoantigen-primed DC Vaccine Therapy for Refractory Non-small Cell Lung Cancer</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Sichuan University</agency>
      <agency_class>Other</agency_class>
