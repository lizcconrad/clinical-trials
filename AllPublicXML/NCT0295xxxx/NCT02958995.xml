  <brief_title>Epidemiologic Study Of Diabetes And Cancer Risk</brief_title>
  <official_title>Epidemiologic Study Of Diabetes And Cancer Risk</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Takeda</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
