      <agency>Pfizer</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Pfizer</source>
  <brief_summary>
    <textblock>
      To compare short-term efficacy and safety of Exubera vs Lantus in patients with type 2
      diabetes mellitus
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>September 2006</start_date>
  <completion_date type="Actual">February 2007</completion_date>
  <phase>Phase 4</phase>
