    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Variability of blood glucose over a period of 72 hours</measure>
  </primary_outcome>
  <secondary_outcome>
    <measure>Insulin pharmacokinetics</measure>
  </secondary_outcome>
  <enrollment>30</enrollment>
  <condition>Diabetes Mellitus, Type 2</condition>
