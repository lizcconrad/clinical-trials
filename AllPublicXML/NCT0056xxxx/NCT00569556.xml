  <brief_title>Effect of Care Management on Diabetes Outcomes</brief_title>
  <official_title>Effect of Care Management on Diabetes Outcomes</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Minneapolis Veterans Affairs Medical Center</agency>
