  <brief_title>Nutrient Levels Alter Transplant Outcome</brief_title>
  <official_title>Role of Immunonutrient Levels in Preventing Complications After Renal Transplantation</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Cincinnati</agency>
      <agency_class>Other</agency_class>
