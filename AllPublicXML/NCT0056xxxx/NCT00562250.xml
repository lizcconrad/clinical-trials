  <brief_title>Pharmacokinetic Drug Interaction Study With Dapagliflozin and Glimepiride in Healthy Subjects</brief_title>
  <official_title>Pharmacokinetic Drug Interaction Study With Dapagliflozin and Glimepiride in Healthy Subjects</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AstraZeneca</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
