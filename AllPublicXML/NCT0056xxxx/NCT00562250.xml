  <primary_outcome>
    <measure>Maximum plasma concentration and exposure to glimepiride and dapagliflozin when administered alone and administered together</measure>
    <time_frame>plasma concentrations will be measures as specified timepoints for 72 hours after each administred dose</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Measures of the plasma concentration of the active metabolite of glimepiride when administered alone or coadministered with glimepiride</measure>
    <time_frame>for 72 hours after each administered dose</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Recorded adverse events</measure>
    <time_frame>for 72 hours after each administered dose</time_frame>
  </secondary_outcome>
