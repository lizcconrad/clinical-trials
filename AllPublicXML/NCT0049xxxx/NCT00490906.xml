  <brief_title>Comparison of Bone Effects With Copaxone and Interferon in Multiple Sclerosis</brief_title>
  <official_title>Comparison of Bone Effects With Copaxone and Interferon in Multiple Sclerosis: A Pilot Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Nancy Hammond, MD</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
