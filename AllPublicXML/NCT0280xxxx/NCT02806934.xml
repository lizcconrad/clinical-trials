    <observational_model>Case-Only</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Implantation rate by biological test</measure>
    <time_frame>20 days after implantation</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Implantation rate by ultrasound test</measure>
    <time_frame>20 days after implantation</time_frame>
  </primary_outcome>
  <enrollment type="Actual">120</enrollment>
  <condition>Infertility</condition>
  <intervention>
    <intervention_type>Procedure</intervention_type>
    <intervention_name>biological and ultrasound test</intervention_name>
    <description>biological and ultrasound test 20 days after implantation in order to see the implantation rate</description>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        Women undergoing intracytoplasmic sperm injection (ICSI) due to male infertility factors
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Women

          -  normal karyotype,

          -  age under 38 years

          -  serum follicle-stimulating hormone (Day-3 FSH) levels below 10 UI/l

        Exclusion Criteria:

          -  age under 18 years
      </textblock>
    </criteria>
    <gender>Female</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>37 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
