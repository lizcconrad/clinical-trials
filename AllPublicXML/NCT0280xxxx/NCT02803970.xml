  <brief_title>KOrean Borderline Ovarian Tumor (KOBOT) Registry</brief_title>
  <acronym>KOBOT</acronym>
  <official_title>Establishing the Registry of Korean Patients With Borderline Ovarian Tumors</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Kangbuk Samsung Hospital</agency>
      <agency_class>Other</agency_class>
