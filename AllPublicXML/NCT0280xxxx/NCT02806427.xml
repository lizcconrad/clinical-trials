  <brief_title>Tolerance of a Calorically Dense Enteral Nutrition Formula</brief_title>
  <official_title>Tolerance of a Calorically Dense Enteral Nutrition Formula</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Nestlé</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Nestlé</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      This prospective observational study seeks to demonstrate the ability to meet nutritional
      needs of a calorically dense enteral formula in critically ill patients.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date type="Actual">June 30, 2016</start_date>
  <completion_date type="Actual">March 11, 2017</completion_date>
  <primary_completion_date type="Actual">March 11, 2017</primary_completion_date>
