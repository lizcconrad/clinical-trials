  <brief_title>A Study to Determine the Safety and Tolerability of Dupilumab (REGN668/SAR231893) in Patients Aged ≥6 to &lt;18 Years With Atopic Dermatitis (Eczema)</brief_title>
  <official_title>A Phase 2a Study Investigating the Safety, Pharmacokinetics, Immunogenicity, and Exploratory Efficacy of Dupilumab in Patients Aged ≥6 to &lt;18 Years With Atopic Dermatitis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Regeneron Pharmaceuticals</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>Sanofi</agency>
      <agency_class>Industry</agency_class>
    </collaborator>
  </sponsors>
