  <brief_title>Renal Perfusion, Filtration and Oxygenation During Cardiopulmonary Bypass (CPB)</brief_title>
  <acronym>ECCSTUD</acronym>
  <official_title>Renal Perfusion, Filtration and Oxygenation During Cardiopulmonary Bypass (CPB)</official_title>
  <sponsors>
