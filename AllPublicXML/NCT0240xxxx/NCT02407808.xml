  <primary_outcome>
    <measure>Rey Auditory Verbal Learning Test (RAVLT)</measure>
    <time_frame>+35 minutes after infusion</time_frame>
    <description>a measure of verbal episodic memory</description>
  </primary_outcome>
  <primary_outcome>
    <measure>N-Back Task</measure>
    <time_frame>+65 minutes after infusion</time_frame>
    <description>a measure of working memory</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Clinician Administered Dissociative Symptoms Scale (Total Score)</measure>
    <time_frame>Baseline, +10, +85, and +240 minutes after the first infusion</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Visual Analog Scale (Total Score)</measure>
    <time_frame>Baseline, +10, +85, +240 minutes after the first infusion</time_frame>
    <description>a measure of THC intoxication</description>
  </secondary_outcome>
