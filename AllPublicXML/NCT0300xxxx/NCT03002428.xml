  <primary_outcome>
    <measure>Blood levels of anti-drug antibody (ADA) against teriparatide</measure>
    <time_frame>24 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Mean percentage change in lumbar-spine bone mineral density (BMD)</measure>
    <time_frame>24 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Median percentage change in serum N-terminal propeptide of type 1 procollagen (P1NP)</measure>
    <time_frame>24 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Median percentage change in serum crosslinked C-terminal telopeptide of type 1 collagen (CTX)</measure>
