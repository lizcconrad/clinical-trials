  <brief_title>Does Premature Luteinization Matter?</brief_title>
  <official_title>Does Measurement of Serum Progesterone on the Day of Trigger Matter?</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Assiut University</agency>
      <agency_class>Other</agency_class>
