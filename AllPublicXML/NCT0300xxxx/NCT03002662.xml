  <brief_title>Cecal Intubation Time in Colonoscopy: Prospective Clinical Trial</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Kartal Kosuyolu Yuksek Ihtisas Education and Research Hospital</agency>
      <agency_class>Other</agency_class>
