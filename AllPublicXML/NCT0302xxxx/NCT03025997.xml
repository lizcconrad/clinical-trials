  <primary_outcome>
    <measure>ad libitum food intake</measure>
    <time_frame>2 hours after yoghurt snack</time_frame>
    <description>t=0: standardized breakfast, t=90 minutes: yoghurt, t=210 minutes: ad libitum food intake</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Satiety feelings</measure>
    <time_frame>Up to 3.5 hours on each of the 2 test days.</time_frame>
    <description>Every 15-30 min, 5 attributes will be measured on a Visual Analogue Scale</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>occurrence and severity of GI symptoms</measure>
    <time_frame>Up to 3.5 hours on each of the 2 test days.</time_frame>
    <description>Every 15-30 min, 4 attributes will be measured on a Visual Analogue Scale</description>
  </secondary_outcome>
