  <primary_outcome>
    <measure>Change from Baseline: Static postural control</measure>
    <time_frame>3, 6, 9, and 11 weeks</time_frame>
    <description>Postural sway (center of pressure sway velocity) during unipedal quiet standing, eyes opened (20 seconds)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Change from Baseline: Dynamic postural control</measure>
    <time_frame>3, 6, 9, and 11 weeks</time_frame>
    <description>Star Excursion Balance Test (SEBT)</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">41</enrollment>
  <condition>Postural Balance</condition>
  <arm_group>
    <arm_group_label>Neuromuscular</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Neuromuscular warm-up</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Control</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>Standard warm-up</description>
  </arm_group>
  <intervention>
    <intervention_type>Behavioral</intervention_type>
    <intervention_name>Neuromuscular warm-up</intervention_name>
    <description>The intervention group replaced their regular warm-up routine with seven neuromuscular warm-up exercises performed for 15 minutes prior to each practice session (3x/week) over a period of eleven weeks.</description>
    <arm_group_label>Neuromuscular</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Behavioral</intervention_type>
    <intervention_name>Standard warm-up</intervention_name>
    <description>Athletes in the control group followed their regular 15 minutes warm-up routine provided by the team coaches.</description>
    <arm_group_label>Control</arm_group_label>
  </intervention>
