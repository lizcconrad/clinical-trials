  <brief_title>Team-Based Delivery of Care in Outpatient Settings</brief_title>
  <official_title>Team-Based Delivery of Care in Outpatient Settings: An Experimental Investigation</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mayo Clinic</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
