  <phase>Phase 3</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Reduction in glycosylated hemoglobin A1c</measure>
    <time_frame>Randomization to 69 weeks</time_frame>
    <description>To determine whether ITCA 650 is non-inferior either to empagliflozin or to glimepiride in reducing glycosylated hemoglobin A1c (HbA1c) in patients with T2D following 65 weeks of treatment and 4 weeks of post treatment follow up.</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Reduction in weight</measure>
    <time_frame>Randomization to 69 weeks</time_frame>
    <description>To determine whether ITCA 650 is non-inferior either to empagliflozin or to glimepiride in reducing weight in patients with T2D following 65 weeks of treatment and 4 weeks of post treatment follow up.</description>
  </primary_outcome>
