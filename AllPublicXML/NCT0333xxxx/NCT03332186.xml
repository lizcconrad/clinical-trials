    <description>Moderate renal impairment defined as eGFR 30 to &lt;60 mL/min/1.73 m^2</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Severe renal impairment</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
