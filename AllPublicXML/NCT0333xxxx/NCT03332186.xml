  </arm_group>
  <arm_group>
    <arm_group_label>Normal renal function</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Normal renal function defined as eGFR ≥90 mL/min/1.73 m^2</description>
  </arm_group>
