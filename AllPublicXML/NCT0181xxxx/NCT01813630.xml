  <primary_outcome>
    <measure>Annualized height velocity(cm/year) after 52 weeks</measure>
    <time_frame>52 weeks</time_frame>
    <description>Height velocity calculated with height measured at Baseline and after 52 weeks was converted to annual growth rate</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Changes in height standard deviation score after 52 weeks</measure>
    <time_frame>52 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Changes in bone maturation(changes in bone ages/changes in chronological age)</measure>
    <time_frame>52 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Changes in IGF-1</measure>
    <time_frame>52 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Changes in IGFBP-3</measure>
    <time_frame>52 weeks</time_frame>
  </secondary_outcome>
  <other_outcome>
    <measure>Changes in anti-growth hormone antibody</measure>
    <time_frame>52 weeks</time_frame>
  </other_outcome>
