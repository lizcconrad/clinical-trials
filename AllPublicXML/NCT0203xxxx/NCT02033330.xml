  <brief_title>Assessment of Bioavailability of Phenolics From an Orange Peel Extract</brief_title>
  <official_title>Assessment of Bioavailability of Phenolics From an Orange Peel Extract</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Maastricht University Medical Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
