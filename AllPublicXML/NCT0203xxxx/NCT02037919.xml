  <primary_outcome>
    <measure>Nexplanon use</measure>
    <time_frame>6 months</time_frame>
    <description>Current use of Nexplanon, confirmed via phone by participant palpation, at 6 months post-insertion.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Repeat pregnancy</measure>
    <time_frame>6 months</time_frame>
    <description>Occurrence of a pregnancy within 6 months after second-trimester abortion.</description>
  </secondary_outcome>
  <secondary_outcome>
