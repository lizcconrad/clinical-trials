  <brief_title>Surgical Outcomes for Proliferative Diabetic Retinopathy by Preoperative Glycemic Control and Renal Function</brief_title>
  <official_title>Evaluation of 25-gauge Vitrectomy Surgical Outcomes for Proliferative Diabetic Retinopathy by Preoperative Glycemic Control and Renal Function</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Kyorin University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
