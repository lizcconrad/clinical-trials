  </study_design_info>
  <primary_outcome>
    <measure>Visual acuity measurement</measure>
    <time_frame>6 months</time_frame>
    <description>Visual acuity improvement from baseline</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Presence of postoperative vitreous hemorrhage</measure>
    <time_frame>6 months</time_frame>
    <description>Incidence of postoperative vitreous hemorrhage</description>
  </secondary_outcome>
  <other_outcome>
    <measure>Presence of postoperative retinal detachment</measure>
    <time_frame>6 months</time_frame>
    <description>Incidence of postoperative retinal detachment</description>
  </other_outcome>
  <enrollment type="Actual">251</enrollment>
  <condition>Proliferative Diabetic Retinopathy</condition>
