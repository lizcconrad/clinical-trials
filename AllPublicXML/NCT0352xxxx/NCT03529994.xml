  <primary_outcome>
    <measure>Work of breathing indices</measure>
    <time_frame>2 hours</time_frame>
    <description>Altering HFNC flow rates and its effect on work of breathing indices in preterm infants with respiratory insufficiency.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Intermittent hypoxia</measure>
    <time_frame>2 hours</time_frame>
    <description>Altering HFNC flow rates and its effect on intermittent hypoxia in preterm infants with respiratory insufficiency.</description>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Anticipated">30</enrollment>
