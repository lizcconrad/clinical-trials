    <arm_group_type>Experimental</arm_group_type>
    <description>PTSD patients in this arm will receive a single inhalation dose of 50% nitrous oxide and 50% oxygen for 1 hour</description>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Nitrous Oxide</intervention_name>
    <description>One hour inhalation of 50% nitrous oxide, an odorless, colorless gas FDA approved as an induction agent for dental sedation</description>
    <arm_group_label>Nitrous Oxide</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  age 18-65

          -  primary diagnosis of PTSD

          -  sufficient severity of PTSD symptoms

          -  fluency in English

          -  capacity to provide informed consent

        Exclusion Criteria:

          -  psychiatric or medical conditions that make participation unsafe

          -  pregnant or nursing females

          -  allergy to nitrous oxide
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>65 Years</maximum_age>
