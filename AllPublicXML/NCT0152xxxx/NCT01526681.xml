  <brief_title>Registry of Avance Nerve Graft Evaluating Utilization and Outcomes for the Reconstruction of Peripheral Nerve Discontinuities</brief_title>
  <acronym>RANGER</acronym>
  <official_title>Registry Study: A Multicenter, Retrospective Study of Avance Nerve Graft Utilization, Evaluations and Outcomes in Peripheral Nerve Injury Repair</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AxoGen, Inc</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>AxoGen, Inc</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      This study is a registry of general use of Avance Nerve Graft and is intended to evaluate the
      uses, response rates, and safety of Avance Nerve Graft in the real life clinical setting.
    </textblock>
  </brief_summary>
  <overall_status>Recruiting</overall_status>
  <start_date>November 2008</start_date>
  <completion_date type="Anticipated">December 2020</completion_date>
  <primary_completion_date type="Anticipated">December 2018</primary_completion_date>
  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
  <primary_outcome>
    <measure>Incidence of Avance Nerve Graft Related Adverse Events</measure>
    <time_frame>Up to 3 years or until lost to follow-up</time_frame>
    <description>Participants will be followed through the course of their recovery, not expected to exceed 3 years or until lost to follow-up.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Improvement in Function in Injured Peripheral Nerve</measure>
    <time_frame>Up to 3 years or until lost to follow-up</time_frame>
    <description>Participants will be followed through the course of their recovery, not expected to exceed 3 years or until lost to follow-up.</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Rate of Return of Meaningful Recovery Following Peripheral Nerve injury</measure>
    <time_frame>Up to 3 years or until lost to follow-up</time_frame>
    <description>Participants will be followed through the course of their recovery, not expected to exceed 3 years or until lost to follow-up.</description>
  </secondary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Anticipated">500</enrollment>
  <condition>Peripheral Nerve Injuries</condition>
  <arm_group>
    <arm_group_label>Avance Nerve Graft</arm_group_label>
    <description>Processed Human Nerve Tissue Scaffold</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Historical Control for Standard Treatment</arm_group_label>
    <description>Literature review for outcomes from standard treatments, i.e. Autogenous Nerve Graft.</description>
  </arm_group>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>Processed Human Nerve Tissue Scaffold</intervention_name>
    <description>Implantation of appropriate length of processed human nerve tissue scaffold at the time of surgery</description>
    <arm_group_label>Avance Nerve Graft</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>Standard Treatment, Autogenous Nerve Graft, Direct Suture, etc.</intervention_name>
    <description>Historical Control from Established Literature</description>
    <arm_group_label>Historical Control for Standard Treatment</arm_group_label>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        Subjects treated with Avance Nerve Graft to reconstuct peripheral nerve discontinuities.
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Adult patients treated with Avance Nerve Graft

        Exclusion Criteria:

          -  Subjects who do not provide informed consent or decline participation.
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
