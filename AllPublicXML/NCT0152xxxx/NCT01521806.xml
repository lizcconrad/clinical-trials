  <brief_title>Effects of Dietary Fiber on Insulin Sensitivity</brief_title>
  <official_title>Effects of Dieatry Fiber on Insulin Sensitivity in Pre- and Post-menopausal Women</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ingredion Incorporated</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
