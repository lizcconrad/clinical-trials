  <brief_title>PharmacoMRI of Parkinson Disease</brief_title>
  <official_title>A Pilot Study of the Drug Effects on Brain Connectivity of Parkinson's Disease</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Northwestern University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
