  </study_design_info>
  <primary_outcome>
    <measure>mean change of morning predose FEV1 from baseline</measure>
    <time_frame>day 14</time_frame>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">56</enrollment>
  <condition>Asthma</condition>
