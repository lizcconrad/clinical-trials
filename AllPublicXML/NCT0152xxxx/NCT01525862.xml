  <brief_title>XprESS Maxillary Balloon Dilation Study</brief_title>
  <official_title>XprESS Maxillary Sinus Balloon Dilation Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Entellus Medical, Inc.</agency>
