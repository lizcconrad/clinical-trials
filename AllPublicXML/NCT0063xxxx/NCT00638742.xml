  </study_design_info>
  <primary_outcome>
    <measure>Plasma latanoprost acid concentrations at steady-state.</measure>
    <time_frame>2 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Adverse events related to systemic exposure of latanoprost</measure>
    <time_frame>1 Day</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">47</enrollment>
  <condition>Glaucoma</condition>
