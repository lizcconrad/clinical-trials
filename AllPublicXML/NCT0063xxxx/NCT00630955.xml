  <brief_title>Family History Study of Alcohol Consumption Using Memantine</brief_title>
  <official_title>NMDA Antagonist Efficacy in Reducing Human Alcohol Consumption: Impact of Family History</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Yale University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
