  <brief_title>Efficacy and Safety of THR-4109 in Obese Subjects</brief_title>
  <official_title>A Phase II Randomized, Double-Blind, Double-Dummy, Parallel Group, Placebo Controlled Study of THR-4109 in Obese Subjects</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Theracos</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>TFS Trial Form Support</agency>
      <agency_class>Industry</agency_class>
    </collaborator>
  </sponsors>
  <source>Theracos</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to evaluate the safety and effect of THR-4109 on weight loss in
      obese subjects over a 24-week treatment period.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>August 2007</start_date>
  <completion_date type="Actual">June 2008</completion_date>
  <primary_completion_date type="Actual">June 2008</primary_completion_date>
  <phase>Phase 2</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Percent change from baseline weight</measure>
    <time_frame>24 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Proportion of subjects who achieve weight loss of at least 5% and at least 10% of baseline body weight</measure>
    <time_frame>24 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Serum lipids, waist circumference, fasting plasma glucose and blood pressure</measure>
    <time_frame>24 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Safety and tolerability of THR-4109</measure>
    <time_frame>24 weeks</time_frame>
  </secondary_outcome>
  <number_of_arms>4</number_of_arms>
  <enrollment type="Anticipated">220</enrollment>
