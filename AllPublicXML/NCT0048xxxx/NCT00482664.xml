  </study_design_info>
  <primary_outcome>
    <measure>Diary events, including Satisfactory Sexual Experiences (SSEs) over 6 weeks.</measure>
    <time_frame>6 weeks</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Score in Abbreviated Sexual Function Questionnaire after 6 weeks of treatment.</measure>
    <time_frame>6 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Exit interview at end of study. Meaningful Benefit Question at end of study.</measure>
    <time_frame>End of study</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Measure of Female Sexual Distress questionnaire after 6 weeks of treatment.</measure>
    <time_frame>6 weeks</time_frame>
  </secondary_outcome>
