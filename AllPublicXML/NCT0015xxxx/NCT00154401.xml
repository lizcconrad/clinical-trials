  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Subjects diagnosed with type 2 diabetes mellitus and treated with either diet or
             single oral drug therapy

          -  Diet treated subjects: 7.5% &lt; HbA1c &lt; 10%

          -  Single oral drug therapy subjects: 7.0% &lt; HbA1c &lt; 9.5%

          -  Body Mass Index (BMI): max 40 kg/m^2

        Exclusion Criteria:

          -  Subjects treated with thiazolidinediones or insulin

          -  Subjects with any serious medical conditions
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
