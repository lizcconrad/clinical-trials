  <brief_title>Community Diabetes Education Program Evaluation &quot;Code Evaluation&quot;</brief_title>
  <official_title>Community Diabetes Education Program Evaluation</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>The University of Texas Health Science Center, Houston</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>The Institute for Faith Health Research of Dallas</agency>
