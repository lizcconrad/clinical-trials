    <masking>Double</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Evaluation of the efficacy of pregabalin and Venlafaxine XR in the treatment of generalized anxiety disorder.</measure>
  </primary_outcome>
  <enrollment>390</enrollment>
  <condition>Anxiety</condition>
