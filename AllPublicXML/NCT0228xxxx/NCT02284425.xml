  <brief_title>Study of REGN1193 in Patients With Type 2 Diabetes Mellitus</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Regeneron Pharmaceuticals</agency>
      <agency_class>Industry</agency_class>
