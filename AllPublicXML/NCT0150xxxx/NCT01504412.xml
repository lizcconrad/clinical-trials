  <brief_title>Asian Phase 2 Study for Treatment of Pain Associated With Diabetic Peripheral Neuorpathy</brief_title>
  <official_title>An Asian, Phase 2, Multicenter, Randomized, Double-blind, Placebo- and Pregabalin-controlled, Dose-finding Study of DS-5565 in Patients With Pain Associated With Diabetic Peripheral Neuropathy</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Daiichi Sankyo Co., Ltd.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Daiichi Sankyo, Inc.</source>
  <brief_summary>
    <textblock>
      The purpose of this study is to investigate the effectiveness and safety of DS-5565, compared
      to placebo, in subjects with pain associated with diabetic peripheral neuropathy.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>January 2012</start_date>
  <completion_date type="Actual">June 2013</completion_date>
  <primary_completion_date type="Actual">June 2013</primary_completion_date>
  <phase>Phase 2</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Change in average daily pain score as determined by the participants entries in daily pain diary</measure>
    <time_frame>Baseline to 7 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change in short form McGill Pain Questionnaire</measure>
    <time_frame>Baseline to week 7</time_frame>
  </secondary_outcome>
  <number_of_arms>5</number_of_arms>
  <enrollment type="Actual">450</enrollment>
  <condition>Pain</condition>
  <condition>Diabetic Peripheral Neuropathy</condition>
  <arm_group>
    <arm_group_label>DS-5565 Low Dose</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>DS-5565 10mg/day, administered in 2 doses. Treatment period: 1 week titration and 6 weeks of fixed dose.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>DS-5565 Middle Dose</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>DS-5565 20mg/day, administered in 2 doses. Treatment period: 1 week titration and 6 weeks of fixed dose.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>DS-5565 High Dose</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>DS-5565 30mg/day, administered in 2 doses. Treatment period: 1 week titration and 6 weeks of fixed dose.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Placebo</arm_group_label>
    <arm_group_type>Placebo Comparator</arm_group_type>
    <description>DS-5565 placebo oral tablets and pregabalin placebo oral capsules administered 2 times per day.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Pregabalin</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>Pregabalin capsules 300mg/day administered in 2 doses</description>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>DS-5565</intervention_name>
    <description>Oral tablets administered twice daily</description>
    <arm_group_label>DS-5565 Low Dose</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>DS-5565</intervention_name>
    <description>Oral tablets administered twice daily</description>
    <arm_group_label>DS-5565 Middle Dose</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>DS-5565</intervention_name>
    <description>Oral tablets administered twice daily</description>
    <arm_group_label>DS-5565 High Dose</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Placebo</intervention_name>
    <description>DS-5565 placebo oral tablets and pregabalin placebo oral capsules administered 2 times per day</description>
    <arm_group_label>Placebo</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Pregabalin capsules</intervention_name>
    <description>Pregabalin oral capsules 150 mg administered twice a day</description>
    <arm_group_label>Pregabalin</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Type 1 or Type 2 diabetes mellitus

          -  Painful distal symmetric polyneuropathy

          -  Average daily pain score is great than or equal to 4

        Exclusion Criteria:

          -  HbA1c greater than 9.0
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>20 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
