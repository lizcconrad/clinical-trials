    <measure>Weight</measure>
  </primary_outcome>
  <primary_outcome>
    <measure>Blood lipids</measure>
  </primary_outcome>
  <secondary_outcome>
    <measure>Micronutrients intake</measure>
  </secondary_outcome>
  <number_of_arms>4</number_of_arms>
  <enrollment type="Actual">179</enrollment>
  <condition>Obesity</condition>
  <arm_group>
    <arm_group_label>Partial meal replacement</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
  </arm_group>
  <arm_group>
    <arm_group_label>Partial meal replacement with inulin</arm_group_label>
