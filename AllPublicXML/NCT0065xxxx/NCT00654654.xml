  <brief_title>Safety and Efficacy Study of Isolagen TherapyTM in Treatment of Facial Wrinkles and Creases</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Fibrocell Technologies, Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
