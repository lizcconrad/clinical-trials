  <brief_title>Vitamin D, Insulin Resistance and Inflammation in ESRD</brief_title>
  <official_title>Vitamin D, Insulin Resistance and Inflammation in ESRD</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Vanderbilt University</agency>
