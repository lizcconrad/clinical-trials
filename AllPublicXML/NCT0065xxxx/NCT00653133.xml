  <brief_title>Multicenter Continuous Peripheral Nerve Block Surveillance Study</brief_title>
  <acronym>cpnb</acronym>
  <official_title>Multicenter Continuous Peripheral Nerve Block Surveillance Study Comparing Ultrasound Guided Catheter Placement to Non Ultrasound Guided Catheter Placement Techniques</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Halyard Health</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Halyard Health</source>
  <oversight_info>
