  <brief_title>Evaluation of Transient Dyspnea During Gadoxetic Acid Enhanced Multiphasic Liver MRI</brief_title>
  <official_title>Evaluation of Transient Dyspnea During Gadoxetic Acid Enhanced Multiphasic Liver MRI</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Seoul National University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
