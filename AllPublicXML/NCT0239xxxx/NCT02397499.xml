  </study_design_info>
  <primary_outcome>
    <measure>Safety as measured by Physical examination, adverse events, vital signs, and laboratory tests.</measure>
    <time_frame>8 weeks</time_frame>
    <description>Physical examination, adverse events, vital signs, and laboratory tests.</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Change in the clinician reported photonumeric score</measure>
    <time_frame>8 weeks post the start of treatment</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Change in the patient reported global abdominal perception score</measure>
    <time_frame>8 weeks post the start of treatment</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change in waist circumference</measure>
    <time_frame>8 weeks post the start of treatment</time_frame>
  </secondary_outcome>
