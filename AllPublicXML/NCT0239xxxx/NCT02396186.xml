  <brief_title>An Assessment of the Glider Balloon in Complex Lesions</brief_title>
  <acronym>ANTELOPE</acronym>
  <official_title>An Assessment of the Glider Balloon in Complex Lesions</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>TriReme Medical, LLC</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
