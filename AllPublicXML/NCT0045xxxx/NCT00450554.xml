  <eligibility>
    <study_pop>
      <textblock>
        children born between 01/01/2000 and 06/23/2005, living in Dieppe-Est, Dieppe-Ouest and
        Offranville
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  children born between 01/01/2000 and 06/23/2005, living in Dieppe-Est, Dieppe-Ouest
             and Offranville, vaccinated twice by MenBVac

          -  parental authority(ies)assent

        Exclusion Criteria:

          -  no parental authority(ies)assent

          -  no blood sample during the third vaccination

          -  impossibility of third vaccination
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>2 Years</minimum_age>
    <maximum_age>7 Years</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
