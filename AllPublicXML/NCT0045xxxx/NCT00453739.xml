  <primary_outcome>
    <measure>Standing cough stress test</measure>
    <time_frame>12 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>I-QOL score</measure>
    <time_frame>3, 6, 12 and 24 months</time_frame>
  </secondary_outcome>
