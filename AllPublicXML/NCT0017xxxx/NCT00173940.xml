  <brief_title>Collection of Blood and Urine Specimens From Volunteers Utilizing Health Examination Services</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>National Taiwan University Hospital</agency>
      <agency_class>Other</agency_class>
