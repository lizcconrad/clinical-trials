    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Double</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Assessment of duration of action, safety &amp; toleration of the investigational drug and Cialis</measure>
  </primary_outcome>
  <secondary_outcome>
    <measure>Assess safety &amp; toleration over 4 week treatment period</measure>
  </secondary_outcome>
  <enrollment>300</enrollment>
  <condition>Impotence</condition>
