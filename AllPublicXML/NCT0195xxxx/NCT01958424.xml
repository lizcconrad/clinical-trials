  </study_design_info>
  <primary_outcome>
    <measure>success or failure of ivf treatment</measure>
    <time_frame>within 1 year from intiation of ivf treatment</time_frame>
    <description>success or failure of a treatment will be determined by beta-Human chorionic gonadotropin test, number of oocyte retrieved or fertilized and embryos quality.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>blood levels of C-reactive protein</measure>
    <time_frame>within 1 year from initiation of ivf treatment</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>follicle liquid levels of C-reactive protein</measure>
    <time_frame>within 1 year from initiation of ivf treatment</time_frame>
  </secondary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Actual">30</enrollment>
  <condition>Metabolic Syndrome</condition>
  <arm_group>
    <arm_group_label>women with metabolic syndrome</arm_group_label>
    <description>women undergoing ivf treatment who have BMI&gt;25 and meet the criteria for metabolic syndrome</description>
  </arm_group>
  <arm_group>
    <arm_group_label>women without metabolic syndrome</arm_group_label>
    <description>women undergoing ivf treatment who have BMI&gt;25 and who do not meet the criteria for metabolic syndrome</description>
  </arm_group>
