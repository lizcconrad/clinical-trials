  <brief_title>Effect of Medium Chain Triglyceride Consumption on Appetite</brief_title>
  <acronym>MCT</acronym>
  <sponsors>
    <lead_sponsor>
      <agency>St. Luke's-Roosevelt Hospital Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
