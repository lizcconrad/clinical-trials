        <time_frame>14 +- 1 days</time_frame>
        <group_list>
          <group group_id="O1">
            <title>Test 1</title>
            <description>Fraction of baseplates with No leakage/seeping under the baseplate for subjects testing Coloplast Test 1</description>
          </group>
