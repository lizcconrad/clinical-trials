  <brief_title>Extensively Hydrolyzed Formula: Hypoallergenicity</brief_title>
  <official_title>Evaluation of Hypoallergenicity of a 100% Whey, Extensively Hydrolyzed Infant Formula</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Nestlé</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
