  <brief_title>Infrapatellar Nerve Block for Post-operative Knee Arthroscopy Pain Control</brief_title>
  <official_title>Infrapatellar Nerve Block for Post-operative Analgesia for Knee Arthroscopy</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Northwestern University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
