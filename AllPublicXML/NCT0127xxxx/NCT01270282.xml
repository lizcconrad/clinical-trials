    <arm_group_label>Placebo</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Tinnitus following acute acoustic trauma, acute otitis media, middle ear surgery or
             inner ear barotrauma; with onset less than three months ago.

        Exclusion Criteria:

          -  Tinnitus that is not completely maskable

          -  Fluctuating tinnitus

          -  Intermittent tinnitus

          -  Meniere's Disease

          -  Ongoing acute or chronic otitis media or otitis externa.
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>65 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
