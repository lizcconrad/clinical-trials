  <arm_group>
    <arm_group_label>traditional misoprostol</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>80 microgram misoprostol intake per 4 hours</description>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Misoprostol</intervention_name>
    <description>20 microgram PO per hour for 8 doses or 80 microgram PO per 4 hours for 2 doses</description>
    <arm_group_label>hourly misoprostol</arm_group_label>
    <arm_group_label>traditional misoprostol</arm_group_label>
    <other_name>cytotec</other_name>
  </intervention>
