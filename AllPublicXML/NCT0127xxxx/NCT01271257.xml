    <primary_purpose>Basic Science</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>AUC480 (the area under the curve of serum concentration of misoprostol acid against time up to 480 minutes)</measure>
    <time_frame>480 min post start of oral misoprostol administration</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>AUC240 (the area under the curve of serum concentration of misoprostol acid against time up to 240 minutes)</measure>
    <time_frame>240 min post start of oral misoprostol administration</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>AUC360 (the area under the curve of serum concentration of misoprostol acid against time up to 360 minutes)</measure>
    <time_frame>360 min post start of oral misoprostol administration</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">10</enrollment>
