  <brief_title>ROsuvastatin LOading and Clinical Outcomes Trial</brief_title>
  <acronym>ROLOCO</acronym>
  <sponsors>
    <lead_sponsor>
      <agency>Yuksek Ihtisas Hospital</agency>
