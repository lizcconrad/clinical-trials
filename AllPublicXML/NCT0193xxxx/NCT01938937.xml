  <brief_title>The Effect of Acute Transcranial Bright Light on Anxiety Symptoms</brief_title>
  <official_title>The Effect of Acute Transcranial Bright Light on Anxiety Symptoms</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Oulu</agency>
      <agency_class>Other</agency_class>
