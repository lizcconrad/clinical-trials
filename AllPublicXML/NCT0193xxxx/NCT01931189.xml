  <brief_title>Pharmacokinetics Study of NI-071</brief_title>
  <official_title>A Clinical Pharmacokinetics Study of NI-071</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Nichi-Iko Pharmaceutical Co., Ltd.</agency>
