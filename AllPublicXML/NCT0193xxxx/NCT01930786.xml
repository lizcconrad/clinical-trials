  <brief_title>An Adult Spasticity Registry of OnabotulinumtoxinA Treatment</brief_title>
  <acronym>ASPIRE</acronym>
  <sponsors>
    <lead_sponsor>
      <agency>Allergan</agency>
      <agency_class>Industry</agency_class>
