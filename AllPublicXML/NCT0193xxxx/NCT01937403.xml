  <brief_title>Sport Cord Test Following ACL Reconstruction</brief_title>
  <official_title>The Use of the Sport Cord Test as a Measure of Physical Performance Following Anterior Cruciate Ligament Reconstruction</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>The Hawkins Foundation</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
