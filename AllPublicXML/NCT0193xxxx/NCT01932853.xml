  <brief_title>Association Between Dose Dialysis by Kt and Mortality</brief_title>
  <official_title>Evaluation of the Association of Dialysis Dose Obtained by Kt and Mortality in Hemodialysis Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>NephroCare Spain</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
