  <primary_outcome>
    <measure>Percentage of subjects achieving HbA1c (glycosylated haemoglobin A1c) below 7.0%</measure>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change in HbA1c</measure>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Total number of hypoglycemic events</measure>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Adverse events</measure>
  </secondary_outcome>
