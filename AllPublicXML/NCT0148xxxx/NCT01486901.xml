  <primary_outcome>
    <measure>Area under the serum insulin concentration-time curve (AUC 0-24 hours)</measure>
  </primary_outcome>
  <primary_outcome>
    <measure>Maximum serum insulin concentration (Cmax)</measure>
  </primary_outcome>
  <secondary_outcome>
