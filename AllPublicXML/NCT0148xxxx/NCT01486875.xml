  </study_design_info>
  <primary_outcome>
    <measure>Incidence of serious adverse drug reactions (SADRs)</measure>
    <time_frame>Week 26</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Incidence of major hypoglycaemic events</measure>
    <time_frame>Week 26</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number of all hypoglycaemic events</measure>
    <time_frame>In the 4 weeks prior to week 12 and week 26, respectively</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>HbA1c (glycosylated haemoglobin A1c)</measure>
    <time_frame>Week 26</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Fasting Plasma Glucose (FPG)</measure>
    <time_frame>Week 26</time_frame>
  </secondary_outcome>
