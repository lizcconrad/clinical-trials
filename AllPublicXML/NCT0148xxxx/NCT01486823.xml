  <primary_outcome>
    <measure>Area under the plasma concentration versus time curve (AUC) of MLDL1278A.</measure>
    <time_frame>Up to 70 days post administration.</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Peak plasma concentration (Cmax) of MLDL1278A.</measure>
    <time_frame>Up to 70 days post administration.</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Plasma clearance (CL) of MLDL1278A.</measure>
    <time_frame>Up to 70 days post administration.</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of participants with an anti therapeutic antibody (ATA) response.</measure>
    <time_frame>Up to 140 days post administration.</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of participants with adverse events.</measure>
    <time_frame>Up to 140 days post administration.</time_frame>
  </secondary_outcome>
