  <brief_title>Investigation of Popular Recovery Techniques for Ultramarathon Recovery</brief_title>
  <official_title>Investigation of Popular Recovery Techniques for Ultramarathon Recovery</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Western States Endurance Run Research Foundation</agency>
      <agency_class>Other</agency_class>
