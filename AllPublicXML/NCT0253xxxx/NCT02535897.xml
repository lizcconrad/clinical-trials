    <intervention_name>Flavoured water + carbohydrate</intervention_name>
    <arm_group_label>CHO</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Dietary Supplement</intervention_type>
    <intervention_name>Flavoured water + carbohydrate + protein</intervention_name>
    <arm_group_label>CHO-PRO</arm_group_label>
  </intervention>
  <eligibility>
