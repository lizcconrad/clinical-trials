    <time_frame>Multiple time point up to 3 day</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number of participants with adverse events as a measure of safety and tolerability</measure>
    <time_frame>Up to 30 day</time_frame>
  </secondary_outcome>
