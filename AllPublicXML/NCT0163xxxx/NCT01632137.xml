    <intervention_name>Rebamipide 2% ophthalmic suspension</intervention_name>
    <description>Instill one drop into each eye 4 times a day for 4 weeks.</description>
    <arm_group_label>Rebamipide 2% ophthalmic suspension</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
