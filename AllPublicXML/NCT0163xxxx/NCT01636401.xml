  <primary_outcome>
    <measure>Change from baseline to Week 12 in morning predose (trough) FEV1</measure>
    <time_frame>Week 12</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change from baseline to Week 12 in peak FEV1</measure>
    <time_frame>Week 12</time_frame>
  </secondary_outcome>
