  <primary_outcome>
    <measure>Maximum Observed Plasma Concentration (Cmax)</measure>
    <time_frame>0, 0.5, 0.75, 1, 1.5, 2, 3, 4.5, 6, 9, 16, 24, 48 hours post-dose</time_frame>
  </primary_outcome>
