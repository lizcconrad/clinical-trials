  <brief_title>Effect of Squat Training on Body Composition, Muscular Strength and Motor Fitness for Youth- Randamized Trial</brief_title>
  <acronym>CET</acronym>
  <official_title>Phase 1 Study of Child Exercise Training</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>National Institute of Fitness and Sports in Kanoya</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
