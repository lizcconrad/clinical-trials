  <brief_title>PEDS-C: Pegylated Interferon +/- Ribavirin for Children With Hepatitis C</brief_title>
  <official_title>Pegylated Interferon +/- Ribavirin for Children With Hepatitis C</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>National Institute of Diabetes and Digestive and Kidney Diseases (NIDDK)</agency>
      <agency_class>NIH</agency_class>
    </lead_sponsor>
