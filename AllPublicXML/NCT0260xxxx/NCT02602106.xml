  <brief_title>Regular Aquatic Activities and Pregnancy Outcomes</brief_title>
  <acronym>aquapreg</acronym>
  <official_title>Effects of Regular Aquatic Activities on Pregnancy Outcomes</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Universidad Politecnica de Madrid</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
