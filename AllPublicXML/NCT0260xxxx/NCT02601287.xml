  <brief_title>A Safety and Efficacy Study of Botulinum Toxin Type A (BOTOX®) to Treat Urinary Incontinence Due to Neurogenic Detrusor Overactivity or Overactive Bladder in Korean Patients</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Allergan</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Allergan</source>
  <oversight_info>
