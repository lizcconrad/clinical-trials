  <brief_title>The Expecting Study</brief_title>
  <official_title>Prenatal Physical Activity Intervention</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Arkansas Children's Hospital Research Institute</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
