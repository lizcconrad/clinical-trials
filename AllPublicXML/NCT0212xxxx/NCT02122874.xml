    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Single (Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Serum glucose levels (mg/dl)</measure>
    <time_frame>Baseline and 12 weeks after beginning the therapy</time_frame>
    <description>Serum glucose levels will be recorded will be recorded at baseline and 12 weeks after beginning the treatment. Measurement units will be mg/dl.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Homeostasis model assessment (HOMA)</measure>
    <time_frame>Baseline and 12 weeks after beginning the therapy</time_frame>
    <description>Insulin resistance will be recorded by the Homeostasis model assessment (HOMA). The calculation formula is: Serum Glucose x Serum Insulin / 405</description>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">60</enrollment>
  <condition>Diabetes Mellitus Type 2 in Obese</condition>
