  <brief_title>Fetal Viability Estimation in Egypt</brief_title>
  <official_title>Correlation Between Fetal Gestational Age, Viability and Survival Rates in Egypt</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Woman's Health University Hospital, Egypt</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
