  <brief_title>IOL Power Calculation Using Dual Schiempflug Analyzer</brief_title>
  <official_title>Precision of Optical Biometry and Intraocular Lens Power Calculation Using Dual Schiempflug Analyzer</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Chuncheon Sacred Heart Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
