  <primary_outcome>
    <measure>Proportion of subjects who achieve endoscopic remission</measure>
    <time_frame>Up to Week 16</time_frame>
    <description>Endoscopic remission will be determined using Simplified Endoscopic Score for Crohn's Disease (SES-CD).</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Proportion of subjects who achieve clinical remission</measure>
    <time_frame>Week 16</time_frame>
    <description>Clinical remission to be determined at Week16</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Proportion of subjects who achieve Crohn's Disease Activity Index (CDAI) less than 150</measure>
    <time_frame>Week 16</time_frame>
