  <brief_title>Safety and Efficacy of Combigan® Ophthalmic Solution in Korea</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Allergan</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
