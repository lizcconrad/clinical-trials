  <brief_title>Does Allopurinol Prolong a Treated, Acute Gout Flare?</brief_title>
  <official_title>Does Allopurinol Prolong a Treated, Acute Gout Flare?</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Wilford Hall Medical Center</agency>
      <agency_class>U.S. Fed</agency_class>
    </lead_sponsor>
