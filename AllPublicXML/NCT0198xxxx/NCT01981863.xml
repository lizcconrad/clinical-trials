  <brief_title>Fibrinolysis Before Cardiopulmonary Bypass?</brief_title>
  <official_title>Fibrinolysis Before Cardiopulmonary Bypass?</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Stanford University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
