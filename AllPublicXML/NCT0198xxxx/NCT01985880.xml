  <brief_title>The Efficacy of Biomarker in Patient With Interstitial Cystitis/Painful Bladder Syndrome</brief_title>
  <official_title>Experimental Study of Biomarker in Patient With Interstitial Cystitis/Painful Bladder</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Asan Medical Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
