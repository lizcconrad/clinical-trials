  </study_design_info>
  <primary_outcome>
    <measure>Vaccine Immunity</measure>
    <time_frame>after the fourth dose of MenbVac</time_frame>
    <description>Assessment of vaccine Immunity with measure of percentage of children who have hSBA title &gt;= 4 six weeks after the fourth dose of vaccine</description>
  </primary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Actual">120</enrollment>
  <condition>Neisseria Meningitidis</condition>
