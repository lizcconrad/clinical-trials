  <primary_outcome>
    <measure>Area under the serum insulin degludec concentration-of s.c. administration</measure>
    <time_frame>From 0 to infinity after single dose</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Area under the serum insulin degludec concentration-of i.v. administration</measure>
    <time_frame>From 0 to infinity after single dose</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Terminal half-life for insulin degludec</measure>
    <time_frame>From 0 to 30 hours after single dose</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Volume of distribution of insulin degludec, estimated during the terminal phase</measure>
    <time_frame>From 0 to 30 hours after single dose</time_frame>
  </secondary_outcome>
