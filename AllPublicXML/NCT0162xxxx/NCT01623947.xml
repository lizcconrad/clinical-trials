    <primary_purpose>Basic Science</primary_purpose>
    <masking>Triple (Participant, Investigator, Outcomes Assessor)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Improved Time Trial Performance</measure>
    <time_frame>After 2.5 hr interval cycling</time_frame>
    <description>Participants will perform an interval ride on a cycling ergometer for 2.5 hr. Intervals will be at 50% or 75% of the participant's VO2max. The time trial will consist of 4 km up a constant grade.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Improved Mental Function</measure>
    <time_frame>After time trial</time_frame>
    <description>Mental function will be assessed using a computer-based Stroop Test. Measurements will be collected before treatment, before exercise and after exercise.</description>
  </secondary_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment type="Anticipated">14</enrollment>
  <condition>Healthy</condition>
  <arm_group>
    <arm_group_label>Placebo</arm_group_label>
    <arm_group_type>Placebo Comparator</arm_group_type>
  </arm_group>
  <arm_group>
