    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Sexual encounter profile question 3 observed within 15 minutes to 4 hours for vardenafil and 22 to 26 hours for the tadalafil group</measure>
    <time_frame>4 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Sexual encounter profile question 2</measure>
    <time_frame>4 weeks</time_frame>
  </secondary_outcome>
