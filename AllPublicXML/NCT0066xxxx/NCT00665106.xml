  </arm_group>
  <arm_group>
    <arm_group_label>cohort 3</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>5 up to 6 patients per arm. Emulsion at 3.2% of drug product.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>cohort 4</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>5 up to 6 patients per arm. Oily solution at 3.4% of drug product.</description>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>NOVA63035 &quot;Corticosteroid&quot;</intervention_name>
    <description>Single injection in the study eye</description>
    <arm_group_label>cohort 1</arm_group_label>
    <arm_group_label>cohort 2</arm_group_label>
    <arm_group_label>cohort 3</arm_group_label>
