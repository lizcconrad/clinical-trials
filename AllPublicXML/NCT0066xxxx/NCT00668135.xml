    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>International Index of Erectile Function (IIEF) Questionnaire and Sexual Encounter Profile question 2 (SEP 2) and Sexual Encounter Profile question 3 (SEP 3)</measure>
    <time_frame>12 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Global Assessment Question</measure>
    <time_frame>12 weeks</time_frame>
  </secondary_outcome>
