  <primary_outcome>
    <measure>Safety assessments for the study include AE collection, vital signs, postural vital signs, weight, physical and neurological examinations, ECG, Hopkins Verbal Learning Test -Revised (HVLT-R™), and routine laboratory tests.</measure>
    <time_frame>120 days</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Western Ontario and McMaster Universities Arthritis (WOMAC 3.1™) scale.</measure>
    <time_frame>120 days</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>VAS for index knee pain during walking in the past 24 hours.</measure>
    <time_frame>120 days</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>VAS for index knee pain in the past 24 hours.</measure>
    <time_frame>120 days</time_frame>
  </primary_outcome>
  <primary_outcome>
