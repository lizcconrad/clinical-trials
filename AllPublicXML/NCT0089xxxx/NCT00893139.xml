    <primary_purpose>Treatment</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Mean change from baseline in sodium fluorescein corneal staining score</measure>
    <time_frame>Baseline, up to Day 35</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Mean change from baseline in phenol red thread length</measure>
    <time_frame>Baseline, up to Day 35</time_frame>
  </secondary_outcome>
