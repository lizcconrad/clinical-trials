  <brief_title>Evaluation of GLUCOFACTS® Deluxe Diabetes Management System</brief_title>
  <official_title>Evaluation of GLUCOFACTS® Deluxe Diabetes Management System In a Clinical Setting</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ascensia Diabetes Care</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
