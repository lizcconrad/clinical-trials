    <time_frame>baseline, 12weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in lobular inflammation score</measure>
    <time_frame>baseline, 12weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in ballooning score</measure>
    <time_frame>baseline, 12weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in lipid profile</measure>
    <time_frame>baseline, 4weeks, 8weeks, 12weeks</time_frame>
  </secondary_outcome>
