  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Ketorolac 0.4%</intervention_name>
    <description>One drop 4 times a day in the pre-operative eye beginning day 0 for 4 days</description>
    <arm_group_label>1</arm_group_label>
    <other_name>Acular LS™</other_name>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Lubricating Eye Drop</intervention_name>
    <description>One drop 4 times a day in the operative eye beginning one day prior to the peripheral iridotomy and continuing until the day of IOL implantation</description>
    <arm_group_label>2</arm_group_label>
    <other_name>Soothe® XP</other_name>
  </intervention>
