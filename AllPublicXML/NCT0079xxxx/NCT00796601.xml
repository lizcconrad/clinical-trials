  <primary_outcome>
    <measure>Change from baseline to 3 and 6 months in the mean daily pain rating score measured by the 11 point pain intensity Numerical Rating Scale (NRS).</measure>
    <time_frame>6 months</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Change from baseline to 3 and 6 months in the FIQ-Total score</measure>
    <time_frame>6 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pain (including other measures of daily pain and responder analyses);</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Function (including sub-scales of the FIQ, responder analyses of FIQ and SF-36);</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Global efficacy measures PGIC);</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Sleep (MOS);</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Work productivity;</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Depression (HADS)</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Safety (vital signs, ECG, laboratory, suicidal ideation)</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
