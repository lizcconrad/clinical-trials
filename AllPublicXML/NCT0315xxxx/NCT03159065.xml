  <brief_title>Postprandial Efficacy of Probiotic Fruit Beverages on Glucose Tolerance and Insulin Resistance</brief_title>
  <official_title>Early Postprandial Efficacy of Probiotic Fruit Beverages on Glucose Tolerance and Insulin Resistance in Healthy Adults</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Lund University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
