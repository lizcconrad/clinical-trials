  <primary_outcome>
    <measure>any death, stroke and major hemorrhagic event</measure>
    <time_frame>2 years after treatment</time_frame>
    <description>any death, stroke and major hemorrhagic event within 2 years after treatment</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>any adverse event</measure>
    <time_frame>2 years</time_frame>
    <description>death, stroke, hemorrhagic event within 30days or between 31days to 2years after treatment
any change of anti-thrombotic therapy within 2years after treatment
recanalization or re-treatment within 2years after treatment
stent stenosis or occlusion within 2years after treatment
any adverse event within 2years after treatment</description>
  </secondary_outcome>
