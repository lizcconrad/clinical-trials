  </secondary_outcome>
  <secondary_outcome>
    <measure>7-point blood glucose profile</measure>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">89</enrollment>
  <condition>Diabetes</condition>
