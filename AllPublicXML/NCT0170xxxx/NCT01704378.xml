  </study_design_info>
  <primary_outcome>
    <measure>Number of Hypoglycaemic episodes</measure>
  </primary_outcome>
  <primary_outcome>
    <measure>Occurrence of adverse events</measure>
  </primary_outcome>
  <secondary_outcome>
