    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Measure early allograft dysfunction (EAD)</measure>
    <time_frame>7 days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Measure primary non-function rates</measure>
    <time_frame>10 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Measure graft survival rates</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Measure subject survival rates</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Measure post-reperfusion syndrome via mean arterial pressure (MAP)</measure>
    <time_frame>1 day</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Measure biochemical liver function via Bilirubin, GGT, ALT, AST, ALP, INR, and Lactate</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Measure evidence of ischemia-reperfusion injury via comparison of pre-reperfusion biopsies to post-reperfusion biopsies</measure>
    <time_frame>1 day</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Measure evidence of biliary complications</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Measure incidence of livers randomized but not transplanted</measure>
    <time_frame>1 day</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Measure organ utilization</measure>
    <time_frame>10 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Measure healthcare costs</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Measure quality of life measures</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
