  <brief_title>the Effect of Spray on Propofol Injection Pain</brief_title>
  <official_title>the Effect of Ethyl Chloride Spray on Propofol Injection Pain</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>The Catholic University of Korea</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
