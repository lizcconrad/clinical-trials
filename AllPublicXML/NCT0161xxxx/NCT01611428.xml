  <brief_summary>
    <textblock>
      A study to assess the absolute bioavailability of ipragliflozin in healthy subjects.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>June 2011</start_date>
  <completion_date type="Actual">July 2011</completion_date>
