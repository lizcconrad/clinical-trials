  <brief_title>Effects of Food Cooking on Diabetes-2 Risk Factors</brief_title>
  <acronym>Age-2</acronym>
  <sponsors>
    <lead_sponsor>
      <agency>University of Copenhagen</agency>
      <agency_class>Other</agency_class>
