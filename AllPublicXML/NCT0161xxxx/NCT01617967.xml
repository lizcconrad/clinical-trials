  <primary_outcome>
    <measure>Safety and tolerability in ATTR patients</measure>
    <time_frame>Up to 56 Days</time_frame>
    <description>The proportion of subjects experiencing adverse events (AEs), serious adverse events (SAEs) and study drug discontinuation</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetics (PK) of ALN-TTR02</measure>
    <time_frame>Up to 208 days</time_frame>
    <description>Cmax, Area Under Curve, Tmax</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Serum transthyretin (TTR) protein</measure>
    <time_frame>Up to 208 days</time_frame>
    <description>Determination of % Lowering to pretreatment/Baseline Levels</description>
  </secondary_outcome>
