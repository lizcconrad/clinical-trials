  <brief_summary>
    <textblock>
      Compare changes in bowel function before, 6 months and one year after Robotic-assisted
      laparoscopic sacral colpopexy. Compare objective anatomic outcomes before, 6 months and one
      year after Robotic-assisted laparoscopic sacral colpopexy.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>January 2007</start_date>
  <completion_date type="Actual">August 2012</completion_date>
  <primary_completion_date type="Actual">August 2012</primary_completion_date>
  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
