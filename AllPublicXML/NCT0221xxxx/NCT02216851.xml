  <primary_outcome>
    <measure>Change in volume of the nasal dorsum and/or nasal root</measure>
    <time_frame>6 months</time_frame>
    <description>The change in volume will be measured using 3 D photography technique</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
