  <brief_title>GLOBAL Clinical Study</brief_title>
  <acronym>GLOBAL</acronym>
  <official_title>Genetic Loci and the Burden of Atherosclerotic Lesions</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Global Genomics Group, LLC</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
