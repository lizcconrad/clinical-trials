  <brief_title>Efficacy and Safety of Prunus Mume Extract on Improvement of Constipation</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Chonbuk National University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
