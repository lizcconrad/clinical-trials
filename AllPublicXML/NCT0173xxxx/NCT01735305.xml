  <brief_title>Optimal Antiplatelet Therapy for Chinese Patients With Coronary Artery Disease</brief_title>
  <acronym>OPT-CAD</acronym>
  <official_title>Optimal Antiplatelet Therapy for Chinese Patients With Coronary Artery Disease</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Shenyang Northern Hospital</agency>
