  <brief_title>Results of Manual Therapy on Pulmonary Parameters</brief_title>
  <official_title>Manual Therapy Results on Pulmonary Function and Musculoskeletal Movement</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Universidad de Granada</agency>
      <agency_class>Other</agency_class>
