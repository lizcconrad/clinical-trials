  <brief_title>Surgical vs. Medical Treatment of Urge Urinary Incontinence in Women</brief_title>
  <acronym>URGE-I</acronym>
  <official_title>Surgical vs. Medical Treatment of Urge Urinary Incontinence in Women</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Klinikum der Universität Köln</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
