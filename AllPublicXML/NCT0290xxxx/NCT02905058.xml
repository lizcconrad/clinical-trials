  <brief_title>Efficacy of Ketoprofen Before Intrauterine Device Insertion</brief_title>
  <official_title>Effect of Oral Ketoprofen on Intra Uterine Device Insertion Pain: A Randomized Controlled Trial</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Assiut University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
