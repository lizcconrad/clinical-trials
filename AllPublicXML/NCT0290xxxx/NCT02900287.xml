  <enrollment type="Anticipated">18000</enrollment>
  <condition>Neoplasms</condition>
  <eligibility>
    <study_pop>
      <textblock>
        Study population extracted from Reunion Island Cancer Registry
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Patients domiciled in Reunion Island at diagnosis

          -  Patients with at least 15 years of age at diagnosis

          -  Patients with cancer diagnosis between 01/01/1998 and 31/12/2008

        Exclusion Criteria:

          -  Cutaneous malignant cancer expected melanoma
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>15 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
