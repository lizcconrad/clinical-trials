  <primary_outcome>
    <measure>5 years related survival rate by cancer site and gender</measure>
    <time_frame>5 years</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>1, 3, 5 and 10 years related survival rate by cancer site and gender</measure>
