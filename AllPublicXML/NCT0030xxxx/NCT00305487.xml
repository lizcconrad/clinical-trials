  <secondary_outcome>
    <measure>average of morning and evening parent/caregiver reported instantaneous total nasal symptom score over the two weeks of treatment</measure>
  </secondary_outcome>
  <secondary_outcome>
    <measure>safety</measure>
  </secondary_outcome>
  <enrollment>660</enrollment>
  <condition>Hay Fever</condition>
  <condition>Seasonal Allergic Rhinitis</condition>
