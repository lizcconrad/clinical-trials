  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Crossover Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Determine the action of Ixabepilone in the body over a period of time including the process of absorption, distribution, metabolism and elimination.</measure>
  </primary_outcome>
  <secondary_outcome>
    <measure>Determine the rate and extent to which Ixabepilone is absorbed or otherwise available to the treatment site in the body.</measure>
  </secondary_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment>30</enrollment>
  <condition>Cancer</condition>
