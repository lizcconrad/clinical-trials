  <brief_title>Study Evaluating The Safety Of Xyntha In Usual Care Settings</brief_title>
  <official_title>A Postauthorization Safety Surveillance Study Of Xyntha In Usual Care Settings</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Pfizer</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
