  <primary_outcome>
    <measure>The primary outcome will be weight gain in g/kg/d over the observation period. Body weight will be measured daily.</measure>
    <time_frame>2 - 6 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Hospital stay and feeding intolerance</measure>
    <time_frame>2-6 weeks</time_frame>
  </secondary_outcome>
