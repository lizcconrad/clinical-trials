    <primary_purpose>Prevention</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Appetite questionnaire, urine collection, strength testing, body composition, resting metabolic rate, food records, breath sample, activity assessments, dietary supplements, nightly logs, and resistive exercise.</measure>
    <time_frame>23 weeks</time_frame>
  </primary_outcome>
  <number_of_arms>4</number_of_arms>
  <enrollment type="Actual">49</enrollment>
  <condition>Exercise</condition>
