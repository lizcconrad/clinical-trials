  <primary_outcome>
    <measure>Number and frequency of S. pneumonia positive serotypes</measure>
    <time_frame>57 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Changes in number and frequency of S. pneumonia positive children at different ages over time</measure>
    <time_frame>57 month</time_frame>
  </secondary_outcome>
