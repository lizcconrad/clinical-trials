  <brief_title>Study Evaluating the Nasopharyngeal Carriage in Healthy Children</brief_title>
  <acronym>Carriage</acronym>
  <official_title>Nasopharyngeal Carriage in Children After Introduction of General Pneumococcal Immunization in Germany</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Pfizer</agency>
      <agency_class>Industry</agency_class>
