  <brief_title>Spinal Cord Stimulation Research Study</brief_title>
  <acronym>ADR-SCS</acronym>
  <official_title>Neuro Ambulatory Data Recorder (ADR) Spinal Cord Stimulation (SCS) Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>MedtronicNeuro</agency>
      <agency_class>Industry</agency_class>
