    <time_frame>within 1 year after CABG surgery</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>MACCE, assessed by questionnaire</measure>
    <time_frame>within 1 and 2 years after CABG surgery</time_frame>
  </secondary_outcome>
