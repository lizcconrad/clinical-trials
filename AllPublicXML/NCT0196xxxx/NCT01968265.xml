    <intervention_name>ISIS-GCCRRx</intervention_name>
    <description>3 doses on alternate days during the first week and then once weekly for 5 weeks + daily metformin</description>
    <arm_group_label>ISIS-GCCRRx</arm_group_label>
  </intervention>
