  <brief_title>Exemestane in Advanced and Recurrent Endometrial Carcinoma</brief_title>
  <official_title>Phase 2 Study of Exemestane in Advanced and Recurrent Endometrial Carcinoma</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Nordic Society for Gynaecologic Oncology</agency>
      <agency_class>Other</agency_class>
