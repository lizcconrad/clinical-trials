  <brief_title>Indwelling Stent Discoloration Project</brief_title>
  <official_title>Examining the Underlying Factors That Cause Ureteral Stent Discoloration in Japanese and Canadian Ureteral Stents</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of British Columbia</agency>
      <agency_class>Other</agency_class>
