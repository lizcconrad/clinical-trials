  <brief_title>Comparison of Pneumatic and Electromagnetic Muscle Vibrators to Stimulate Muscle Proprioceptors</brief_title>
  <official_title>Comparison of fMRI Compatible Pneumatic and Electromagnetic Muscle Vibrators to Stimulate Muscle Proprioceptors</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Katholieke Universiteit Leuven</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
