  <brief_title>Novel Dietary Interventions for Treating Insulin Resistance</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of California, San Diego</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
