  <primary_outcome>
    <measure>First-pass intubation success rate</measure>
    <time_frame>between 1 hour to 3 hours after inclusion</time_frame>
    <description>Measured by the proportion of successful intubation in the first laryngoscopy.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Incidence of difficult intubation</measure>
    <time_frame>between 1 hour to 3 hours after inclusion</time_frame>
