  </study_design_info>
  <primary_outcome>
    <measure>3D kinematic range of motion of hip adduction during ascending and descending stairs</measure>
    <time_frame>4 weeks</time_frame>
    <description>Kinematic analysis during ascent and descent stairs</description>
  </primary_outcome>
  <primary_outcome>
    <measure>3D kinematic range of motion of hip internal rotation during ascending and descending stairs</measure>
    <time_frame>4 weeks</time_frame>
    <description>Kinematic analysis during ascent and descent stairs</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pain Scores on the Numerical Pain Rating Scale</measure>
    <time_frame>4 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Strength of hip and knee extensor muscles measured through hand-held dynamometer</measure>
    <time_frame>4 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Lower Limb Function measured through the questionnaires Anterior Knee Pain Scale -AKPS and Functional Index Questionnaire- FIQ .</measure>
    <time_frame>4 weeks</time_frame>
  </secondary_outcome>
