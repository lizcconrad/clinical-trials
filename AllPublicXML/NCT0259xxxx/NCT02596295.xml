  <brief_title>Human Milk Sample Composition in Israeli Mothers and Correlation With Their Diet</brief_title>
  <official_title>Human Milk Sample Collection From Israeli Mothers and Its Analysis and Correlation Between Composition, Demographics and Diet</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Enzymotec</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
