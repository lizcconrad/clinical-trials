  <brief_title>Enhanced vs. Routine Clear Liquid Intake in Labor</brief_title>
  <official_title>Clear Liquids vs Enhanced Clear Liquids; Effect on Labor Performance</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Temple University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>Stanford University</agency>
