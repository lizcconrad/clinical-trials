  <brief_title>Pivotal Study for Validation of Philips Dx (PDx)</brief_title>
  <official_title>Pivotal Study for Validation of Philips Dx for Diagnosis in Surgical Pathology</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Philips Digital Pathology Solutions</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
