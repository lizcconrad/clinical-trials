  <brief_title>OVERWEIGHT AND OBESITY ADVERSELY AFFECT IVF OUTCOMES IN WOMEN WITH PCOS</brief_title>
  <official_title>OVERWEIGHT AND OBESITY ADVERSELY AFFECT IN VITRO FERTILIZATION (IVF) OUTCOMES IN WOMEN WITH POLYCYSTIC OVARY SYNDROME (PCOS)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Assiut University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
