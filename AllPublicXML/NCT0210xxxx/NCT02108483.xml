  <brief_title>Phototoxicity Test of Dapsone Gel in Healthy Volunteers</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Allergan</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
