    <arm_group_label>Women with endometrial hyperplasia</arm_group_label>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        asymptomatic women with endometrial hyperplasia diagnosed using a vaginal ultrasound
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  asymptomatic women with endometrial hyperplasia diagnosed using a vaginal ultrasound

        Exclusion Criteria:

          -  postmenopausal bleeding
      </textblock>
    </criteria>
    <gender>Female</gender>
