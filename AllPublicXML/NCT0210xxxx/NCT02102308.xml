  <study_design_info>
    <allocation>Non-Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Single (Participant)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>spatial and temporal gait parameters</measure>
    <time_frame>12 weeks</time_frame>
    <description>velocity (cm/s), stride time (ms), stride length (cm), stride time variability (%) and stride length variability (%) under single- or two dual-task conditions</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Executive Interview</measure>
    <time_frame>12 weeks</time_frame>
    <description>This test consisted of 25-item rating for executive function on tasks. Scores ranged from 0 to 2 for each item, and higher scores indicated worse performance.</description>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">29</enrollment>
