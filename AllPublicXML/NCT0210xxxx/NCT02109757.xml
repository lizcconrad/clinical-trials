  <brief_title>Evaluation of Hearing Protection Fit Testing Program</brief_title>
  <official_title>Evaluation of Hearing Protection Fit Testing Program</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>United States Air Force</agency>
      <agency_class>U.S. Fed</agency_class>
