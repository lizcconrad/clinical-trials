  <brief_title>A Population-based Investigation of Related Factors of Birth Weight in China</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Chinese Neonatal Network</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
