  <brief_summary>
    <textblock>
      This study will evaluate hyperemia and ocular surface tolerability in patients on
      prostaglandin analogues
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>September 2007</start_date>
  <completion_date type="Actual">September 2008</completion_date>
