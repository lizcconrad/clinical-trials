  <brief_title>A 3-Month Non-Interventional Study of Asthmatics Treatment With Symbicort® Turbuhaler®</brief_title>
  <acronym>Symb NIS</acronym>
  <official_title>A 3-Month Non-Interventional Study of Asthmatics Treatment With Symbicort® Turbuhaler®</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AstraZeneca</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>AstraZeneca</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to investigate real practices of treatment with Symbicort
      Turbuhaler for 3 months in asthma and patient compliance.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>April 2007</start_date>
  <completion_date type="Actual">November 2007</completion_date>
  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
