  <brief_title>Effects of Caffeine on Intermittent Hypoxia in Infants Born Preterm</brief_title>
  <official_title>Pilot Study of Effects of Caffeine on Intermittent Hypoxia in Infants Born Preterm</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>American SIDS Institute</agency>
      <agency_class>Other</agency_class>
