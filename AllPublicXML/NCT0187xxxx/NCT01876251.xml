  <condition>Breast Cancer Metastatic</condition>
  <arm_group>
    <arm_group_label>PF-03084014 plus docetaxel</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>PF 03084014 will be administered orally, continuously, twice daily at doses from 80 to 150 mg in combination with docetaxel given every 3 weeks at doses from 75 to 100 mg/m^2</description>
