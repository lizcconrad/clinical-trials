  <brief_title>Iron Bioavailability From MyPyramid Menus</brief_title>
  <official_title>Iron Bioavailability From MyPyramid Menus</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>USDA Grand Forks Human Nutrition Research Center</agency>
