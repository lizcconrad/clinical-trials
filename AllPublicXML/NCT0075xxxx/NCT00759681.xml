            <dispersion_value>0.0538</dispersion_value>
            <ci_percent>95</ci_percent>
            <ci_n_sides>2-Sided</ci_n_sides>
            <ci_lower_limit>0.103</ci_lower_limit>
            <ci_upper_limit>0.314</ci_upper_limit>
