  <brief_title>HOP-2A - Intratesticular Hormone Levels</brief_title>
  <acronym>HOP-2A</acronym>
  <official_title>Intratesticular Hormone Levels in Healthy Young Men</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Washington</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
