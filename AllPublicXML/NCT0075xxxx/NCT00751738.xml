  <primary_outcome>
    <measure>safety of azimilide in this patient population</measure>
    <time_frame>1 year</time_frame>
  </primary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">330</enrollment>
  <condition>Arrhythmias</condition>
