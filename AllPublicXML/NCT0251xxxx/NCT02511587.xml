  <brief_title>Comparison of Vaccination Routes: Subcutaneous Versus Intramuscular Application of FSME-Immun®</brief_title>
  <official_title>Comparison of Vaccination Routes: Subcutaneous Versus Intramuscular Application of FSME-Immun®</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Medical University of Vienna</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
