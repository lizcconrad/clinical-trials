  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria

          1. Male or female pre-term infant 26 and up to 32 weeks gestation. Gestational age
             matching (±2 weeks) between maternal dates and/or early antenatal ultrasound

          2. Birth weight ≥ 500g

          3. Singleton or twin birth

        Exclusion Criteria

        1. Complete enteral feeding
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>N/A</minimum_age>
    <maximum_age>5 Days</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
