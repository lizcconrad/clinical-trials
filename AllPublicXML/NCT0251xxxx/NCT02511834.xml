  <brief_title>VEST III PMS Clinical Protocol</brief_title>
  <acronym>VEST III</acronym>
  <sponsors>
    <lead_sponsor>
      <agency>Vascular Graft Solutions Ltd.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
