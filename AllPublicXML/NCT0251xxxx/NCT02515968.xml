    <intervention_name>Propofol</intervention_name>
    <arm_group_label>Propofol</arm_group_label>
    <other_name>Anesthesia is maintained with propofol in Propofol group according to the randomly allocated groups.</other_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  adult patients aged over 20 yrs who are scheduled for laparoscopic or robotic
             gastrectomy, ASA 1 or 2.

        Exclusion Criteria:

          -  patients with allergy to anesthetic agents

          -  body mass index more than 35 kg/m2

          -  pregnancy
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>20 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
