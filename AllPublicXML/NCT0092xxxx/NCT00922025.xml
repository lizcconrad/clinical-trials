  <primary_outcome>
    <measure>Correlation between smoking patterns and best objective tumor response rate</measure>
    <time_frame>6 Months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>correlation between smoking patterns and progression free survival (PFS)</measure>
    <time_frame>9 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>correlation between smoking patterns and overall survival (OS)</measure>
    <time_frame>9 Months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>changes from baseline in quality of life questionnaires (EQ-5D) after EGFR-TKI therapy</measure>
    <time_frame>3 Months</time_frame>
  </secondary_outcome>
