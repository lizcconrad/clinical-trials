  <brief_title>Acute Clinical Evaluation of Multi-purpose Solutions (MPS) in Soft Contact Lens Wearers</brief_title>
  <official_title>Acute Clinical Evaluation of MPS in Soft Contact Lens Wearers</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alcon Research</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
