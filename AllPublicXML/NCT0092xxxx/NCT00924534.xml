  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria Type 2 diabetes ,stable dose of Metformin,HbA1c &gt;= 7%, but &lt; 9%
        Exclusion Criteria Type 1 diabetes mellitus, Body Mass Index (BMI) &gt;40.0 kg/m2, evidence of
        unstable cardiovascular diseases, NYHA class I to IV, ALT &gt; 1.5 times UNL, creatinine
        clearance &lt;60 mL/min
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
