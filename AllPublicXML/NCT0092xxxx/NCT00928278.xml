  <primary_outcome>
    <measure>Plasma pharmacokinetic parameters: AUClast and Cmax</measure>
    <time_frame>48 hours</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Plasma pharmacokinetic parameters: AUC0-12, AUC0-24, AUCinf, Tmax and terminal half-life.</measure>
    <time_frame>48 hours</time_frame>
  </secondary_outcome>
