    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      Birth Control Patch Study
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>June 2009</start_date>
  <completion_date type="Actual">September 2010</completion_date>
  <primary_completion_date type="Actual">August 2010</primary_completion_date>
  <phase>Phase 3</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Prevention</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Cycle control parameters and bleeding pattern indices</measure>
    <time_frame>Treatment cycles 2-7</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number of pregnancies while on treatment up to 14 days after removal of the last patch</measure>
    <time_frame>7 treatment cycles each consisting of 28 days and follow-up period of 14 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Evaluation of blood pressure changes during the dosing-free interval</measure>
    <time_frame>7 treatment cycles each consisting of 28 days</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">346</enrollment>
  <condition>Contraception</condition>
  <arm_group>
    <arm_group_label>Arm 1</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
  </arm_group>
  <arm_group>
