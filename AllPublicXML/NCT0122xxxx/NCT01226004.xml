  <brief_title>Multi-Institutional Registry for Prostate Cancer Radiosurgery</brief_title>
  <acronym>RPCR</acronym>
  <official_title>Multi-Institutional Registry for Prostate Cancer Radiosurgery. An IRB Approved Observational Trial</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Florida Robotic Radiosurgery Association</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
