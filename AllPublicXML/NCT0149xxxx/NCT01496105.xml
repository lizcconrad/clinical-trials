  <brief_title>Efficacy Study of Topical Application of Lidacaine Spray Prior to IUD Insertion</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ain Shams University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
