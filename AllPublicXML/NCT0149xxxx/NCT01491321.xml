  <condition>Chronic Low Back Pain</condition>
  <arm_group>
    <arm_group_label>Bee Venom Acupuncture &amp; Loxoprofen</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
  </arm_group>
