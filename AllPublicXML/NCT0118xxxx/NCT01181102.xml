        <time_frame>2 weeks</time_frame>
        <population>Efficacy Analysis population. 22 (7.4%) - DU-176b 41 (13.9%) - enoxaparin</population>
        <group_list>
          <group group_id="O1">
            <title>DU-176b</title>
            <description>DU-176b oral tablets, 30 mg., taken once daily for 2 weeks
edoxaban</description>
          </group>
          <group group_id="O2">
            <title>Enoxaparin Sodium</title>
