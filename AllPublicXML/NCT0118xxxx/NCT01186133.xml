    <time_frame>at 12 months and annually up to 5 years</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>target-lesion and target-vessel revascularization</measure>
    <time_frame>at 12 months and annually up to 5 years</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>procedural success</measure>
    <time_frame>at 1 day</time_frame>
  </secondary_outcome>
