  <condition>Tinnitus</condition>
  <arm_group>
    <arm_group_label>Group 1 - mp3 users</arm_group_label>
    <description>Subjects who have been using mp3 players for at least 1 hour per day for at least 1 year</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Group 2 - mp3 non-users</arm_group_label>
    <description>Subjects who does not listen regularly to mp3 music</description>
  </arm_group>
  <eligibility>
    <study_pop>
      <textblock>
        teenagers and young adults (15 to 30 years old)
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  15 to 30 years old

          -  A-n tympanogram

        Exclusion Criteria:

          -  muscular tinnitus

          -  vascular tinnitus

          -  somatosensorial tinnitus

          -  concomitant TMJ disorders

          -  external and middle ear diseases

          -  professional or recreative exposure to other noise sources other than mp3 players
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>15 Years</minimum_age>
    <maximum_age>30 Years</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
