  </study_design_info>
  <primary_outcome>
    <measure>Is there a difference in ancestry percentage between the controls (no cancer) and the cases (prostate cancer)</measure>
    <time_frame>12 months</time_frame>
  </primary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Actual">102</enrollment>
