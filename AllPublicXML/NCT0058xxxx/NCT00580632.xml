  <brief_title>Study Evaluating Workload, Clinical and Therapeutic Management of Psoriatic Arthritis</brief_title>
  <official_title>Workload, Clinical and Therapeutic Management of Psoriatic Arthritis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Wyeth is now a wholly owned subsidiary of Pfizer</agency>
      <agency_class>Industry</agency_class>
