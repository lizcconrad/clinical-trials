  <brief_title>Does Omeprazole Decrease Intestinal Calcium Absorption?</brief_title>
  <official_title>Does Omeprazole Decrease Intestinal Calcium Absorption?</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Wisconsin, Madison</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
