  <primary_outcome>
    <measure>Organ uptake of Ga-68 P16-093 for dosimetry calculation</measure>
    <time_frame>0 to approximately 150 min post injection</time_frame>
    <description>Total organ uptake of Ga-68 P16-093 as a function of time for major organs defined by whole body PET/CT</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Specificity of Ga-68 P16-093</measure>
    <time_frame>approximately 60 - 120 min post injection</time_frame>
    <description>Qualitative comparison of detection local versus distant metastases compared to clinical PSMA-11 results</description>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Anticipated">10</enrollment>
