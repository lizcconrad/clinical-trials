  <primary_outcome>
    <measure>Sensitivity and specificity of amblyopia risk factors detected by Photorefraction</measure>
    <time_frame>1 day</time_frame>
    <description>Identification of risk factors compared to gold standard cycloplegic refraction</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number of patients with gaze error identified by photo screening</measure>
    <time_frame>1 day</time_frame>
    <description>Detection of gaze error compared to cover test examination results</description>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
