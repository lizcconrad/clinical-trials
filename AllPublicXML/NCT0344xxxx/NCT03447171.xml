  <eligibility>
    <study_pop>
      <textblock>
        Study population will be selected from eye clinics.
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Age 12 months to 8 years inclusive.

          -  Informed consent given by parent or legal guardian.

        Exclusion Criteria:

        - History of ocular surgery and any condition that would preclude the ability of the
        investigator to obtain a reliable image and measurement
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>1 Year</minimum_age>
    <maximum_age>8 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
