  <brief_title>Evaluation of the Safety and Effectiveness of the Kinamed Gem Ceramic Femoral Component for Total Knee Replacement</brief_title>
  <official_title>Clinical Trial on the Zirconia Femoral Component for the Gem Total Knee System</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Kinamed Incorporated</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
