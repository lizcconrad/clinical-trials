  <brief_title>AZD5672 Absolute Bioavailability Study</brief_title>
  <official_title>A Phase I Study to Assess Absolute Bioavailability of AZD5672 at Steady-state in Healthy Volunteers</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AstraZeneca</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
