  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Selara</intervention_name>
    <description>Treatment should be initiated at 25 mg once daily and titrated to the recommended dose of 50 mg once daily, preferably within 4 weeks as tolerated by the patient. Selara may be administered with or without food.</description>
    <arm_group_label>Eplerenone</arm_group_label>
  </intervention>
