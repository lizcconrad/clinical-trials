  <brief_title>Ultrasound Evaluation of the Rotator Cable and Associated Structures.</brief_title>
  <official_title>Ultrasound Evaluation of the Rotator Cable and Associated Structures.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Michigan</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
