  </study_design_info>
  <primary_outcome>
    <measure>Ongoing pregnancy</measure>
    <time_frame>1 year</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>peak estradiol, number of follicles, number of oocytes, implantation rate, multiple pregnancy rate</measure>
    <time_frame>1 year</time_frame>
  </secondary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Anticipated">200</enrollment>
  <condition>Pregnancy</condition>
  <arm_group>
