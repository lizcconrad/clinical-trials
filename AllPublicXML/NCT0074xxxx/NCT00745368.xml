  <brief_title>Comparing Raltegravir Genital Tract Distribution in HIV-infected Men and Women</brief_title>
  <official_title>Comparing Raltegravir Genital Tract Distribution in HIV-infected Men and Women</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Rochester</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
