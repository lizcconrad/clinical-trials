  <brief_title>Effect of PTH(1-34) Treatment on Fracture Healing in Vivo</brief_title>
  <official_title>Effect of PTH(1-34) Treatment on Fracture Healing in Vivo</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Glostrup University Hospital, Copenhagen</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
