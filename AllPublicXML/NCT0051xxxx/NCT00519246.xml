  <brief_title>Comparison of Pain Therapeutic Effects After Benign Mastectomy</brief_title>
  <acronym>COPTEAM</acronym>
  <official_title>Clinical Comparison of Postoperative Pain Therapeutic Effects After Benign Mastectomy</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Nanjing Medical University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
