  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      A double-blind, placebo-controlled study to evaluate changes in pain, urgency and urinary
      frequency following administration of URG101 compared to placebo.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>August 2007</start_date>
  <completion_date type="Actual">April 2008</completion_date>
  <primary_completion_date type="Actual">April 2008</primary_completion_date>
  <phase>Phase 2</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
