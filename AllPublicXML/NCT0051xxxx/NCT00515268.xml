  <primary_outcome>
    <measure>Absolute BAL neutrophils at 24 h post LPS exposure The total and differential cell count (absolute and percentage cell counts) in BAL at 24 h post LPS exposure</measure>
    <time_frame>24 hours</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Vital signs, ECG, FEV1, Serum concentration of protein inflammatory biomarkers, BAL concentrations of protein inflammatory biomarkers BAL concentrations of GSK256066, Day 1,Day 7&amp;DAY 8</measure>
    <time_frame>Day 1,Day 7&amp;DAY 8</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">37</enrollment>
