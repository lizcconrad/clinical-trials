    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      To assess the safety, tolerability, pharmacokinetics, and pharmacodynamics of IMA-026
      administered subcutaneously (SC) or intravenously (IV) in healthy adults.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>September 2007</start_date>
  <completion_date type="Actual">July 2008</completion_date>
  <primary_completion_date type="Actual">July 2008</primary_completion_date>
  <phase>Phase 1</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
