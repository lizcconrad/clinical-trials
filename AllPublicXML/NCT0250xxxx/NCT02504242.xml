  </study_design_info>
  <primary_outcome>
    <measure>Bone Fusion With CT and Dynamic radiographs(X-ray)(post operative 48 weeks)</measure>
    <time_frame>at 48 weeks after surgery</time_frame>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Anticipated">128</enrollment>
