  <brief_title>Normobaric Hypoxic Training and Multiple Sclerosis</brief_title>
  <acronym>MS_EPOXI</acronym>
  <official_title>Cardiorespiratory, Metabolic and Immunoregulatory Response to Hypoxia in Multiple Sclerosis Patients: A Pilot Study</official_title>
  <sponsors>
    <lead_sponsor>
