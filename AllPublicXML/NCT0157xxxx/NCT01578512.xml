  <arm_group>
    <arm_group_label>Face-to-Face</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>8 sessions of weekly group behavioral weight loss treatment</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Web-based</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
