  </study_design_info>
  <primary_outcome>
    <measure>Long term efficacy evaluated by Assessment of Spondylo Arthritis International Society(ASAS) criteria and Long term safety evaluated by immunogenicity and clinical laboratory test</measure>
    <time_frame>Up to Week 40</time_frame>
  </primary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">174</enrollment>
