  <brief_title>Atrial Fibrillation Progression Trial</brief_title>
  <acronym>ATTEST</acronym>
  <official_title>Atrial Fibrillation Progression Trial</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Biosense Webster, Inc.</agency>
