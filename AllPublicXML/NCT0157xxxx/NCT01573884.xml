  <brief_title>Ex Vivo Exploratory Analysis in Healthy Full-term Infants</brief_title>
  <official_title>Ex Vivo Exploratory Analysis in Healthy Full-term Infants</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Abbott Nutrition</agency>
      <agency_class>Industry</agency_class>
