  </study_design_info>
  <primary_outcome>
    <measure>Metabolic profile: metabolite concentrations (blood/urine/feces)</measure>
    <time_frame>up to Day 5</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Correlation of metabolic profile with enzyme/transporter genotypes</measure>
    <time_frame>approximately 3 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Safety: Incidence of adverse events</measure>
    <time_frame>up to 29 days</time_frame>
  </secondary_outcome>
