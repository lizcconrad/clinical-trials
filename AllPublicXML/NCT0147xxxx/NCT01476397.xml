  <brief_title>Probiotic Formula and Infant Growth</brief_title>
  <official_title>ASSESSMENT OF GROWTH OF INFANTS FED FORMULA WITH PROBIOTICS</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Nestlé</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
