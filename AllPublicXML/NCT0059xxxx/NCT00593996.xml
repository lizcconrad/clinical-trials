    <allocation>Non-Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>change in lingual strength and perceived swallow function</measure>
    <time_frame>4 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>change in perceived bowel and bladder function</measure>
    <time_frame>4 weeks</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Anticipated">50</enrollment>
  <condition>Dysphagia</condition>
  <condition>Fecal Incontinence</condition>
  <condition>Urinary Incontinence</condition>
  <arm_group>
    <arm_group_label>1</arm_group_label>
    <arm_group_type>Placebo Comparator</arm_group_type>
    <description>oral placebo 3 times weekly</description>
  </arm_group>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>Ergocalciferol (vitamin D2)</intervention_name>
    <description>50,000 IU 3 times weekly for 4 weeks</description>
    <arm_group_label>1</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Patients who present for evaluation to the UW Geriatric clinic including Dr.
             Pryzbelski's subspecialty, or Osteoporosis clinics

          -  Age 70 years or older

          -  Cognitively intact, as determined by the attending physicians at above mentioned
             clinics

        Exclusion Criteria:

          -  History of, on ongoing, renal failure

          -  History of liver failure

          -  Known malabsorption

          -  Known disorders of parathyroid function, hyper/hypocalcemia, or other abnormalities of
             calcium or phosphate metabolism

          -  Known history of vitamin D intoxication or granulomatous disease

          -  active seizure disorder

          -  Pain associated with temporomandibular joint (TMJ)
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>70 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
