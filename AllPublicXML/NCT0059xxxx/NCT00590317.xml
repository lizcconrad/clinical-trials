  <brief_title>Ondansetron vs Prochlorperazine for Nausea and Vomiting in the Emergency Department</brief_title>
  <official_title>Ondansetron vs Prochlorperazine for Nausea and Vomiting in the Emergency Department</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Emory University</agency>
      <agency_class>Other</agency_class>
