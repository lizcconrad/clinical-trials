  <brief_title>Impact of Providing High Protein Bar to Dialysis Patients With Low Serum Albumin</brief_title>
  <official_title>Impact of Providing High Protein Bar to Dialysis Patients With Low Serum Albumin</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Satellite Healthcare</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
