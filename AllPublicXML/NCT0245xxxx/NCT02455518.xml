  <brief_title>Comparative Efficacy of 4 Oral Analgesics</brief_title>
  <official_title>Comparative Efficacy of 4 Oral Analgesics for the Initial Management of Acute Musculoskeletal Extremity Pain</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Montefiore Medical Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Montefiore Medical Center</source>
