  <brief_title>Altered PSA Test Due to Possible Suspected Heterophilic Antibodies</brief_title>
  <official_title>Altered PSA Test Due to Possible Suspected Heterophilic Antibodies</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Zurich</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
