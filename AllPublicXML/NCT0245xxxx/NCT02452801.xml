  <primary_outcome>
    <measure>Area under the plasma concentration versus time curve from time 0 to infinity (AUC0-inf) following a single dose of ALKS 5461</measure>
    <time_frame>Up to 168 hours postdose</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Area under the plasma concentration versus time curve from time 0 to time of last measureable concentration (AUC0-last) following a single dose of ALKS 5461</measure>
    <time_frame>Up to 168 hours postdose</time_frame>
  </primary_outcome>
  <primary_outcome>
