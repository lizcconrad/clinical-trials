  <brief_title>Improving Sensitivity of Urine Cytology for Bladder Cancer</brief_title>
  <official_title>Non-Invasive Vibrational Device to Improve Sensitivity of Urine Cytology for Bladder Cancer</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Johns Hopkins University</agency>
      <agency_class>Other</agency_class>
