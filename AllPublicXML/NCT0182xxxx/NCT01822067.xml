  <brief_title>Effects of Maternal Body Composition on Offspring Brain Structure and Function</brief_title>
  <official_title>Effects of Maternal Body Composition on Offspring Brain Structure and Function</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Arkansas Children's Hospital Research Institute</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
