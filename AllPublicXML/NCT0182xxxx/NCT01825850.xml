    <intervention_model>Crossover Assignment</intervention_model>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Cmax,ss</measure>
    <time_frame>up to 72h post-dose</time_frame>
    <description>To evaluate Cmax,ss of Gemigliptin and Irbesartan</description>
  </primary_outcome>
  <primary_outcome>
    <measure>AUCτ,ss</measure>
    <time_frame>up to 72h post-dose</time_frame>
    <description>To evaluate AUCτ,ss of Gemigliptin and Irbesartan</description>
  </primary_outcome>
  <secondary_outcome>
