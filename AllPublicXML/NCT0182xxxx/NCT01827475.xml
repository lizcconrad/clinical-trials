  <brief_title>Ibuprofen Versus Acetaminophen vs Their Combination in the Relief of Musculoskeletal Pain in the Emergency Setting</brief_title>
  <official_title>Ibuprofen Versus Acetaminophen vs Their Combination in the Relief of Musculoskeletal Pain in the Emergency Setting</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Stony Brook University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
