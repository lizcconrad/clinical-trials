  <primary_outcome>
    <measure>modified Functional Independence Measurement (FIM) score</measure>
    <time_frame>Hospital discharge, an expected average stay of 5 weeks</time_frame>
    <description>Functional status as measured by modified Functional Independence Measurement (FIM) score</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
