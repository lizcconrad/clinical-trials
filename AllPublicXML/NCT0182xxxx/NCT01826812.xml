  <brief_title>Impairment of Reading Ability in Dry Eye Patients</brief_title>
  <official_title>Functional Impairment of Reading Ability in Dry Eye Patients and Effects of Sustained Reading on the Ocular Surface</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Johns Hopkins University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
