    <arm_group_label>carbetocin and placebo</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>100microgram carbetocin IV and 1cc normal saline(placebo)infusion</description>
  </arm_group>
  <arm_group>
    <arm_group_label>oxytocin and placebo</arm_group_label>
