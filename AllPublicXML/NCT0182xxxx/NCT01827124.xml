  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      Intravenous Carbetocin is more efficient than intravenous infusion oxytocin in management of
      placental delivery in 2nd trimester interruption.
    </textblock>
  </brief_summary>
  <overall_status>Unknown status</overall_status>
  <last_known_status>Recruiting</last_known_status>
  <start_date>April 2013</start_date>
  <completion_date type="Anticipated">December 2013</completion_date>
