  <phase>Phase 4</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Factorial Assignment</intervention_model>
    <primary_purpose>Prevention</primary_purpose>
    <masking>Triple (Participant, Care Provider, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Record duration of spontaneous placental expulsion after delivery of fetus in second trimester interruption</measure>
    <time_frame>at the time of delivery</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Estimation of blood loss in each group by weighing all pads put under the patient after delivery of fetus tell expulsion of placenta( before and after using of pads).</measure>
    <time_frame>at time of delivery</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Anticipated">140</enrollment>
  <condition>CARBETOCIN FOR PREVENTION OF PLACENTAL RETAINED</condition>
