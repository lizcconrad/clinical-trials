  <eligibility>
    <study_pop>
      <textblock>
        Adult patients undergoing total knee arthroplasty
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Adults ages 20-80 years old

          -  Body mass index &lt;40

          -  Undergoing primary total knee arthroplasty for osteoarthritis of the knee

        Exclusion Criteria:

          -  Inflammatory arthritis

          -  Revisions

        Other criteria may apply
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>20 Years</minimum_age>
    <maximum_age>80 Years</maximum_age>
