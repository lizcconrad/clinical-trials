    <measure>Types of lead-related adverse device effects</measure>
    <time_frame>5 years</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Percent of subjects with changes in electrode programming</measure>
    <time_frame>5 years</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of fractures with and without loss of function</measure>
    <time_frame>5 years</time_frame>
