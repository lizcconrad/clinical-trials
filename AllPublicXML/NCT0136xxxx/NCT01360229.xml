  <brief_title>Acupuncture for Fatigue in Parkinson's Disease</brief_title>
  <official_title>Acupuncture as a Symptomatic Treatment for Fatigue in Parkinson's Disease</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Colorado, Denver</agency>
      <agency_class>Other</agency_class>
