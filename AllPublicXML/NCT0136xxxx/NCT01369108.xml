  <brief_title>Clinical Study on a New Flowable Composite as a Restorative in Adult Teeth</brief_title>
  <official_title>Clinical Evaluation of a Low Shrinkage Flowable Resin Composite in Adult Teeth</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>3M</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
