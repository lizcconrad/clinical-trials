    <primary_purpose>Prevention</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>catheterization success rate</measure>
    <time_frame>immediately after catheterization (an expected average of 1 hour)</time_frame>
    <description>Subclavian vein catheterization will be performed after the induction of anesthesia and before the start of surgery.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>location of catheter tip</measure>
    <time_frame>an expected average of one day after surgery</time_frame>
    <description>Postoperative chest X-ray will be taken to check for the location of catheter tip immediately after surgery.</description>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">362</enrollment>
