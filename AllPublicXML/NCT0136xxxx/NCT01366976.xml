  <brief_title>Inhibition of Lipid Peroxidation During Cardiac Surgery</brief_title>
  <official_title>Inhibition of Lipid Peroxidation During Cardiopulmonary Bypass</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Vanderbilt University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Vanderbilt University</source>
  <oversight_info>
