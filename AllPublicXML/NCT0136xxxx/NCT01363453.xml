  <brief_title>User Surveillance in Ulcerative Colitis</brief_title>
  <acronym>CARE</acronym>
  <official_title>Acute and Remission Therapy of Ulcerative Colitis With Oral Mesalazine</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ferring Pharmaceuticals</agency>
      <agency_class>Industry</agency_class>
