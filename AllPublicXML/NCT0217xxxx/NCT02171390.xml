  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Testosteron 50 mg transdermal gel</intervention_name>
    <description>50mg testosterone gel implemented on every night</description>
    <arm_group_label>Daily testosterone transdermal gel</arm_group_label>
    <other_name>Testogel 50mg transdermal gel</other_name>
  </intervention>
