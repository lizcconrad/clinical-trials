  </arm_group>
  <arm_group>
    <arm_group_label>Injectable Testosterone esters</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>Testosteron 250mg injection per 3-4 weeks for 6 months</description>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Testosterone 250mg injection</intervention_name>
    <description>The testosterone 250mg ester intramuscular injections performed in three weeks</description>
    <arm_group_label>Injectable Testosterone esters</arm_group_label>
