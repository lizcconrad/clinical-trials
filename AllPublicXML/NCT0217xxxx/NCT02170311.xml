    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Pharmacokinetics of total serum iron (Cmax), (Tmax), (AUC0-t), (AUC0-24),(AUC0-72),(T1/2), (CL), (Vd, area), (Vd, ss), (MRT) or total urine iron (CLren), (Ae).</measure>
    <time_frame>8days</time_frame>
  </primary_outcome>
