  </study_design_info>
  <primary_outcome>
    <measure>Change in delayed onset muscle soreness over time.</measure>
    <time_frame>At 3, 6, 9, 12, 15, 21, 24, 27, 30, 33, 36, 39, 42, 45, 48 Hours</time_frame>
    <description>Likert pain surveys</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pain tolerance</measure>
    <time_frame>Baseline, 24 hours, 2 days 3 days 4 days 33 days</time_frame>
    <description>Pain tolerance measured by mmHg pressure required to elicit a delayed-onset muscle pain response.</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Bio-markers for muscle inflammation</measure>
    <time_frame>Baseline, 24 hours, 2 days 3 days 4 days 33 days</time_frame>
    <description>Bio-markers for muscle inflammation: C-reactive protein, creatine phosphokinase, and white blood cell count.</description>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">18</enrollment>
