  </study_design_info>
  <primary_outcome>
    <measure>Area under the flupentixol plasma concentration-time curve from zero to 72 hours post dose (AUC0-72h)</measure>
    <time_frame>Up to 72 hours post dose</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Maximum observed concentration (Cmax) of flupentixol</measure>
    <time_frame>Up to 312 hours post dose</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Area under the melitracen plasma concentration-time curve from zero to 72 hours post dose (AUC0-72h)</measure>
    <time_frame>Up to 72 hours post dose</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Maximum observed concentration (Cmax) of melitracen</measure>
    <time_frame>Up to 312 hours post dose</time_frame>
  </primary_outcome>
