  <brief_title>Effect of Cancer Nurse Navigators on Patient Outcomes</brief_title>
  <official_title>Pilot Test of the Effect of Cancer Nurse Navigators on Patient Outcomes</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Wisconsin, Madison</agency>
      <agency_class>Other</agency_class>
