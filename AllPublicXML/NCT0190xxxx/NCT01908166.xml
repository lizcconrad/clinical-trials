  <brief_title>Ultrasound Elastography in Diagnosing Patients With Kidney or Liver Solid Focal Lesions</brief_title>
  <official_title>US Elastography for Characterizing Focal Lesions in the Liver and Kidney</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Stanford University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
