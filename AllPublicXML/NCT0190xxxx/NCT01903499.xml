  <brief_title>Satiety Effects of Beef Compared to Beans</brief_title>
  <official_title>Satiety Effects of Beef Compared to Beans</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Minnesota - Clinical and Translational Science Institute</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
