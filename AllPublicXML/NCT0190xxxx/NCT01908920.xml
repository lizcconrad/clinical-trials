  <brief_title>Osteopathic Manipulative Treatment Effects on High Frequency Parameters in Healthy Subjects</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>European Institute for Evidence Based Osteopathic Medicine</agency>
      <agency_class>Other</agency_class>
