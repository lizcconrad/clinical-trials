  <brief_title>The Safety and Efficiency of Propranolol as an Initial Treatment for Pediatric Hemangioma</brief_title>
  <official_title>The Safety and Efficiency of Propranolol as an Initial Treatment for Pediatric Hemangioma</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Seoul National University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
