  <brief_title>Subcutaneous Botulinum Toxin for Cutaneous Allodynia</brief_title>
  <official_title>Subcutaneous Botulinum Toxin for Cutaneous Allodynia</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Stanford University</agency>
