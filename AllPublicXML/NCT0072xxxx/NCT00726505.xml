  <primary_outcome>
    <measure>The change in urinary glucose excretion dynamics</measure>
    <time_frame>after 7 days of treatment</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Differences in urinary glucose between healthy and diabetic subjects</measure>
    <time_frame>at 7 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
