  <condition>Type 2 Diabetes, Hispanic/Latinos, Diabetes Self-management Education</condition>
  <intervention>
    <intervention_type>Behavioral</intervention_type>
    <intervention_name>diabetes self-management education</intervention_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria: Self-identification as Hispanic/Latino, over the age of 18 years of
        age, diagnosis of type 2 diabetes and willingness to participate in the diabetes
        self-management education class.

        Exclusion Criteria: non-diabetic persons, non English-speaking Hispanic/Latinos with type 2
        diabetes
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>50 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
