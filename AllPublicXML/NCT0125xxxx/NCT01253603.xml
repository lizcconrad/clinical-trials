    <arm_group_type>Experimental</arm_group_type>
    <description>Fluticasone propionate inhaler twice daily for 28 days</description>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>QAW039 capsules once daily for 28 days</intervention_name>
    <arm_group_label>1</arm_group_label>
  </intervention>
