  <brief_title>Pelvic and Aortic-cava Lymphadenectomy Randomized for Ovarian Cancer</brief_title>
  <acronym>CARACO</acronym>
  <official_title>Pelvic and Aortic-cava Lymphadenectomy Randomized Phase III for Advanced Ovarian Cancer</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Institut Cancerologie de l'Ouest</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
