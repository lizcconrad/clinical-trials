  </study_design_info>
  <primary_outcome>
    <measure>Clinical Global Impression;</measure>
    <time_frame>November 2007</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Global psychosocial functioning (GAF), PANSS-Negative Subscale,changes in EEG, heart rate variability, cortisol daytime profile, catecholamines, cognitive functioning</measure>
    <time_frame>November 2007</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">40</enrollment>
  <condition>Schizophrenia</condition>
  <arm_group>
    <arm_group_label>rTMS</arm_group_label>
    <arm_group_type>Sham Comparator</arm_group_type>
    <description>sham coil system versus verum rTMS stimulation</description>
  </arm_group>
  <intervention>
    <intervention_type>Procedure</intervention_type>
    <intervention_name>rTMS, &quot;repetitive transcranial magnetic stimulation&quot;</intervention_name>
    <description>10 Hz rTMS (10.000 stimuli) over left dorsolateral prefrontal cortex</description>
    <arm_group_label>rTMS</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>rTMS</intervention_name>
    <description>repetitive transcranial magnetic stimulation, 10 Hz, 10.000 stimuli, 2 weeks</description>
    <arm_group_label>rTMS</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  DSM-IV criteria for schizophrenia

          -  age between 18 and 60

          -  informed consent

          -  right handed patient

          -  no change of antipsychotic treatment &gt; 2 weeks before recruitment

        Exclusion Criteria:

          -  seizures, epilepsy

          -  history of severe head injury

          -  status post implantation of a cardiac pacemaker or other electronic devi-ses
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>60 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
