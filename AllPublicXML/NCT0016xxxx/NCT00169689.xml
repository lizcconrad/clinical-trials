  <brief_title>Repetitive Transcranial Magnetic Stimulation</brief_title>
  <official_title>Therapeutic and Biological Effects of High Frequency Repetitive Transcranial Magnetic Stimulation (rTMS) in Schizophrenia</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Heinrich-Heine University, Duesseldorf</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
