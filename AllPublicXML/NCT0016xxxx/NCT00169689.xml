  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The study is a prospective, randomized, double-blind sham-controlled study.
    </textblock>
  </brief_summary>
  <detailed_description>
    <textblock>
      The principal objective of this trial is to investigate the effect of left prefrontal high
      frequency rTMS on negative symptoms in schizophrenia.
    </textblock>
  </detailed_description>
  <overall_status>Completed</overall_status>
  <start_date>June 2003</start_date>
  <completion_date type="Actual">August 2007</completion_date>
  <phase>N/A</phase>
