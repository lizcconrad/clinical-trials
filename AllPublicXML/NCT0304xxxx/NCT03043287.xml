  <brief_title>Voiding Efficiency, a Predictor of Clean Intermittent Catheterization (CIC)</brief_title>
  <official_title>Voiding Efficiency, a Predictor of CIC</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Allergan</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
