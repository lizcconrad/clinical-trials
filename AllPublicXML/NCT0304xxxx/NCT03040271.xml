  <oversight_info>
    <has_dmc>Yes</has_dmc>
    <is_fda_regulated_drug>No</is_fda_regulated_drug>
    <is_fda_regulated_device>No</is_fda_regulated_device>
  </oversight_info>
  <brief_summary>
    <textblock>
      Evaluate the feasibility and effectiveness of a multi-component school-based intervention to
      promote healthy eating and physical activity with school children ages 9 to 11 years in
      Lebanon.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date type="Actual">February 10, 2009</start_date>
  <completion_date type="Actual">June 10, 2013</completion_date>
