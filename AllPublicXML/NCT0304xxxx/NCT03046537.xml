  <study_design_info>
    <allocation>Non-Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <intervention_model_description>ovulatory women will be compared to women who do not ovulate regularly (PCOS)</intervention_model_description>
    <primary_purpose>Diagnostic</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Detection of different metabolic state between groups.</measure>
    <time_frame>two years</time_frame>
    <description>Finding that there special markers in the breath test that indicate a different metabolic state in PCOS.</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Anticipated">200</enrollment>
