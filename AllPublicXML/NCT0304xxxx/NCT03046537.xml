  <brief_title>Detection of PCOS by Sensors Array</brief_title>
  <official_title>Detection of PCOS and Ovulation Timing by NA-Nose</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Meir Medical Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
