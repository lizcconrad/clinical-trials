  <brief_title>Rosuvastatin Treatment for Intracranial Arterial Stenosis Based on Magnetic Resonance Angiography</brief_title>
  <official_title>Rosuvastatin Treatment for Intracranial Arterial Stenosis Based on Magnetic Resonance Angiography</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Tokyo Medical University</agency>
      <agency_class>Other</agency_class>
