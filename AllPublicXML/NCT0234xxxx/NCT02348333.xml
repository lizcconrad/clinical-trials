  <primary_outcome>
    <measure>Pharmacokinetics (Cmax: Maximum plasma concentration)</measure>
    <time_frame>1-, 4-, and 7-day</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Pharmacokinetics (Cmin: Minimum plasma concentration)</measure>
    <time_frame>1-, 2-, 3-, 4-, 5-, 6- and 7-day</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Pharmacokinetics (Tmax: Time to reach the peak plasma concentration)</measure>
    <time_frame>1-, 4-, and 7-day</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Pharmacokinetics (T1/2: Elimination half-life of plasma concentration)</measure>
    <time_frame>1-, 4-, and 7-day</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Pharmacokinetics (AUC: Area under the plasma concentration-time curve)</measure>
    <time_frame>1-, 4-, and 7-day</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Pharmacokinetics (Rauc: Accumulation ratio of AUC0-24)</measure>
    <time_frame>4-, and 7-day</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Pharmacokinetics (kel: Elimination rate constant)</measure>
    <time_frame>1-, 4-, and 7-day</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Pharmacokinetics (CLtot/F: Total clearance / Fraction of dose absorbed)</measure>
    <time_frame>1-, 4-, and 7-day</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Pharmacokinetics (Ae: Amount of drug excreted in urine)</measure>
    <time_frame>9 days</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Pharmacokinetics (fe: Fraction of dose excreted in urine)</measure>
    <time_frame>9 days</time_frame>
  </primary_outcome>
