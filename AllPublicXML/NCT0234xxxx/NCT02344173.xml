  <brief_title>ABLATOR Ablation Observational Registry</brief_title>
  <acronym>ABLATOR</acronym>
  <official_title>ABLATOR Ablation Observational Registry</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>St. Jude Medical</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
