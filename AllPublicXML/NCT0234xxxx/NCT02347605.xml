  <brief_title>Medicinal Nicotine for Preventing Cue Induced Craving</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Minnesota - Clinical and Translational Science Institute</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
