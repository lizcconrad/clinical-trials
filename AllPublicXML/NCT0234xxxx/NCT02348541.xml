  <brief_title>CollaGUARD Adhesion Barrier</brief_title>
  <official_title>A Post-Market Clinical Follow-up Study to Investigate the Usability of the CollaGUARD Adhesion Barrier Following Hysteroscopic Adhesiolysis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Innocoll</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
