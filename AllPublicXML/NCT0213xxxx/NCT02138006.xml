  <brief_title>Long-term Effects of Intensive Insulin Treatment in Type 1 Diabetes</brief_title>
  <acronym>SDIS</acronym>
  <official_title>Stockholm Diabetes Intervention Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Karolinska Institutet</agency>
