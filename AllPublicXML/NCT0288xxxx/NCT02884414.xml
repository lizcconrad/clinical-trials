  <brief_title>Gait Modifications and Cutaneous Stimulation</brief_title>
  <official_title>Analysis of Joint Mechanics in Response to Gait Modifications and Cutaneous Stimulation</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Stanford University</agency>
      <agency_class>Other</agency_class>
