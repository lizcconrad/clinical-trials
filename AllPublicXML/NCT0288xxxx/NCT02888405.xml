  <brief_title>Effects of Acute Dietary Changes on Estimates of Body Composition</brief_title>
  <official_title>Effects of Acute Dietary Changes on Estimates of Body Composition</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Baylor University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
