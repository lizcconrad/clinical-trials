  <intervention>
    <intervention_type>Dietary Supplement</intervention_type>
    <intervention_name>Porimore - A Carnitine based food supplement</intervention_name>
    <description>A daily dose of 2 grams L-carnitine, 1 gr acetyl L-carnitine, 500mg Vitamin C, 400 IU vitamin E, 60 mg Q10, 30mg Zinc Picolinate, 500mcg Folic Acid, 200 Mcg Selenium, 100 MCG Vitamin B12.</description>
    <arm_group_label>Porimore arm</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Dietary Supplement</intervention_type>
    <intervention_name>Zinc and Folate Supplementation</intervention_name>
    <description>500 Mcg Folic acid, 30-60 mg Zinc tablets/ day</description>
    <arm_group_label>Folate and Zinc</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  infertility

          -  at least one abnormal sperm quality parameter

          -  female age &lt; 35

          -  normal ovarian reserve

        Exclusion Criteria:

          -  known allergy to one of the food additives planned for treatment
      </textblock>
    </criteria>
    <gender>Male</gender>
