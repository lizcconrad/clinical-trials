  <enrollment type="Anticipated">20</enrollment>
  <condition>Ischaemia-reperfusion (IR) Injury</condition>
  <arm_group>
    <arm_group_label>CNP</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Infusion of CNP prior to IR injury</description>
  </arm_group>
  <arm_group>
