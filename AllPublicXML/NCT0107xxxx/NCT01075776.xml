  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>C-type natriuretic peptide (CNP)</intervention_name>
    <description>360 pmol/min for 15 minutes intra-arterially</description>
    <arm_group_label>CNP</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Saline</intervention_name>
    <description>Sodium Chloride 0.9% intra-arterially 0.5ml/min</description>
    <arm_group_label>Saline</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Acetylcholine</intervention_name>
    <description>2 stepped infusions for a total 9min per limb, totalling 1.05micromoles per limb</description>
    <arm_group_label>CNP</arm_group_label>
    <arm_group_label>Saline</arm_group_label>
  </intervention>
  <eligibility>
