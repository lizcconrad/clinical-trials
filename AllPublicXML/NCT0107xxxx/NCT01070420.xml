    <primary_purpose>Diagnostic</primary_purpose>
    <masking>Single (Participant)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>compare fractional flow reserve at maximal hyperemia</measure>
    <time_frame>interval of 10 minutes</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>compare the time to maximal hyperemia and changes in heart rate</measure>
    <time_frame>interval of 10 minutes</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Anticipated">66</enrollment>
