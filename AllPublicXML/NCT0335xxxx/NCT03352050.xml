  <brief_title>Mushroom Effects on Satiety and Gut Health Markers</brief_title>
  <official_title>Mushroom Effects on Satiety and Gut Health Markers</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Minnesota - Clinical and Translational Science Institute</agency>
