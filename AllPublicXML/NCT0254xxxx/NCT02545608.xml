  </study_design_info>
  <primary_outcome>
    <measure>The efficacy of Restylane Vital on skin structure compared to no treatment using a validated scale.</measure>
    <time_frame>12 weeks</time_frame>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">109</enrollment>
