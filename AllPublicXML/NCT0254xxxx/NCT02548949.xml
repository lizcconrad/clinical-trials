  <brief_title>Korean Post Marketing Surveillance to Observe Effectiveness and Safety of PRISTIQ</brief_title>
  <acronym>PMS</acronym>
  <official_title>Korean Post Marketing Surveillance To Observe Effectiveness And Safety Of Pristiq (Registered) In Patients With Major Depressive Disorder.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Pfizer</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
