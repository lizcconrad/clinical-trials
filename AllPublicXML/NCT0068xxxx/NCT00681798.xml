  <brief_title>Dose Escalation Study With Zactima and Chemotherapy in Metastatic Pancreas Carcinoma</brief_title>
  <official_title>Dose Escalation Study of the Combination of ZD6474, Gemcitabine and Capecitabine in Locally Advanced Unresectable or Metastatic Pancreatic Adenocarcinoma</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Sanofi</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
