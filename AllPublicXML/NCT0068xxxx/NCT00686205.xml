  <brief_title>Evaluation of a Human Immunodeficiency Virus (HIV) Blood Screening Assay</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Abbott Diagnostics Division</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
