  <primary_outcome>
    <measure>The percentage of subjects who report substantial improvement and consider the surgery successful, as measured by the Patient's Global Impression</measure>
    <time_frame>12, 24 and 36 months post operatively</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Evaluation of reported Adverse Events</measure>
    <time_frame>12 months post operatively</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>The percentage of subjects who remain continent or improved following treatment at timed intervals</measure>
    <time_frame>3 and 12 months post-operative</time_frame>
  </primary_outcome>
