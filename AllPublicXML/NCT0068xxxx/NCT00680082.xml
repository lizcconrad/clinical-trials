  <brief_title>Observational Usage Study of Statins in Primary Care in France</brief_title>
  <official_title>Observational Usage Study of Statins in Primary Care in France</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AstraZeneca</agency>
      <agency_class>Industry</agency_class>
