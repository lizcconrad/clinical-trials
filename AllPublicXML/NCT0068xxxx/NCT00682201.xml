  <brief_title>Hemodynamic Changes During Normal Pregnancy</brief_title>
  <official_title>Hemodynamic Changes During Normal Pregnancy: A Prospective Cohort Study of 100 Healthy Pregnant Women</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Oslo University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
