  <brief_title>Effect of Resistant Starch Type 4 on Glycemia and Insulin Sensitivity in Young Adults</brief_title>
  <official_title>Effect of Resistant Starch Type 4 on Glycemia and Insulin Sensitivity in Young Adults</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Kansas State University</agency>
      <agency_class>Other</agency_class>
