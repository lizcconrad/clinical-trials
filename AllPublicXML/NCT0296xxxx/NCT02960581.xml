  </primary_outcome>
  <primary_outcome>
    <measure>Impact of viral load and/or ART on PGT121 disposition clearance (CL/F)</measure>
    <time_frame>6 Months post infusion</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Impact of viral load and/or ART on PGT121 disposition volume of distribution (Vz/F)</measure>
    <time_frame>6 months post infusion</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Impact of viral load and/or ART on PGT121 disposition total exposure</measure>
    <time_frame>6 months post infusion</time_frame>
  </primary_outcome>
