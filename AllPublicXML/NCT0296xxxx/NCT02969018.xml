  <brief_title>Study To Evaluate Safety And Efficacy Of PF-06700841 In Subjects With Moderate To Severe Plaque Psoriasis</brief_title>
  <official_title>A Phase 2a, Randomized, Double-blind, Placebo-controlled Study To Evaluate Safety And Efficacy Of Pf-06700841 In Subjects With Moderate To Severe Plaque Psoriasis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Pfizer</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Pfizer</source>
  <oversight_info>
