    <arm_group_label>cohort 3</arm_group_label>
    <description>Bicyclol</description>
  </arm_group>
  <arm_group>
    <arm_group_label>cohort 4</arm_group_label>
    <description>Silibinin capsules</description>
  </arm_group>
  <arm_group>
    <arm_group_label>cohort 5</arm_group_label>
    <description>Ursodeoxycholic acid capsules</description>
  </arm_group>
  <arm_group>
    <arm_group_label>cohort 6</arm_group_label>
