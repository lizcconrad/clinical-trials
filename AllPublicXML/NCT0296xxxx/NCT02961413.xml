  <biospec_descr>
    <textblock>
      serum, plasma, histology, urine
    </textblock>
  </biospec_descr>
  <eligibility>
    <study_pop>
      <textblock>
        all patients with drug-induced liver injury
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Diagnosis of DILI patients with various types and severity according to the 2015
             version of the Chinese DILI treatment guidelines

          -  RUCAM ≥6, or RUCAM between 3-5 is required by the three experts determined that the
             drug-induced liver injury

          -  patient can provide informed consent form

        Exclusion Criteria:

          -  non-drug-induced liver injury
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>N/A</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
