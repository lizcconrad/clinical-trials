  <brief_title>The Effect of Isotretinoin on the Etonogestrel Contraceptive Implant</brief_title>
  <official_title>The Effect of Isotretinoin on the Etonogestrel Contraceptive Implant</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Colorado, Denver</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
