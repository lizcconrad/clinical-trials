  <primary_outcome>
    <measure>First pass success without hypoxemia</measure>
    <time_frame>10 minutes</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change in peri-intubation saturation</measure>
    <time_frame>0, 30, 60 and 120 seconds</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Saturation at time of tube placement</measure>
    <time_frame>10 minutes</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Time to desaturation &lt;93%</measure>
    <time_frame>measured time until saturations drop below 93% during the peri-intubation period, up to 10 minutes</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Saturation at post intubation</measure>
    <time_frame>0, 30, 60, and 120 seconds</time_frame>
  </secondary_outcome>
