  <primary_outcome>
    <measure>Progress of labour</measure>
    <time_frame>up to 2 month</time_frame>
    <description>Cervical dilated 1 cm each hour</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>New born</measure>
    <time_frame>up to 1 month</time_frame>
    <description>Apgar scor at 1 minute and Apgar scor at 5 minutes</description>
  </secondary_outcome>
