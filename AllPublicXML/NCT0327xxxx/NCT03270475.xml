  <primary_outcome>
    <measure>Skeletal muscle proteome signature</measure>
    <time_frame>Change from baseline proteome signature at 5 weeks</time_frame>
    <description>proteomics</description>
  </primary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">8</enrollment>
  <condition>Healthy</condition>
  <arm_group>
    <arm_group_label>Exercise</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>12-15 training sessions of 30 min over 4-5 weeks</description>
  </arm_group>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>exercise training</intervention_name>
    <description>subjects complete 4-5 weeks of training</description>
    <arm_group_label>Exercise</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Healthy

          -  VO2max of 45-55 ml/kg/min (+/- SD)

          -  Body mass index of 19-26 kg/m2

          -  Normal ECG

        Exclusion Criteria:

          -  Serious adverse effects to exercise

          -  Chronic disease
      </textblock>
    </criteria>
    <gender>Male</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>40 Years</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
