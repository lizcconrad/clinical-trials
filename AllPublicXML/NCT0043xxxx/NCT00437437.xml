    <allocation>Randomized</allocation>
    <intervention_model>Crossover Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>To determine the effect on pharmacokinetics of BMS-540215, the active metabolite of brivanib alaninate, when administered following a high fat meal versus administration in a fasted state</measure>
    <time_frame>throughout the study</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>To assess safety and tolerability of Brivanib alaninate when administered in a fasted state, or following a high fat meal</measure>
    <time_frame>throughout the study</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">29</enrollment>
