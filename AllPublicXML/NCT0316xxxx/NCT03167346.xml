  <brief_summary>
    <textblock>
      To investigate public awareness of core stroke symptoms in more than 40 years old population
      and its related factors.
    </textblock>
  </brief_summary>
  <overall_status>Recruiting</overall_status>
  <start_date type="Actual">January 1, 2017</start_date>
  <completion_date type="Anticipated">December 31, 2018</completion_date>
  <primary_completion_date type="Anticipated">May 31, 2017</primary_completion_date>
