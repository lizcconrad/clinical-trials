    <observational_model>Ecologic or Community</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>awareness rate</measure>
    <time_frame>2014-2017</time_frame>
    <description>The awareness rate of core symptoms (FAST: facial droop, arm weakness, speech disturbance, time to call an ambulance)</description>
  </primary_outcome>
  <enrollment type="Anticipated">200000</enrollment>
  <condition>Stroke</condition>
  <condition>Awareness</condition>
  <condition>Health Knowledge, Attitudes, Practice</condition>
  <condition>Core Symptoms</condition>
  <condition>FAST</condition>
  <eligibility>
    <study_pop>
      <textblock>
        Participants are more than 40 years old without specific requirements. This study is a
        population-based screening programme.
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  More than 40 years old (born before December 31st 1976) without specific requirements.

          -  Be able to finish the questionnaire.

        Exclusion Criteria:

          -  Participants with disturbance of consciousness.
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>40 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
