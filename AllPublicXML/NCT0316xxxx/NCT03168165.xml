  <brief_title>PNE Effectiveness Cluster Trial</brief_title>
  <acronym>PNE</acronym>
  <official_title>The Effectiveness of Training Physical Therapists in Pain Neuroscience Education on Patient Reported Outcomes for Patients With Chronic Spinal Pain</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Utah</agency>
      <agency_class>Other</agency_class>
