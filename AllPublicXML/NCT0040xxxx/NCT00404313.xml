  </study_design_info>
  <primary_outcome>
    <measure>Pulmonary function test data measured over 6 weeks</measure>
    <time_frame>6 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Overall daytime and nighttime symptoms score, beta-agonist use, asthma exacerbations, asthma-specific quality of life questionnaire score, measured over 6 weeks</measure>
    <time_frame>6 weeks</time_frame>
  </secondary_outcome>
