    <allocation>Non-Randomized</allocation>
    <intervention_model>Single Group Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>To evaluate the pharmacokinetic (PK) profile of HCV-796 in healthy men and women of different ages.</measure>
    <time_frame>7 days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>To assess the safety and tolerability of a single oral dose of HCV-796 in healthy men and women of different ages.</measure>
    <time_frame>15 days</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
