  <brief_title>Dichotic Listening as a Predictor of Medication Response in Depression</brief_title>
  <official_title>Dichotic Listening as a Predictor of Medication Response in Depression</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>New York State Psychiatric Institute</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
