  </primary_outcome>
  <primary_outcome>
    <measure>Maximum observed concentration (Cmax) of vortioxetine</measure>
    <time_frame>Up to 72 hours postdose</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Nominal time corresponding to the occurrence of Cmax (tmax)</measure>
    <time_frame>Up to 72 hours postdose</time_frame>
  </primary_outcome>
