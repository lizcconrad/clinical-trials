  <brief_title>ARMOR1: Study of TOK-001 to Treat Castration Resistant Prostate Cancer</brief_title>
  <acronym>ARMOR1</acronym>
  <official_title>ARMOR1: Phase 1, Open Label, Dose Escalation Trial of TOK-001 for the Treatment of Chemotherapy Naive Castration Resistant Prostate Cancer</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Novus Therapeutics, Inc</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
