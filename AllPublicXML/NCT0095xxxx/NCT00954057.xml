  <primary_outcome>
    <measure>Safety: physical examinations, laboratory tests, VISA activity score, AE assessments</measure>
    <time_frame>8 weeks treatment and 1 week follow up</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Efficacy: change in proptosis, VISA activity score, VAS for Eye Appearance Satisfaction</measure>
    <time_frame>8 weeks treatment and 1 week follow up</time_frame>
