  </study_design_info>
  <primary_outcome>
    <measure>Summarize treatment area recurrence of AK lesions in the selected treatment area</measure>
    <time_frame>12 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Summarize long-term safety data (incidence of adverse events in the treatment area)</measure>
    <time_frame>12 months</time_frame>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Actual">98</enrollment>
  <condition>Actinic Keratosis</condition>
  <arm_group>
