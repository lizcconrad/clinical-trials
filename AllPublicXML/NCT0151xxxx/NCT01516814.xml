    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Number of participants with newly onset of symptomatic venous thromboembolism (VTE)</measure>
    <time_frame>Up to 12 months</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Number of clinically relevant bleedings</measure>
    <time_frame>Up to 2 days after last dose</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number of participants with improvement in thrombotic burden</measure>
    <time_frame>At week 3</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of participants with deterioration in thrombotic burden</measure>
    <time_frame>Up to 12 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of participants with the composite of newly onset of symptomatic VTE or asymptomatic deterioration of thrombus</measure>
    <time_frame>Up to 12 months</time_frame>
  </secondary_outcome>
