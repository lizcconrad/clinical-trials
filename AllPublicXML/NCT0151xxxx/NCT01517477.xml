    <masking>Single (Outcomes Assessor)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Mean diurnal intraocular pressure (IOP) reduction of greater than or equal to 20% at month 12 vs. baseline</measure>
    <time_frame>12 Months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Mean diurnal IOP &lt;18 mmHg at month 12</measure>
    <time_frame>12 months</time_frame>
  </secondary_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment type="Anticipated">251</enrollment>
