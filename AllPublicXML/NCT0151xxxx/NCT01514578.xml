  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      This study will evaluate the safety, blood levels, and effects of TRV130A on pain perception
      and sedation in healthy adult males.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>January 2012</start_date>
