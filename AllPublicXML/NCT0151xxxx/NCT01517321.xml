    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Incidences of adverse experiences and change in vital sign, safety lab etc. as parameters of safety and tolerability</measure>
    <time_frame>12 weeks and 52 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>HbA1c</measure>
    <time_frame>12 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>2-hour postmeal glucose</measure>
    <time_frame>12 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Fasting plasma glucose</measure>
    <time_frame>12 weeks</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <condition>Type 2 Diabetes</condition>
