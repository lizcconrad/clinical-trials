  <brief_title>MAVRIC VR ICD Registry</brief_title>
  <official_title>Marquis/Maximo VR ICD Programming Practices Registry (MAVRIC VR ICD Registry)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Medtronic Cardiac Rhythm and Heart Failure</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
    <collaborator>
