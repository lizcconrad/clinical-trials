  <eligibility>
    <study_pop>
      <textblock>
        Subjects treated for glabellar lines (hyperfunctional facial lines) with botulinum toxin
        Type A
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Previously treated for glabellar lines (hyperfunctional facial lines) with botulinum
             toxin Type A preparation

          -  Received 2 treatment cycles wtih botulinum toxin Type A (BOTOX®) prior to at least 1
             treatment with botulinum toxin Type A (Xeomin®)

        Exclusion Criteria:
      </textblock>
    </criteria>
    <gender>Female</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
