  <brief_title>The Effect of a Liquid Protein Modular on Growth and Outcomes in the NICU</brief_title>
  <official_title>The Effect of a Liquid Protein Modular on Growth and Outcomes in the NICU</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Nebraska</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>Alegent Health Bergan Mercy Medical Center</agency>
