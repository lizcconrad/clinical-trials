  <brief_title>Surveillance of Safety and Efficacy of Wilate in Patients With Von Willebrand Disease</brief_title>
  <official_title>Surveillance of Safety and Efficacy of Wilate in Patients With Von Willebrand Disease</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Octapharma</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
