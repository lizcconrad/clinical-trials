  <brief_title>ZOSTAVAX™ (Zoster Vaccine Live) Long-term Effectiveness Study (V211-024)</brief_title>
  <official_title>Post-Licensure Observational Study of the Long-term Effectiveness of ZOSTAVAX™</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Merck Sharp &amp; Dohme Corp.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>Kaiser Permanente</agency>
      <agency_class>Other</agency_class>
    </collaborator>
  </sponsors>
  <source>Merck Sharp &amp; Dohme Corp.</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      This study will describe the impact of vaccination with ZOSTAVAX™ on the epidemiology of
      herpes zoster (HZ) in a cohort of vaccinated participants 50 years or age or older, compared
      to a cohort of unvaccinated participants.
    </textblock>
  </brief_summary>
  <overall_status>Active, not recruiting</overall_status>
  <start_date type="Actual">May 15, 2012</start_date>
  <completion_date type="Anticipated">October 30, 2023</completion_date>
  <primary_completion_date type="Anticipated">October 30, 2023</primary_completion_date>
  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Cohort</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Incidence of Herpes Zoster in Vaccinated and Unvaccinated Cohorts, Overall, and by Age (50-59, 60-69, 70-79 ≥80) at Vaccination</measure>
    <time_frame>10 years</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Incidence of Herpes Zoster in Vaccinated and Unvaccinated Cohorts by Time since Vaccination</measure>
    <time_frame>10 years</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Incidence of Postherpetic Neuralgia in Vaccinated and Unvaccinated Cohorts, Overall, and by Age (50-59, 60-69, 70-79, ≥80) at Vaccination</measure>
    <time_frame>10 years</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Incidence of Postherpetic Neuralgia in Vaccinated and Unvaccinated Cohorts by Time since Vaccination</measure>
    <time_frame>10 years</time_frame>
  </secondary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Anticipated">30000</enrollment>
