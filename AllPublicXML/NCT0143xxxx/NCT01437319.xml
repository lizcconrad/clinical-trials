  <brief_title>Mucin Balls and Corneal Inflammation Events</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Johnson &amp; Johnson Vision Care, Inc.</agency>
      <agency_class>Industry</agency_class>
