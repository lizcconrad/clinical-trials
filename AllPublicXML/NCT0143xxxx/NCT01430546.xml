  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to quantify the burden of treatment in relapsed or refractory
      multiple myeloma in patients receiving lenalidomide after one prior treatment for myeloma.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>November 2010</start_date>
  <primary_completion_date type="Actual">January 2016</primary_completion_date>
