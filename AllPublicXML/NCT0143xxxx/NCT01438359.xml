    <arm_group_type>Experimental</arm_group_type>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>PA21 and Furosemide with food</intervention_name>
    <description>The maximum dose of PA21 will be 15.0 g/day. The maximum dose of Furosemide will be 40 mg/day</description>
    <arm_group_label>PA21 and Furosemide with food</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>No PA21; Furosemide with food</intervention_name>
    <description>The maximum dosage of Furosemide will be 40 mg/day</description>
    <arm_group_label>No PA21; Furosemide with food</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>PA21 with food and Furosemide 2hrs later</intervention_name>
    <description>The maximum dose of PA21 will be 15 g/day. The maximum dose of Furosemide will be 40 mg/day</description>
    <arm_group_label>PA21 with food and Furosemide 2hrs later</arm_group_label>
  </intervention>
