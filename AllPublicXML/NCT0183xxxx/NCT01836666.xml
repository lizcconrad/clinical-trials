  <brief_title>Effects on Health Related Quality of Life Among Patients With Chronic Pain by Interventional Pain Management</brief_title>
  <official_title>Effects on Health Related Quality of Life Among Patients With Chronic Pain by Interventional Pain Management</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Eques Indolor AB</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
