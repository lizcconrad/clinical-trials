  <brief_title>Brain Fuel - Morning Nutrition and Cognitive Function</brief_title>
  <official_title>Morning Nutrition and Cognitive Function in Preadolescents</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Arkansas Children's Hospital Research Institute</agency>
      <agency_class>Other</agency_class>
