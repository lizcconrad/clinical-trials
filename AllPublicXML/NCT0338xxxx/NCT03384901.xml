  <brief_title>The Results of Pars Plana Vitrectomy in Diabetic Macular Edema</brief_title>
  <official_title>The Results of Pars Plana Vitrectomy in Diabetic Macular Edema</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Hillel Yaffe Medical Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Hillel Yaffe Medical Center</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
    <is_fda_regulated_drug>No</is_fda_regulated_drug>
    <is_fda_regulated_device>No</is_fda_regulated_device>
  </oversight_info>
  <brief_summary>
    <textblock>
      This study will be a retrospective analysis of vitrectomy results in patients suffering from
      diabetic macular edema.
    </textblock>
  </brief_summary>
  <overall_status>Not yet recruiting</overall_status>
  <start_date type="Anticipated">January 2018</start_date>
  <completion_date type="Anticipated">January 2019</completion_date>
  <primary_completion_date type="Anticipated">January 2019</primary_completion_date>
  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Case-Only</observational_model>
    <time_perspective>Retrospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Visual acuity</measure>
    <time_frame>6 months</time_frame>
    <description>Visual acuity will be measured using the Snellen Chart before and after vitrectomy</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Macular thickness</measure>
    <time_frame>6 months</time_frame>
    <description>Macular thickness will be measured using Optical Coherence Tomography before and after vitrectomy</description>
  </secondary_outcome>
  <enrollment type="Anticipated">70</enrollment>
  <condition>Diabetic Macular Edema</condition>
  <intervention>
    <intervention_type>Procedure</intervention_type>
    <intervention_name>vitrectomy</intervention_name>
    <description>Pars plana vitrectomy and ILM (internal limiting membrane) peeling</description>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        Patients attending the Hillel Yaffe Medical Center Ophthalmology Clinic
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Diabetic macular edema patients after vitrectomy

        Exclusion Criteria:

          -  All others
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
