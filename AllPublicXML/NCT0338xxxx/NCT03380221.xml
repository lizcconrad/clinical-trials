    <primary_purpose>Prevention</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Change of Satiety</measure>
    <time_frame>0, 20, 40, 60, 90 and 120 minutes post snack consumption</time_frame>
    <description>Hunger feeling (appetite) will be examined using visual analog scale (0-10): 0 is least and 10 greatest.</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Anticipated">40</enrollment>
  <condition>Satiety</condition>
  <arm_group>
    <arm_group_label>Watermelon</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
  </arm_group>
  <arm_group>
    <arm_group_label>Low fat cookies</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
  </arm_group>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>Watermelon</intervention_name>
    <description>Participants will consume 100 Kcal of watermelon fruit (2 cups) daily for 4 weeks.</description>
    <arm_group_label>Watermelon</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>Low fat cookies</intervention_name>
    <description>Participants will consume 100 Kcal of low fat cookies daily for 4 weeks.</description>
    <arm_group_label>Low fat cookies</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  18-55 years old

          -  BMI 25-40

        Exclusion Criteria:

          -  Smoker

          -  Pregnant woman

          -  Required dietary supplement use

          -  Required medication of metabolic disorders

          -  Allergy to watermelon or gluten
      </textblock>
