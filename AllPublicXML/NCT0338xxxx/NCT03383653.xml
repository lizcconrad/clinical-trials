  <brief_title>Safety Profile Following Td Vaccination in Indonesian Pregnant Woman</brief_title>
  <official_title>Safety Profile Following Td Vaccination in Indonesian Pregnant Woman</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>PT Bio Farma</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>Indonesian Vaccine Safety Advisory Committee</agency>
      <agency_class>Other</agency_class>
    </collaborator>
  </sponsors>
