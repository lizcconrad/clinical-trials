  <primary_outcome>
    <measure>Safety and toleration: Adverse events, vital signs measurements (supine and standing), 12-lead ECGs, spirometry (FEV1, FVC), laboratory safety tests, physical examination, and subject-reported dry mouth assessment.</measure>
    <time_frame>3 Weeks</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Pharmacokinetics: Plasma: Cmax, Tmax, AUCinf, AUCτ, t½, accumulation ratio, linearity ratio.</measure>
    <time_frame>3 Weeks</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Pharmacodynamics: Salivary flow rate</measure>
    <time_frame>3 weeks</time_frame>
  </primary_outcome>
