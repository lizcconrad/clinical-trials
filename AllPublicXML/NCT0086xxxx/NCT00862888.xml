  <primary_outcome>
    <measure>Assess effect of single doses of PF-00446687 on erectile function (using the Rigiscan+ device)</measure>
    <time_frame>Day of dosing</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Evaluate subjective self-assessment of sexual interest and desire by means of a questionnaire</measure>
    <time_frame>Day of dosing</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Diary of sexual activities</measure>
    <time_frame>From day of dosing to 7 days post-dose</time_frame>
  </primary_outcome>
  <secondary_outcome>
