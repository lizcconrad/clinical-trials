        <analysis_list>
          <analysis>
            <group_id_list>
              <group_id>O1</group_id>
              <group_id>O2</group_id>
            </group_id_list>
            <groups_desc>Null hypothesis: fesoterodine treatment is not associated with greater reduction in urgency incontinence frequency compared to placebo in women diagnosed using the 3IQ</groups_desc>
            <non_inferiority_type>Superiority or Other</non_inferiority_type>
            <p_value>&lt;0.001</p_value>
            <p_value_desc>No adjustment for multiple comparisons, threshold for significance is P &lt; .05</p_value_desc>
