    <intervention_type>Biological</intervention_type>
    <intervention_name>tanezumab 10 mg</intervention_name>
    <description>tanezumab 10 mg one dose at weeks 0 and 8</description>
    <arm_group_label>1</arm_group_label>
  </intervention>
