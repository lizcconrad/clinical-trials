    <intervention_model>Crossover Assignment</intervention_model>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Blood atazanavir pharmacokinetic sampling to be collected on days 1, 2 and 3 of each of the 2 periods of the study</measure>
  </primary_outcome>
  <secondary_outcome>
    <measure>Blood ritonavir pharmacokinetic sampling to be collected on days 1, 2 and 3 of each of the 2 periods of the study</measure>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Safety parameters including physical exam (PE), vital signs (VS), electrocardiogram (ECG) and clinical laboratory tests will be collected at study discharge</measure>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment>46</enrollment>
  <condition>HIV Infections</condition>
