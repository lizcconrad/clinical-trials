    <primary_purpose>Basic Science</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Adverse events, vital signs measurements, 12-lead ECGs, physical examination findings, blood safety tests.</measure>
    <time_frame>1 month</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>PF-02413873 pharmacokinetics</measure>
    <time_frame>1 month</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Endometrial thickness, Ovarian follicular estradiol secretion, Pre-ovulatory LH surge, Corpus Luteum progesterone secretion.</measure>
    <time_frame>14 days</time_frame>
  </primary_outcome>
