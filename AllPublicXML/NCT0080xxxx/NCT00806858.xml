  </study_design_info>
  <primary_outcome>
    <measure>The change in subjects' satisfaction with insulin therapy, measured by change from visit 1 to visit 2 in subject treatment satisfaction scores on the DTSQ (Diabetes Treatment Satisfaction Questionnaire)</measure>
    <time_frame>3 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Subjects' device preference</measure>
    <time_frame>3 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Subjects' NovoPen® 4 evaluation</measure>
    <time_frame>3 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of clinical technical complaints</measure>
    <time_frame>3 months</time_frame>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
