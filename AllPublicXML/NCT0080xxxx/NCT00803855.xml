    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Safety and tolerability of AZD1446 by assessment of serious adverse events, ECGs, vital signs and laboratory variables</measure>
    <time_frame>From first to last visit</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Determine the single ascending dose pharmacokinetics of AZD1446</measure>
    <time_frame>PK sampling taken at defined timepoints during residential period</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Determine the single dose of AZD1446 on food interaction</measure>
    <time_frame>From first to last visit</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">110</enrollment>
