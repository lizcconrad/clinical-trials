  <primary_outcome>
    <measure>Incidence of ocular hyperemia</measure>
    <time_frame>12 months</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>safety of treatment</measure>
    <time_frame>12 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>effectiveness in IOP lowering</measure>
    <time_frame>12 months</time_frame>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
