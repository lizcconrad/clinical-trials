  <primary_outcome>
    <measure>SCL 90 questionnaire</measure>
    <time_frame>day 0 and day 30</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Stress perçu, coping de vitaliano, hospital anxiety and depression questionnaires and urinary cortisol measurement</measure>
    <time_frame>day-14 ; day 0 and day 30</time_frame>
  </secondary_outcome>
