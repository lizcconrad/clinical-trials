    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Bioequivalence was assessed on the pharmacokinetic variables Cmax, AUC,0-t, and AUC,0-infinity</measure>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">30</enrollment>
  <condition>Bioequivalency</condition>
  <arm_group>
