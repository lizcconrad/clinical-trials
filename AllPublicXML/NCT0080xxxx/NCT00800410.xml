  <brief_title>REACH Detroit Partnership Family Intervention</brief_title>
  <official_title>A Pilot Randomized Diabetes Intervention Among Inner-city African American and Latino Adults</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Michigan</agency>
      <agency_class>Other</agency_class>
