  <primary_outcome>
    <measure>Occurrence of adverse events</measure>
    <time_frame>up to 15 days postdose</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Cmax (maximum measured concentration of the analyte in plasma)</measure>
