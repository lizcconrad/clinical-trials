  <primary_outcome>
    <measure>Levels of lipids in the endometrial fluid as a diagnostic factor.</measure>
    <time_frame>1 year</time_frame>
  </primary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">40</enrollment>
