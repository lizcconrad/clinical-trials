    <intervention_name>Chocolate milk</intervention_name>
    <description>500ml chocolate milk, immediately after training, 9 weeks</description>
    <arm_group_label>Whey protein</arm_group_label>
    <other_name>Milk</other_name>
  </intervention>
  <intervention>
    <intervention_type>Dietary Supplement</intervention_type>
    <intervention_name>Sugar</intervention_name>
    <description>55-65g maltodextrin solution, immediately after training, 9 weeks</description>
    <arm_group_label>Sugar</arm_group_label>
    <other_name>Maltodextrin</other_name>
  </intervention>
