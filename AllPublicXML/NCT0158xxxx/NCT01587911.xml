  <brief_title>Casinomacropeptide and Satiety</brief_title>
  <acronym>CMP2</acronym>
  <official_title>Characterization of the Physiological and Subjective Satiety Response to Preloads Varying in Bioactive Dairy Proteins</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of California, Davis</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
