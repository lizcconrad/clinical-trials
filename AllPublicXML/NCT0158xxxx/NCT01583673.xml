  <brief_title>Growth of Infants Fed an Amino Acid Infant Formula</brief_title>
  <official_title>Assessment of Growth of Infants Fed an Amino Acid Based Formula</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Nestlé</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
