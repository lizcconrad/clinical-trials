  <brief_title>Bipolar Disorder Study for Men and Women</brief_title>
  <official_title>Double Blind Placebo Controlled Study of Lamictal in Acute Bipolar Depression</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>GlaxoSmithKline</agency>
      <agency_class>Industry</agency_class>
