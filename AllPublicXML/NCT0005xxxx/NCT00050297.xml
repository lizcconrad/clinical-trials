  <brief_title>YM598 in Patients With Rising PSA After Initial Therapy for Localized Prostate Cancer</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Astellas Pharma Inc</agency>
      <agency_class>Industry</agency_class>
