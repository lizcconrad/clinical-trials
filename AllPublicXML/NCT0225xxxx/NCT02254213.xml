  <brief_summary>
    <textblock>
      The main objective is to evaluate the stability of EEG features on an individual level.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>June 2005</start_date>
  <completion_date type="Actual">August 2007</completion_date>
