    <intervention_name>Replenine®-VF (High Purity Factor IX)</intervention_name>
    <arm_group_label>Replenine®-VF</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

