  <study_design_info>
    <primary_purpose>Treatment</primary_purpose>
  </study_design_info>
  <primary_outcome>
    <measure>Incremental recovery for Factor IX</measure>
    <time_frame>90 min post-dose</time_frame>
    <description>Incremental recovery is defined as peak rise in plasma Factor IX levels divided by Factor IX dose in IU/KG</description>
  </primary_outcome>
  <number_of_arms>1</number_of_arms>
  <condition>Haemophilia B</condition>
