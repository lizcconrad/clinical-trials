  <primary_outcome>
    <measure>Mean average BCVA change</measure>
    <time_frame>12 months</time_frame>
    <description>Mean average BCVA change from Month 1 through Month 12 compared to baseline.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Mean BCVA change by visit</measure>
    <time_frame>12 months</time_frame>
    <description>Mean BCVA change from baseline at each visit</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Mean change in Central subfield Thickness (CSFT)</measure>
    <time_frame>12 month</time_frame>
    <description>Mean change in Central Sub-Field Thickness (CSFT) from baseline at each visit</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>BCVA improvement of ≥ 10 and ≥ 15 letters</measure>
    <time_frame>12 months</time_frame>
    <description>Proportion of patients achieving BCVA improvement of ≥ 10 and ≥ 15 letters from baseline to Month 12</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>BCVA loss of &lt; 10 and &lt; 15 letters</measure>
    <time_frame>12 months</time_frame>
    <description>Proportion of patients with BCVA loss of &lt; 10 and &lt; 15 letters from baseline to Month 12</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Visual acuity ≥ 73 letters</measure>
    <time_frame>month 12</time_frame>
    <description>Proportion of patients with visual acuity ≥ 73 letters (approximate 20/40 Snellen chart equivalent) at Month 12</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Mean average BCVA change after month 3</measure>
    <time_frame>12 month</time_frame>
    <description>Mean average BCVA change from Month 4 to Month 12 compared to Month 3</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Mean change in patient reported visual functioning scale</measure>
    <time_frame>6 and 12 month</time_frame>
    <description>Mean change in the patient-reported visual functioning through VFQ-25 composite and subscale scores at Month 6 and Month 12 compared to baseline.</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Adverse events</measure>
    <time_frame>12 months</time_frame>
    <description>To evaluate the safety of 0.5 mg ranibizumab intravitreal injections relative to laser photocoagulation, as assessed by the type, frequency and severity of ocular and non-ocular adverse events over 12 months.</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Evaluation of treatment patterns</measure>
    <time_frame>12 months</time_frame>
    <description>To evaluate the number of re-treatments and retreatment patterns (assessed by time of stabilization/time to re-initiation of treatment) in patients treated with 0.5 mg ranibizumab.</description>
  </secondary_outcome>
