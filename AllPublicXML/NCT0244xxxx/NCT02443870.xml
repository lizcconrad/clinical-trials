  <brief_title>Long-term Urodynamics in Individuals With Neurogenic Lower Urinary Tract Dysfunction</brief_title>
  <official_title>Retrospective Analysis of Long-term Urodynamics in Individuals With Neurogenic Lower Urinary Tract Dysfunction</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Swiss Paraplegic Centre Nottwil</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
