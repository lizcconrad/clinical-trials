  <primary_outcome>
    <measure>Prognostic Model of Overall Survival (OS)</measure>
    <time_frame>10 years</time_frame>
    <description>The OS prognostic model uses Individual patient data from a pooled analysis of 12 ipilimumab studies.</description>
  </primary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Actual">600</enrollment>
  <condition>Melanoma</condition>
  <arm_group>
