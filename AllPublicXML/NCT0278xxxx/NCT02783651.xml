  <brief_title>An Study of Patients With Ph- Chromosome-negative Relapsed or Refractory Acute Lymphoblastic Leukemia in the US</brief_title>
  <official_title>An Observational Study of Patients With Philadelphia Chromosome-negative Relapsed or Refractory Acute Lymphoblastic Leukemia in the US</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Amgen</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Amgen</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      A retrospective chart review study of Philadelphia chromosome-negative R/R ALL patients in
      the US.
    </textblock>
  </brief_summary>
  <overall_status>Recruiting</overall_status>
  <start_date type="Actual">June 30, 2016</start_date>
  <completion_date type="Anticipated">March 29, 2019</completion_date>
  <primary_completion_date type="Anticipated">March 29, 2019</primary_completion_date>
  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Other</observational_model>
    <time_perspective>Retrospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Treatment patterns in patients with Ph- R/R ALL</measure>
    <time_frame>30 months</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Drug utilization in patients with Ph- R/R ALL</measure>
    <time_frame>30 months</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Healthcare resource utilization in patients with Ph- R/R ALL</measure>
    <time_frame>30 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Receipt of allogeneic stem cell transplantation following salvage treatment</measure>
    <time_frame>30 Months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Incidence of selected adverse events including cytokine release syndrome, neurologic events, any events resulting in hospitalizations, and other serious adverse events.</measure>
    <time_frame>30 Months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Best response to salvage treatment (chemotherapy or blinatumomab) within 8 weeks and within 12 weeks of initiation of salvage treatment.</measure>
    <time_frame>12 Weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>MRD status within 12 weeks of initiation of salvage treatment</measure>
    <time_frame>12 Weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Overall survival from the time of initiation of salvage treatment</measure>
    <time_frame>30 Months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>RFS from time remission achieved with salvage treatment</measure>
    <time_frame>30 Months</time_frame>
  </secondary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Anticipated">300</enrollment>
  <condition>Relapsed/Refractory Acute Lymphoblastic Leukemia</condition>
  <arm_group>
    <arm_group_label>No treatment 1</arm_group_label>
    <description>It is planned to have ~20-30 sites participating on the trial for chart review of approximately 300 patients initiating treatment for Philadelphia chromosome-negative (Ph-) Relapsed or Refractory (R/R) Acute Lymphoblastic Leukemia (ALL) between January 2013 and October 2017.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>No Treatment 2</arm_group_label>
    <description>Initial record abstraction will occur at study site with subsequent reviews occurring at the site every 3 months thereafter until study conclusion on October 2018.</description>
  </arm_group>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>Other</intervention_name>
    <description>No intervention other than routine medical care</description>
    <arm_group_label>No treatment 1</arm_group_label>
    <arm_group_label>No Treatment 2</arm_group_label>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        Patients initiating treatment for Philadelphia chromosome-negative (Ph-) R/R ALL between
        January 2013 and October 2017 at participating clinical sites in the US
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Medical records of patients initiating treatment for Ph- R/R ALL at participating
             clinical centers in the US between January 2013 and October 2017 will be eligible for
             inclusion.

        Exclusion Criteria:

          -  Medical records of patients with Ph+ ALL will be excluded

          -  If informed consent is required, medical records of patients who do not provide
             informed consent will be excluded
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>N/A</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
