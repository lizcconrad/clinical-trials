  <brief_title>Prophylactic Pregabalin to Decrease Pain During Medical Abortion</brief_title>
  <official_title>Prophylactic Pregabalin to Decrease Pain During Medical Abortion: a Randomized Controlled Trial</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Hawaii</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>Society of Family Planning</agency>
