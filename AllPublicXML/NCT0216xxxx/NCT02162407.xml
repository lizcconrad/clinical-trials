    <measure>Steady state glucose infusion rate (GIR) / interstitial insulin steady state concentration</measure>
    <time_frame>8 hours</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Steady state glucose infusion rate (GIR)</measure>
    <time_frame>8 hours</time_frame>
  </secondary_outcome>
