  <brief_title>Endotracheal Tube (ETT) Cuff Pressure Study in the Emergency Department (ED)</brief_title>
  <official_title>Endotracheal Tube Cuff Pressure in the Pediatric Emergency Department: A Pilot Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Edward Ferenczy</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
