  <brief_title>AGN-229666 Ophthalmic Solution in Japanese Patients With Allergic Conjunctivitis</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Allergan</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
