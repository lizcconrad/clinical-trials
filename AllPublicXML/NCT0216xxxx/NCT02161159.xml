  </study_design_info>
  <primary_outcome>
    <measure>Percentage of Patients Who have Greatly Improved or Improved on the 4-Point Treatment Benefit Scale (TBS)</measure>
    <time_frame>Baseline, Week 12</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Change From Baseline in Number of Urinary Incontinence Episodes</measure>
    <time_frame>Baseline, Week 12</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Time to Re-Injection of BOTOX®</measure>
    <time_frame>12 Months</time_frame>
  </secondary_outcome>
