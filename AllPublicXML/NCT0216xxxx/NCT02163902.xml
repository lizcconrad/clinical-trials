    <time_frame>up to 36 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Efficacy assessments using patient reported scales</measure>
    <time_frame>up to 36 months</time_frame>
  </secondary_outcome>
  <other_outcome>
