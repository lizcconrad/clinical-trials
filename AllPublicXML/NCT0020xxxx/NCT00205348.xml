  <brief_title>Swallowing Function Before and After Surgery for Thyroid Goiter</brief_title>
  <official_title>Swallowing Function Before and After Surgery for Thyroid Goiter</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Wisconsin, Madison</agency>
