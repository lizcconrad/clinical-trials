  <brief_title>Clinical Outcomes Study of the Nexel Total Elbow</brief_title>
  <official_title>Retrospective and Prospective Clinical Outcomes Study of the Zimmer® Nexel™ Total Elbow</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Zimmer Biomet</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
