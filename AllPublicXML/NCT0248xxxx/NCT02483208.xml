  <primary_outcome>
    <measure>AUC(0-tlast)</measure>
    <time_frame>Pre-Dose, 0.25, 0.5, 1, 3, 6, 8, 24, 30, 48 hours after treatment</time_frame>
    <description>Area under the blood concentration vs time curve from zero to the last data point &gt; lower limit of quantification</description>
  </primary_outcome>
