  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to detect recurrent bladder cancer using a combination of
      protein and DNA biomarkers in urine
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>May 2009</start_date>
  <completion_date type="Actual">October 2010</completion_date>
  <primary_completion_date type="Actual">April 2010</primary_completion_date>
