  <brief_title>Observational Study of Subjects Having Routine Recurrence Screening for Bladder Cancer</brief_title>
  <official_title>Phase 3 Study of Bladder Cancer Recurrence Surveillance</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Predictive Biosciences</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
