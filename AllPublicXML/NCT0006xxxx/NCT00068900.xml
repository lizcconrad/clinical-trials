  <brief_title>FFA Metabolism in Different Types of Human Obesity</brief_title>
  <official_title>FFA Metabolism in Different Types of Human Obesity</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mayo Clinic</agency>
