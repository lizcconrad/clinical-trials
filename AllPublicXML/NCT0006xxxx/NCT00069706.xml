    <primary_purpose>Treatment</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Mean Intraocular Pressure (IOP)</measure>
  </primary_outcome>
  <number_of_arms>5</number_of_arms>
  <enrollment type="Actual">118</enrollment>
