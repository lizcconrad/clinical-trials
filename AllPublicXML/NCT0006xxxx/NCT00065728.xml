  <primary_outcome>
    <measure>Percent of patients who maintained stable vision</measure>
    <time_frame>Month 24</time_frame>
    <description>Stable vision is defined as &lt; 15-letter loss of best-corrected visual acuity scores)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Mean change in best-corrected visual acuity at Month 24 from baseline</measure>
    <time_frame>Baseline, Month 24</time_frame>
  </primary_outcome>
