  <primary_outcome>
    <measure>The primary objective of this study was to determine the tumor response rate for single-agent cetuximab in EGFR+ subjects.</measure>
    <time_frame>Every 8 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Determine the tumor response rate in the total population and in EGFR- subjects</measure>
    <time_frame>Every 8 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Time to disease progression in the EGFR+ and total populations.</measure>
    <time_frame>Every 8 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Survival in the EGFR+ and total populations.</measure>
    <time_frame>Every 8 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Safety in the EGFR+ and total populations.</measure>
    <time_frame>Every 8 weeks</time_frame>
  </secondary_outcome>
