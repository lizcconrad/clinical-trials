  <brief_summary>
    <textblock>
      This study investigates the impact and adhesion of new adhesives to the skin.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date type="Actual">July 3, 2017</start_date>
  <completion_date type="Actual">September 4, 2017</completion_date>
  <primary_completion_date type="Actual">September 4, 2017</primary_completion_date>
  <phase>N/A</phase>
