  <primary_outcome>
    <measure>Proportion of dosing instances among user population taken within 120 hours (5 days) of most recent episode of unprotected sex.</measure>
    <time_frame>Up to 6 Weeks</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Proportion of dosing instances among user population in which no more than one tablet was taken.</measure>
    <time_frame>Up to 6 Weeks</time_frame>
  </primary_outcome>
  <primary_outcome>
