  <brief_title>Safety of Different Forms of Creatine</brief_title>
  <acronym>NB9</acronym>
  <official_title>Short-Term Safety and Dose Effects of Different Forms of Creatine</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Texas A&amp;M University</agency>
      <agency_class>Other</agency_class>
