  <brief_title>Predicting Effective Therapy in Pancreatic Cancer</brief_title>
  <official_title>Circulating Tumor Cell and Tumor Tissue Models for Predicting Effective Pancreatic Cancer Response</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Memorial Sloan Kettering Cancer Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
