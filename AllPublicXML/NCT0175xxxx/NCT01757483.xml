  <brief_title>Canadian Ticagrelor Survey</brief_title>
  <official_title>Effectiveness of Risk Minimisation Interventions for Ticagrelor in Canada</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AstraZeneca</agency>
      <agency_class>Industry</agency_class>
