  <brief_title>NEAT! Technology to Increase Breaks in Sedentary Behavior in Adults With Diabetes</brief_title>
  <official_title>NEAT! Technology to Increase Breaks in Sedentary Behavior in Adults With Diabetes</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Northwestern University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
