  <primary_outcome>
    <measure>To establish the safety of five dose levels (1.0, 2.5, 5, 7.5 and 10 mg/kg)of V10153 in patients with acute ischaemic stroke.</measure>
    <time_frame>Ongoing</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>To compare recanalisation rates across dose levels.</measure>
    <time_frame>Ongoing</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>To compare clinical outcome between treatments by NIHSS, Modified Rankin Scale and Barthel Index.</measure>
    <time_frame>Post-study completion</time_frame>
  </secondary_outcome>
