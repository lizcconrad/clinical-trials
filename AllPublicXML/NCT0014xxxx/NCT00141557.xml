    <arm_group_label>2</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

        Hysterectomized, menopausal women between the ages of 30 and 65 years of age (inclusive)
        with symptoms of estrogen and/or androgen deficiency not currently controlled by estrogen
        therapy as determined by patient report and the Investigator's clinical judgement
      </textblock>
    </criteria>
    <gender>Female</gender>
