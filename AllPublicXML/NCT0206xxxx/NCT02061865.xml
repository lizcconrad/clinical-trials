  <brief_title>Phase 1 Safety Study With Intravitreal (IVT) REGN2176-3 in Patients Aged 50 Years and Older With Wet AMD</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Regeneron Pharmaceuticals</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Regeneron Pharmaceuticals</source>
  <oversight_info>
