  </study_design_info>
  <primary_outcome>
    <measure>Pressure Pain Threshold as a measure by algometry</measure>
    <time_frame>5 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pain scores on visual analog scale</measure>
    <time_frame>5 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Wrist Range of motion as a Measure by goniometer</measure>
    <time_frame>5 weeks</time_frame>
  </secondary_outcome>
