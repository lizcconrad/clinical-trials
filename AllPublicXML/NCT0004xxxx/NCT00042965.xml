  <brief_title>Capecitabine and Gemcitabine in Treating Patients With Metastatic Kidney Cancer</brief_title>
  <official_title>A Phase II Study Of Capecitabine Plus Gemcitabine For Metastatic Renal Cell Carcinoma</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alliance for Clinical Trials in Oncology</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
