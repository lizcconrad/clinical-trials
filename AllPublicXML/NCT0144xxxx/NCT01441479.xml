  <primary_outcome>
    <measure>Streptococcus Group B identification</measure>
    <time_frame>Immediate</time_frame>
    <description>The purpose of this study was to identify via immunofluorescence technology, Group A Streptococcus, which was successfully done.</description>
  </primary_outcome>
  <enrollment type="Actual">1282</enrollment>
