  <brief_title>Alkaline Ionized Water: Sports Recovery and Inflammation Management</brief_title>
  <official_title>Alkaline Ionized Water: Sports Recovery and Inflammation Management</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Natural Immune Systems Inc</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
