  <brief_title>Comparative Study of Menstrual Blood and Systemic Blood</brief_title>
  <official_title>Comparative Study of Menstrual Blood and Systemic Blood: Quantifying the Difference</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Stanford University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
