  <brief_title>Acute Haemodynamic Impact of an IV Glucose-Injection</brief_title>
  <official_title>Acute Haemodynamic Impact of an IV Glucose-Injection</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University Hospital, Ghent</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
