  </study_design_info>
  <primary_outcome>
    <measure>Recording of allergy symptoms</measure>
    <time_frame>Grass pollen season 2007</time_frame>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">329</enrollment>
