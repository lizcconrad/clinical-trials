  <brief_title>Contrast Nephropathy in Type 2 Diabetes</brief_title>
  <acronym>VCG4</acronym>
  <official_title>Prevalence of Contrast Nephropathy in Type 2 Diabetes Patients With Microalbuminuria</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ahmed Abdelkawi Hammad</agency>
