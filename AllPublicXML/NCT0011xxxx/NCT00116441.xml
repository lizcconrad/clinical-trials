  <condition>Multiple Myeloma</condition>
  <intervention>
    <intervention_type>Biological</intervention_type>
    <intervention_name>Therapeutic Cellular Vaccine, GM-CSF Producing</intervention_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  De novo multiple myeloma

          -  ECOG 0-2

          -  No serious co-morbid illnesses and adequate organ function

          -  &gt; 4 weeks from systemic steroids

        Exclusion Criteria:

          -  No existing secondary malignancies and no history of secondary malignancies in the
             past 5 years

          -  No active autoimmune disease
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>70 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
