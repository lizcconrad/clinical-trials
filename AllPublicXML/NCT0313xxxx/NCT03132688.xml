  <brief_title>A Study on Safety and Efficacy of Propofol in Small Children Under 2 Years of Age</brief_title>
  <official_title>A Retrospective Study on Safety and Efficacy of Propofol in Small Children</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Seoul National University Hospital</agency>
      <agency_class>Other</agency_class>
