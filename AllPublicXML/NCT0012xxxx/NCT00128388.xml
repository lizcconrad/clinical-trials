  <brief_title>Psychodynamic Therapy for Treating Panic Disorder</brief_title>
  <official_title>Randomized Controlled Trial of Psychodynamic Psychotherapy vs. Applied Relaxation for Panic Disorder</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Weill Medical College of Cornell University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
