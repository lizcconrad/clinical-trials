  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Single (Outcomes Assessor)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Percentage of patients with complete healing (100% of epithelialization).</measure>
    <time_frame>D21</time_frame>
    <description>Using photographs,independent assessor blinded assessment.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Time to complete healing (100% epithelialization)</measure>
    <time_frame>At each visit (D1, D21)</time_frame>
    <description>Assessment by the investigator</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Quality of healing according to Vancouver scale</measure>
    <time_frame>D21</time_frame>
    <description>Assessment by the investigator</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Frequency of painless dressing removal (pain &lt; 30 mm on 100 mm VAS)</measure>
    <time_frame>During first three removals</time_frame>
    <description>Assessment by the patient</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Safety and tolerability of the dressings</measure>
    <time_frame>At each visit (D1, D21)</time_frame>
    <description>number of patients with adverse event as a measure of safety and tolerability</description>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">208</enrollment>
  <condition>Surgical Acute Wounds</condition>
  <arm_group>
    <arm_group_label>TulleGras M.S.</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>Sterile dressing that consists of viscose tissue coated with mineral vaseline</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Urgotul</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>Sterile, hydrocolloid dressing, that consists of a polyester fabric coated with hydrocolloid particles and vaseline</description>
  </arm_group>
  <intervention>
    <intervention_type>Device</intervention_type>
    <intervention_name>Urgotul: Low-adherent dressing</intervention_name>
