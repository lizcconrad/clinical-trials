  <brief_title>Comparison of Different Botulinum Toxin A Injection Patterns in the Treatment of Blepharospasm</brief_title>
  <official_title>Comparison of Different Botulinum Toxin A Injection Patterns in the Treatment of Blepharospasm</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>YangHui</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
