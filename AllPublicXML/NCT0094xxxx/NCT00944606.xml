    <intervention_name>Vitamin D</intervention_name>
    <description>2000-3000 IU Vitamin D daily x 6 weeks</description>
    <arm_group_label>Vitamin D</arm_group_label>
    <other_name>Rainbow Light</other_name>
  </intervention>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>Placebo candy</intervention_name>
    <description>candy gum drops</description>
    <arm_group_label>Placebo</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  ages 5-14

          -  healthy

          -  vague musculoskeletal pain without diagnosis

        Exclusion Criteria:

          -  medications (steroids, seizure medications, birth control pills or shot)

          -  endocrinopathies
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>5 Years</minimum_age>
    <maximum_age>14 Years</maximum_age>
