    <primary_purpose>Treatment</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Hemoglobin A1c (HbA1c)</measure>
    <time_frame>HbA1c is drawn at Visit 1 (Week -16 to -4), Visit 3 (Week -2), Visit 4 (Week 0), Visit 5 (Week 4), Visit 6 (Week 10), Visit 7 (Week 18), Visit 8 (Week 26).</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Fasting Plasma Glucose (FPG)</measure>
    <time_frame>FPG is drawn at Visit 1 (Week -16 to -4), Visit 2 (Week -4), Visit 3 (Week -2), Visit 4 (Week 0), Visit 5 (Week 4), Visit 6 (Week 10), Visit 7 (Week 18), Visit 8 (Week 26).</time_frame>
  </secondary_outcome>
