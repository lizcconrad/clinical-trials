  <primary_outcome>
    <measure>The percentage of P2Y12 receptor inhibition assessed by Ultegra rapid platelet function analyzer, (Baseline platelet reaction unit (PRU)- posttreatment PRU)/Baseline PRU * 100 (%))</measure>
    <time_frame>1 month</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Adverse events after study medication</measure>
    <time_frame>1 month</time_frame>
  </secondary_outcome>
