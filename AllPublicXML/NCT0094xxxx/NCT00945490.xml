  <brief_title>Clinical Evaluation of NX-1207 for the Treatment of Benign Prostatic Hyperplasia (BPH) NX02-0018</brief_title>
  <official_title>Phase III Multicenter Prospective Randomized Parallel-Group Placebo-Controlled Double Blind Clinical Evaluation of NX-1207 for the Treatment of BPH NX02-0018</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Nymox Corporation</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
