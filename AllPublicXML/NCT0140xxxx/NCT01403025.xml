  <primary_outcome>
    <measure>The incidence of SADRs (Serious Adverse Drug Reactions)</measure>
    <time_frame>After 36 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>The incidence rate and type of SAEs (Serious Adverse Events)</measure>
    <time_frame>At month 1, 3, 6, 12, 24 and 36</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>The incidence rate and type of ADRs (Adverse Drug Reactions)</measure>
    <time_frame>At month 1, 3, 6, 12, 24 and 36</time_frame>
  </secondary_outcome>
