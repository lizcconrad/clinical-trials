    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <enrollment type="Actual">40</enrollment>
  <condition>Acid-base Analysis</condition>
  <eligibility>
    <study_pop>
      <textblock>
        Intensive Care Unit patients requiring both an arterial line and central venous line as
        part of their clinical care
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Patients admitted to the ICU and requiring both an arterial line and central venous
             line as prt of their clinical care

        Exclusion Criteria:

          -  Patient did not desire to participate
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
