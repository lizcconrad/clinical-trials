  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to see if blood obtained from a vein (smaller blood vessel) can
      be used instead of blood obtained from an artery (larger blood vessel).
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>January 2008</start_date>
  <completion_date type="Actual">July 2008</completion_date>
  <primary_completion_date type="Actual">July 2008</primary_completion_date>
