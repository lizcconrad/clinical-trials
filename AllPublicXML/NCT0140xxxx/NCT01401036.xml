  <brief_title>Nobori And Uncoated Stent In Coronary Attack</brief_title>
  <official_title>Clinical Trial of Nobori Versus Uncoated Stents In Acute Myocardial Infarction</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Shigeru Saito</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
