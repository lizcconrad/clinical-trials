  <brief_title>Comparing Diabetes Prevention Programs</brief_title>
  <official_title>Comparative Effectiveness of Practice-Based Diabetes Prevention Programs - A Pilot Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Rochester</agency>
