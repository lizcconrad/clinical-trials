    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Prevention</primary_purpose>
  </study_design_info>
  <primary_outcome>
    <measure>Tiobarbituric acid reactive species measures</measure>
    <time_frame>First seven postoperative days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Serum creatinine</measure>
    <time_frame>7, 15, 30, 60, 90 postoperative day</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <condition>Deceased Kidney Transplantation</condition>
  <arm_group>
    <arm_group_label>NAC</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>treatment</description>
  </arm_group>
  <arm_group>
    <arm_group_label>control</arm_group_label>
    <arm_group_type>No Intervention</arm_group_type>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>N-acetylcysteine</intervention_name>
    <description>600 mg bid orally from day 1 to 7 postoperative</description>
    <arm_group_label>NAC</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  First transplant

          -  Deceased kidney transplant

        Exclusion Criteria:

