  <brief_title>Subjective Well Being of Schizophrenic Patients in Greece</brief_title>
  <acronym>AURA</acronym>
  <official_title>Subjective Well Being of Schizophrenic Patients in Greece</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AstraZeneca</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
