  <brief_title>Supplementation in Tennis</brief_title>
  <official_title>Supplementation in Tennis: an Observational Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Vrije Universiteit Brussel</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
