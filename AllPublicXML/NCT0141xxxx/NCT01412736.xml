  <brief_title>A Phase IIa Study of KHK4563</brief_title>
  <acronym>4563-003</acronym>
  <official_title>A Phase IIa , Double-blind, Placebo-controlled Dose-ranging Study to Evaluate the Efficacy and Safety of KHK4563 in Adults With Uncontrolled, Suspected Eosinophilic Asthma</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Kyowa Hakko Kirin Co., Ltd</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
