    <intervention_type>Drug</intervention_type>
    <intervention_name>Nebivolol</intervention_name>
    <description>Nebivolol (non-trade 5, 10 or 20 mg tablet), oral administration</description>
    <arm_group_label>1</arm_group_label>
  </intervention>
  <intervention>
