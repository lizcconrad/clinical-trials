    <primary_purpose>Basic Science</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Area under the plasma concentration versus time curve (AUC)</measure>
    <time_frame>0-72hr</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Peak Plasma Concentration (Cmax)</measure>
    <time_frame>0-72hr</time_frame>
  </primary_outcome>
