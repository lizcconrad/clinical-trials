  <brief_title>Music During Labor Epidural Placement and Patient Satisfaction</brief_title>
  <official_title>Music During Labor Epidural Placement and Patient Satisfaction</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Brigham and Women's Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
