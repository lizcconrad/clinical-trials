  <brief_title>Efficacy Study of Intravenous Iron Preparation to Iron Deficiency Anemia</brief_title>
  <official_title>Phase III Study of Z-213 in Subjects With Iron-deficiency Anemia</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Zeria Pharmaceutical</agency>
      <agency_class>Industry</agency_class>
