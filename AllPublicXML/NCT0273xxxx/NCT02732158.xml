  <brief_summary>
    <textblock>
      This prospective, nonrandomized, feasibility study will compare laboratory measures at the
      beginning and end of the study period in adults consuming a nutritional study product.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date type="Actual">April 2016</start_date>
  <completion_date type="Actual">September 2016</completion_date>
  <primary_completion_date type="Actual">September 2016</primary_completion_date>
