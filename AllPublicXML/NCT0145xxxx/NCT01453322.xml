  <primary_outcome>
    <measure>Valve and Refill Confirmation</measure>
    <time_frame>Immediately after flush-Day 1</time_frame>
    <description>Using VeinViewer, the clinician will be able to view valves and refill. It will be confirmed using Ultrasound.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Video Documentation of localized vascular access complications</measure>
    <time_frame>Immediately after attempt-Day 1</time_frame>
    <description>Using VeinViewer, the user will be able to detect hematomas and infiltrations.</description>
  </secondary_outcome>
  <enrollment type="Actual">10</enrollment>
