  <brief_title>Health Benefits of Chronic vs. Acute Exercise in Overweight Adults</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Michigan</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
