  <brief_title>Prehospital Traumatic Brain Injury</brief_title>
  <official_title>Severe Traumatic Brain Injury - Comparison of Outcome in Paramedic Versus Physician Staffed Emergency Medical Services</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Tampere University Hospital</agency>
      <agency_class>Other</agency_class>
