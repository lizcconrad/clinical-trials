  <brief_title>Study of COV795 in Subjects With Osteoarthritis or Chronic Low Back Pain</brief_title>
  <official_title>An Open Label Safety Study of COV795 in Subjects With Osteoarthritis or Chronic Low Back Pain</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mallinckrodt</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
