  <brief_title>Evaluate Long-Term Safety and Efficacy WC3011</brief_title>
  <official_title>A Multicenter, Open-Label Extension Study to Evaluate the Long-Term Safety and Efficacy of WC3011 in the Treatment of Symptoms of Vulvovaginal Atrophy in Postmenopausal Women</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Warner Chilcott</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Warner Chilcott</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      This is an open-label extension study evaluating the long-term safety and efficacy of WC3011
      in non-hysterectomized, healthy, postmenopausal women with vulvovaginal atrophy.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>October 2011</start_date>
  <completion_date type="Actual">December 2012</completion_date>
  <primary_completion_date type="Actual">December 2012</primary_completion_date>
  <phase>Phase 3</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <intervention_model>Single Group Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Number of Subjects without Endometrial Hyperplasia or Worse at Visit 7/Final Visit</measure>
    <time_frame>Week 40/Final Visit</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change in Subject's Self-Assessment of Intensity of Vaginal Dryness from Baseline to Visit 7/Final Visit</measure>
    <time_frame>Week 40/Final Visit</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in Vaginal Cytology (Maturation Index) from Baseline to Visit 7/Final Visit</measure>
    <time_frame>Week 40/Final Visit</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in Vaginal pH from Baseline to Visit 7/Final Visit</measure>
    <time_frame>Week 40/Final Visit</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in Investigator Assessment of Vulvovaginal Atrophy (VVA) from Baseline to Visit 7/Final Visit</measure>
    <time_frame>Week 40/Final Visit</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">309</enrollment>
  <condition>Vulvovaginal Atrophy</condition>
  <arm_group>
    <arm_group_label>Estradiol Vaginal Gel</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>WC3011 Estradiol Vaginal Gel, administered 3X weekly for 40 weeks</description>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Estradiol Vaginal Gel</intervention_name>
    <description>Estradiol vaginal gel containing 0.017 mg estradiol per gram, 3X weekly for 40 weeks</description>
    <arm_group_label>Estradiol Vaginal Gel</arm_group_label>
    <other_name>WC3011</other_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Completed Study PR-04409

        Exclusion Criteria:

          -  Developed any of the following during Study-PR04409 or has begun taking hormone
             therapy other than WC3011:

          -  Hypersensitivity to estrogen and/or progestin therapy

          -  Known or suspected premalignant or malignant disease (except successfully treated skin
             cancers)

          -  Manifestation of or treatment for significant cardiovascular disease, stroke or
             ischemic attack

          -  Insulin-dependent diabetes mellitus

          -  Smoking ≥ 15 cigarettes daily

          -  Uncontrolled hypertension - systolic blood pressure (BP) ≥ 160 mmHG or diastolic ≥ 95
             mmHg
      </textblock>
    </criteria>
    <gender>Female</gender>
    <minimum_age>35 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
