  <brief_title>A Trial to Test for Bioequivalence Between NN1045 and NN5401 in Subjects With Type 1 Diabetes</brief_title>
  <official_title>A Trial to Test for Bioequivalence Between NN1045 and NN5401 in Subjects With Type 1 Diabetes</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Novo Nordisk A/S</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
