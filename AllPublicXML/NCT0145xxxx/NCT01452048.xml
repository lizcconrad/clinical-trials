  <brief_title>Relationship Between Systemic Fatty Acid Availability and Insulin Sensitivity</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Michigan</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
