  <primary_outcome>
    <measure>Area Under the Curve (AUC) to time infinity (inf) (if data permit), otherwise (AUC) to last quantifiable concentration (last) of hydrocodone.</measure>
    <time_frame>0, 0.083, 0.25, 0.5, 0.75, 1, 1.5, 2, 3, 4, 6, 8, 10, 12, 16, and 24 hours post dose</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Area Under the Curve (AUC) to time infinity (inf) (if data permit), otherwise (AUC) to last quantifiable concentration (last) of acetaminophen</measure>
    <time_frame>0, 0.083, 0.25, 0.5, 0.75, 1, 1.5, 2, 3, 4, 6, 8, 10, 12, 16, and 24 hours post dose</time_frame>
  </primary_outcome>
  <primary_outcome>
