  <brief_title>Efficacy and Safety of Colchicine for the Prevention of Gout Flares During the Initiation of Allopurinol</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Regeneron Pharmaceuticals</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Regeneron Pharmaceuticals</source>
  <oversight_info>
