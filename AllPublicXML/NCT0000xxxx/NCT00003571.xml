  <brief_title>Gene Testing in Patients With Colon Cancer</brief_title>
  <official_title>Tumor Replication Error (RER) Status and Outcome in a Colon Cancer Adjuvant Chemotherapy Trial</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alliance for Clinical Trials in Oncology</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
