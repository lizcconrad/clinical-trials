  <brief_title>Antineoplaston Therapy in Treating Patients With Ependymoma</brief_title>
  <official_title>Phase II Study of Antineoplastons A10 and AS2-1 Infusions in Patients With Ependymoma</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Burzynski Research Institute</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
