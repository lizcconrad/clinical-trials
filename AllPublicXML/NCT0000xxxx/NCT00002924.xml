  <brief_title>Gene Mutations in Patients With Advanced Prostate Cancer That Is Not Responsive to Hormone Therapy</brief_title>
  <official_title>Androgen Receptor Mutations in Hormone Refractory Prostate Cancer</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alliance for Clinical Trials in Oncology</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
