  </study_design_info>
  <primary_outcome>
    <measure>Percent change in LDL-C from baseline</measure>
    <time_frame>12 Weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>The percent change from baseline in fasting serum total cholesterol (TC )</measure>
    <time_frame>12 Weeks</time_frame>
  </secondary_outcome>
