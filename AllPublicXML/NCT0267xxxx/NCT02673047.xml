  <brief_title>Observational Study of Botulinum Toxin Type A in Patients With Urinary Incontinence Associated With Overactive Bladder</brief_title>
  <acronym>BOREAL</acronym>
  <sponsors>
    <lead_sponsor>
      <agency>Allergan</agency>
      <agency_class>Industry</agency_class>
