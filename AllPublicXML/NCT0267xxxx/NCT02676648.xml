  <brief_title>Web-Delivered Interventions for Blood Sugar Control for Individuals With Type 2 Diabetes</brief_title>
  <official_title>Randomized Trial of Web-Delivered Interventions for Blood Sugar Control for Individuals With Type 2 Diabetes</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Michigan</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
