  <intervention>
    <intervention_type>Device</intervention_type>
    <intervention_name>Coronary Stent Implantation</intervention_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Stable, unstable or silent angina,

          -  De novo lesion

        Exclusion Criteria:

          -  Allergy to ticlopidine

          -  Recent myocardial infarction

          -  Expected survival &gt; 1 year
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
