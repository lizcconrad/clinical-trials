      <agency>Pfizer</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Pfizer</source>
  <brief_summary>
    <textblock>
      To evaluate efficacy of tolterodine extended release formulation compared with immediate
      release formulation in subjects with symptoms of overactive bladder
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>May 2005</start_date>
