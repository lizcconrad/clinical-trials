    <primary_purpose>Treatment</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Rate of Grade 2,3,4 Diarrhea - patients on study drug.</measure>
  </primary_outcome>
  <secondary_outcome>
    <measure>Safety monitored w/follow-up period. Assess Best Objective response, Disease control rate, progression free survival, overall survival, duration/best objective response, Time/best objective response. PK, immunogenicity &amp; pharmacodynamic analysis</measure>
    <time_frame>at Week 24</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">115</enrollment>
  <condition>Malignant Melanoma</condition>
