  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Ipilimumab+ Placebo</intervention_name>
    <description>Solution, Intravenous, 10 mg/kg, 3 weeks, 12 - 48 weeks depending on the response.</description>
    <arm_group_label>A1</arm_group_label>
    <other_name>BMS-734016</other_name>
    <other_name>(MDX-010)</other_name>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Ipilimumab+ Budesonide</intervention_name>
    <description>Solution/Capsule, Intravenous/Oral, 10 mg/kg + 9 mg, 3 weeks (Ipilimumab) + once daily until week 16 (Budesonide), 12 - 48 weeks depending on the response.</description>
    <arm_group_label>A2</arm_group_label>
    <other_name>BMS-734016</other_name>
    <other_name>(MDX-010)</other_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Histologic or cytologic diagnosis of unresectable Stage III or IV malignant melanoma
             (excluding ocular melanoma)

          -  Flexible Sigmoidoscopy and colonic biopsy required

        Exclusion Criteria:

          -  Patients with active, untreated central nervous system metastasis. Patients with
             autoimmune disease.
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
