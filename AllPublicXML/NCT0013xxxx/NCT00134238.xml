  </study_design_info>
  <primary_outcome>
    <measure>Change in intima media thickness as measured by carotid ultrasound</measure>
  </primary_outcome>
  <secondary_outcome>
    <measure>Changes in levels of lipids and other biomarkers.</measure>
  </secondary_outcome>
  <enrollment type="Actual">755</enrollment>
  <condition>Mixed Hyperlipidemia</condition>
