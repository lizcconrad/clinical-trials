  <brief_title>TIPE2 Associated With Kidney Transplant</brief_title>
  <acronym>TIPE2</acronym>
  <official_title>Department of Renal Transplantation, the First Affiliated Hospital of Xi'an Jiaotong University, Xi'an, China</official_title>
  <sponsors>
    <lead_sponsor>
