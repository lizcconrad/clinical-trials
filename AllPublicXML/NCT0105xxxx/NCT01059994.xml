  <brief_title>Role of Skeletal Muscle Nitric Oxide Production in Age-related Fatigue and Fatigability</brief_title>
  <official_title>Role of Skeletal Muscle Nitric Oxide Production in Age-related Fatigue and Fatigability</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>The University of Texas Medical Branch, Galveston</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
