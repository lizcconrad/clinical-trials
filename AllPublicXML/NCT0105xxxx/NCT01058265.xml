  <brief_title>Integrative Medicine Consultation for Patients With Pain or Fatigue</brief_title>
  <official_title>Effect of an Integrative Medicine Consultation on Outcome for Patients With Pain or Fatigue</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mayo Clinic</agency>
      <agency_class>Other</agency_class>
