  </study_design_info>
  <primary_outcome>
    <measure>pharmacokinetics</measure>
    <time_frame>at postoperative 10~14 days and at postoperative 6 months for crossover study</time_frame>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Anticipated">80</enrollment>
