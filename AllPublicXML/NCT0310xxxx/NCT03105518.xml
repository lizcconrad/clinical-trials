  <brief_title>Predictors of Postoperative Pain Following Oocyte Retrieval for Assisted Reproduction</brief_title>
  <official_title>Predictors of Postoperative Pain Following Oocyte Retrieval for Assisted Reproduction</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Brigham and Women's Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
