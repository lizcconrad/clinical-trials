    <masking>Double (Participant, Outcomes Assessor)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>The change of pain numerical rating score from postoperative 30 min until postoperative 3 month</measure>
    <time_frame>Postoperative 30 min, postoperative 1 day, postoperative 1 week, postoperative 3 month</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>The dose of ketorolac administered to the patient.</measure>
    <time_frame>postoperative 1 day</time_frame>
  </secondary_outcome>
