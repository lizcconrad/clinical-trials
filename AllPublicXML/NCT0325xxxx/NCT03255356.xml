  <oversight_info>
    <has_dmc>No</has_dmc>
    <is_fda_regulated_drug>No</is_fda_regulated_drug>
    <is_fda_regulated_device>No</is_fda_regulated_device>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to evaluate prospectively ventilatory practices in the
      perioperative cardiac surgery period.
    </textblock>
  </brief_summary>
  <detailed_description>
    <textblock>
      This is a International Multicentre Observational Trial on all adult cardiac surgery patients
      with cardiopulmonary bypass (CPB).
    </textblock>
  </detailed_description>
  <overall_status>Not yet recruiting</overall_status>
  <start_date type="Anticipated">November 1, 2017</start_date>
  <completion_date type="Anticipated">December 2017</completion_date>
  <primary_completion_date type="Anticipated">December 2017</primary_completion_date>
  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Cohort</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>incidence of use prophylactic protective mechanical ventilation defined with with low tidal volume (6 to 8 ml / kg based on the ideal body weight)</measure>
    <time_frame>1 day</time_frame>
    <description>percent</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>incidence of using positive end-expiratory pressure</measure>
    <time_frame>1 day</time_frame>
    <description>(PEEP)&gt; 5 cmH2O</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>incidence of using at least 2 alveolar lung recruitment manoeuvres</measure>
    <time_frame>1 day</time_frame>
    <description>percent</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>incidence of using a protective ventilation bundle combining prophylactic protective mechanical ventilation and at least 2 alveolar recruitment manoeuvres</measure>
    <time_frame>1 day</time_frame>
    <description>percent</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>incidence of postoperative pulmonary complications</measure>
    <time_frame>1 month postoperatively</time_frame>
    <description>percent</description>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Anticipated">1000</enrollment>
  <condition>Respiratory Complication</condition>
  <condition>Respiration, Artificial</condition>
  <condition>Anesthesia, General</condition>
  <condition>Bypass, Cardiopulmonary</condition>
  <arm_group>
    <arm_group_label>Cohort</arm_group_label>
    <description>An anonymised questionnaire will be answered for each includable consecutive patient after verifying the absence of exclusion criteria.</description>
  </arm_group>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>Survey</intervention_name>
    <description>An anonymised questionnaire will be answered for each includable consecutive patient after verifying the absence of exclusion criteria.</description>
    <arm_group_label>Cohort</arm_group_label>
    <other_name>not adapted</other_name>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        All adult cardiac surgery patients with cardiopulmonary bypass (CPB).
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  All consecutive cardiac surgery patients with CPB during one predefined period,

          -  Elective or urgent surgery as defined by EuroSCORE2.

        Exclusion Criteria:

          -  Refusal of consent,

          -  Pregnant women,

          -  Age &lt; 18 years,

          -  Heart transplant, ECLS, ventricular assist

          -  Emergency or salvage surgery as defined by EuroSCORE2
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
