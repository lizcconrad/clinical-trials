    <arm_group_label>Open-label (OL) GLM SC QM ---&gt; PBO SC QM</arm_group_label>
    <arm_group_label>OL GLM SC QM ---&gt; DB GLM SC QM</arm_group_label>
    <arm_group_label>OL GLM SC QM ---&gt; DB GLM SC Q2M</arm_group_label>
    <other_name>MK-8259</other_name>
  </intervention>
  <intervention>
    <intervention_type>Biological</intervention_type>
    <intervention_name>Golimumab QM: Period 2 (DB)</intervention_name>
    <description>DB monthly injections of active golimumab 50 mg or 100 mg (two 50 mg injections) for 12 months, or until &quot;flare&quot; (Period 2, full treatment)</description>
