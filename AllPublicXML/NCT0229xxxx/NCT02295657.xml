  <primary_outcome>
    <measure>Success of intubation</measure>
    <time_frame>1 day</time_frame>
    <description>effectiveness of first, second and third intubation attempts and overall effectiveness of intubation by participants</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Intubation time</measure>
    <time_frame>1 day</time_frame>
    <description>time in seconds required for a successful intubation attempt</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Cormack-Lehane grading</measure>
    <time_frame>1 day</time_frame>
    <description>self-reported percentage the vocal cord visualization using the Cormack-Lehane grading (grade 1-4)</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Preferred ETI device</measure>
    <time_frame>1day</time_frame>
    <description>participants were asked which method of ETI they would prefer in a real-life.</description>
  </secondary_outcome>
