    <arm_group_label>Clariti® 1-Day (Somofilcon A)</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Be at least 18 years old

          -  Have provided written informed consent

          -  Have discomfort when wearing contact lenses

        Exclusion Criteria:

          -  Have an active ocular infection or significant slit lamp findings

          -  Participated in any other studies in the past 30 days
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
