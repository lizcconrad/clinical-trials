  <brief_title>DOvEE - Diagnosing Ovarian &amp; Endometrial Cancer Early</brief_title>
  <acronym>DOvEE</acronym>
  <official_title>Diagnosing Ovarian &amp; Endometrial Cancer Early (DOvEE) by Targeting Symptomatic Women</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>McGill University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
