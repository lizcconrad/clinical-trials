            <ci_n_sides>2-Sided</ci_n_sides>
            <ci_lower_limit>2.36</ci_lower_limit>
            <ci_upper_limit>2.70</ci_upper_limit>
            <estimate_desc>70%Isopropyl Alcohol 10 minute abdomen</estimate_desc>
          </analysis>
          <analysis>
            <group_id_list>
