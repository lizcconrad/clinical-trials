    <intervention_name>Beta-hydroxy beta-methylbutyrate</intervention_name>
    <description>3 grams per day of the calcium salt form of beta-hydroxy beta-methylbutyrate (HMB).</description>
    <arm_group_label>Time-Restricted Feeding plus HMB</arm_group_label>
    <other_name>HMB</other_name>
  </intervention>
