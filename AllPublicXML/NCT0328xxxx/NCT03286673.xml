  <brief_title>Comparison of Three Calcium Supplements</brief_title>
  <official_title>Comparison of Three Calcium Supplements With Regard to Their Metabolic Effects in Healthy Adults</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Jena</agency>
      <agency_class>Other</agency_class>
