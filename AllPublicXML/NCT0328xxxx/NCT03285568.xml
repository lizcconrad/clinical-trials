  <eligibility>
    <study_pop>
      <textblock>
        All patients seen at RUMC
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  women at least 18 years old

          -  ER+, HER2- advanced breast cancer

          -  receiving palbociclib

        Exclusion Criteria:

          -  brain metastases

          -  on palbociclib clinical trial
      </textblock>
    </criteria>
    <gender>All</gender>
