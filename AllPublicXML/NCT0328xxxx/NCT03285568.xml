  <brief_title>Patterns of Prescribing and Monitoring of Palbociclib</brief_title>
  <official_title>Patterns of Prescribing and Monitoring of Palbociclib</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Rush University Medical Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
