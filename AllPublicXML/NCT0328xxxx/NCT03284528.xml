  <primary_outcome>
    <measure>neurodevelopmental outcome at 18-24 month of Age</measure>
    <time_frame>7 years</time_frame>
    <description>The investigators will analyse if there is a correlation of placenta histology and neurodevelopmental outcome</description>
  </primary_outcome>
  <enrollment type="Actual">100</enrollment>
