  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of participants with physical examination abnormalities</measure>
    <time_frame>Up to 12 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of participants with clinical laboratory abnormalities</measure>
    <time_frame>Up to 12 days</time_frame>
  </secondary_outcome>
  <number_of_arms>3</number_of_arms>
