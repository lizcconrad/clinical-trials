    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Cohens Perceived Stress Scale 14 Items</measure>
    <time_frame>4 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Quality of Life (SF 36)</measure>
    <time_frame>Baseline, 4 weeks, 12 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Subjective Stress Level (Visual Analogue Scale)</measure>
    <time_frame>Baseline, 4 weeks, 12 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Assessment of effect on a Likert - Scale</measure>
    <time_frame>4 weeks, 12 weeks</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">74</enrollment>
  <condition>Healthy Subjects</condition>
  <arm_group>
    <arm_group_label>Mindfulness walking</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
  </arm_group>
  <arm_group>
    <arm_group_label>No intervention (waiting list)</arm_group_label>
    <arm_group_type>No Intervention</arm_group_type>
  </arm_group>
  <intervention>
    <intervention_type>Behavioral</intervention_type>
    <intervention_name>Mindfulness Walking</intervention_name>
    <description>Mindful walking in a small group twice a week for 60 minutes within 4 weeks.</description>
    <arm_group_label>Mindfulness walking</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Healthy men and women between 18 and 65 years

          -  Increased level of stress (visual analog scale &gt;40 mm (0-100))

        Exclusion Criteria:

          -  Regular walking training in the last 6 weeks

          -  Medical therapy with psychopharmacological drugs

          -  Regular mindfulness based meditations/exercises in the last 6 weeks

          -  CAM therapies against stress in the last 6 weeks

          -  medium or severe chronic diseases

          -  Stress due to chronic diseases

          -  Acute disease at inclusion

          -  Not being able to walk
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>65 Years</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
