  <brief_title>Preoperative Warm-up in Gynecologic Surgery</brief_title>
  <official_title>Preoperative Warm-up in Gynecologic Surgery Using a Virtual Reality Simulator.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Medical University of Vienna</agency>
      <agency_class>Other</agency_class>
