  <primary_outcome>
    <measure>Objectively measured physical activity overall (minutes of moderate-to-vigorous physical activity per day)</measure>
    <time_frame>up to 3 years</time_frame>
    <description>Accelerometry (ActiGraph, FL, USA)</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Objectively measured physical activity during commuting to and from school (minutes of moderate-to-vigorous physical activity during commuting)</measure>
    <time_frame>up to 3 years</time_frame>
    <description>Accelerometry (ActiGraph, FL, USA)</description>
  </secondary_outcome>
  <number_of_groups>2</number_of_groups>
