  <brief_title>Human-cl-rhFVIII in Previously Untreated Patients</brief_title>
  <official_title>Immunogenicity, Efficacy and Safety of Treatment With Human-cl-rhFVIII in Previously Untreated Patients With Severe Hemophilia A</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Octapharma</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Octapharma</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      Investigate the inhibitor development rate of Human cl rhFVIII in previously untreated
      patients with severe Hemophilia A.
    </textblock>
  </brief_summary>
  <overall_status>Active, not recruiting</overall_status>
  <start_date>February 2013</start_date>
  <completion_date type="Anticipated">December 2018</completion_date>
  <primary_completion_date type="Anticipated">December 2018</primary_completion_date>
  <phase>Phase 3</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <intervention_model>Single Group Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Immunogenicity</measure>
    <time_frame>maximum 5 years (100 exposure days)</time_frame>
    <description>Number of patients that develop an inhibitor</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Efficacy</measure>
    <time_frame>Maximum 5 years (100 exposure days)</time_frame>
    <description>Efficacy of human cl rhFVIII during prophylactic treatment (frequency of break thorugh blleds) Efficacy of human cl rhFVIII during treatment of bleeds (Number of injections needed to stop a bleed) Efficacy of human cl rhFVIII during surgical prophylaxis (compare expected estimated blood loss versus actual estimated blood loss)</description>
  </secondary_outcome>
  <other_outcome>
    <measure>Safety and tolerability</measure>
    <time_frame>5 years</time_frame>
    <description>Measure vital signs, Adverse events, and standard hematology and chemistry laboratory parameters. This assessment is descriptive)</description>
  </other_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">110</enrollment>
  <condition>Severe Hemophilia A</condition>
  <arm_group>
    <arm_group_label>Human cl rhFVIII</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
  </arm_group>
  <intervention>
    <intervention_type>Biological</intervention_type>
    <intervention_name>Human cl rhFVIII</intervention_name>
    <arm_group_label>Human cl rhFVIII</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Male patients

          -  Severe Hemophilia A (FVIII:C &lt;1%)

          -  No previous treatment with with FVIII concentrates or other blood prodcuts containing
             FVIII

        Exclusion Criteria:

          -  Diagnosis with a coagulation disorder other than Hemophilia A

          -  Severe liver or kidney disease

          -  Concomitant treatment with any systemic immunosuppressive drug
      </textblock>
    </criteria>
    <gender>Male</gender>
    <minimum_age>N/A</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
