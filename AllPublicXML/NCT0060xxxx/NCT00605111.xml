  </study_design_info>
  <primary_outcome>
    <measure>HbA1c</measure>
    <time_frame>after 11 weeks of treatment</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>The proportion of subjects achieving treatment target of HbA1c below 7%</measure>
    <time_frame>at 11 and 24 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>8-point plasma glucose profiles</measure>
    <time_frame>performed at 11 and 24 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>FPG</measure>
    <time_frame>at 11 weeks and 24 weeks</time_frame>
  </secondary_outcome>
  <enrollment type="Actual">193</enrollment>
  <condition>Diabetes</condition>
