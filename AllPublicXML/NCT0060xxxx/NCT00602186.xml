  <brief_title>Effect of Tamsulosin vs Prazosin in Treatment of Female Voiding Dysfunction</brief_title>
  <official_title>Effect of Tamsulosin vs Prazosin in Treatment of Female Voiding Dysfunction</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Tabriz University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
