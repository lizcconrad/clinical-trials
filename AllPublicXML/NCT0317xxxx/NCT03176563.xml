  <brief_title>Observational Follow-up Study of REGATTA</brief_title>
  <acronym>REGATTA II</acronym>
  <official_title>Observational Study of Patients With Uncomplicated Urinary Tract Infection Treated With Antibiotics or Herbal Medicinal Product</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University Medical Center Goettingen</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
