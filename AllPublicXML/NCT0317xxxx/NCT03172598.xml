  <brief_summary>
    <textblock>
      A Study to Investigate the Safety, Tolerability and Efficacy of Multiple Doses of MT-8554 in
      Subjects with Painful Diabetic Peripheral Neuropathy
    </textblock>
  </brief_summary>
  <overall_status>Recruiting</overall_status>
  <start_date type="Actual">July 25, 2017</start_date>
  <completion_date type="Anticipated">August 2018</completion_date>
  <primary_completion_date type="Anticipated">August 2018</primary_completion_date>
