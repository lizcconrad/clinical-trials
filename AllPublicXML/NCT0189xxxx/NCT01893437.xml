  </study_design_info>
  <primary_outcome>
    <measure>Part 1: Safety: Incidence of adverse events</measure>
    <time_frame>18 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Part 1 and 2: Pharmacokinetics: plasma concentration of RO6799477</measure>
    <time_frame>Pre-dose, Day 1, 2, 3, 4</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
