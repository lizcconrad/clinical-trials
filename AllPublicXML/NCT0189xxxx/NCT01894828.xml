  </arm_group>
  <intervention>
    <intervention_type>Dietary Supplement</intervention_type>
    <intervention_name>Nutritional supplements</intervention_name>
    <description>Patients are asked to drink two 200ml bottles of nutritional supplement daily for 14 days before surgery</description>
