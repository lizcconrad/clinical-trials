  </study_design_info>
  <primary_outcome>
    <measure>levels of biochemical serum markers</measure>
    <time_frame>10-13 wks gestation</time_frame>
  </primary_outcome>
  <enrollment type="Actual">200</enrollment>
  <condition>Pregnancy</condition>
