    <time_frame>36 months</time_frame>
    <description>Mean Total Cholesterol (TC), Low Density Lipoprotein (LDL), High Density Lipoprotein (HDL), and Triglyceride blood concentrations.</description>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Actual">264</enrollment>
