  <brief_title>IV Dexketoprofen vs Placebo in Migranie Attack</brief_title>
  <official_title>Study of Comparing Dexketoprofen to Placebo in Migraine Attack</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Akdeniz University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
