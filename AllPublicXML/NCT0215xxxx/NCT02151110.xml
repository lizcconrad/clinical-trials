  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      Phase 1 single IV dose study to evaluate safety and tolerability of MEDI4920
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>May 2014</start_date>
  <completion_date type="Actual">May 2016</completion_date>
