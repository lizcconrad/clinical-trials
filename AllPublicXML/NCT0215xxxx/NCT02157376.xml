            <method>Pearson's Chi-square</method>
            <param_type>Risk Difference (RD)</param_type>
            <param_value>1.9</param_value>
            <ci_percent>95</ci_percent>
            <ci_n_sides>2-Sided</ci_n_sides>
            <ci_lower_limit>-2.8</ci_lower_limit>
            <ci_upper_limit>6.5</ci_upper_limit>
            <estimate_desc>The difference in treatment group percentages is given by Cimetidine minus Esomeprazole.</estimate_desc>
