  <brief_title>Immune Effects of Oral Insulin in Relatives at Risk for Type 1 Diabetes Mellitus (TN20)</brief_title>
  <official_title>Exploring Immunologic Effects of Oral Insulin in Relatives at Risk for Type 1</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>National Institute of Diabetes and Digestive and Kidney Diseases (NIDDK)</agency>
      <agency_class>NIH</agency_class>
    </lead_sponsor>
