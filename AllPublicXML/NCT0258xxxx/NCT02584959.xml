  <primary_outcome>
    <measure>Time-Normalized Number of Attacks (NNA) for participants</measure>
    <time_frame>Weeks 1 to 14 for treatment period 1 and 2</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Proportion of subjects meeting at least a 50% reduction in the NNA during the experimental injection treatment period relative to the placebo period.</measure>
    <time_frame>Weeks 1 to 14 for treatment period 1 and 2</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Cumulative attack severity</measure>
    <time_frame>Weeks 1 to 14 for treatment period 1 and 2</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of attack-free days</measure>
    <time_frame>Weeks 1 to 14 for treatment period 1 and 2</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of angioedema attacks requiring acute treatment</measure>
    <time_frame>Each treatment period: Weeks 1 to 14</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Response to icatibant when administered for an acute attack</measure>
    <time_frame>Weeks 1 to 14 for treatment period 1 and 2</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of subjects with adverse events (AEs)</measure>
    <time_frame>Weeks 1 to 14 for treatment period 1 and 2</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of patients with injection site reactions</measure>
    <time_frame>Weeks 1 to 14 for treatment period 1 and 2</time_frame>
  </secondary_outcome>
