  <brief_title>Pharmacokinetic Behavior of Phenolic Acids Derived From Black Tea</brief_title>
  <official_title>Pharmacokinetic Behavior of Phenolic Acids Derived From Black Tea</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Unilever R&amp;D</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
