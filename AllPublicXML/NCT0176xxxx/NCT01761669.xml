  <brief_title>Does Deficiency in Vitamin D Induced Hypercoagulation or Thrombin Generation</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>HaEmek Medical Center, Israel</agency>
      <agency_class>Other</agency_class>
