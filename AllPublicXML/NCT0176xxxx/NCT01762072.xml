  <brief_title>Effect of Vitamin B12 and n-3 Polyunsaturated Fatty Acids on Plasma Homocysteine</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Zhejiang University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
