  </study_design_info>
  <primary_outcome>
    <measure>Hamilton Rating Scale for Anxiety (HAM-A) total score</measure>
    <time_frame>8 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Sheehan Disability Scale (SDS) total score</measure>
    <time_frame>8 weeks</time_frame>
  </secondary_outcome>
