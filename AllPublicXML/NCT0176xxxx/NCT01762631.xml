  <brief_title>Remote Food Photography Method in Infants</brief_title>
  <acronym>BabyBottle</acronym>
  <official_title>Baby Bottle: Remote Food Photography Method in Infants</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Pennington Biomedical Research Center</agency>
