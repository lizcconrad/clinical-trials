  <brief_title>Safety and Pharmacokinetic Interaction Study of Tadalafil and Dutasteride</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Dongkook Pharmaceutical Co., Ltd.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
