  <brief_title>ENTegral Artificial Larynx Clinical Trial</brief_title>
  <official_title>An Evaluation of the ENTegral Artificial Larynx (AL) in Patients Indicated for Total Laryngectomy</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>ProTiP Medical</agency>
      <agency_class>Industry</agency_class>
