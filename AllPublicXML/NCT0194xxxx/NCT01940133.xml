  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      This study is a Phase I, open label study to determine the Maximum Tolerated Dose(MTD) and
      the Recommended Phase II Dose (RP2D) in patients with advanced solid tumors.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>January 2014</start_date>
  <completion_date type="Actual">December 2015</completion_date>
  <primary_completion_date type="Actual">March 2015</primary_completion_date>
