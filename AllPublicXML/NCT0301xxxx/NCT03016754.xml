  <brief_title>Heart Failure Optimization Study</brief_title>
  <acronym>HF-Opt</acronym>
  <official_title>Heart Failure Optimization Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Zoll Medical Corporation</agency>
      <agency_class>Industry</agency_class>
