  </study_design_info>
  <primary_outcome>
    <measure>% of subjects originating from the active arm of PEPITES reaching an Eliciting Dose (ED) ≥ 1,000 mg after 24 months of additional treatment in PEOPLE</measure>
    <time_frame>Month 24</time_frame>
  </primary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">300</enrollment>
