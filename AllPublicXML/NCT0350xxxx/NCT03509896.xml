  <brief_summary>
    <textblock>
      Observational medical record review of newly diagnosed CML-CP participants in China
    </textblock>
  </brief_summary>
  <overall_status>Active, not recruiting</overall_status>
  <start_date type="Actual">November 30, 2017</start_date>
