  <brief_title>Comparison of Different Materials Contribution to Implants Stability</brief_title>
  <official_title>Comparison of Different Materials Contribution to Immediate Implants Stability With Resonance Frequency Analysis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Necmettin Erbakan University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
