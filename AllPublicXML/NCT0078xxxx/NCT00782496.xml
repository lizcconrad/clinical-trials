  <brief_title>Glucose Meter Study</brief_title>
  <official_title>A New Contour Blood Glucose Monitoring System With High Frequency Testing Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ascensia Diabetes Care</agency>
      <agency_class>Industry</agency_class>
