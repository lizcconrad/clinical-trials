  <brief_title>Lycopene or Green Tea for Men at Risk of Prostate Cancer</brief_title>
  <acronym>ProDiet</acronym>
  <official_title>Lycopene or Green Tea a Feasibility Study in Men at Risk of Prostate Cancer</official_title>
  <sponsors>
