  <brief_title>Telemedicine Interventions for Obstructive Sleep Apnea (OSA)</brief_title>
  <official_title>An RCT of Telemedicine Interventions for OSA</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>VA Office of Research and Development</agency>
      <agency_class>U.S. Fed</agency_class>
    </lead_sponsor>
  </sponsors>
