  </intervention>
  <intervention>
    <intervention_type>Device</intervention_type>
    <intervention_name>Transcutaneous electrical nerve stimulation (TENS)</intervention_name>
    <description>Placebo</description>
    <arm_group_label>Placebo-TENS</arm_group_label>
  </intervention>
