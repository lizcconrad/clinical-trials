    <intervention_type>Drug</intervention_type>
    <intervention_name>CP-690,550</intervention_name>
    <description>Days 1 and 6 dose of 10 mg once daily Days 2-5 doses of 10 mg twice daily</description>
    <arm_group_label>CP-690,550 Dose Group</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Healthy Chinese Volunteers

          -  No evidence of active or latent or inadequately treated infection with Mycobacterium
             tuberculosis (TB)

        Exclusion Criteria:

          -  Clinically significant infections within the past 3 months

          -  Positive screening test for hepatitis B surface antigen, anti-hepatitis C antibody, or
             human immunodeficiency virus

          -  Pregnant or nursing women
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>45 Years</maximum_age>
