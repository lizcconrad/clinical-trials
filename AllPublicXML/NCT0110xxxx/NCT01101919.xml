  <primary_outcome>
    <measure>Single-dose: Cmax, Tmax, t½, AUCinf and AUClast</measure>
    <time_frame>1 day</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Multiple-dose: Cmin, Cmax, Ctrough, Tmax, t½, AUCtau, and Rac</measure>
    <time_frame>5 days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number of adverse events and number of participants with adverse events</measure>
    <time_frame>6 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Changes in complete blood count and serum chemistry profile</measure>
    <time_frame>6 days</time_frame>
  </secondary_outcome>
