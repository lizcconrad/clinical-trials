  <brief_title>Study to Assess the Long-term Safety, Tolerability, and Efficacy of AMG 827 in Subjects With Psoriasis</brief_title>
  <official_title>A Long-term Assessment of the Safety and Efficacy of AMG 827 Subcutaneous Treatment in Subjects With Psoriasis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Amgen</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Amgen</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      This study is an open-label extension of study 20090062 to evaluate extended subcutaneous
      dosing of AMG 827 for up to 362 weeks.
    </textblock>
  </brief_summary>
  <overall_status>Terminated</overall_status>
  <why_stopped>
    Sponsor Decision
  </why_stopped>
  <start_date>April 2010</start_date>
  <completion_date type="Actual">September 2015</completion_date>
  <primary_completion_date type="Actual">September 2015</primary_completion_date>
  <phase>Phase 2</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <intervention_model>Single Group Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Adverse events</measure>
    <time_frame>362 weeks</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Change in laboratory parameters and vital signs</measure>
    <time_frame>360 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>The proportion of subjects with a static physician's global assessment (sPGA) of clear (0) or clear/almost clear (0 or 1)</measure>
    <time_frame>360 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Percent improvement in Psoriasis Area and Severity Index (PASI)</measure>
    <time_frame>360 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>The proportion of subjects with a 50% improvement in PASI (PASI 50), PASI 75, PASI 90, and PASI 100</measure>
    <time_frame>360 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Body surface area (BSA) involvement</measure>
    <time_frame>360 weeks</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">181</enrollment>
  <condition>Psoriasis</condition>
