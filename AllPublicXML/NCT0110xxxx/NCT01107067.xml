  </study_design_info>
  <primary_outcome>
    <measure>paraoxonase level after testosterone treatment</measure>
  </primary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">32</enrollment>
  <condition>Hypogonadism</condition>
  <arm_group>
