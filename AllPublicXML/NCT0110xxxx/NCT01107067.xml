    <arm_group_type>Experimental</arm_group_type>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Sustanon</intervention_name>
    <description>250 mg/21 days</description>
    <arm_group_label>testosterone replacement therapy</arm_group_label>
    <other_name>no name</other_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Volunteers patients with hypogonadism

        Exclusion Criteria:

          -  Coronary heart disease

          -  Other pituitary/hypothalamic disorders or other non-hypogonadism diseases

          -  None was receiving vitamins, lipid-lowering drugs, or other medications known to
             interfere with PON1 activity, lipid profile, or gonadal function
      </textblock>
    </criteria>
    <gender>Male</gender>
    <minimum_age>20 Years</minimum_age>
    <maximum_age>25 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
