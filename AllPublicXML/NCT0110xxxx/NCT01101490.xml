    <time_frame>1 hour after operation</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>blood flow</measure>
    <time_frame>1 hour after operation</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>velocity of superficial temporal artery in right and left side</measure>
