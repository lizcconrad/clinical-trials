  <brief_title>Assessment of Digital Breast Tomosynthesis (DBT) in the Screening Environment</brief_title>
  <official_title>Assessment of Digital Breast Tomosynthesis (DBT) in the Screening Environment: a Prospective Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Pittsburgh</agency>
      <agency_class>Other</agency_class>
