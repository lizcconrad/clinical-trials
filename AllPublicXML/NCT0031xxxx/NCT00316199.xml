  <brief_title>Efficacy Study of Gemcitabine-Paclitaxel to Treat Metastatic Breast Cancer</brief_title>
  <official_title>Phase II Study of Gemcitabine-Paclitaxel 3-Weekly Schedule as First-Line Treatment in Metastatic Breast Cancer After Anthracycline Failure</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Eli Lilly and Company</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
