  <brief_title>Trial of Oxytocin Alone Versus Oxytocin and Propranolol for the Treatment of Abnormal Labor</brief_title>
  <official_title>Placebo-Controlled Randomized Trial of Oxytocin Alone Versus Oxytocin and Propranolol for the Treatment of Dysfunctional Labor</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of California, Irvine</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
