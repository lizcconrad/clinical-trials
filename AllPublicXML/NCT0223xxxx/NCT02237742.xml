    <intervention_name>PF-06427878</intervention_name>
    <description>Intravenous, 100 micrograms, single dose, 30 minute infusion</description>
    <arm_group_label>PF-06427878</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          1. Healthy male subjects between 18-55 years of age

          2. BMI of 17.5-30.5 kg/m2

        Exclusion Criteria:

          1. History of drug or alcohol abuse within 2 years of screening

          2. Subjects without suitable veins for multiple venipuncture/cannulation
      </textblock>
    </criteria>
    <gender>Male</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>55 Years</maximum_age>
