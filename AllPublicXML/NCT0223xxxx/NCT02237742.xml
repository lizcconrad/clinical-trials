  <primary_outcome>
    <measure>Area Under the Plasma Curve From Time Zero to Last Quantifiable Concentration (AUClast)</measure>
    <time_frame>Day 1 Min 0,5,15,30,35,45,60,75,90, Hr 2,2.5,3.5,4.5,5.5,6.5,8.5,10.5,12.5,24.5</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Area under the Plasma Concentration-Time Curve From Time Zero extrapolated to Infinite Time (AUCinf)</measure>
    <time_frame>Day 1 Min 0,5,15,30,35,45,60,75,90, Hr 2,2.5,3.5,4.5,5.5,6.5,8.5,10.5,12.5,24.5</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Maximum Observed Plasma Concentration (Cmax)</measure>
