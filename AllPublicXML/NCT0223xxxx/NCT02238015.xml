  <brief_title>Tear Film Evaluation After Phacoemulsification</brief_title>
  <acronym>TFAP</acronym>
  <official_title>To Investigate Tear Film Change After Phacoemulsification With SS-OCT</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Zheng Qian</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
