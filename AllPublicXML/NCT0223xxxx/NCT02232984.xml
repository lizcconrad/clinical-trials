  <brief_title>Quadripolar CRT-D On Currently Approved Lead SystemS</brief_title>
  <acronym>CROSS X4</acronym>
  <official_title>Quadripolar CRT-D On Currently Approved Lead SystemS</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Boston Scientific Corporation</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
