  <brief_title>EndoTic - Endothelium and Ticagrelor</brief_title>
  <official_title>EndoTic - Endothelium and Ticagrelor: Pharmacological Effects Beyond Antiplatelet Therapy</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Fundación Investigación Sanitaria en León</agency>
      <agency_class>Other</agency_class>
