  <primary_outcome>
    <measure>Number of patients with adverse events</measure>
    <time_frame>8 days</time_frame>
    <description>Measurement of safety laboratories, ECGs, vitals signs and physical exams</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Sodium levels in stool and urine</measure>
    <time_frame>8 days</time_frame>
    <description>Pharmacodynamic activity</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Plasma drug concentration to calculate AUC</measure>
    <time_frame>8 days</time_frame>
    <description>Pharmacokinetics</description>
  </secondary_outcome>
  <number_of_arms>3</number_of_arms>
