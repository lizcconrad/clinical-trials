  </study_design_info>
  <primary_outcome>
    <measure>Number of participants with serious and non-serious adverse events</measure>
    <time_frame>Up to 34 months</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Change from Baseline in laboratory test values</measure>
    <time_frame>Up to 34 months</time_frame>
    <description>Standard hematology and serum chemistry parameters to be measured</description>
  </primary_outcome>
