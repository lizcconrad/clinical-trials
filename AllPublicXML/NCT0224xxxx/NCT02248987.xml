  <brief_title>Study on Abnormal Dopamine Synthesis and Connectivity According to the Antipsychotic Treatment Response in Schizophrenia</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Seoul National University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Seoul National University Hospital</source>
  <brief_summary>
    <textblock>
      The purpose of this study is to evaluate abnormal dopamine synthesis and connectivity
      according to the antipsychotic treatment response in schizophrenia.
    </textblock>
  </brief_summary>
  <overall_status>Unknown status</overall_status>
  <last_known_status>Recruiting</last_known_status>
  <start_date>May 2014</start_date>
  <primary_completion_date type="Anticipated">October 2014</primary_completion_date>
  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Case Control</observational_model>
    <time_perspective>Cross-Sectional</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Striatal [18F]-DOPA uptake measured using positron emission tomographic 18F-dopa imaging.</measure>
    <time_frame>within 2 weeks from enrollment</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>resting state functional magnetic resonance imaging</measure>
    <time_frame>within 2 weeks from enrollment</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>diffusion tensor imaging</measure>
    <time_frame>within 2 weeks from enrollment</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>clinical measure including Positive and Negative Syndrome Scale</measure>
    <time_frame>within 2 weeks from enrollment</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>neurocognitive function test</measure>
    <time_frame>within 2 weeks from enrollment</time_frame>
  </secondary_outcome>
  <number_of_groups>3</number_of_groups>
  <enrollment type="Anticipated">36</enrollment>
  <condition>Schizophrenia</condition>
  <arm_group>
    <arm_group_label>Clozapine</arm_group_label>
    <description>stable patients treated with clozapine</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Other antipsychotics</arm_group_label>
    <description>stable patients treated with risperidone or paliperidone or olanzapine</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Healthy volunteer</arm_group_label>
    <description>healthy controls</description>
  </arm_group>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>patients treated with antipsychotics</intervention_name>
    <arm_group_label>Clozapine</arm_group_label>
    <arm_group_label>Other antipsychotics</arm_group_label>
    <arm_group_label>Healthy volunteer</arm_group_label>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        1. Schizophrenia patients treated with clozapine

          2. Schizophrenia patients treated with risperidone or paliperidone or olanzapine

          3. Healthy volunteers
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  diagnosis of schizophrenia

          -  clinically stable state

        Exclusion Criteria:

          -  history of previous head injury, epileptic disorder, other medical comorbidity

          -  substance abuse/dependence
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>20 Years</minimum_age>
    <maximum_age>55 Years</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
