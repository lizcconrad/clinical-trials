    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Number of participants with febrile infection</measure>
    <time_frame>1 week</time_frame>
    <description>consider fever over 38 degree celsius or admission due to fever</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>voiding questionnaire</measure>
    <time_frame>1-2 week</time_frame>
    <description>check International Prostate Symptom Score (IPSS)</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>sexual function questionnaire</measure>
    <time_frame>1-2 week</time_frame>
    <description>International Index of Erectile Function-5 (IIEF-5)</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>quality of life questionnaire</measure>
    <time_frame>1-2 week</time_frame>
    <description>European Organization for Research and Treatment fo Cancer (EORTC) Quality of life Questionnaire (QLQ)-C30 questionnaire</description>
  </secondary_outcome>
