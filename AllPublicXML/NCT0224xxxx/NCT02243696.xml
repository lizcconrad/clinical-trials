  <brief_title>Protego DF4 Post Approval Registry</brief_title>
  <official_title>Protego DF4 Post Approval Registry</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Biotronik, Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
