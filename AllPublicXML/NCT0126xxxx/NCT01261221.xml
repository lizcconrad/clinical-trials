  <brief_title>Pilot Study of Metabolic Syndromes and HIV Infections in Indian Tibetans</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Kaohsiung Medical University Chung-Ho Memorial Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
