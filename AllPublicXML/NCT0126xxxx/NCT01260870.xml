  <brief_title>European Study of POBA Versus Cotavance(R) Paclitaxel Coated Balloon for the Treatment of Infrapopliteal Lesions in Critical Limb Ischemia (EURO CANAL)</brief_title>
  <official_title>European Study of POBA Versus Cotavance Paclitaxel Coated Balloon for the Treatment of Infrapopliteal Lesions in Critical Limb Ischemia</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Bayer</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
