  <brief_title>Risk Factor Analysis for Developing Metabolic Syndrome in Korean Adults</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Korea University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
