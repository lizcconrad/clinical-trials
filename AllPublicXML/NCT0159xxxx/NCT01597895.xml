  <primary_outcome>
    <measure>Maraviroc plasma pharmacokinetic parameters: AUC12, Cmax, and C12h on Period 1, Day 5 and Periods 2 and 3, Day 10</measure>
    <time_frame>25 days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Maraviroc plasma pharmacokinetic parameters: Tmax on Period 1, Day 5 and Periods 2 and 3, Day 10</measure>
    <time_frame>25 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Boceprevir plasma pharmacokinetic parameters: AUC8, Cmax, C8h, and Tmax on Period 2, Day 10</measure>
    <time_frame>10 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Telaprevir plasma pharmacokinetic parameters: AUC8, Cmax, C8h, and Tmax on Period 3, Day 10</measure>
    <time_frame>10 days</time_frame>
  </secondary_outcome>
