  </study_design_info>
  <enrollment type="Anticipated">250</enrollment>
  <condition>Orbital Volume</condition>
  <eligibility>
    <study_pop>
      <textblock>
        pregnant women 13-37w
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  well dated singleton pregnancies

        Exclusion Criteria:

          -  multiple pregnancy

          -  pregnancies with known malformations,

          -  chromosomal abnormalities or

          -  intrauterine infections
      </textblock>
    </criteria>
    <gender>Female</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>45 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
