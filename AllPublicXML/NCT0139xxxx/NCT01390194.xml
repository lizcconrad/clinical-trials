  <arm_group>
    <arm_group_label>subjects with an underlying liver disease</arm_group_label>
    <description>(1)underlying liver disease; (2) have a lesion on a prior imaging study; (3) must be prior standard MR</description>
  </arm_group>
  <eligibility>
    <study_pop>
      <textblock>
        patients with an underlying liver disease
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          1. underlying liver disease

          2. have a lesion on a prior imaging study

          3. must have prior standard MR

        Exclusion Criteria:

          1. Contra-indication for MR imaging including pacemaker, claustrophobia, pregnancy,
             electronic or vascular devices that are contraindicated;

          2. patients with severe kidney dysfunction
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>19 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
