    <outcome_list>
      <outcome>
        <type>Primary</type>
        <title>Comparison of SureCALL® and Tocodynamometer Detection of Contraction Event Timing</title>
        <description>Time Stamps of the Peaks of Corresponding Contractions</description>
        <time_frame>9 - 41 Minutes</time_frame>
        <group_list>
          <group group_id="O1">
            <title>Pregnant Patents With Uterine Contractions</title>
            <description>Patient contractions were monitored with both SureCALL® Labor Monitor® and Tocodynamometer. The times of peak contractions recorded by both devices were compared, and the time differences between contractions were assessed.</description>
          </group>
        </group_list>
        <measure>
          <title>Comparison of SureCALL® and Tocodynamometer Detection of Contraction Event Timing</title>
          <description>Time Stamps of the Peaks of Corresponding Contractions</description>
          <units>Seconds</units>
          <param>Mean</param>
          <dispersion>Standard Deviation</dispersion>
          <analyzed_list>
            <analyzed>
              <units>Participants</units>
              <scope>Measure</scope>
              <count_list>
                <count group_id="O1" value="30"/>
              </count_list>
            </analyzed>
          </analyzed_list>
          <class_list>
            <class>
              <category_list>
                <category>
                  <measurement_list>
                    <measurement group_id="O1" value="0.99" spread="15.17"/>
                  </measurement_list>
                </category>
              </category_list>
            </class>
          </class_list>
        </measure>
        <analysis_list>
          <analysis>
            <group_id_list>
              <group_id>O1</group_id>
            </group_id_list>
            <non_inferiority_type>Non-Inferiority or Equivalence</non_inferiority_type>
            <non_inferiority_desc>Agreement between SureCALL® and Tocodynamometer Contraction Peak Times
Null hypothesis: The mean peak difference between RMS and TOCO is equal to 0. Alternative hypothesis: The mean peak difference is not equal to 0.</non_inferiority_desc>
            <p_value>0.4901</p_value>
            <method>Mixed Models Analysis</method>
            <param_type>Mean Difference (Net)</param_type>
            <param_value>.99</param_value>
            <dispersion_type>Standard Error of the Mean</dispersion_type>
            <dispersion_value>1.4086</dispersion_value>
            <ci_percent>95</ci_percent>
            <ci_n_sides>2-Sided</ci_n_sides>
            <ci_lower_limit>-28.74</ci_lower_limit>
            <ci_upper_limit>30.72</ci_upper_limit>
          </analysis>
        </analysis_list>
      </outcome>
