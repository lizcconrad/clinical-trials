  <brief_title>Evaluation of the GORE® C3 Delivery System Module</brief_title>
  <official_title>Global Registry for Endovascular Aortic Treatment(GREAT)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>W.L.Gore &amp; Associates</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
