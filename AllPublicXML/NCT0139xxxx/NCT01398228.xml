  <brief_title>Clinical Pathways for the Management of Acute Coronary Syndromes - Phase 3,CPACS-3</brief_title>
  <acronym>CPACS-3</acronym>
  <official_title>Clinical Pathways for the Management of Acute Coronary Syndromes - Phase 3</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>The George Institute for Global Health, China</agency>
      <agency_class>Other</agency_class>
