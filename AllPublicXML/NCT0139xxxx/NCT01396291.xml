    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Quadruple (Participant, Care Provider, Investigator, Outcomes Assessor)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>The time (in days) to recurrence of any mood event during the double-blind treatment period</measure>
    <time_frame>From Week 12 or 16 to Week 38 or 42</time_frame>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">561</enrollment>
  <condition>Bipolar 1 Disorder</condition>
