    <intervention_name>Exercise training</intervention_name>
    <description>Different dose of exercise training</description>
    <arm_group_label>moderate exercise training</arm_group_label>
    <arm_group_label>intensive exercise training</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Postmenopausal women aged 40 to 80 years and age matched men without major diseases

        Exclusion Criteria:

          -  Known cardiac disease, excessive weight change during last year, cancer and mental
             disorders
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>40 Years</minimum_age>
    <maximum_age>80 Years</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
