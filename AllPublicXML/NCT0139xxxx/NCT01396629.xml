  <brief_title>Clinical Evaluation of a New Cartridge for the Implantation of Foldable Intraocular Lenses</brief_title>
  <official_title>Clinical Evaluation of a New Cartridge for the Implantation of Foldable Intraocular Lenses</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Aurolab</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
