  </study_design_info>
  <primary_outcome>
    <measure>clinical performance</measure>
    <time_frame>3 years</time_frame>
    <description>Subjective evaluation of restorations using USPHS criteria</description>
  </primary_outcome>
  <secondary_outcome>
