  <brief_title>Endometrial Cancer and Robotic-assisted Versus Traditional Laparoscopy</brief_title>
  <official_title>Robotic-assisted vs. Traditional Laparoscopic Surgery in Treatment of Endometrial Cancer Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Tampere University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
