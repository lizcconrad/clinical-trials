  <brief_title>Effect of Pasta on Blood Glucose Response in Normal Subjects</brief_title>
  <official_title>Effect of Pasta on Blood Glucose Response in Normal Subjects</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Minneapolis Veterans Affairs Medical Center</agency>
      <agency_class>U.S. Fed</agency_class>
    </lead_sponsor>
  </sponsors>
