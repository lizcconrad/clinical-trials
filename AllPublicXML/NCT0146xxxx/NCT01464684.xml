  <enrollment type="Actual">0</enrollment>
  <condition>Infant Body Composition and Metabolism</condition>
  <biospec_retention>Samples With DNA</biospec_retention>
  <biospec_descr>
    <textblock>
      Blood, urine, and a cheek swab may be collected from study participants
    </textblock>
  </biospec_descr>
  <eligibility>
    <study_pop>
      <textblock>
        infants age 12 months (+/- 9 months)
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  mother's prior participation in study #111576 with successful collection of placenta

          -  infant age 12 months (+/- 9 months)

        Exclusion Criteria:

          -  mother did not participate in study #111576

          -  mother did participate in study #111576, but collection of placenta was not successful

          -  infant not age 12 months (+/- 9 months)
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>3 Months</minimum_age>
    <maximum_age>21 Months</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
