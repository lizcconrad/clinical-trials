  </study_design_info>
  <primary_outcome>
    <measure>Incidence of hypoglycaemic episodes during the treatment</measure>
    <time_frame>Week 0; week 24</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Change in HbA1c (glycosylated haemoglobin A1c)</measure>
    <time_frame>Week 0; week 24</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Incidence of Adverse Events (AEs)</measure>
    <time_frame>Week 0; week 24</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">26</enrollment>
  <condition>Diabetes</condition>
  <condition>Diabetes Mellitus, Type 1</condition>
  <arm_group>
    <arm_group_label>IAsp</arm_group_label>
