  <brief_title>Safety Study of Three Formulations of the Dermal Implant ELAPR</brief_title>
  <official_title>A Phase I Study to Assess the Safety of Three Formulations of the Dermal Implant ELAPR in Healthy Subjects</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Elastagen Pty Ltd</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
