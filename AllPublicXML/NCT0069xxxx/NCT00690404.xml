  <primary_outcome>
    <measure>AZ2066 pharmacokinetics</measure>
    <time_frame>several samples within 72 hrs</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Effect of food on AZD2066 pharmacokinetics</measure>
    <time_frame>several samples over 72 hrs</time_frame>
  </secondary_outcome>
