  <primary_outcome>
    <measure>Safety measures, i.e. assessing adverse events, vital signs, ECG, safety laboratory values</measure>
    <time_frame>About 8 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetics</measure>
    <time_frame>5 days after each dose</time_frame>
