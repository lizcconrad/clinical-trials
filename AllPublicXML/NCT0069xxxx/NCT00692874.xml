  <brief_title>Quality of Life Analysis of Treatment in Patients With Prostate Cancer on Adjuvant Zoladex Therapy</brief_title>
  <acronym>AZTEK EXT</acronym>
  <official_title>Quality of Life Analysis of Prostate Cancer Patient's Therapy</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AstraZeneca</agency>
