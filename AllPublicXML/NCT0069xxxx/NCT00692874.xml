    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      This is an open label, non-interventional quality of life analysis for Zoladex treated
      prostate cancer patients by investigator's questionnaire
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>June 2008</start_date>
  <completion_date type="Actual">September 2010</completion_date>
  <primary_completion_date type="Actual">July 2010</primary_completion_date>
  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Cohort</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Elements of a general and three disease-specific investigator questionnaire</measure>
    <time_frame>Quarterly (plus or minus 3 days )</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Investigator Assessment Report</measure>
    <time_frame>Yearly (plus or minus 1 week )</time_frame>
  </secondary_outcome>
  <enrollment type="Actual">1646</enrollment>
  <condition>Prostate Cancer</condition>
  <eligibility>
    <study_pop>
      <textblock>
        Early or locally-advanced prostate cancer patients on androgen deprivation therapy
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Provision of informed consent

          -  Early or locally-advanced prostate cancer patients on androgen deprivation therapy

          -  Investigators are requested to recruit patients only with at least 1 month prior
             Zoladex treatment.

        Exclusion Criteria:

          -  Allergy to substance of medication

          -  Prostate cancer patients with advanced disease
      </textblock>
    </criteria>
    <gender>Male</gender>
    <minimum_age>N/A</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
