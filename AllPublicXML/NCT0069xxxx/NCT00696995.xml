  </study_design_info>
  <primary_outcome>
    <measure>The number of serious adverse drug reactions, including major hypoglycaemic events</measure>
    <time_frame>at 26 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>The number of all hypoglycaemic events</measure>
    <time_frame>at 26 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>HbA1c</measure>
    <time_frame>at 12 weeks and 26 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Fasting plasma glucose at visits</measure>
    <time_frame>at 12 weeks and 26 weeks</time_frame>
  </secondary_outcome>
