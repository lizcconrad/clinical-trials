  <brief_title>A Clinical Investigation of the Discovery™ Elbow System</brief_title>
  <official_title>A Prospective, Non-controlled, Clinical Investigation of the Discovery™ Elbow System</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Biomet Orthopedics, LLC</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
