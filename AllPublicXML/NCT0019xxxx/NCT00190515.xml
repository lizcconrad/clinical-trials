  <brief_title>A Trial Comparing Adjuvant Oral UFT/LV to 5-FU/l-LV in Stage III Colorectal Cancer (JCOG-0205-MF)</brief_title>
  <official_title>Randomized Phase III Trial Comparing Adjuvant Oral UFT/LV to 5-FU/l-LV in Stage III Colorectal Cancer (JCOG-0205-MF)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Haruhiko Fukuda</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
