  <eligibility>
    <study_pop>
      <textblock>
        Premature infants &lt; 1000 gram birthweight who are followed post discharge as part of
        standard clinical care
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  All premature infants born &lt; 1000 grams who had MRI scans of the brain at term
             gestation

        Exclusion Criteria:

          -  Any known genetic or chromosomal abnormalities
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Months</minimum_age>
    <maximum_age>2 Years</maximum_age>
