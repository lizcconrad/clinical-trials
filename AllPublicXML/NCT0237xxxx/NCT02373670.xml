  <brief_title>Parent Mentors Using Positive Deviance in Childhood Obesity</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>The University of Texas Health Science Center at San Antonio</agency>
      <agency_class>Other</agency_class>
