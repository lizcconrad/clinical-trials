  <brief_title>Allergen Biocube Study Evaluating Efficacy and Safety of Nasapaque Nasal Solution in Adult Subjects With Allergic Rhinitis</brief_title>
  <official_title>A Single-Center, Double-Masked, Randomized, Placebo-Controlled, Parallel-Group Allergen BioCube Study Evaluating the Efficacy and Safety of Nasapaque Nasal Solution in a Population of Adult Subjects With Seasonal Allergic Rhinitis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>3E Therapeutics Corporation</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>ORA, Inc.</agency>
      <agency_class>Industry</agency_class>
    </collaborator>
  </sponsors>
  <source>3E Therapeutics Corporation</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      Single-center, double-masked, randomized, placebo-controlled, parallel-group, efficacy and
      safety study using the Allergen BioCube (ABC).
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>March 2015</start_date>
  <completion_date type="Actual">July 2015</completion_date>
  <primary_completion_date type="Actual">July 2015</primary_completion_date>
  <phase>Phase 1/Phase 2</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>TNSS Total Nasal Symptom Score</measure>
    <time_frame>Day 1 and Day 8</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Nasal Inspiratory Flow</measure>
    <time_frame>Day 1 and Day 8</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Nasal Inflammation Score</measure>
    <time_frame>Day 1 and Day 8</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">73</enrollment>
  <condition>Seasonal Allergic Rhinitis</condition>
  <arm_group>
    <arm_group_label>Nasapaque Nasal Solution</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>250 ul in each nostril at Day 1 and Day 8</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Placebo Saline Nasal Solution</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>250 ul in each nostril at Day 1 and Day 8</description>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Nasapaque Nasal Solution</intervention_name>
    <arm_group_label>Nasapaque Nasal Solution</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Placebo Saline Nasal Solution</intervention_name>
    <arm_group_label>Placebo Saline Nasal Solution</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Key Inclusion Criteria:

          -  Must be 18 years of age and provide written informed consent and sign the HIPAA form

          -  Must have history of allergic rhinitis

          -  Must have positive response to Allergen BioCube

        Key Exclusion Criteria:

          -  Must not have a significant illness such as moderate to severe allergic asthmatic
             reactions

          -  Must not have compromised lung function

          -  Must not use any disallowed medications

          -  Must not have been in an investigational study in the last 30 days
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
