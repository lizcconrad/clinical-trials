  <brief_title>Treatment of Pelvic Organ Prolapse by PIVS Versus Surgeon Cut Mesh</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Western Galilee Hospital-Nahariya</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
