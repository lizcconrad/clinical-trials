  <brief_title>Manual Therapy Treatment for Myofascial Pain</brief_title>
  <official_title>Effect of Manual Therapy After Trigger Point Injection for Myofascial Pain Syndrome</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Federal University of São Paulo</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
