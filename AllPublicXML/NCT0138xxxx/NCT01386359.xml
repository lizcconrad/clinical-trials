  <brief_title>Evaluating Nulojix (Belatacept) Long-Term Safety in Transplant</brief_title>
  <acronym>ENLIST</acronym>
  <official_title>Evaluating Nulojix Long-Term Safety in Transplant</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Bristol-Myers Squibb</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
