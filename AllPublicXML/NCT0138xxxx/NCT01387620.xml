  <brief_title>Comparison of DisCoVisc and Hydroxypropylmethylcellulose 2%</brief_title>
  <official_title>Intraindividual Randomized Clinical Comparison of DisCoVisc and Hydroxypropylmethylcellulose 2% in Phacoemulsification</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Sao Paulo</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
