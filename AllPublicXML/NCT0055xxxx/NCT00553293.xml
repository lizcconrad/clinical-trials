  </study_design_info>
  <primary_outcome>
    <measure>Ongoing pregnancy rate per started stimulation cycle</measure>
    <time_frame>ultrasound showing ongoing pregnancy</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Ongoing pregnancy in subgroups of patients with lo0w serum LH levels</measure>
    <time_frame>ultrasound for ongoing pregnancy in subgroups</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">526</enrollment>
  <condition>Infertility</condition>
  <condition>In Vitro Fertilization</condition>
