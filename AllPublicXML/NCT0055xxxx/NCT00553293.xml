    <intervention_type>Drug</intervention_type>
    <intervention_name>recombinant luteinizing hormone (Luveris)</intervention_name>
    <description>75 iu/day form cycle day 6</description>
    <arm_group_label>A,1</arm_group_label>
    <other_name>Luveris</other_name>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>recombinant follicle stimulating hormone (Gonal-f)</intervention_name>
    <description>rFSH 150 iu/day</description>
    <arm_group_label>A,2</arm_group_label>
    <other_name>Gonal-f</other_name>
  </intervention>
