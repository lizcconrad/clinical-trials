  </study_design_info>
  <primary_outcome>
    <measure>To evaluate the safety and tolerability of BIIB022</measure>
    <time_frame>ongoing</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>To evaluate pharmacokinetics</measure>
    <time_frame>ongoing</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
