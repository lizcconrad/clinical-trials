  <brief_title>ProSat - Effect of Probiotics on Satiety</brief_title>
  <acronym>ProSat</acronym>
  <official_title>Probiotics and Satiety - Acute Effects and Persistence of Acute Effects</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Copenhagen</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>Chr Hansen</agency>
