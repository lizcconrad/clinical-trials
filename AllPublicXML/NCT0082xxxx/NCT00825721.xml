    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Best-corrected visual acuity by ETDRS</measure>
    <time_frame>360 days</time_frame>
  </primary_outcome>
  <number_of_arms>4</number_of_arms>
  <enrollment type="Actual">81</enrollment>
  <condition>Nuclear Cataract</condition>
