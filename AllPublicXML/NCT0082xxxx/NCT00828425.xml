  <brief_title>Management of Diabetes Mellitus Patients With Retinopathy</brief_title>
  <official_title>Management of Diabetes Mellitus Patients With Retinopathy</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AstraZeneca</agency>
