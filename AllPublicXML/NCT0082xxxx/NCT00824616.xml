  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Participant has Type 2 Diabetes Mellitus

        Exclusion Criteria:

          -  Participant has a history of Type 1 Diabetes Mellitus or ketoacidosis

          -  Participant is on a weight loss program and is not in the maintenance phase or is
             taking a weight loss medication

          -  Participant has had surgery within 30 days of starting the study or has planned major
             surgery
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>21 Years</minimum_age>
    <maximum_age>70 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
