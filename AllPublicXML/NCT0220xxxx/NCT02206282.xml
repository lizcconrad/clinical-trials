  <brief_title>Prevalence of Asymptomatic Rupture in SILIMED Breast Implants</brief_title>
  <official_title>Prevalence of Asymptomatic Rupture in SILIMED Breast Implants</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Silimed Industria de Implantes Ltda</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
