    <allocation>Non-Randomized</allocation>
    <intervention_model>Single Group Assignment</intervention_model>
    <primary_purpose>Supportive Care</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Patient perception of the use of the device</measure>
    <time_frame>Day 1</time_frame>
    <description>Small questionnaires will be used to assess how the patient perceives the use of the device (i.e., did the patient have a positive attitude towards the device?)</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Physician perception of the use of the device</measure>
    <time_frame>Day 1</time_frame>
    <description>Physicians will complete a small questionnaire to understand what their perception of the use of the device to be (i.e, did the physician have a positive attitude towards the device while using it).</description>
  </secondary_outcome>
  <other_outcome>
    <measure>Detection of adverse events related to the standard of care procedure</measure>
    <time_frame>2nd day</time_frame>
    <description>VeinViewer may be used to detect adverse events related to the standard of care procedure (e.g., bruising).</description>
  </other_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Anticipated">100</enrollment>
  <condition>Cosmetic Procedures</condition>
  <arm_group>
    <arm_group_label>VeinViewer</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
