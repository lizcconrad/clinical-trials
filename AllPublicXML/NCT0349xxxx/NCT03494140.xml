  <brief_title>Viscontour® Serum Med in Hydration of Facial Skin</brief_title>
  <official_title>Viscontour® Serum Med in Hydration of Facial Skin</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>TRB Chemedica AG</agency>
