  <brief_title>Effects of Maternal Obesity on Offspring Brain Development</brief_title>
  <official_title>Effects of Maternal Obesity on Offspring Brain Development</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Arkansas Children's Hospital Research Institute</agency>
