  <brief_title>Multi-Centre Clinical Evaluation of Two Reusable Soft Contact Lenses</brief_title>
  <official_title>Multi-Centre Clinical Evaluation of Two Reusable Soft Contact Lenses</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Coopervision, Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
