  <brief_title>Accuracy of Corneal Astigmatism in Different Region Modes</brief_title>
  <official_title>Accuracy of Corneal Astigmatism in Different Region Modes</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Wenzhou Medical University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
