  <brief_title>Comparison of Extraperitoneal and Intraperitoneal Cesarean Technique</brief_title>
  <official_title>Comparison of Extraperitoneal and Intraperitoneal Cesarean Technique: Double Blind Controlled Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Zeynep Kamil Maternity and Pediatric Research and Training Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
