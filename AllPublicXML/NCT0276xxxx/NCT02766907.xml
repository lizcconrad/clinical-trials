  <primary_outcome>
    <measure>improvement of ocular surface integrity in terms of wavefront error, K reading and cylinder</measure>
    <time_frame>Change from Baseline to 1 month</time_frame>
    <description>Assessed by corneal topography</description>
  </primary_outcome>
  <primary_outcome>
    <measure>improvement of intraocular lens power calculation</measure>
    <time_frame>Change from Baseline to 1 month</time_frame>
    <description>Assessed using biometry and calculated using Holladay I formula</description>
  </primary_outcome>
