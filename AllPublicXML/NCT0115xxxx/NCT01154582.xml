  <brief_title>Nutritional Quality of Egg Proteins and Effect on Satiety</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Institut National de la Recherche Agronomique</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
