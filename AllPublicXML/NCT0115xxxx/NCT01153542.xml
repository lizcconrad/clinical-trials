  <primary_outcome>
    <measure>VX-770 and Desipramine pharmacokinetic parameters</measure>
    <time_frame>3 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Safety as measured by adverse events, vital signs, electrocardiograms (ECGs), and clinically significant laboratory assessments</measure>
    <time_frame>3 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Metabolites pharmacokinetic parameters in plasma</measure>
    <time_frame>3 weeks</time_frame>
  </secondary_outcome>
