  <primary_outcome>
    <measure>Comparing frequency and severity of pain</measure>
    <time_frame>up to 84 days</time_frame>
    <description>Patient diary</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Changes in fat malabsorption</measure>
    <time_frame>up to 124 days</time_frame>
    <description>Assess the coefficient of fat absorption</description>
  </secondary_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment type="Anticipated">15</enrollment>
