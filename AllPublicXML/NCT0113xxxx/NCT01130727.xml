    <measure>arterial blood pressure and cardiac rate</measure>
    <time_frame>up to 4 hours</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>plasma levels of flavonols</measure>
    <time_frame>up to 4 hours</time_frame>
  </secondary_outcome>
