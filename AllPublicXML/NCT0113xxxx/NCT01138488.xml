  <primary_outcome>
    <measure>Area under the serum insulin degludec concentration-time curve</measure>
    <time_frame>from 0 to infinity after single-dose</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Area under the serum insulin aspart concentration-time curve</measure>
    <time_frame>from 0 to 12 hours after single-dose</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Maximum observed insulin degludec concentration of NN5401 (insulin degludec/insulin aspart) observed</measure>
    <time_frame>from 0 to 57 hours after single-dose</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Maximum observed insulin aspart concentration of NN5401 (insulin degludec/insulin aspart) observed</measure>
    <time_frame>from 0 to 12 hours after single-dose</time_frame>
  </secondary_outcome>
