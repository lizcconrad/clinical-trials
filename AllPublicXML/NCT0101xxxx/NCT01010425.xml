  <primary_outcome>
    <measure>Safety and Tolerability (adverse events, local reactions, immunogenicity)</measure>
    <time_frame>0-42 days</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Pharmacokinetics; AUC and Cmax for hGH, TransCon-PEG hGH; and Pharmacodynamics measured by IGF-1 and IFG-BP</measure>
    <time_frame>0-28 days</time_frame>
  </primary_outcome>
