  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Crossover Assignment</intervention_model>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Pharmacokinetic profile of rosiglitazone (Cohort 1), midazolam &amp; metabolites and s-warfarin (Cohort 2) during steady state exposure to BIIB014 or placebo</measure>
    <time_frame>24 hours (Cohort 1); 144 hours (Cohort 2)</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Adverse Events</measure>
    <time_frame>Duration of subject participation</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">28</enrollment>
