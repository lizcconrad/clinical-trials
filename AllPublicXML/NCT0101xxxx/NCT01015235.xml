    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>The effect of KAI-1678 on summed pain intensity difference over 4 hours (SPID 4).</measure>
    <time_frame>Post operative Day 1</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>The effect of KAI-1678 on total pain relief over 4 hours (TOTPAR 4)</measure>
    <time_frame>Post operative Day 1</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>The effect of KAI-1678 on pain intensity difference (PID) at 4 hours</measure>
    <time_frame>Post-operative Day 1</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>The effect of KAI-1678 on total quality analgesia</measure>
    <time_frame>Post-operative Day 1</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>The effect of KAI-1678 on time to meaningful pain relief</measure>
    <time_frame>Post-operative Day 1</time_frame>
  </secondary_outcome>
