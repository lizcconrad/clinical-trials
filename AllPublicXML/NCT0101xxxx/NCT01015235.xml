  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to determine whether KAI-1678 is effective in the treatment of
      postoperative pain following total hip or total knee replacement.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>December 2008</start_date>
  <completion_date type="Actual">February 2010</completion_date>
  <primary_completion_date type="Actual">February 2010</primary_completion_date>
