  <brief_title>Long-Term Safety Of PF-00547659 In Ulcerative Colitis</brief_title>
  <acronym>TURANDOT II</acronym>
  <official_title>A Multicenter Open-Label Extension Study To Assess Long-Term Safety Of PF-00547659 In Subjects With Ulcerative Colitis (TURANDOT II)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Shire</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Shire</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
    <is_fda_regulated_drug>Yes</is_fda_regulated_drug>
    <is_fda_regulated_device>No</is_fda_regulated_device>
  </oversight_info>
  <brief_summary>
    <textblock>
      Subjects with Ulcerative Colitis who have completed an induction study with PF-00547659 will
      receive an additional 144 weeks of open-label treatment to evaluate the long-term safety of
      the drug.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date type="Actual">March 1, 2013</start_date>
  <completion_date type="Actual">December 13, 2017</completion_date>
  <primary_completion_date type="Actual">December 13, 2017</primary_completion_date>
  <phase>Phase 2</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Frequency of on-treatment Adverse Events (AEs), AEs leading to withdrawal, and Severe Adverse Events (SAEs).</measure>
    <time_frame>Week 144</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Frequency of the development of anti-drug antibodies (ADAs) and neutralizing antibodies (NAbs).</measure>
    <time_frame>Week 72</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Serum trough concentrations of PF 00547659.</measure>
    <time_frame>Weeks 4, 8, 12, 16, 20, 24, 28, 32, 36, 40, 44, 48, 52, 56, 60, 64, 68, 72</time_frame>
  </secondary_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment type="Actual">270</enrollment>
  <condition>Ulcerative Colitis</condition>
  <arm_group>
    <arm_group_label>Open-Label Treatment Period 1A</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>75mg PF-00547659 subcutaneously every 4 weeks for 72 weeks</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Open-Label Treatment Period 1B</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>225mg PF-00547659 subcutaneously every 4 weeks for 72 weeks (Weeks 0-72)</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Open-Label Treatment Period 2</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>75mg PF-00547659 subcutaneously every 4 weeks for 18 months (Weeks 76-144)</description>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>75mg PF-00547659</intervention_name>
    <description>75 mg sterile liquid injected subcutaneously once monthly for 72 weeks in treatment period 1.</description>
    <arm_group_label>Open-Label Treatment Period 1A</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>225mg PF-00547659</intervention_name>
    <description>225 mg sterile liquid injected subcutaneously once monthly for 72 weeks in treatment period 1.</description>
    <arm_group_label>Open-Label Treatment Period 1B</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>75mg PF-00547659</intervention_name>
    <description>75 mg sterile liquid injected subcutaneously once monthly for 72 weeks in treatment period 2.</description>
    <arm_group_label>Open-Label Treatment Period 2</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Subjects between 18 and 66 years of age.

          -  Subjects previously enrolled in study A7281009 (NCT01620255) who have completed the 84
             day (12 week) induction period.

        Exclusion Criteria:

          -  Subjects that have completed Day 84 (Week 12) of study A7281009 but have experienced
             serious event(s) related to the investigational product, an unstable medical
             condition, or any other reason, in the opinion of the investigator, would preclude
             entry or participation in this study.

          -  Subjects who are taking any dose of Azathioprine (AZA), 6-mercaptopurine(6 MP) or
             Methotrexate (MTX).
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>66 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
