    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Clinical pregnancy rate</measure>
    <time_frame>4 weeks after the embryo transfer</time_frame>
    <description>Number of clinical pregnancies (/No. of embryo transfers)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Implantation rate</measure>
    <time_frame>4 weeks after embryo transfer</time_frame>
    <description>Number of embryos implanted (/number of embryos transferred)</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Fertilization rate</measure>
    <time_frame>18 hours after sperm injection</time_frame>
    <description>Number of oocytes fertilized. (only in intracytoplasmic sperm injection cycles)</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Embryo quality</measure>
    <time_frame>36 and 72 hours after fertilization</time_frame>
    <description>Number and quality of blastomeres, amount of fragmentation embryo grade</description>
  </secondary_outcome>
  <other_outcome>
    <measure>Time needed for evaluation of zygotes and embryos</measure>
    <time_frame>18, 24, 36 and 72 hours after fertilization</time_frame>
  </other_outcome>
