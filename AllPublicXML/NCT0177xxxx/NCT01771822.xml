    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Assessment of local skin irritation at patch sites</measure>
    <time_frame>Daily for 21 days, Days 2 through 22</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number of Participants With Adverse Events (AEs) and Serious Adverse Events (SAEs)</measure>
    <time_frame>Days 1 through 22, + 28 days after last product administration</time_frame>
  </secondary_outcome>
