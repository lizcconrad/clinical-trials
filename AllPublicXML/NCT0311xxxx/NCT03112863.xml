  <brief_title>Comparison of the Cosmetic Effects of Bakuchiol and Retinol</brief_title>
  <official_title>Comparison of the Cosmetic Effects of Bakuchiol and Retinol</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of California, Davis</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
