  </study_design_info>
  <primary_outcome>
    <measure>Change in wrinkle appearance</measure>
    <time_frame>12 week</time_frame>
    <description>Assessed using image analysis based assessment of wrinkle severity (depth and length)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Change in appearance of skin pigmentation</measure>
    <time_frame>12 week</time_frame>
    <description>Assessed using image analysis based assessment of facial pigment</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Subjective Tolerability Assessment</measure>
    <time_frame>Assessed at Week 4, 8, and 12.</time_frame>
    <description>Subjective tolerability assessment of stinging, burning, itching</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Facial Erythema Assessment</measure>
    <time_frame>week 4, week 8 and week 12</time_frame>
    <description>Image analysis based assessment of facial erythema</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in appearance of skin pigmentation</measure>
    <time_frame>week 4 and week 8</time_frame>
    <description>Assessed using image analysis based assessment of facial pigment</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in wrinkle appearance</measure>
    <time_frame>week 4 and week 8</time_frame>
    <description>Assessed using image analysis based assessment of wrinkle severity (depth and length)</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Clinical assessment of facial redness</measure>
    <time_frame>week 0, week 4, week 8, and week 12</time_frame>
    <description>dermatologist based assessment of redness at each visit based on Likert scale</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Clinical assessment of facial scaling</measure>
    <time_frame>week 0, week 4, week 8, and week 12</time_frame>
    <description>dermatologist based assessment of scaling at each visit based on Likert scale</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Clinical assessment of facial hyperpigmentation</measure>
    <time_frame>week 0, week 4, week 8, and week 12</time_frame>
    <description>dermatologist based assessment of facial hyperpigmentation at each visit based on Likert scale</description>
  </secondary_outcome>
