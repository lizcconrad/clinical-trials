  <arm_group>
    <arm_group_label>Group I</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>this group of patients will receive their standard insulin treatment in addition to 100 mg of coenzyme Q10 soft gelatin capsule once daily for three month</description>
  </arm_group>
  <arm_group>
