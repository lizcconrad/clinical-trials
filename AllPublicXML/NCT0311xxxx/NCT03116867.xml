    <observational_model>Cohort</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>The presence of fluid in douglas pouch detected by transvaginal ultrasound after saline infusion through the cervix</measure>
    <time_frame>immediate</time_frame>
    <description>after saline infusion is done, presence of fluid in douglas pouch means that hysteroscopic occlusion of tubes was not successful.</description>
  </primary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Anticipated">56</enrollment>
  <condition>Fallopian Tube Occlusion</condition>
  <arm_group>
    <arm_group_label>patients receiving hysteroscopic tubal occlusion</arm_group_label>
    <description>This group will have sonosalpingography done for them one month after the tubal occlusion.</description>
  </arm_group>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>sonosalpingography</intervention_name>
    <description>transvaginal ultrasound with saline infusion</description>
    <arm_group_label>patients receiving hysteroscopic tubal occlusion</arm_group_label>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        patients requiring tubal occlusion
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Infertile patients with hydrosalpinges requiring in vitro fertilization

          -  irreversible contraception

        Exclusion Criteria:

          -  Acute pelvic inflammatory disease
      </textblock>
    </criteria>
    <gender>Female</gender>
    <minimum_age>20 Years</minimum_age>
    <maximum_age>40 Years</maximum_age>
  </eligibility>
