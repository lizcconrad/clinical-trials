  <brief_title>Assessing Fitting Guides in Alcon Multifocal Contact Lenses</brief_title>
  <official_title>Assessing Fitting Guides in Alcon Multifocal Contact Lenses</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alcon Research</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
