  <brief_title>Diagnosing Adverse Drug Reactions Registry</brief_title>
  <acronym>DART</acronym>
  <official_title>DART Registry: Diagnosing Adverse Drug Reactions Registry</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Renaissance RX</agency>
      <agency_class>Industry</agency_class>
