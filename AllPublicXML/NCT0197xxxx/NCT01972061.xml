  <brief_title>Foot/Hand Neuromodulation for Overactive Bladder (OAB)</brief_title>
  <acronym>FootStim</acronym>
  <official_title>Foot/Hand Neuromodulation for Overactive Bladder (OAB)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Pittsburgh</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
