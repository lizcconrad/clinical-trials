  <brief_title>The Effect Evaluation of Laser Acupuncture in Obesity</brief_title>
  <official_title>The Effect Evaluation of Laser Acupuncture in Obesity</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Kaohsiung Medical University Chung-Ho Memorial Hospital</agency>
