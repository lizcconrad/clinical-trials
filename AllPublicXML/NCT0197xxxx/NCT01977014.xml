  <brief_title>NIMS- New Implantable Pump in Pulmonary Arterial Hypertension, a Safety Follow up</brief_title>
  <official_title>NIMS- New Implantable Pump in Pulmonary Arterial Hypertension, a Safety Follow up</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>NordicInfu Care AB</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
