  <condition>Pulmonary Hypertension Group 1 and 4 According to Nice Classification</condition>
  <arm_group>
    <arm_group_label>Patient with LenusPro pump</arm_group_label>
  </arm_group>
  <eligibility>
    <study_pop>
      <textblock>
        Patients in Scandinavia who received a LenusPro pump
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  The patient should have been diagnosed with Pulmonary Arterial Hypertension

          -  The patient has received or will receive treatment with the implanted pump LenusPro

        Exclusion Criteria:

          -  The patient is participating in another study which means treatment of the patient no
             longer follows the normal clinical practice
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>N/A</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
