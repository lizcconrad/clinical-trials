    <measure>Number of dominant follicles with a mean diameter of ≥ 18mm</measure>
    <time_frame>From Day 10 of the treatment period up to approximately 21 full treatment days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Maximum progesterone levels</measure>
    <time_frame>Treatment Days 19, 21, 23, 25, and 27</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Mean progesterone levels</measure>
    <time_frame>Treatment Days 19, 21, 23, 25, and 27</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change from baseline in follicle-stimulating hormone (FSH) and LH</measure>
    <time_frame>Treatment Days 1 and 10</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Mean serum FSH and LH levels</measure>
    <time_frame>Treatment Days 1 and 10</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Estradiol serum levels</measure>
    <time_frame>At treatment Day 1 and Day 10</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Proportion of subjects with at least 1 post-baseline progesterone level ≥ 10 ng/mL</measure>
