  <brief_title>PregText: Feasibility of Monitoring Influenza Vaccine Safety in Pregnant Women Using Text Messaging</brief_title>
  <official_title>PregText: Assessing the Feasibility of Monitoring Influenza Vaccine Safety in Pregnant Women Using Text Messaging</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Columbia University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
