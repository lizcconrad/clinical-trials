  <brief_title>Greek NIS Spiolto ELLACTO</brief_title>
  <official_title>Changes in Health and Functional Status in Patients With Chronic Obstructive Pulmonary Disease (COPD) During Therapy With Spiolto® Respimat® [ELLACTO]</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Boehringer Ingelheim</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Boehringer Ingelheim</source>
  <brief_summary>
    <textblock>
      Open-label observational study: including COPD patients receiving treatment with Spiolto®
      Respimat® for approximately 6 weeks, which is the average time between two medical
      consultations
    </textblock>
  </brief_summary>
  <overall_status>Recruiting</overall_status>
  <start_date type="Actual">March 19, 2018</start_date>
  <completion_date type="Anticipated">December 31, 2018</completion_date>
  <primary_completion_date type="Anticipated">September 21, 2018</primary_completion_date>
  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Cohort</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>the proportion of patients achieving the &quot;therapeutic success&quot; defined as a ≥ 0.4 point of decrease in the Clinical COPD Questionnaire (CCQ) score</measure>
    <time_frame>upto week 6</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>absolute change in Clinical COPD using COPD questionaire</measure>
    <time_frame>upto 6 weeks</time_frame>
    <description>Clinical COPD questionaire</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>absolute change in CCQ-4 (subdomain 4) using COPD questionaire</measure>
    <time_frame>upto 6 weeks</time_frame>
    <description>subdomain 4</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>patient's general condition (physician's evaluation) at baseline and end of study</measure>
    <time_frame>1 week- 6 weeks</time_frame>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Anticipated">1300</enrollment>
