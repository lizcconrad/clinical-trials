  <brief_title>Therapy Coil Electrogram Collection Study</brief_title>
  <acronym>TEC</acronym>
  <official_title>Therapy Coil Electrogram Collection Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Medtronic BRC</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
