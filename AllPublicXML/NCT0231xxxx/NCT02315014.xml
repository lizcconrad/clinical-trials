  <primary_outcome>
    <measure>muscle protein synthesis</measure>
    <time_frame>up to 4 weeks</time_frame>
    <description>protein synthesis is evaluated in muscle biopsy tissue</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>body composition is evaluated using DEXA</measure>
    <time_frame>up to 4 weeks</time_frame>
  </secondary_outcome>
