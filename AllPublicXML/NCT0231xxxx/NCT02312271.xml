  <brief_title>Enteral Formula Tolerance of Standard Tube Feedings</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Nestlé</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Nestlé</source>
  <brief_summary>
    <textblock>
      This prospective observational study seeks to assess ability to achieve enteral feeding goals
      with standard tube feeding formulas.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>December 2014</start_date>
  <completion_date type="Actual">April 2017</completion_date>
  <primary_completion_date type="Actual">March 2017</primary_completion_date>
