    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Daily percentage of caloric nutritional goal met</measure>
    <time_frame>21 days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Daily percentage of protein goal met</measure>
    <time_frame>21 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
