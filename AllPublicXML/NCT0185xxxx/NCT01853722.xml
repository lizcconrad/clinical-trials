  <primary_outcome>
    <measure>Change from baseline in periocular region bacterial load</measure>
    <time_frame>Baseline to 10 min post dose</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Proportion of periocular regions with a reduction from baseline in bacterial load.</measure>
    <time_frame>Baseline to 10 min post dose</time_frame>
  </secondary_outcome>
  <other_outcome>
    <measure>Adverse Events (reported, elicited, observed)</measure>
    <time_frame>Post dose and up to 2 weeks after (Day 1-14)</time_frame>
  </other_outcome>
