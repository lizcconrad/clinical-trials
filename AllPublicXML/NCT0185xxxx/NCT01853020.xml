  <primary_outcome>
    <measure>Cerebellum Dependent Associative Learning (% Conditioned Responses)</measure>
    <time_frame>+15 minutes after infusion</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Grooved Pegboard Motor Task (Reaction Time/Accuracy)</measure>
    <time_frame>+55 minutes after infusion</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>CANTAB Motor Screening Test (Reaction Time/Accuracy)</measure>
    <time_frame>+55 minutes after infusion</time_frame>
  </primary_outcome>
  <secondary_outcome>
