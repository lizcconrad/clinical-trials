  <brief_summary>
    <textblock>
      Compare the fit of two pediatric face masks in children.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>January 2014</start_date>
  <completion_date type="Actual">January 2014</completion_date>
