  <brief_title>DISCOVERing Treatment Reality of Type 2 Diabetes in Real World Setting in Japan</brief_title>
  <acronym>J-DISCOVER</acronym>
  <official_title>J-DISCOVER: DISCOVERing Treatment Reality of Type 2 Diabetes in Real World Setting in Japan</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AstraZeneca</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
