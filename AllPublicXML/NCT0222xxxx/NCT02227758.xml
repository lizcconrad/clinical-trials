  <brief_title>Peripheral Nerve Stimulation Registry for Intractable Migraine Headache</brief_title>
  <acronym>Relief</acronym>
  <official_title>Peripheral Nerve Stimulation Registry for Intractable Migraine Headache</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>St. Jude Medical</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
