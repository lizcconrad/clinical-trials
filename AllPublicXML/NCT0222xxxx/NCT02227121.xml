  <brief_title>Acute Defibrillation Study</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Medtronic Cardiac Rhythm and Heart Failure</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
