  <brief_title>Zirconia Based Fixed Dental Prostheses</brief_title>
  <official_title>Prospective Evaluation of Zirconia Based Tooth- and Implant- Supported Fixed Dental Prostheses</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Technische Universität Dresden</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
