  <primary_outcome>
    <measure>To evaluate the safety and tolerability of ascending single doses of SBI-087 in subjects with rheumatoid arthritis.</measure>
    <time_frame>12 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>To provide the initial pharmacokinetics and pharmacodynamic profile of SBI-087 in subjects with rheumatoid arthritis.</measure>
    <time_frame>12 months</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">62</enrollment>
