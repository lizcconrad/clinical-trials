  <brief_title>Project Sexual Awareness for Everyone (SAFE)</brief_title>
  <acronym>SAFE</acronym>
  <official_title>Preventing Recurrent Sexually Transmitted Infections in Minority Women</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>The University of Texas Health Science Center at San Antonio</agency>
      <agency_class>Other</agency_class>
