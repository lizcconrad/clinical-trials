  <brief_title>A Study To Evaluate The Safety And Efficacy Of Atorvastatin In Patients With Diabetes And High Cholesterol</brief_title>
  <official_title>A Randomized, 12-Week, Open Labeled Study To Evaluate The Efficacy And Safety Of Once Daily Atorvastatin In Diabetes Mellitus Type 2 With Hyperlipidemia</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Pfizer</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Pfizer</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to evaluate the efficacy and safety of atorvastatin for the
      treatment of Taiwanese patients with diabetes and high cholesterol.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>December 2003</start_date>
  <completion_date type="Actual">October 2004</completion_date>
  <phase>Phase 4</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Percentage change in LDL-C levels from baseline to Week 12</measure>
    <time_frame>Week 12</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Percentage of subjects with low-density lipoprotein cholesterol (LDL-C) levels of &lt;100 mg/dL (LDL-C responders) at Week 12</measure>
    <time_frame>Week 12</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Percentage change from baseline in TC, high-density lipoprotein cholesterol, triglycerides, and high sensitivity C-reactive protein at Weeks 4, 8, and 12</measure>
    <time_frame>Weeks 4, 8, and 12</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Adverse events at Weeks 4, 8, and 12</measure>
    <time_frame>Baseline and at Weeks 4, 8, and 12</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Clinical laboratory changes at Weeks 4, 8, and 12</measure>
    <time_frame>Screening and at Weeks 4, 8, and 12</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Vital signs at Weeks 4, 8, and 12</measure>
    <time_frame>Screening and at Weeks 4, 8, and 12</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Percentage change in LDL-C levels from baseline at Weeks 4 and 8</measure>
    <time_frame>Weeks 4 and 8</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Percentage of LDL-C responders at Weeks 4 and 8</measure>
    <time_frame>Weeks 4 and 8</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Percentage of subjects with total cholesterol (TC) levels of &lt;160 mg/dL (TC responders) at Weeks 4, 8, and 12</measure>
    <time_frame>Weeks 4, 8, and 12</time_frame>
  </secondary_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment type="Actual">155</enrollment>
  <condition>Hyperlipidemias</condition>
