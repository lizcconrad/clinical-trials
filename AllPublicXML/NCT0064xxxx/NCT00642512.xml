  <primary_outcome>
    <measure>Total response of nausea and vomiting/retching was defined as no vomiting and/or retching, an intensity of nausea of &lt; 5 mm on the visual analog scale (VAS) and no use of rescue medication.</measure>
    <time_frame>5 days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Complete responder rate (no vomiting/retching, intensity of nausea of ≤ 30 mm on the VAS and no use of rescue medication)</measure>
    <time_frame>5 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Presence or absence of nausea</measure>
    <time_frame>5 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Episodes of vomiting and/or retching</measure>
    <time_frame>5 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Duration of nausea and vomiting and/or retching</measure>
    <time_frame>5 days</time_frame>
  </secondary_outcome>
