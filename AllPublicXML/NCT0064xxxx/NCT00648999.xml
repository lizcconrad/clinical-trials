  <brief_title>Safety and Efficacy of Kaletra in ARV Therapy Experienced Patients</brief_title>
  <official_title>Phase IV, Non Randomized Study in ARV Experienced Patients Under Switch Therapy With Kaletra</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AbbVie (prior sponsor, Abbott)</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
