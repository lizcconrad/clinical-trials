    <primary_purpose>Treatment</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Number of oocytes retrieved</measure>
  </primary_outcome>
  <primary_outcome>
    <measure>Clinical pregnancy rate per embryo transfer</measure>
  </primary_outcome>
  <secondary_outcome>
    <measure>Live birth rate per cycle</measure>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">201</enrollment>
  <condition>Infertility</condition>
