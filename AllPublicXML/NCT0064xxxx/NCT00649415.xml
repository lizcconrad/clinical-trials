  </secondary_outcome>
  <secondary_outcome>
    <measure>Subjects' global evaluation of the study drug</measure>
    <time_frame>8 hours and 72 hours</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Summed pain intensity difference</measure>
    <time_frame>8 hours</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Percent of subjects who took rescue medication</measure>
    <time_frame>72 hours</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Percent of subjects who took the second dose of study medication</measure>
    <time_frame>72 hours</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">154</enrollment>
  <condition>Dysmenorrhea</condition>
