    <arm_group_label>1</arm_group_label>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        Patients with diabetes and/or complex coronary artery disease who are treated with a
        Multi-Link Vision™ Stent.
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Women and Men Age &gt;18 years, Intention to treat all stenosis with Multi-Link VisionTM
             Stents, documented patient consent

        Exclusion Criteria:

          -  No Multi-Link Vision™ Stent implanted, life expectation &gt;9 M, cardiogenic shock,
             STEMI, NSTEMI or unstable angina pectoris with pathologic markers within the last 48
             hours, intolerance of Clopidogrel/ Tiklopidin
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
