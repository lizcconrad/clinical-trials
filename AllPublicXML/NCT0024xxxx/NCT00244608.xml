    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Double</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Change in eosinophils in bronchial biopsies (first and last visit) and in sputum (5 times during the 12-month treatment period)</measure>
  </primary_outcome>
  <secondary_outcome>
    <measure>Immunopathology and remodeling in biopsies</measure>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Immunopathology and mediators in induced sputum</measure>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Severe asthma exacerbations (number of and time to first)</measure>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Forced expiratory volume in 1 second (FEV1)</measure>
  </secondary_outcome>
  <secondary_outcome>
    <measure>As-needed use</measure>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Safety variables, including adverse events and vital signs</measure>
  </secondary_outcome>
  <secondary_outcome>
    <measure>All variables assessed over the 12-month treatment period</measure>
  </secondary_outcome>
  <enrollment>100</enrollment>
