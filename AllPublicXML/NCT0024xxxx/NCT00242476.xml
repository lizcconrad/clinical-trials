  <primary_outcome>
    <measure>Mediolateral body sway at 16 weeks.</measure>
    <time_frame>at 16 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Functional status assessed using the Short Physical Performance Battery Test determined at 16 weeks. General safety and tolerability at 16 weeks.</measure>
    <time_frame>at 16 weeks</time_frame>
  </secondary_outcome>
