  <brief_title>Enbrel Liquid Immunogenicity Protocol</brief_title>
  <official_title>Open-label Study to Assess the Antibody Response to Etanercept Liquid in Subjects With Rheumatoid Arthritis (RA)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Amgen</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>Immunex Corporation</agency>
