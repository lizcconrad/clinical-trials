    <observational_model>Cohort</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>AKI-stadium 1 (KDIGO)</measure>
    <time_frame>30 days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>all cause mortality</measure>
    <time_frame>30 days</time_frame>
  </secondary_outcome>
  <other_outcome>
    <measure>AKI Stadium 2-3 (KDIGO)</measure>
    <time_frame>30 days</time_frame>
  </other_outcome>
  <enrollment type="Actual">110</enrollment>
  <condition>Acute Kidney Injury</condition>
  <eligibility>
    <study_pop>
      <textblock>
        adults with planned cardiac surgery and cardiopulmonary bypass
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  planned cardiac surgery

        Exclusion Criteria:

          -  renal dialysis

          -  minors

          -  emergency

          -  no cardiopulmonary bypass
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
