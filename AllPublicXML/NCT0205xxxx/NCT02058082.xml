    <arm_group_type>Active Comparator</arm_group_type>
    <description>Intracytoplasmic sperm injection (ICSI) cycles using testicular extracted sperm</description>
  </arm_group>
  <intervention>
    <intervention_type>Procedure</intervention_type>
    <intervention_name>Intracytoplasmic sperm injection</intervention_name>
    <arm_group_label>ejaculated sperm</arm_group_label>
