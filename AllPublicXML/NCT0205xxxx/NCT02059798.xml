  <brief_title>Urinary and Sexual Function Before and After Surgical Decompression of Cervical Myelopathy</brief_title>
  <official_title>Urinary and Sexual Function Before and After Surgical Decompression of Cervical Myelopathy</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>The Catholic University of Korea</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
