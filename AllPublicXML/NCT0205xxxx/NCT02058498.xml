  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      Will adult liver transplant patients who are provided with physical activity (walking)
      instructions increase their physical activity and perceive an improved quality of life?
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>February 2014</start_date>
  <completion_date type="Actual">December 2014</completion_date>
  <primary_completion_date type="Actual">December 2014</primary_completion_date>
  <phase>N/A</phase>
