  <brief_title>Increasing Activity in Liver Transplant Patients</brief_title>
  <official_title>Increasing Physical Activity in Post Liver Transplant Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mayo Clinic</agency>
