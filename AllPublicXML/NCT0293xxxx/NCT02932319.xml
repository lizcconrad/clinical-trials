  <primary_outcome>
    <measure>Bishop score</measure>
    <time_frame>at term birth +5 days</time_frame>
    <description>Measure the evolution of Bishop score,change between the randomization and the return of the patient</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Labour ward</measure>
    <time_frame>at term birth +5 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Total dose of prostaglandin used in case of cervical ripening.</measure>
    <time_frame>During labor</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Utilization rate of oxytocin</measure>
    <time_frame>During labor</time_frame>
  </secondary_outcome>
