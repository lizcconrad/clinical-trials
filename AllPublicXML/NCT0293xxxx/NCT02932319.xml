    <measure>number of birth</measure>
    <time_frame>between the term consultation + 4 days (randomization) and delivery time</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>delivery rate</measure>
    <time_frame>within 24 hours and 48 hours after randomization</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Caesarean rate</measure>
    <time_frame>before labor and during labor</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Maternal complications</measure>
    <time_frame>through study completion, at 4 day</time_frame>
