  </secondary_outcome>
  <secondary_outcome>
    <measure>Blood pressure</measure>
    <time_frame>Up to 6 weeks</time_frame>
    <description>Diastolic and systolic</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Lung function</measure>
    <time_frame>Up to 6 weeks</time_frame>
