  </arm_group>
  <arm_group>
    <arm_group_label>ICSI</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Oocytes fertilized by intracytoplasmic sperm injection (ICSI) method</description>
  </arm_group>
  <intervention>
    <intervention_type>Procedure</intervention_type>
    <intervention_name>ICSI</intervention_name>
