    <masking>Double (Participant, Outcomes Assessor)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>the Penile Circumference difference at 24 weeks from baseline</measure>
    <time_frame>24 weeks</time_frame>
    <description>Penile Circumference: The average of Distal-, Mid-, and Proximal-penis measurements when relaxed by tape measurement</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">76</enrollment>
