  <study_design_info>
    <observational_model>Cohort</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>the number of participants who will deliver by NVD without instrumental intervention</measure>
    <time_frame>within 36 hours</time_frame>
    <description>the number of pregnant ladies in first stage of labour who will deliver by NVD and who will have normal progress or prolonged progress of labour</description>
  </primary_outcome>
  <enrollment type="Anticipated">300</enrollment>
  <condition>Labour</condition>
  <intervention>
    <intervention_type>Diagnostic Test</intervention_type>
    <intervention_name>TP us in labour</intervention_name>
    <description>TP US will be made to pregnant lady in first stage of labour with OS and PC angel measurements</description>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        - primigravida who is pregnant full term with no medical disorders
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  primigravida

          -  pregnant full term

          -  no medical disorders

        Exclusion Criteria:

          -  multigravida

          -  any medical disorders

          -  preterm labour
      </textblock>
    </criteria>
    <gender>Female</gender>
    <gender_based>Yes</gender_based>
    <gender_description>pregnant ladies ful term</gender_description>
    <minimum_age>18 Years</minimum_age>
