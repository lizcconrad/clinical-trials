  <brief_summary>
    <textblock>
      Clinical Evaluation of Blood Glucose Monitoring Systems (BGMSs)
    </textblock>
  </brief_summary>
  <detailed_description>
    <textblock>
      User performance, system use, system accuracy, user instruction for use and marketing claims
      evaluation of Blood Glucose Monitoring Systems.
    </textblock>
  </detailed_description>
  <overall_status>Recruiting</overall_status>
  <start_date type="Actual">April 9, 2018</start_date>
