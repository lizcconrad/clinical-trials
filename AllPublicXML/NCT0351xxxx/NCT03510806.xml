  <brief_title>Nutritional Supplements for Exercise Recovery</brief_title>
  <official_title>Evaluation of Nutritional Supplements for Exercise Recovery</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Catherine Mikus</agency>
      <agency_class>Industry</agency_class>
