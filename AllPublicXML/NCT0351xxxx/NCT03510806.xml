    <masking>Triple (Participant, Investigator, Outcomes Assessor)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>post-exercise muscle soreness</measure>
    <time_frame>7 days</time_frame>
    <description>VAS score</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>post-exercise muscle strength</measure>
    <time_frame>7 days</time_frame>
    <description>countermovement jump performance</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>post-exercise muscle function</measure>
    <time_frame>7 days</time_frame>
    <description>repetitive countermovement jumps</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>satiety</measure>
    <time_frame>4-5 hours</time_frame>
    <description>VAS scores</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>sleep quality</measure>
    <time_frame>15 days</time_frame>
    <description>sleep metrics, Fitbit</description>
  </secondary_outcome>
