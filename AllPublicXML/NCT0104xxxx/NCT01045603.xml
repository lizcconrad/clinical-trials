  <primary_outcome>
    <measure>Success of titration (Y/N), defined as reaching a maintenance dose</measure>
    <time_frame>At week 4 and month 3 after baseline</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Dose level of Instanyl® after titration, defined as the maintenance dose</measure>
    <time_frame>At week 4 and month 3 after baseline</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Adverse Drug Reactions (ADR)</measure>
    <time_frame>At week 4 and month 3 after baseline</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Reason and time for Instanyl® termination</measure>
