  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to determine if chewing gum increases energy expenditure and
      decreases food intake.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>September 2008</start_date>
  <completion_date type="Actual">January 2011</completion_date>
