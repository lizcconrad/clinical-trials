  <brief_title>Aortic Valve Replacement With Trifecta(TM)</brief_title>
  <official_title>An Observational, Prospective Evaluation of the Trifecta Valve</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>St. Jude Medical</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
