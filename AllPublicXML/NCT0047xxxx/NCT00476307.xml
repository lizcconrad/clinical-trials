  <primary_outcome>
    <measure>Immune response 21 days post vaccination</measure>
  </primary_outcome>
  <secondary_outcome>
    <measure>Safety: solicited local and general symptoms, unsolicited symptoms, serious adverse events</measure>
  </secondary_outcome>
  <enrollment type="Anticipated">120</enrollment>
