  <brief_title>Methylphenidate-Duloxetine Combinations for Cocaine Dependence</brief_title>
  <official_title>Randomized Clinical Trial: 03 (Methylphenidate and Duloxetine)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Kentucky</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>National Institute on Drug Abuse (NIDA)</agency>
      <agency_class>NIH</agency_class>
