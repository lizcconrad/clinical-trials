    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Peak Plasma Concentration (Cmax)</measure>
    <time_frame>up to 72 hours post dose</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Area under the plasma concentration versus time curve (AUC)</measure>
    <time_frame>up to 72 hours post dose</time_frame>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">40</enrollment>
  <condition>Healthy</condition>
