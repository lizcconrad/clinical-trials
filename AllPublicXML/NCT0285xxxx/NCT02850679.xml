  <brief_title>FINnish CardioVersion Study II</brief_title>
  <acronym>FinCV2</acronym>
  <official_title>Finnish Elective Cardioversion for Persistent Atrial Fibrillation Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Turku</agency>
      <agency_class>Other</agency_class>
