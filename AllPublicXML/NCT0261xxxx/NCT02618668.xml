  <condition>Cholangitis</condition>
  <arm_group>
    <arm_group_label>Ketamine / Propofol Admixture</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Ketamine / Propofol Admixture: I.V propofol-ketamine 3:1 mixture(%1 15 ml propofol + 1 ml 50mg/ml ketamine+ 4 ml saline in a 20-ml syringe which resulted in 0.25 mg.ml-1 ketamine and 0.75 mg.ml-1 propofol.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Propofol</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>I.V propofol 2 mg/kg</description>
  </arm_group>
