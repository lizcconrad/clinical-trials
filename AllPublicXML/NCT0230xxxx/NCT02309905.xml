    <observational_model>Case Control</observational_model>
    <time_perspective>Retrospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Proportion of a dermatological expertise among those required</measure>
    <time_frame>1 month after the end of the study</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number of hospitalizations for a dermatological reason</measure>
    <time_frame>1 month after the end of the study</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Overall costs</measure>
    <time_frame>6 months after the end of the study</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Time to a dermatological expertise</measure>
    <time_frame>1 month after the end of the study</time_frame>
  </secondary_outcome>
  <secondary_outcome>
