  </study_design_info>
  <primary_outcome>
    <measure>Proportion of patients experiencing adverse events</measure>
    <time_frame>from Week 0 to Week 192</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change in total volume of T2 lesions</measure>
    <time_frame>from Week 0 to Week 84</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in number of T1 hypointense lesions</measure>
    <time_frame>from Week 0 to Week 84</time_frame>
  </secondary_outcome>
