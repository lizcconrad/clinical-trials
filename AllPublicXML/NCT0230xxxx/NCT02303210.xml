  <brief_title>Evaluation of Frequency Lowering Technique</brief_title>
  <official_title>Evaluation of Frequency Lowering Technique</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Phonak AG, Switzerland</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
