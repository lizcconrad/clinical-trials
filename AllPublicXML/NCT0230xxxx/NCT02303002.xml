  <primary_outcome>
    <measure>Proportion of subjects assessed as responders by investigator assessment of severity of glabellar lines at maximum frown</measure>
    <time_frame>Week 24</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Duration of response assessed by investigator assessment of severity of glabellar lines at maximum frown from the date of injection to when subject reverts to baseline severity</measure>
    <time_frame>Up to Week 36</time_frame>
  </primary_outcome>
