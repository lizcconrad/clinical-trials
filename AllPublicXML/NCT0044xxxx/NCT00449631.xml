  <brief_title>Multidrug Resistance After Trabeculectomy With Mitomycin C</brief_title>
  <official_title>Multidrug Resistance After Trabeculectomy With Mitomycin C</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Cologne</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
