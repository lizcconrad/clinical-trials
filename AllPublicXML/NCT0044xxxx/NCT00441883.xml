  <brief_title>Dose-Finding Study Comparing the Safety and Efficacy of Latanoprost to a Novel Treatment for Glaucoma</brief_title>
  <official_title>A Phase 2, 28 Day Parallel-Group, Double-Masked, Dose-Finding Study Comparing the Safety and Efficacy of PF-03187207 to Latanoprost</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Bausch &amp; Lomb Incorporated</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>NicOx Inc.</agency>
      <agency_class>Industry</agency_class>
    </collaborator>
  </sponsors>
  <source>Bausch &amp; Lomb Incorporated</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      This study will evaluate the safety and efficacy of PF 03187207.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>March 2007</start_date>
  <completion_date type="Actual">July 2008</completion_date>
  <primary_completion_date type="Actual">February 2008</primary_completion_date>
  <phase>Phase 2</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Triple (Participant, Care Provider, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Change in diurnal intraocular pressure (IOP) at day 28</measure>
    <time_frame>28 days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change in resting pulse and blood pressure, and clinical laboratories throughout the study period</measure>
    <time_frame>28 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Determine the impact of dose timing</measure>
    <time_frame>28 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change from baseline IOP at the Day 7, 14, 21, and 28 visits</measure>
    <time_frame>28 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Proportion of patients at target IOP across all study visits</measure>
    <time_frame>28 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in safety assessments, ocular and systemic adverse events throughout study period</measure>
    <time_frame>28 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in refraction, visual acuity, visual fields throughout study period</measure>
    <time_frame>28 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in pupil diameter, biomicroscopic examination throughout study period</measure>
    <time_frame>28 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in ophthalmoscopy, gonioscopy, pachymetry throughout study period</measure>
    <time_frame>28 days</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">242</enrollment>
  <condition>Primary Open Angle Glaucoma</condition>
  <condition>Ocular Hypertension</condition>
