  <brief_title>Darbepoetin Alpha in Myelodysplastic Syndromes (MDS)</brief_title>
  <official_title>A Phase II Study of Darbepoetin Alpha in Myelodysplastic Syndromes (MSD)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Groupe Francophone des Myelodysplasies</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
