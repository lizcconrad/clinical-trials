  <arm_group>
    <arm_group_label>Control</arm_group_label>
    <arm_group_type>No Intervention</arm_group_type>
    <description>Diet consists of 15% dairy protein and low Glycemic Index (GI &lt; 40), 55 En% carbohydrates and 30 En% fat</description>
  </arm_group>
  <arm_group>
    <arm_group_label>High dairy protein</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Diet consists of 25% dairy protein and low GI (GI &lt; 40), 45 En% carbohydrates and 30 En% fat</description>
  </arm_group>
  <arm_group>
    <arm_group_label>vegetable protein</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Diet consists of 15 En% vegetable protein and low GI (GI &lt; 40), 55 En% carbohydrates and 30 En% fat</description>
  </arm_group>
  <arm_group>
    <arm_group_label>High GI</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Diet consists of 15 En% dairy protein and high GI(GI &gt; 60, 55 En% carbohydrates and 30 En% fat</description>
  </arm_group>
  <intervention>
    <intervention_type>Dietary Supplement</intervention_type>
    <intervention_name>Control</intervention_name>
