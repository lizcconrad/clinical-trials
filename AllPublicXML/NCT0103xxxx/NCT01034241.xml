    <arm_group_label>vegetable protein</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Dietary Supplement</intervention_type>
    <intervention_name>High GI</intervention_name>
    <description>Diet consists of 15 En% dairy protein and high GI, 55 En% carbohydrates and 30 En% fat</description>
