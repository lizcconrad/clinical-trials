  <primary_outcome>
    <measure>Glucose and insulin metabolism (24 h)</measure>
    <time_frame>24 h</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Substrate partitioning and metabolic markers</measure>
    <time_frame>24 h</time_frame>
  </secondary_outcome>
