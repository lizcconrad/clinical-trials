  <brief_title>An Open-Label, Long-Term Study With AL-4943A Ophthalmic Solution, 0.2% in Patients With Allergic Conjunctivitis</brief_title>
  <official_title>An Open-Label, Long-Term Study With AL-4943A Ophthalmic Solution, 0.2% in Patients With Allergic Conjunctivitis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alcon Research</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
