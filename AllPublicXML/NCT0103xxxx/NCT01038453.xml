  <primary_outcome>
    <measure>Birth Weight</measure>
    <time_frame>0-24 hours after delivery of the child</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Post-partum effects of vitamin D supplement on maternal bone mineral density (BMD).</measure>
    <time_frame>Day 1 to 4 months after delivery.</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Infections of the newborn</measure>
