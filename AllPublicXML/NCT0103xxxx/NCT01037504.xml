  <primary_outcome>
    <measure>Safety variables: adverse events</measure>
    <time_frame>Assessment taken at screening visit, defined timepoints pre-dose and post-dose during treatment periods and follow-up visit. Volunteers will be monitored throughout the study for adverse events.</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetics profile: concentration of AZD5423 in plasma</measure>
    <time_frame>Samples taken at defined timepoints pre-dose and post-dose during treatment periods. Samples taken at approx. 39 timepoints.</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Investigation of PD effects</measure>
    <time_frame>Samples taken at screening visit, defined timepoints pre-dose and post-dose during treatment periods and follow-up.</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
