  <brief_title>Fatty Acid Quality and Overweight (FO-study)</brief_title>
  <official_title>Fatty Acid Quality and Overweight</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Oslo and Akershus University College of Applied Sciences</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Oslo and Akershus University College of Applied Sciences</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to study the health effects of fatty acid quality in healthy,
      overweight individuals.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>February 2009</start_date>
  <primary_completion_date type="Actual">June 2009</primary_completion_date>
  <phase>N/A</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Basic Science</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Will intake of fat as diacylglycerols give weight reduction in overweight individuals?</measure>
    <time_frame>baseline (0 weeks), 12 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Will a high intake of ALA involve reduced levels of inflammatory markers?</measure>
    <time_frame>baseline (0 weeks), 12 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Will a high intake of ALA involve changes in insulin sensitivity?</measure>
    <time_frame>baseline (0 weeks), 12 weeks</time_frame>
  </secondary_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment type="Actual">41</enrollment>
  <condition>Weight Reduction and Insulin Sensitivity</condition>
  <arm_group>
    <arm_group_label>Low intake of ALA and triacylglycerols</arm_group_label>
    <arm_group_type>Placebo Comparator</arm_group_type>
    <description>Sunflower oil</description>
  </arm_group>
  <arm_group>
    <arm_group_label>high intake of ALA and triacylglyceroles</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Canola and linseed oils</description>
  </arm_group>
  <arm_group>
    <arm_group_label>high intake of ALA and diacylglycerols</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Canola and linseed oils</description>
  </arm_group>
  <intervention>
    <intervention_type>Dietary Supplement</intervention_type>
    <intervention_name>fatty acid quality and overweight</intervention_name>
    <description>Duration 12 weeks</description>
    <arm_group_label>Low intake of ALA and triacylglycerols</arm_group_label>
    <arm_group_label>high intake of ALA and triacylglyceroles</arm_group_label>
    <arm_group_label>high intake of ALA and diacylglycerols</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  no use of medication, healthy, 27&lt;BMI&lt;40

        Exclusion Criteria:

          -  pregnant and lactating women, chronic diseases.
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>70 Years</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
  <overall_official>
