    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Prevention</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Incidence of Venous thromboembolism and Major bleeding</measure>
    <time_frame>Throughout entire study</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Incidence of Pulmonary embolism Incidence of Deep vein thrombosis Incidence of symptomatic Venous thromboembolism</measure>
    <time_frame>Throughout entire study</time_frame>
  </secondary_outcome>
  <enrollment type="Actual">127</enrollment>
