  </study_design_info>
  <primary_outcome>
    <measure>Retina vasculature</measure>
    <time_frame>Day 1</time_frame>
  </primary_outcome>
  <number_of_groups>3</number_of_groups>
  <enrollment type="Actual">70</enrollment>
