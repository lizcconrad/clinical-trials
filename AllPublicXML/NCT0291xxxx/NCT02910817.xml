  <primary_outcome>
    <measure>Total number of retrieved oocytes</measure>
    <time_frame>Two weeks Metformin treatment</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Live birth rate</measure>
    <time_frame>End of the pregnancy</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
