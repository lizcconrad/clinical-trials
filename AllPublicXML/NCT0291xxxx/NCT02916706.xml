  <brief_title>Efficacy and Safety of Teneligliptin in Chinese Patients With Type 2 Diabetes Mellitus</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mitsubishi Tanabe Pharma Corporation</agency>
      <agency_class>Industry</agency_class>
