    <time_frame>Day 1 to 168 days after the last dose of study drug</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Proportion of subjects with AEs leading to discontinuation</measure>
    <time_frame>Day 1 to 168 days after the last dose of study drug</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Proportion of Deaths</measure>
    <time_frame>Day 1 to 56 days after the last dose of study drug</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Proportion of subjects with laboratory abnormalities</measure>
    <time_frame>Day 1 to 56 days after the last dose of study drug</time_frame>
  </secondary_outcome>
