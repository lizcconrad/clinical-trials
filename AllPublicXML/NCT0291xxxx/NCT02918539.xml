  <brief_title>Registry of Amyloid Positive Patients for Alzheimer's Disease Drug Research Trials</brief_title>
  <acronym>RAmP</acronym>
  <official_title>Registry of Amyloid Positive Patients for Alzheimer's Disease Drug Research Trials</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Avid Radiopharmaceuticals</agency>
