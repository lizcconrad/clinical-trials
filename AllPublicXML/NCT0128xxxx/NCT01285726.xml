  <brief_title>Initiative for Patient Outcomes in Dialysis - Peritoneal Dialysis (PD) (IPOD-PD Study)</brief_title>
  <acronym>IPOD-PD</acronym>
  <official_title>Initiative for Patient Outcomes in Dialysis - PD (IPOD-PD Study)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Fresenius Medical Care Deutschland GmbH</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
