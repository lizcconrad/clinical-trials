  </arm_group>
  <arm_group>
    <arm_group_label>BI 135585 (R)</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>single dose per subject as tablet formulation after an overnight fast</description>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>BI 135585</intervention_name>
