  <primary_outcome>
    <measure>Assess efficacy of different doses of SYN115 for reducing the mean total hours of awake time per day spent in the off state</measure>
    <time_frame>12 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>To assess the effect of SYN115 on dyskinesia</measure>
    <time_frame>12 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>To assess the effect of SYN115 on UPDRS scores</measure>
    <time_frame>12 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>To assess investigator and patient impressions of PD severity and change</measure>
    <time_frame>12 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>To assess the effect of SYN115 on non motor symptoms of PD</measure>
    <time_frame>12 weeks</time_frame>
  </secondary_outcome>
