    <masking>Triple (Participant, Care Provider, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Recovery of GI Function</measure>
    <time_frame>up to 7 days of dosing or until hospital discharge</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Ancillary GI Functions</measure>
    <time_frame>up to 7 days of dosing or hospital discharge</time_frame>
  </secondary_outcome>
