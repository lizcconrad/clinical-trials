  <brief_title>The Effect of Transcutaneous Electrical Nerve Stimulation (TENS) in Multiple Sclerosis</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Hasselt University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
