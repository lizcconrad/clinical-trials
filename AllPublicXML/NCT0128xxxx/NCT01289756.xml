  <primary_outcome>
    <measure>Area under the plasma concentration versus time curve (AUC)of clomiphene</measure>
    <time_frame>1, 2, 4, 6, 8, 10, 12, 24, 72 and 168 hours after durg application</time_frame>
    <description>AUC of clomiphene and metabolites</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Peak Plasma Concentration (Cmax)of Clomiphene</measure>
    <time_frame>1, 2, 4, 6, 8, 10, 12, 24, 72 and 168 hours after drug application</time_frame>
