  <primary_outcome>
    <measure>To assess safety and tolerability and to identify a dose for Phase II evaluation</measure>
    <time_frame>during the dose escalation phase</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>To evaluate pharmacokinetics</measure>
    <time_frame>throughout the study</time_frame>
  </secondary_outcome>
  <secondary_outcome>
