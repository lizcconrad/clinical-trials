    <arm_group_label>2</arm_group_label>
    <other_name>Arimidex</other_name>
    <other_name>ZD1033</other_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Postmenopausal advanced breast cancer with oestrogen receptor positive

          -  Progression under first-line anti-oestrogen therapy.

        Exclusion Criteria:

          -  Life-threatening metastasis; contraindication to anastrozole

          -  &gt;2 regimens of hormonotherapy for advanced breast cancer
      </textblock>
    </criteria>
    <gender>Female</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
