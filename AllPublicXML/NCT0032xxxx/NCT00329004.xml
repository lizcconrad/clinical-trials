  <primary_outcome>
    <measure>Safety</measure>
    <time_frame>results of weekly vital sign measurements, ECGs, laboratory tests and physical exams, echocardiograms every 8 weeks</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Highest dose tolerated</measure>
    <time_frame>continuous medical review of adverse event reports</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Effective dose or doses of BMS-690514</measure>
    <time_frame>tumor measurement by imaging every eight weeks. Continuous medical review of adverse event reports</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
