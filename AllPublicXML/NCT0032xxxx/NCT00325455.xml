    <intervention_type>Drug</intervention_type>
    <intervention_name>MK0859</intervention_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Eligible patients will be men and women of non-child bearing potential, 18 through 75
             years of age diagnosed with high cholesterol

        Exclusion Criteria:

          -  Patients with CHD or CHD-equivalent disease (except diabetes)

          -  Diabetics on statins
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>75 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
