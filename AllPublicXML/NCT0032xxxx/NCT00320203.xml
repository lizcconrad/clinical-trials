    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Mean change in intraocular pressure at 9 am from baseline</measure>
    <time_frame>Weeks 2 and 6, Months 2, 3, 4, 5, 6, 7.5, 9, 10.5, and 12</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Time to treatment failure</measure>
    <time_frame>At time point</time_frame>
  </secondary_outcome>
