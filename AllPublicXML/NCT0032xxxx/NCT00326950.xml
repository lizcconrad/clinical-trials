  <brief_title>Phase I Clinical Study of E7389</brief_title>
  <official_title>Phase I Clinical Study of E7389 in Patients With Solid Tumors</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Eisai Co., Ltd.</agency>
