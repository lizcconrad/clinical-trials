  <target_duration>1 Day</target_duration>
  <primary_outcome>
    <measure>correlation between maternal anxiety and age according to emergent or elective C/S under general anesthesia.</measure>
    <time_frame>during operation</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>correlation between maternal anxiety and body mass index according to emergent or elective C/S under general anesthesia.</measure>
    <time_frame>during operation</time_frame>
  </primary_outcome>
  <secondary_outcome>
