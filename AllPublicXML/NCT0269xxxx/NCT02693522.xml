  <primary_outcome>
    <measure>Degree of fat mass (FM) reduction</measure>
    <time_frame>baseline and 24 weeks</time_frame>
    <description>statistically difference for change from baseline (kg)</description>
  </primary_outcome>
  <secondary_outcome>
