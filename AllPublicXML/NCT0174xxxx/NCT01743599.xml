  <primary_outcome>
    <measure>Retrograde ejaculation (a minimum of a total of 1 million sperm in postejaculatory urine)</measure>
    <time_frame>The examination program for each patient was finished within one day.</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Erectile dysfunction</measure>
    <time_frame>The examination program (including a questionaire on erectile dysfunction) for each patient was finished within one day.</time_frame>
  </secondary_outcome>
