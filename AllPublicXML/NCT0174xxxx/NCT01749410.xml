  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Case-Only</observational_model>
    <time_perspective>Retrospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Change From Baseline in the Number of Headache Days</measure>
    <time_frame>Treatment Cycle 7 (approximately 1.5 years)</time_frame>
    <description>The mean number of headache days was counted as the number of headache days occurring during the 30 day period ending with treatment cycle 7. Each treatment cycle was administered approximately every 12 weeks.</description>
  </primary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Actual">33</enrollment>
  <condition>Chronic Migraine, Headaches</condition>
