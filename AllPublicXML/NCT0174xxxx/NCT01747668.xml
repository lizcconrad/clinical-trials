  <brief_title>Lactation Fortification Study</brief_title>
  <official_title>Impact of Nutrient Supplementation on Breast Milk Levels of Lactating Women</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Abbott Nutrition</agency>
      <agency_class>Industry</agency_class>
