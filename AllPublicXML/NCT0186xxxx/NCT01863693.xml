  <primary_outcome>
    <measure>Progression-free survival, defined as time from first administration of first-line therapy to documented disease progression</measure>
    <time_frame>approximately 6 years</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Safety: Incidence of adverse events</measure>
    <time_frame>approximately 6 years</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Overall survival, defined as time from first administration of first-line therapy to documented death</measure>
    <time_frame>approximately 6 years</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Objective response rate, defined as percentage of patients with complete or partial response according to local assessments</measure>
    <time_frame>approximately 6 years</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Quality of life: EQ-5D/EORTC questionnaires</measure>
    <time_frame>approximately 6 years</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Dosage/schedule/duration of Avastin therapy</measure>
    <time_frame>approximately 6 years</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Composition of first-line chemotherapy regimens: drugs/dosage/duration</measure>
    <time_frame>approximately 6 years</time_frame>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Actual">300</enrollment>
  <condition>Ovarian Cancer</condition>
  <arm_group>
    <arm_group_label>Cohort</arm_group_label>
  </arm_group>
  <eligibility>
    <study_pop>
      <textblock>
        Patients with advanced (FIGO stages IIIB, IIIC and IV) epithelial ovarian, fallopian tube,
        or primary peritoneal cancer undergoing first-line treatment with chemotherapy and Avastin
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Adult patients, &gt;/= 18 years of age

          -  Advanced (FIGO stages IIIB, IIIC and IV) epithelial ovarian, fallopian tube or primary
             peritoneal cancer not previously treated with chemotherapy

          -  Initiating Avastin in combination with chemotherapy

        Exclusion Criteria:

          -  Contraindications, warnings and precautions for use as specified in the Avastin
             Summary of Product Characteristics
      </textblock>
    </criteria>
    <gender>Female</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
