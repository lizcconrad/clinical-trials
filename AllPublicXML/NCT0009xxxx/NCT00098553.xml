  <brief_title>Everolimus in Treating Patients With Stage IV Melanoma</brief_title>
  <official_title>Phase II Trial Of RAD-001 In Metastatic Malignant Melanoma</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alliance for Clinical Trials in Oncology</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
