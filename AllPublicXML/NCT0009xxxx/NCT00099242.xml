    <masking>Double</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Change in cognition from baseline at week 24</measure>
  </primary_outcome>
  <primary_outcome>
    <measure>Global clinical impression of change from baseline at week 24</measure>
  </primary_outcome>
