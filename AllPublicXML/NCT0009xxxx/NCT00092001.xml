  <brief_title>Study Of SU011248 In Patients With Metastatic Non-Small Cell Lung Cancer</brief_title>
  <official_title>A Phase 2 Efficacy And Safety Study Of SU011248 In Patients With Metastatic Non-Small Cell Lung Cancer</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Pfizer</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
