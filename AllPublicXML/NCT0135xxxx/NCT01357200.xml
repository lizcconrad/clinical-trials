  <arm_group>
    <arm_group_label>critically ill obese adults</arm_group_label>
    <description>Age ≥ 18 years, body mass index (BMI) ≥ 30, in intensive care unit (ICU) requiring tube feeding ≥ 3 days</description>
  </arm_group>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>enteral formula</intervention_name>
    <description>Complete feeding of study enteral formula, route and regime prescribed by the physician.</description>
    <arm_group_label>critically ill obese adults</arm_group_label>
  </intervention>
