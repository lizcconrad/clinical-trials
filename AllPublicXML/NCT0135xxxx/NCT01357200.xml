  </study_design_info>
  <primary_outcome>
    <measure>time to feeding goal achievement</measure>
    <time_frame>up to 5 days</time_frame>
    <description>on all participants</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>gastrointestinal measures assessment</measure>
    <time_frame>daily for up to 11 days</time_frame>
    <description>on all participants</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>percent of nutrition goal met</measure>
    <time_frame>up to 5 days</time_frame>
    <description>on all participants</description>
  </secondary_outcome>
