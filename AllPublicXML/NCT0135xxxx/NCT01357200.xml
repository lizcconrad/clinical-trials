  <eligibility>
    <study_pop>
      <textblock>
        critically ill obese adults
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Age ≥ 18 years

          -  Surgical or Medical intensive care unit (ICU) patient

          -  Body mass index (BMI) ≥ 30

        Exclusion Criteria:

          -  pregnant or lactating

          -  unable to access gastrointestinal (GI) tract for feeding via tube

          -  other contraindication to tube feeding

          -  admitted with burns

          -  severe head trauma
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
