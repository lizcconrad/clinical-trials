  <brief_title>Performance and Safety Evaluation of an Intradermal Delivery Device</brief_title>
  <official_title>Phase I Performance and Safety Evaluation of an Intradermal Delivery Device</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>PATH</agency>
