    <primary_purpose>Basic Science</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Changes in the varespladib blood levels and radioactivity in blood, urine and fecal samples</measure>
    <time_frame>1 hour prior to drug, and post dose: 0.25, 0.5, 0.75, 1, 1.5, 2, 3, 4, 6, 8, 12, 16, 24, 36, 48, 60, 72, 96, and 120 hours after drug</time_frame>
  </primary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">6</enrollment>
