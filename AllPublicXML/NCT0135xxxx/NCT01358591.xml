  <brief_title>Effect of a High Calcium Breakfast on Exercise Metabolism and Appetite</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Northumbria University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Northumbria University</source>
  <brief_summary>
    <textblock>
      The purpose of this study is to assess the effect of a high calcium breakfast on substrate
      metabolism during exercise and appetite throughout the day.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>May 2011</start_date>
  <completion_date type="Actual">November 2011</completion_date>
  <primary_completion_date type="Actual">November 2011</primary_completion_date>
  <phase>N/A</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Crossover Assignment</intervention_model>
    <primary_purpose>Basic Science</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Total fat oxidised during 60 min of exercise (g).</measure>
    <time_frame>1 hour</time_frame>
    <description>Assessed by indirect calorimetry</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Blood glucose concentration</measure>
    <time_frame>2 hour</time_frame>
    <description>2 h incremental area under the curve for blood glucose following breakfast consumption</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Serum Insulin Concentration</measure>
    <time_frame>2 hour</time_frame>
    <description>2 h area under the curve for serum insulin following breakfast consumption</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Subjective appetite ratings</measure>
    <time_frame>3 hour</time_frame>
    <description>Subjective ratings of appetite assessed by visual analogue scales.</description>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">9</enrollment>
  <condition>Exercise Fat Oxidation</condition>
  <arm_group>
    <arm_group_label>Control Breakfast</arm_group_label>
    <arm_group_type>Sham Comparator</arm_group_type>
    <description>Normal calcium breakfast.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>High calcium breakfast</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>High calcium breakfast.</description>
  </arm_group>
  <intervention>
    <intervention_type>Dietary Supplement</intervention_type>
    <intervention_name>High calcium milk</intervention_name>
    <description>High calcium milk (9 mg/kgBM)</description>
    <arm_group_label>High calcium breakfast</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Dietary Supplement</intervention_type>
    <intervention_name>Control</intervention_name>
    <description>Regular Milk</description>
    <arm_group_label>Control Breakfast</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Male

          -  Aged 18-45 years

          -  Physically active - able to run continuously for 1 h.

        Exclusion Criteria:

          -  Smokers

          -  Food Allergies

          -  Metabolic Disorders

          -  Consuming herbal/dietary supplements

          -  Gastric Problems
      </textblock>
    </criteria>
    <gender>Male</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>45 Years</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
