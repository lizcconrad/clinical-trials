    <measure>Parental Product Assessment Questionnaire completed at Study Visit 2</measure>
    <time_frame>once</time_frame>
  </secondary_outcome>
  <number_of_groups>3</number_of_groups>
  <enrollment type="Actual">511</enrollment>
  <condition>Growth of Term Infants</condition>
  <arm_group>
    <arm_group_label>Control</arm_group_label>
    <description>Marketed hypoallergenic infant formula containing a probiotic</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Experimental 1</arm_group_label>
    <description>An investigational hypoallergenic infant formula with a different protein content, containing the same probiotic as the control</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Experimental 2</arm_group_label>
    <description>An investigational hypoallergenic infant formula with a different protein content, without a probiotic</description>
  </arm_group>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>Infant formula</intervention_name>
    <arm_group_label>Control</arm_group_label>
    <arm_group_label>Experimental 1</arm_group_label>
    <arm_group_label>Experimental 2</arm_group_label>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        Heatly, term infants
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Singleton, 12-16 days of age at randomization

          -  Term infant with birth weight of a minimum of 2500 grams

          -  Solely formula fed

          -  Signed Informed Consent and Protected Health Information

        Exclusion Criteria:

          -  History of underlying metabolic or chronic disease or immunocompromised

          -  Feeding difficulties or formula intolerance
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>N/A</minimum_age>
    <maximum_age>16 Days</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
