  <brief_title>Dose Effect of Limicol on (LDL)-Cholesterol Levels</brief_title>
  <official_title>Dose Effect of Limicol on (LDL)-Cholesterol Levels in Moderate Hypercholesterolaemia</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Lescuyer Laboratory</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
