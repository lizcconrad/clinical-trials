  <brief_title>Aneurysm Study of Pipeline in an Observational Registry</brief_title>
  <acronym>ASPIRE</acronym>
  <official_title>Aneurysm Study of Pipeline in an Observational Registry</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>ev3</agency>
      <agency_class>Industry</agency_class>
