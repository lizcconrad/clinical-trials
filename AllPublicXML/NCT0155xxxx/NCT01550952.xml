  <brief_title>Interscalene Dynamometer Pilot Study</brief_title>
  <official_title>Interscalene Dynamometer Pilot Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Hospital for Special Surgery, New York</agency>
