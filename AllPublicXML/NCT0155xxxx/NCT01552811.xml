    <observational_model>Case Control</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Difference in pancreatic uptake of % injected dose (ID) [11]5-hydroxy tryptophan between healthy volunteers and patients with long-standing type 1 diabetes</measure>
    <time_frame>To be measured at Positron emission tomography investigation for all study persons.</time_frame>
  </primary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Actual">20</enrollment>
