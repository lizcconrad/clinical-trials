    <arm_group_type>Experimental</arm_group_type>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>PA21 and Losartan with Food</intervention_name>
    <description>The maximum dose of PA21 will be 15.0 g/day. The maximum dose of Losartan will be 100 mg/day.</description>
    <arm_group_label>PA21 and Losartan with food</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>No PA21; Losartan with food</intervention_name>
    <description>The maximum dose of Losartan will be 100 mg/day.</description>
    <arm_group_label>No PA21; Losartan with food</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>PA21 with food and Losartan 2 hours later</intervention_name>
    <description>The maximum dose of PA21 will be 15.0 g/day. The maximum dose of Losartan will be 100 mg/day.</description>
    <arm_group_label>PA21 with food and Losartan 2 hrs later</arm_group_label>
  </intervention>
  <eligibility>
