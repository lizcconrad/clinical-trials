  <brief_title>Open Label Extension Study of Conatumumab and AMG 479</brief_title>
  <official_title>A Phase 2 Open Label Extension Study of Conatumumab and AMG 479</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Amgen</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
