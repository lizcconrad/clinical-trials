  <primary_outcome>
    <measure>Comparison of ecchymosis and edema</measure>
    <time_frame>30 days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Resolution of ecchymosis and edema, incidence of hematoma/seroma</measure>
    <time_frame>30 days</time_frame>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Anticipated">10</enrollment>
