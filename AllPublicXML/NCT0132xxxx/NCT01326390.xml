  <brief_summary>
    <textblock>
      Impact on contrast dose or total volume of contrast required to effectively treat the
      targeted tumor.
    </textblock>
  </brief_summary>
  <overall_status>Terminated</overall_status>
  <start_date>May 2010</start_date>
  <completion_date type="Actual">January 2012</completion_date>
