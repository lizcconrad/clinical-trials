  <primary_outcome>
    <measure>Mean PACT-Q2 scores compared to baseline</measure>
    <time_frame>30-45 days and 150-210 days</time_frame>
    <description>Cohort A (patients switched from VKA to Pradaxa)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Mean PACT-Q2 scores compared between 2 treatment groups.</measure>
    <time_frame>30-45 days and 150-210 days</time_frame>
    <description>Cohort B (patients newly initiated Pradaxa or VKA)</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Mean PACT-Q2 score at the last assessment (150-210 days) compared to the second assessment (30-45 days)</measure>
    <time_frame>30-45 days and 150-210 days</time_frame>
