  <brief_title>Pilot Study of Chemoimmunotherapy for High-Risk Neuroblastoma</brief_title>
  <official_title>Pilot Study of Hu3F8, Irinotecan/Temozolomide and Sargramostim (HITS) Chemoimmunotherapy for High-Risk Neuroblastoma</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Memorial Sloan Kettering Cancer Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
