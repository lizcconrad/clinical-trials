  <primary_outcome>
    <measure>Safety and tolerability: AEs, laboratory parameters, ECG, body weight, vital signs</measure>
    <time_frame>monitored throughout study, with weekly ECG and 2x weekly to 2-weekly laboratory assessments on study treatment and at intervals during follow-up</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Change in hemoglobin A1c (HbA1c)</measure>
    <time_frame>from baseline to week 6</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetics: blood concentration of RO5095932 after multiple dosing</measure>
    <time_frame>multiple sampling weeks 1-4 or 1-6 respectively, and weekly during follow-up</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in metabolic parameters: glucose, insulin, C-peptide</measure>
    <time_frame>assessed after 4 or 6 weeks on study treatment</time_frame>
  </secondary_outcome>
