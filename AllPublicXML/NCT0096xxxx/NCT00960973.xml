  <brief_title>The Effect of Vitamin K Supplementation on Glucose Metabolism</brief_title>
  <official_title>The Effect of Vitamin K Supplementation on Glucose Metabolism</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Seoul National University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
