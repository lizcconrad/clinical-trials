    <description>questionnaires</description>
    <arm_group_label>PAO, osteoarhritis, THA</arm_group_label>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        Patients, who despite a joint preserving procedure(periacetabular osteotomy PAO) ended up
        with a total hip arthroplasty (THA).
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  PAO and THA

        Exclusion Criteria:

          -  time since THA less than 4 yrs
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>N/A</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
