  <primary_outcome>
    <measure>Maximum plasma concentration (Cmax) of BAY14-2222</measure>
    <time_frame>Up to 48 hours</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Area under the plasma concentration vs time curve from time 0 to time of last measurable concentration AUC(0-tlast) of BAY14-2222</measure>
    <time_frame>Up to 48 hours</time_frame>
  </primary_outcome>
