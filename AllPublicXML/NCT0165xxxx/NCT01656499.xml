  </study_design_info>
  <primary_outcome>
    <measure>Fecal water toxicity</measure>
    <time_frame>Participants are followed for 10 weeks, with measurements on 4 specific time points</time_frame>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">20</enrollment>
  <condition>Gut Health</condition>
