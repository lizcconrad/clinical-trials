    <arm_group_label>Arabinoxylanoligosaccharides (AXOS)</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>AXOS (2 x 5g WBE/day)</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Maltodextrine (placebo)</arm_group_label>
    <arm_group_type>Placebo Comparator</arm_group_type>
