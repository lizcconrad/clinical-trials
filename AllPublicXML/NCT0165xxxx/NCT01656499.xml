  <brief_title>Effect of AXOS on the Colon Metabolism in Healthy Volunteers</brief_title>
  <official_title>Effect of AXOS on the Colon Metabolism in Healthy Volunteers</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Katholieke Universiteit Leuven</agency>
