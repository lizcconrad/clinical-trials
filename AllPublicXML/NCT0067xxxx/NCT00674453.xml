    <intervention_name>Placebo</intervention_name>
    <description>Matching placebo tablet given once daily for 2 years</description>
    <arm_group_label>Placebo</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Postmenopausal women over 50 years who have low bone mineral density.

        Exclusion Criteria:

          -  Women who are under 50 years of age, are premenopausal, or who currently have
             osteoporosis are excluded.
      </textblock>
    </criteria>
    <gender>Female</gender>
