  <brief_title>Lactacyd Radiance (Lactic Acid) Prophetic Patch Test</brief_title>
  <official_title>Skin Irritation : Prophetic Patch Test of Lactacyd Radiance</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Sanofi</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
