  </study_design_info>
  <primary_outcome>
    <measure>HbA1c after switch from human insulin to modern insulin treatment</measure>
    <time_frame>After 24 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Percentage of subjects to reach HbA1c below 7.0% and = 6.5% at approximately 12 weeks and 24 weeks of treatment</measure>
    <time_frame>After 12 and 24 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Postprandial glycaemic control as measured by PPG</measure>
    <time_frame>After 12 and 24 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Glucose variability as measured by FPG</measure>
    <time_frame>After 12 and 24 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Insulin dose and number of injections</measure>
    <time_frame>After 12 and 24 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Body weight</measure>
    <time_frame>After 12 and 24 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of hypoglycaemic events</measure>
    <time_frame>During treatment</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of adverse drug reactions (ADR)</measure>
    <time_frame>After 12 and 24 weeks</time_frame>
  </secondary_outcome>
