  <brief_title>The Effect of Systane Ultra Lubricant Eye Drops (FID 112903) on Visual Performance</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alcon Research</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Alcon Research</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      To evaluate the effect of 2 different lubricant eye drops on visual function of patients with
      dry eye
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>May 2008</start_date>
  <completion_date type="Actual">June 2008</completion_date>
