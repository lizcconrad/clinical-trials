  <arm_group>
    <arm_group_label>A</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Active treatment with EGb-761 capsules (80mg each capsule), 3 capsules each day for 12 weeks</description>
  </arm_group>
  <arm_group>
    <arm_group_label>B</arm_group_label>
    <arm_group_type>Placebo Comparator</arm_group_type>
