  </study_design_info>
  <primary_outcome>
    <measure>Child Behavior Checklist</measure>
    <time_frame>6 months</time_frame>
    <description>The outcome measure assessed internalizing and externalizing behavior problems in children</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Reactive-Proactive Aggression Questionnaire</measure>
    <time_frame>6 months</time_frame>
    <description>A self-report measure of reactive and proactive forms of aggressive behavior</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Antisocial Personality Screening Device</measure>
    <time_frame>6 months</time_frame>
    <description>A measure of antisocial personality, with a total score and three subscales: callous-unemotional, impulsivity, narcissism</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>WISC</measure>
    <time_frame>6 months</time_frame>
    <description>estimate of IQ</description>
  </secondary_outcome>
