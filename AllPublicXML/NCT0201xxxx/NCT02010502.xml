  </study_design_info>
  <primary_outcome>
    <measure>Time to completion</measure>
    <time_frame>Baseline to week 8</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>O2 consumption</measure>
    <time_frame>Baseline to Week 8</time_frame>
  </secondary_outcome>
