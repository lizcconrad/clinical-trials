  </arm_group>
  <intervention>
    <intervention_type>Dietary Supplement</intervention_type>
    <intervention_name>Beetroot juice</intervention_name>
    <arm_group_label>Concentrated Beet Root Juice</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Dietary Supplement</intervention_type>
    <intervention_name>Beetroot powder</intervention_name>
    <arm_group_label>Beet root powder</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Dietary Supplement</intervention_type>
    <intervention_name>Placebo</intervention_name>
    <arm_group_label>Placebo juice</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  good physical health

          -  trained or competitive cyclist

          -  between 18-40 years of age

        Exclusion Criteria:

        - have used any anabolic or ergogenic supplementation in the last 1-3 months
      </textblock>
    </criteria>
    <gender>Male</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>40 Years</maximum_age>
