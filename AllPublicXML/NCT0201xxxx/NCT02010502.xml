    <arm_group_label>Concentrated Beet Root Juice</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>500ml per day for 6 days</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Beet root powder</arm_group_label>
