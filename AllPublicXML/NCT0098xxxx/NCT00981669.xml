  <brief_title>Rotavirus Vaccine Produced by Butantan Institute</brief_title>
  <official_title>Evaluation of Rotavirus Vaccine Produced by Butantan Institute. Phase I - Safety, Tolerability and Immunogenicity Evaluation</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Butantan Institute</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
