    <study_pop>
      <textblock>
        Primary Care
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Patients with diagnosed chronic heart failure

          -  Patients treated for at least 6 months with candesartan cilexetil

          -  Signed Informed consent

        Exclusion Criteria:

          -  Hypersensitivity to candesartan cilexetil

          -  Pregnancy / lactation

          -  Severe hepatic impairment
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
