  <brief_title>Effect of Succinylcholine on Patients Using Statins</brief_title>
  <official_title>Effect of Succinylcholine on Patients Using Statins</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>The Cleveland Clinic</agency>
      <agency_class>Other</agency_class>
