  </study_design_info>
  <primary_outcome>
    <measure>Assess the bioequivalence (BE) between the commercial and the Phase III risedronate 35 mg DR formulations.</measure>
    <time_frame>72 hours post-dose</time_frame>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">538</enrollment>
  <condition>Osteoporosis</condition>
