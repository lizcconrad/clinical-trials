  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>risedronate DR (Phase III clinical supply)</intervention_name>
    <description>Reference - (Phase III clinical supply) one 35 mg DR tablet administered as a single oral dose after an overnight (10-hour) fast, followed by a 4-hour fast</description>
    <arm_group_label>1</arm_group_label>
    <other_name>Phase III clinical supply</other_name>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>risedronate 35 mg DR (Commercial Tablet)</intervention_name>
    <description>Test - (Commercial tablets) one 35 mg DR tablet administered as a single oral dose after an overnight (10-hour) fast, followed by a 4-hour fast</description>
    <arm_group_label>2</arm_group_label>
    <other_name>Commercial Tablet</other_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  male or female, 18 to 65 years of age

          -  if female, non-lactating and either surgically sterile or postmenopausal

          -  body mass index less than or equal to 32 kg/m2 at screening

        Exclusion Criteria:

          -  no use of a bisphosphonate within 1 month

          -  no history of GI disease

          -  no use of any medications within 7-14 days prior to scheduled dosing day
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>65 Years</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
