  <brief_title>Compatibility of a Mild, Moisturizing Hand Cleanser for Patients With Mild to Moderate Hand Dermatitis</brief_title>
  <official_title>Compatibility of a Mild, Moisturizing Hand Cleanser for Patients With Mild to Moderate Hand Dermatitis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Wake Forest University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
