  <brief_title>Study of Tissue and Blood Samples From Patients With Low-Grade Glioma</brief_title>
  <official_title>Diagnostic and Prognostic Markers in Low-Grade Gliomas</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alliance for Clinical Trials in Oncology</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
