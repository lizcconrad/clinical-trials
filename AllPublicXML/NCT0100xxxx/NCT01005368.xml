  <brief_title>Study of Biomarkers in Blood and Bone Marrow Samples From Patients With Previously Untreated Chronic Lymphocytic Leukemia</brief_title>
  <official_title>Molecular Markers Of Chronic Lymphocytic Leukemia</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alliance for Clinical Trials in Oncology</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
