  <brief_title>Effects of Specified Work Site Physical Activity Intervention Among Employees With Physical Heavy Work</brief_title>
  <official_title>Effects of Specified Work Site Physical Activity Intervention on Musculoskeletal Disorders Among Employees With Physical Heavy Work</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Bibi Gram</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
