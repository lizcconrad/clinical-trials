  <brief_title>Study to Determine if Monitoring of Labor Shortens the Time to Delivery</brief_title>
  <acronym>BirthTrack</acronym>
  <official_title>Management of Labor Using the BirthTrack Computerized Labor Monitoring System</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Barnev, Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
