  <primary_outcome>
    <measure>Late lumen loss on both MV and SB</measure>
    <time_frame>9 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Procedural success (residual stenosis &lt;20% on MV and &lt;50% on SB with TIMI flow 3 for both)</measure>
    <time_frame>immediate</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Binary restenosis rate</measure>
    <time_frame>9 months</time_frame>
