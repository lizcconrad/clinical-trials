  <brief_title>Gestational Diabetes: the Prevention</brief_title>
  <official_title>Gestational Diabetes. The Rol of Exercise in the Prevention</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Technical University of Madrid</agency>
      <agency_class>Other</agency_class>
