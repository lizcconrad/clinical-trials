  <primary_outcome>
    <measure>Success rate : Proportion of eyes that achieve an IOP of &gt; 5 mmHg and &lt;21 mmHg or an IOP reduction &gt; 20%</measure>
    <time_frame>up to 12 months</time_frame>
    <description>Success rate = complete success rate (achieved without ocular hypotensive medication) + qualified success (achieved with ocular hypotensive medication)</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Average change in IOP (mm Hg)</measure>
    <time_frame>3, 6 and 12 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Percentage change in IOP from baseline</measure>
    <time_frame>3, 6 and 12 months</time_frame>
  </secondary_outcome>
