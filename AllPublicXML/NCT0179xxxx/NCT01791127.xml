  <brief_title>Safety and Performance Study of the Siello S Pacing Lead</brief_title>
  <acronym>SIELLO</acronym>
  <sponsors>
    <lead_sponsor>
      <agency>Biotronik, Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Biotronik, Inc.</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The objective of the SIELLO study is to demonstrate the safety and effectiveness of the
      BIOTRONIK Siello S pacing lead.
    </textblock>
  </brief_summary>
  <overall_status>Active, not recruiting</overall_status>
  <start_date>January 2013</start_date>
  <completion_date type="Anticipated">July 2020</completion_date>
  <primary_completion_date type="Anticipated">July 2020</primary_completion_date>
  <study_type>Observational [Patient Registry]</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Case-Only</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <target_duration>5 Years</target_duration>
  <primary_outcome>
    <measure>Atrial Siello S Lead Safety</measure>
    <time_frame>12 month</time_frame>
    <description>Assess the atrial Siello S lead safety by 12-month adverse event-free rate.</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Ventricular Siello S Lead Safety</measure>
    <time_frame>12-month</time_frame>
    <description>Assess the ventricular Siello S lead safety by 12-month adverse event-free rate.</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Siello S Lead Effectiveness</measure>
    <time_frame>12-months</time_frame>
    <description>Success rate of the implanted system to sense and deliver pacing at 12-months post implant.</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Siello S Lead Safety</measure>
    <time_frame>5-Years</time_frame>
    <description>5-Year adverse event free rate for the primary lead position (RV).</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Siello Ventricular Lead Safety</measure>
    <time_frame>5-years</time_frame>
    <description>Individual adverse event rates for primary (RV) lead position.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Adverse Event-Free Rate for Secondary (RA) lead position</measure>
    <time_frame>5-years</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Individual Adverse Event Rates for Secondary (RA) lead position</measure>
    <time_frame>5-years</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Pacing threshold, sensing and impedance measurements for the atrial and ventricular Siello S leads</measure>
    <time_frame>12-months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Adverse event rates for adverse events excluded from primary endpoints</measure>
    <time_frame>5-years</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Pacing threshold, sensing and impedance measurements for the Siello S lead</measure>
    <time_frame>5-years</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Siello S Lead Effectiveness</measure>
    <time_frame>5-years</time_frame>
    <description>Success rate of the implanted system to deliver long-term pacing through 5-years post-implant.</description>
  </secondary_outcome>
