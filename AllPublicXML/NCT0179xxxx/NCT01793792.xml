  <primary_outcome>
    <measure>Successful aneurysm treatment as defined by complete aneurysm angiographic occlusion without retreatment and no significant stenosis of the treated artery</measure>
    <time_frame>12 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Parent artery patency</measure>
    <time_frame>12 months</time_frame>
  </secondary_outcome>
  <other_outcome>
    <measure>Any major stroke or death within 30 days , or ipsilateral stroke or neurological death within 12 months</measure>
    <time_frame>12 months</time_frame>
  </other_outcome>
