  <primary_outcome>
    <measure>Change of force ratio of ground reaction forces in/after landing from baseline</measure>
    <time_frame>1, 5, 10, 15, 20 min after exercise</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Change of the velocity of the center of pressure (COP) from baseline</measure>
    <time_frame>1, 5, 10, 15, 20 min after the exercise</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change of rate of perceived exertion (Borg Scale) from baseline</measure>
    <time_frame>every 10 min during and 1, 5, 10, 15, 20 min after exercise</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
