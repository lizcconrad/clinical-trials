  <primary_outcome>
    <measure>Improvement in wrinkles appearance</measure>
    <time_frame>3 months</time_frame>
    <description>One point or greater improvement in periorbital wrinkle score on a Fitzpatrick scale 3 months after last treatment as determined by at least 2 out of 3 blinded photographs evaluators</description>
  </primary_outcome>
  <number_of_arms>1</number_of_arms>
