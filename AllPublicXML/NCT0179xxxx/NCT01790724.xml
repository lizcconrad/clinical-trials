  <brief_title>Regulating Efficacy and Wellness in Diabetes</brief_title>
  <acronym>REWinD</acronym>
  <official_title>Social Cognitive and Neuropsychological Influences on Physical Activity Behavior in Type II Diabetes</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Illinois at Urbana-Champaign</agency>
      <agency_class>Other</agency_class>
