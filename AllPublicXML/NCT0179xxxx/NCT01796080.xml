  <primary_outcome>
    <measure>Number of subjects with premature myocardial contractions and/or atrial fibrillation</measure>
    <time_frame>20 seconds</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>ECG-derived myocardial de- and repolarization times</measure>
    <time_frame>20 seconds</time_frame>
  </secondary_outcome>
