  <primary_outcome>
    <measure>Abnormal vaginal microbiota</measure>
    <time_frame>minimum 2-7 days from vaginal swab.</time_frame>
    <description>qPCR positive diagnosis as described in intervention.</description>
  </primary_outcome>
  <secondary_outcome>
