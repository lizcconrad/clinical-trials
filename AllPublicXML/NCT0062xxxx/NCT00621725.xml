  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      To determine how patients with advanced cancer and various degrees of hepatic impairment will
      metabolise Cediranib.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>January 2008</start_date>
  <completion_date type="Actual">January 2014</completion_date>
  <primary_completion_date type="Actual">July 2010</primary_completion_date>
  <phase>Phase 1</phase>
