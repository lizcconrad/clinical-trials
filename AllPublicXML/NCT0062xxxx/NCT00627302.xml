    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Quality of vision</measure>
    <time_frame>5 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Efficacy</measure>
    <time_frame>5 months</time_frame>
  </secondary_outcome>
