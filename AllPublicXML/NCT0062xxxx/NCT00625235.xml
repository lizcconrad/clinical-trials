  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  healthy men and women with a BMI &gt;27kg/m2 with co-morbities or BMI &gt;30kg/m2

        Exclusion Criteria:

          -  people with Type II diabetes or other endocrine disorders, women who were pregnant or
             breastfeeding or planning to become pregnant
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>75 Years</maximum_age>
