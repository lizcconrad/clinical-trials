    <arm_group_label>2</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Female

          -  Age 18-65

          -  Diagnosed with classical, centrofacial, erythemato-telangiectatic rosacea

        Exclusion Criteria:

          -  Male, females less than 18 years

          -  Females over 65 years

          -  No diagnosed rosacea
      </textblock>
    </criteria>
    <gender>Female</gender>
