  </study_design_info>
  <primary_outcome>
    <measure>Improvement in signs and symptoms of rosacea</measure>
    <time_frame>16 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Changes in various skin parameters</measure>
    <time_frame>16 weeks</time_frame>
  </secondary_outcome>
