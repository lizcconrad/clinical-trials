  <brief_title>In Vitro Fertilization (IVF) and Acupuncture for Infertility</brief_title>
  <official_title>IVF and Acupuncture for Infertility: A Randomized Controlled Trail</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Pacific Fertility Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
