    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>The Montgomery-Asberg Depression Scale (MADRS) score</measure>
    <time_frame>Visit 1 - &gt; 5</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>The Brief psychiatric Rating Scale (BPRS) scoreThe Clinical Global impression (CGI) scorePatient reported outcomes (PROs)</measure>
    <time_frame>Visit 1 - &gt; 5 (1 week between each visit)</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Sheehan Disability Scale (SDS) score</measure>
    <time_frame>Visit 1 - &gt; 5 (1 week between each visit)</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>The Symptom Checklist 90 Revisred (SCL-90-R) score</measure>
    <time_frame>Visit 1 - &gt; 5 (1 week between each visit)</time_frame>
  </secondary_outcome>
  <condition>Major Depressive Disorder</condition>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Quetiapine fumarate (Seroquel)</intervention_name>
    <description>300 mg, PO, 100 mg morning, 200 mg evening, 4 weeks</description>
    <other_name>Seroquel</other_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Inform consent

          -  Male and female, age between 18 and 65 years.

          -  Na√Øve to any atypical antipsychotic

          -  A diagnosis of major depressive disorder

        Exclusion Criteria:

          -  No use of fluvoxamine

          -  Patients with a history of bipolar I or II disorder

          -  Diagnosis of psychotic major depression disorder
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>65 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
