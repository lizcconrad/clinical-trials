  <brief_title>A Double Blind Clinical Trial of DCS for Food Anxiety</brief_title>
  <official_title>A Double Blind Clinical Trial of DCS for Food Anxiety for Patients With Anorexia and Bulimia Nervosa</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Washington University School of Medicine</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>National Institute of Mental Health (NIMH)</agency>
      <agency_class>NIH</agency_class>
    </collaborator>
  </sponsors>
