  <brief_title>Ticagrelor and Adenosine</brief_title>
  <official_title>The Effect of Ticagrelor on the Adenosine System</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Radboud University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
