  <start_date>January 2016</start_date>
  <completion_date type="Anticipated">September 2018</completion_date>
  <primary_completion_date type="Anticipated">April 2018</primary_completion_date>
  <phase>Phase 2</phase>
  <study_type>Interventional</study_type>
