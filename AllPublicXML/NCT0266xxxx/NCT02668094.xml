  <brief_title>The Effect of Pregabalin on Pain of Propofol Injection</brief_title>
  <official_title>The Effect of Pregabalin on Pain of Propofol Injection</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Kyungpook National University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
