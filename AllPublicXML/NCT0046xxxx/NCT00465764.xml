  <brief_title>Tolerance and Development of Healthy, Term Infants</brief_title>
  <official_title>Pilot Tolerance and Bone Status of Healthy, Term Infants Fed Infant Formulas</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Abbott Nutrition</agency>
      <agency_class>Industry</agency_class>
