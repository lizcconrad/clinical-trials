    <intervention_type>Behavioral</intervention_type>
    <intervention_name>Online advertisement</intervention_name>
    <description>Internet advertisement with messages tailored for young adult smoker's motivated to quit</description>
    <arm_group_label>Ad exposure</arm_group_label>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        Young adult Internet users
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  18 to 30 years old

          -  smoke on some days or everyday

          -  enrollment in specified online cessation program
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>30 Years</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
