  </study_design_info>
  <primary_outcome>
    <measure>Ocular Itching at defined time points up to 2 weeks</measure>
    <time_frame>Baseline to day 14</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Ocular Redness at defined time points up to 2 weeks</measure>
    <time_frame>Baseline to day 14</time_frame>
  </primary_outcome>
