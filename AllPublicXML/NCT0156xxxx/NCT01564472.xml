  <brief_title>Validation of Sleepware G3 Autoscoring Algorithm</brief_title>
  <official_title>Validation of Sleepware G3 Autoscoring Algorithm</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Philips Respironics</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
