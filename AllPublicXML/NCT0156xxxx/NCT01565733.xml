  <primary_outcome>
    <measure>Frequency of severe hypoglycaemic episodes</measure>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number of severe hypoglycaemic episodes</measure>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of mild hypoglycaemic episodes</measure>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Postprandial glucose concentration after breakfast, lunch, and dinner</measure>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Average fasting glucose concentration after waking and before main meals</measure>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Average plasma glucose level at bedtime and at night</measure>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Actual">2223</enrollment>
