  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Type 2 diabetes

          -  Currently treated with insulin

          -  Currently treated with Metformin

          -  HbA1c: 7.5-12.0%

        Exclusion Criteria:

          -  Body Mass Index (BMI) &lt; 25.0 or &gt; 40.0 kg/m2

          -  Metformin contraindications according to local practice

          -  TZDs within 6 months prior to randomisation
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
