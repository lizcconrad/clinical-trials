  <primary_outcome>
    <measure>Subjective appetite ratings</measure>
    <time_frame>3 hour</time_frame>
    <description>Subjective ratings of appetite assessed by visual analogue scales.</description>
  </primary_outcome>
  <secondary_outcome>
