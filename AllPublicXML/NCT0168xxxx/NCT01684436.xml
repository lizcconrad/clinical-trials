  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Patients with moderate to severe dry eye

        Exclusion Criteria:

          -  Previous punctal plug insertion in the last 3 months or presence of plugs at the time
             of study

          -  Contact lens wear in the 7 days prior to study start or during the study

          -  LASIK procedure in the last year

          -  Cataract or other eye surgery in the last 3 months

          -  Corneal grafts
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
