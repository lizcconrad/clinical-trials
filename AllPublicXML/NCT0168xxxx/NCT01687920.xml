  <primary_outcome>
    <measure>Dose proportionality of BAY94-8862 exposure in plasma when given as 1.25, 2.5, 5.0, 7.5 and 10 mg IR tablets</measure>
    <time_frame>0, 0.25, 0.5, 0.75, 1, 1.5, 2, 2.5, 3, 4, 6, 8, 12, 15, 24, 28 &amp; 48 h</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number of participants with adverse events as a measure of safety and tolerability of BAY94-8862</measure>
    <time_frame>Up to 48 h</time_frame>
  </secondary_outcome>
