  <brief_summary>
    <textblock>
      The purpose of this study is to determine whether added ingredient affects glycaemic
      responses to a cracker snack.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>September 2012</start_date>
  <completion_date type="Actual">October 2012</completion_date>
