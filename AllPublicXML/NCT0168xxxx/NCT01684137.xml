  <primary_outcome>
    <measure>Reduction of the total IgE (IU/ml) antibody.</measure>
    <time_frame>1 year</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Improving of vital capacity measured by spirometry (litre).</measure>
    <time_frame>1 year</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Improving of lung volume measured by spirometry (litre).</measure>
    <time_frame>1 year</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Improving of quality of life observed in the visual analogue scale (VAS).</measure>
    <time_frame>1 year</time_frame>
  </secondary_outcome>
  <other_outcome>
    <measure>Asthma bronchiale, age 9-18 year</measure>
