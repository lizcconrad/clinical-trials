  <brief_title>Effect of GLP-1 Analogs on Psoriasis in Type 2 Diabetic Patients</brief_title>
  <official_title>Clinical and Histopathological Effect of GLP-1 Analogs on Psoriasis in Type 2 Diabetic Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Cliniques universitaires Saint-Luc- Université Catholique de Louvain</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
