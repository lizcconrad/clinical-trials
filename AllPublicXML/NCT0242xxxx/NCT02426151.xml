  <primary_outcome>
    <measure>Cmax and AUClast</measure>
    <time_frame>1D before dosing, 1h, 2, 4h, 7h, 10h,12h, 14h, 2D 0h, 2D 8h, 3D 0h, 4D 0h, 5D 0h, 6D 0h,</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>AUCinf, Tmax, t1/2β, CL/F, MRTinf (mean residence time) of Erythropoietin</measure>
    <time_frame>1D before dosing, 1h, 2, 4h, 7h, 10h,12h, 14h, 2D 0h, 2D 8h, 3D 0h, 4D 0h, 5D 0h, 6D 0h,</time_frame>
  </secondary_outcome>
