  </study_design_info>
  <primary_outcome>
    <measure>Paresthesia distribution from subject completed paresthesia drawing questionnaire.</measure>
    <time_frame>An estimated average of 7 days post temporary trial procedure</time_frame>
  </primary_outcome>
  <enrollment type="Anticipated">15</enrollment>
  <condition>Chronic Pain</condition>
