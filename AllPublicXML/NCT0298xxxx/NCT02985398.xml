  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      An evaluation of long term safety of repeat ALD403 doses in Chronic Migraine
    </textblock>
  </brief_summary>
  <overall_status>Active, not recruiting</overall_status>
  <start_date type="Actual">December 2016</start_date>
  <primary_completion_date type="Anticipated">June 2018</primary_completion_date>
  <phase>Phase 3</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
