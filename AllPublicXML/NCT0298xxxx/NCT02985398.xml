    <primary_purpose>Prevention</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Laboratory variables &amp; ECG</measure>
    <time_frame>56 Weeks</time_frame>
    <description>To evaluate the safety and tolerability</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Adverse Events</measure>
    <time_frame>56 Weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
