  <brief_title>e-Health Coping Skills Training for Women Whose Partner Has a Drinking Problem</brief_title>
  <official_title>eHealth Coping Skills Training for Women Whose Partner Has a Drinking Problem</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>State University of New York at Buffalo</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>University of Oregon</agency>
      <agency_class>Other</agency_class>
