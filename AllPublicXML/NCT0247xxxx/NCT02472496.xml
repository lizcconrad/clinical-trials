  <eligibility>
    <study_pop>
      <textblock>
        Patients with coronary lesion treated with overlapping drug-eluting stents
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Age &gt;= 18 years

          -  Drug-eluting stent overlap

        Exclusion Criteria:

          -  Overlap with previous stent

          -  Cardiogenic shock
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
