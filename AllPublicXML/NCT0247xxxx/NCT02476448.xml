  <brief_title>Evaluation of Ureteral Jets on Cystoscopy</brief_title>
  <acronym>JET</acronym>
  <official_title>Evaluation of Ureteral Jets on Cystoscopy: A Randomized Controlled Trial</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>The Cleveland Clinic</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
