  </study_design_info>
  <primary_outcome>
    <measure>Change of distance between clavicle and confluence of internal jugular vein and subclavian vein while arm retraction</measure>
    <time_frame>Outcome measure will be assessed during arm retraction and it would be within 30 second of arm retraction</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Change in diameter of suvclavian vein at the confluence of internal jugular vein and subclavian vein</measure>
    <time_frame>Outcome measure will be assessed during arm retraction and it would be within 30 second of arm retraction</time_frame>
  </primary_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment type="Actual">60</enrollment>
