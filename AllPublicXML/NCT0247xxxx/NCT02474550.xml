  <brief_title>Multicenter Prospective Registry Study of Diffuse Large B Cell Lymphoma</brief_title>
  <official_title>Multicenter Prospective Registry Study of Diffuse Large B Cell Lymphoma</official_title>
  <sponsors>
    <lead_sponsor>
