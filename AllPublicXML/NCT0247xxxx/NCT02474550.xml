  <target_duration>36 Months</target_duration>
  <primary_outcome>
    <measure>incidence of febrile neutropenia</measure>
    <time_frame>within 30 days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>incidence of chemotherapy-related adverse events other than febrile neutropenia</measure>
    <time_frame>4 weeks after completion of chemotherapy</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Overall survival</measure>
    <time_frame>2 year</time_frame>
  </secondary_outcome>
