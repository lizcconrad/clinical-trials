  <study_design_info>
    <observational_model>Case-Only</observational_model>
    <time_perspective>Retrospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>To correlate the tissue of origin predicted by the RT-PCR assay with clinical and pathologic features of patients with carcinoma of unknown primary site.</measure>
    <time_frame>6 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>To correlate the tissue of origin predicted by the RT-PCR with actual primary sites found subsequently in a subset of patients.</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>To evaluate the utility of RT-PCR assay results in guiding treatment selection in patients with carcinoma of unknown primary site.</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
  <enrollment type="Actual">30</enrollment>
  <condition>Unknown Primary Cancer</condition>
  <eligibility>
    <study_pop>
      <textblock>
        Approximately 30 patients at Tennessee Oncology with unknown primary cancer have had
        commercially available RT-PCR assays done on their tissue biopsy since the assay became
        available in 2007. These patients are the subjects of this review.
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Patients with RT-PCR testing completed since commercialization of the RT-PCR assay in
             2007.
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
