  <brief_title>EFFEKT - Efficacy and Safety of Long-term Treatment With KOGENATE Bayer/FS</brief_title>
  <official_title>Efficacy and Safety of Long-term Treatment With KOGENATE Bayer/FS</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Bayer</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
