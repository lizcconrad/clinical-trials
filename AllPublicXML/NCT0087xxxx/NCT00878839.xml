  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Operable cataracts;

          -  Good ocular health;

          -  Qualifies for AcrySof Toric IOL via AcrySof Toric Calculator (1.03-2.06 D cylinder at
             corneal plane)

        Exclusion Criteria:

          -  &gt;2.06 D astigmatism at corneal plane;

          -  irregular astigmatism;

          -  prior or ongoing corneal disease or scarring;

          -  history of ocular disease
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>70 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
