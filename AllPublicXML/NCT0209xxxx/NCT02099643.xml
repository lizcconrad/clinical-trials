    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <target_duration>5 Days</target_duration>
  <primary_outcome>
    <measure>Atria fibrillation termination during procedure</measure>
    <time_frame>240 min</time_frame>
    <description>conversion to atrial tachycardia (AT) or sinus rhythm</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Crucial points</measure>
    <time_frame>240 min</time_frame>
    <description>Crucial points were defined by areas where ablation immediately extended the cycle length&gt; 20% of AF CL, or regularized Atrial Fibrilation (AF) into Atrial Tachycardia (AT) or converted AF (or AT) into Sinus Rythm</description>
  </secondary_outcome>
  <enrollment type="Actual">113</enrollment>
  <condition>Atrial Fibrilation Termination Endpoint</condition>
  <eligibility>
    <study_pop>
