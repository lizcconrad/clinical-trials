    <arm_group_label>Experimental 1</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Ulimorelin Invtravenously (IV)</intervention_name>
    <description>480 µg/kg daily (QD)</description>
    <arm_group_label>Experimental 2</arm_group_label>
  </intervention>
