  <primary_outcome>
    <measure>Changes in Vital signs (blood pressure (BP), pulse rate (PR), respiratory rate(RR))</measure>
    <time_frame>up to 21 days</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Changes in 12-lead ECG (electrocardiogram)</measure>
    <time_frame>up to 21 days</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Changes in Clinical laboratory tests (haematology, clinical chemistry and urinalysis)</measure>
    <time_frame>up to 21 days</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Occurrence of Adverse events</measure>
    <time_frame>up to 21 days</time_frame>
  </primary_outcome>
  <secondary_outcome>
