  <brief_title>Varenicline Pregnancy Cohort Study</brief_title>
  <official_title>Varenicline Pregnancy Cohort Study Protocol Chantix (Registered)/Champix-(Registered) (Varenicline Tartrate)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Pfizer</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>University of Aarhus</agency>
