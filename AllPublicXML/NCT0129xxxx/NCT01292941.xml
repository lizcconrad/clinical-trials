  <brief_title>Clinical Study Evaluating Safety of a New Catheter for Urinary Intermittent Catheter in Healthy Volunteers</brief_title>
  <official_title>Clinical Study Evaluating Safety of a New Catheter for Urinary Intermittent Catheter in Healthy Volunteers</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Coloplast A/S</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
