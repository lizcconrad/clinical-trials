  </study_design_info>
  <primary_outcome>
    <measure>Safety and tolerability assessed via AEs/SAEs, plus where applicable gastrointestinal symptoms checklist, clinical laboratory tests, vital signs (blood pressure and pulse rate)</measure>
    <time_frame>Day 1 to 14</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>CE-326,597 pharmacokinetics by estimating AUC0-24, Cmax and Tmax</measure>
    <time_frame>Day 7</time_frame>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">41</enrollment>
  <condition>Obesity</condition>
