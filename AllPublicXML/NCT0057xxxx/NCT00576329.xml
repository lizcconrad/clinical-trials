  <brief_title>Topical 0.4% Ketorolac and Vitreoretinal Surgery</brief_title>
  <official_title>Topical 0.4% Ketorolac for the Inhibition of Miosis During Vitreoretinal Surgery: a Randomized, Placebo-controlled, Double-masked Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Emory University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
