  <brief_title>The Effect of Kinesio Taping on Ankle Proprioception</brief_title>
  <official_title>The Effect of Kinesio Taping on Ankle Proprioception</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Logan College of Chiropractic</agency>
