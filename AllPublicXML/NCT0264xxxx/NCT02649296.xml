    <description>The intervention will be done twice a week</description>
    <arm_group_label>Treatment with Skanlab Bodywave</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  knee arthrosis in one or both legs

        Exclusion Criteria:

          -  Pacemaker

          -  infusion pump

          -  cancer

          -  diabetes

          -  pregnancy

          -  blood clots

          -  bacteriological inflammation

          -  reduced sensitivity in the treatment area.
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>45 Years</minimum_age>
    <maximum_age>70 Years</maximum_age>
