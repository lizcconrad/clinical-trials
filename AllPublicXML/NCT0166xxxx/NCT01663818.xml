  <brief_title>Tack Optimized Balloon Angioplasty (TOBA) Study</brief_title>
  <acronym>TOBA</acronym>
  <official_title>Tack Optimized Balloon Angioplasty (TOBA) Study for Femoropopliteal Arteries Using the Tack-IT Endovascular Stapler</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Intact Vascular</agency>
      <agency_class>Industry</agency_class>
