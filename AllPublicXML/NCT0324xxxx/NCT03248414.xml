  <brief_title>Investigation of Anti-obesity Properties of Lactobacillus Sakei in Obese Human Subjects</brief_title>
  <official_title>Investigation of Anti-obesity Properties of Lactobacillus Sakei in Obese Human Subjects</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Seoul National University Bundang Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
