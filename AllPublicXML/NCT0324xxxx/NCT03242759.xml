  <primary_outcome>
    <measure>to characterize percentage lung function profile of IPF patients from baseline characteristics</measure>
    <time_frame>1-2 years</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>to assess the time taken for first acute exacerbation of IPF after enrolment</measure>
    <time_frame>1-2 years</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>to assess annual change from baseline using St. George's Respiratory Questionnaire</measure>
    <time_frame>1-2 years</time_frame>
  </secondary_outcome>
