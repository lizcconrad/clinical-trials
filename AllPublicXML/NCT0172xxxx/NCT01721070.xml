  <primary_outcome>
    <measure>AUC (0-inf)</measure>
    <time_frame>0 (pre-dose), 10, 20, 30, 40, 50, 60, 70, 80, 90, 120, 180, 240, 360, 480, 600, 720, and 840 minutes, and 24 hours after dosing.24 hours</time_frame>
    <description>Total amount of sufentanil absorbed; After each dosing of Sufentanil NanoTab, serial blood samples will be taken at regular time points.</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Cmax</measure>
    <time_frame>0 (pre-dose), 10, 20, 30, 40, 50, 60, 70, 80, 90, 120, 180, 240, 360, 480, 600, 720, and 840 minutes, and 24 hours after dosing.</time_frame>
    <description>Maximum plasma concentration; After each dosing of Sufentanil NanoTab, serial blood samples will be taken at regular time points.</description>
  </primary_outcome>
