  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to find how much hyaluronidase should be used when receiving
      injections of fillers.
    </textblock>
  </brief_summary>
  <overall_status>Active, not recruiting</overall_status>
  <start_date>October 2012</start_date>
  <completion_date type="Anticipated">December 2018</completion_date>
