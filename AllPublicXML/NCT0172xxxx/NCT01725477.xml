  <brief_title>Laparoscopic Tubal Patency Assessment</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Pourmatroud, Elham, M.D.</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
