  <brief_title>Effectiveness of Testosterone Undecanoate to Improve Sexual Function in Postmenopausal Women</brief_title>
  <official_title>Effectiveness of Testosterone Undecanoate to Improve Sexual Function in Postmenopausal Women</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Chulalongkorn University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
