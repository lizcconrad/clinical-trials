    <intervention_type>Biological</intervention_type>
    <intervention_name>tanezumab 10 mg</intervention_name>
    <description>tanezumab 10 mg one dose at weeks 0 and 8</description>
    <arm_group_label>1</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Biological</intervention_type>
    <intervention_name>tanezumab 5 mg</intervention_name>
    <description>tanezumab 5 mg one dose at weeks 0 and 8</description>
    <arm_group_label>2</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>naproxen</intervention_name>
    <description>naproxen 1000 mg daily for 16 weeks</description>
    <arm_group_label>3</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>placebo</intervention_name>
    <description>placebo to match tanezumab and naproxen dosing</description>
    <arm_group_label>4</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Osteoarthritis of the knee according to Kellgren-Lawrence x-ray grade of 2

        Exclusion Criteria:

          -  Pregnancy or intent to become pregnant

          -  BMI greater than 39

          -  other severe pain, significant cardiac, neurologic or cardiac disease
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
