  </study_design_info>
  <primary_outcome>
    <measure>oxidative stress</measure>
    <time_frame>Immediately after delivery by cesarean section</time_frame>
    <description>New borns of diabetic mother may have oxidative stress</description>
  </primary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Actual">40</enrollment>
  <condition>Diabetes</condition>
