  <brief_title>Range of Motion of Standard and High-Flexion Posterior Cruciate Retaining Total Knee Prostheses</brief_title>
  <official_title>Range of Motion of Standard and High-Flexion Posterior Cruciate Retaining Total Knee Prostheses A Prospective, Randomized Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ewha Womans University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
