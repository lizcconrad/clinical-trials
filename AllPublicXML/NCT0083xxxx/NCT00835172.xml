  <brief_title>Glimepiride 4 mg Tablets Under Non-Fasting Conditions</brief_title>
  <official_title>A Relative Bioavailability Study of Glimepiride 4 mg Tablets Under Non-Fasting Conditions</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Teva Pharmaceuticals USA</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
