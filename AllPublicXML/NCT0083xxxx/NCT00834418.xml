  <brief_title>Leflunomide 20 mg Tablets Under Fasting Conditions</brief_title>
  <official_title>A Relative Bioavailability Study of 20 mg Leflunomide Tablets Under Fasting Conditions</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Teva Pharmaceuticals USA</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
