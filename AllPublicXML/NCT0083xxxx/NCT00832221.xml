    <allocation>Non-Randomized</allocation>
    <intervention_model>Single Group Assignment</intervention_model>
    <primary_purpose>Basic Science</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Positron emission tomography using the radioligand [11C]raclopride</measure>
    <time_frame>5 visits</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Adverse Events (AEs), vital signs and changes in laboratory parameters, physical examinations, Alertness Visual Analogue Scale.</measure>
    <time_frame>5 visits. Some tests will be done several times per visit. All tests will not be performed at every visit.</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetics of quetiapine and its main active metabolite</measure>
    <time_frame>22 samples</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Anticipated">10</enrollment>
  <condition>Depression</condition>
