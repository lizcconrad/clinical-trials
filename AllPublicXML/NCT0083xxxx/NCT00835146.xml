  <brief_title>Ribavirin 200 mg Tablets Under Fasting Conditions</brief_title>
  <official_title>A Relative Bioavailability Study of Ribavirin 200 mg Tablets Under Fasting Conditions</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Teva Pharmaceuticals USA</agency>
      <agency_class>Industry</agency_class>
