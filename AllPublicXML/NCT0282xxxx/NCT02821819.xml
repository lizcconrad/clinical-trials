  <brief_title>Random-start Ovarian Stimulation in Egg-donors (ROSE)</brief_title>
  <acronym>ROSE</acronym>
  <official_title>Random-start Ovarian Stimulation in Egg-donors (ROSE) Trial: a Self-controlled Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Instituto Bernabeu</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
