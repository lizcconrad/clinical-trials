  <primary_outcome>
    <measure>Change in the mean 24-hour systolic Ambulatory Blood Pressure Measurement (ABPM) from Baseline to ninety (90) days post treatment</measure>
    <time_frame>Baseline to ninety (90) days post treatment</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Incidence of SAEs and UADEs reported for the population from implantation through three (3) years of follow-up.</measure>
    <time_frame>Baseline to three (3) years post treatment</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
