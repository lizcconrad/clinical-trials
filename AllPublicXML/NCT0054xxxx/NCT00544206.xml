    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>mean glucose level</measure>
    <time_frame>24 hours</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>fasting capillary blood glucose level;percentage change from baseline in the dose and amount of antihyperglycemic medication(s), including insulin</measure>
    <time_frame>24 hours</time_frame>
  </secondary_outcome>
