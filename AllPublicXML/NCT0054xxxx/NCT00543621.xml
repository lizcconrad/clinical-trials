  <brief_summary>
    <textblock>
      Study to evaluate the effectiveness of a structured intervention to improve diabetes control.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>May 2005</start_date>
  <phase>Phase 4</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>HbA1c, Fasting blood glucose,blood pressure, weight loss</measure>
    <time_frame>24 weeks</time_frame>
  </primary_outcome>
  <enrollment type="Actual">150</enrollment>
  <condition>Diabetes Mellitus, Type 2</condition>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>daily enteral nutritional product and/or behavioral changes</intervention_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  informed consent

          -  18-70 adults

          -  Type 2 Diabetes

          -  Males and non-pregnant females

        Exclusion Criteria:

          -  Use of exogenous insulin for glucose control

          -  Use of medications or supplements for weight loss

          -  Advanced diabetes complications

          -  Recent history of cardiovascular event
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>70 Years</maximum_age>
  </eligibility>
