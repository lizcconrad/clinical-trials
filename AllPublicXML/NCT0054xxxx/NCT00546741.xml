  <brief_title>Drug Interaction With Metformin</brief_title>
  <official_title>Pharmacokinetic Drug Interaction Study With Dapagliflozin and Metformin in Healthy Subjects</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AstraZeneca</agency>
      <agency_class>Industry</agency_class>
