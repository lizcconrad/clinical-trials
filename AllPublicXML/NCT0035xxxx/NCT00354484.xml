  <brief_title>Safety and Efficacy of a Hematinic Agent in the Treatment of Postpartum Patients</brief_title>
  <official_title>Safety and Efficacy of a Hematinic Agent in the Treatment of Postpartum Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Luitpold Pharmaceuticals</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
