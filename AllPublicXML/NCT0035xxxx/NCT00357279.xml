    <measure>Change in lung function</measure>
    <time_frame>48 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pulmonary exacerbation</measure>
    <time_frame>48 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
