  <brief_title>Polypropylene Mesh in Prolapse Surgery</brief_title>
  <official_title>Polypropylene Mesh in Prolapse Surgery; Long-term Outcome and Complications.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Turku University Hospital</agency>
      <agency_class>Other</agency_class>
