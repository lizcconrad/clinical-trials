  <brief_title>Technology-supported Exercise Therapy for Patients With Chronic Low Back Pain</brief_title>
  <official_title>Technology-supported Exercise Therapy for Patients With Chronic Low Back Pain: Pilot Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Hasselt University</agency>
      <agency_class>Other</agency_class>
