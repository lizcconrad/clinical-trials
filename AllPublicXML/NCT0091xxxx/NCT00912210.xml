  <brief_title>Higher Protein Diet and Exercise for Optimal Weight Loss in Elderly Women</brief_title>
  <official_title>Higher Protein Diet and Exercise for Optimal Weight Loss in Elderly Women</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Illinois at Urbana-Champaign</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
