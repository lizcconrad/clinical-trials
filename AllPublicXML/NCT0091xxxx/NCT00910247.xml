  <brief_title>Eslicarbazepine Acetate Monotherapy Long Term Study</brief_title>
  <official_title>Long Term Eslicarbazepine Acetate Extension Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Sunovion</agency>
      <agency_class>Industry</agency_class>
