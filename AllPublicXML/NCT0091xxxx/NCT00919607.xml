  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Single Group Assignment</intervention_model>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>single dose PK parameters(300mg)</measure>
    <time_frame>Day1 to 48 hour after Day 5</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Steady-state multiple doses PK parameters</measure>
    <time_frame>Day 1 to 48 hours after Day7</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Incidence and severity of AEs</measure>
    <time_frame>sign ICF to Day 9</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Clinical significant change in Lab test</measure>
    <time_frame>baseline up to Day 9</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Clinical significant change in vital signs</measure>
    <time_frame>baseline up to Day 9</time_frame>
  </secondary_outcome>
