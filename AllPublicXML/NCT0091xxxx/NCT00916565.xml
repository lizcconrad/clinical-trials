  <eligibility>
    <study_pop>
      <textblock>
        formula-fed and breastfed infants
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  10-18 days of age

          -  37-42 weeks gestation

          -  birth weight 2500 g or more

          -  consuming one source of nutrition

        Exclusion Criteria:

          -  metabolic or chronic disease

          -  acute infection

          -  feeding difficulties

          -  consumed complementary foods or donor milk

          -  steroid use
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>N/A</minimum_age>
    <maximum_age>18 Days</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
