  </sponsors>
  <source>Pfizer</source>
  <brief_summary>
    <textblock>
      Assessment of safety and efficacy of SU11248 in patients with metastatic breast cancer who
      have failed selected other therapies.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>March 2004</start_date>
  <completion_date type="Actual">November 2005</completion_date>
  <phase>Phase 2</phase>
