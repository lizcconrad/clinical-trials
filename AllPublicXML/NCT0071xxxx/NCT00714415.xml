    <measure>Efficacy in controlling bleeding</measure>
    <time_frame>36 month</time_frame>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Actual">80</enrollment>
  <condition>Hemophilia B</condition>
  <arm_group>
