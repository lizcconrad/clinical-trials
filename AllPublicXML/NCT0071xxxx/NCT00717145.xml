    <allocation>Randomized</allocation>
    <intervention_model>Crossover Assignment</intervention_model>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Assess the relative bioavailability of the risedronate 20 mg DR tablet administered immediately after a high-fat meal compared to the risedronate 35 mg IR tablet administered 30 minutes prior to a high-fat meal.</measure>
    <time_frame>4 Days</time_frame>
  </primary_outcome>
  <number_of_arms>4</number_of_arms>
  <enrollment type="Actual">94</enrollment>
  <condition>Postmenopausal</condition>
  <condition>Non-lactating</condition>
  <condition>Surgically Sterile</condition>
  <arm_group>
    <arm_group_label>1</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>One risedronate 20 mg DR tablet taken following an overnight fast, followed by a 4-hour fast.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>2</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>One risedronate 20 mg DR tablet taken following an overnight fast, within 5 minutes after ingesting a high-fat meal; no additional food will be allowed for at least 4 hours post-dose.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>3</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>One risedronate 35 mg DR tablet taken following an overnight fast, within 5 minutes after ingesting a high-fat meal; no additional food will be allowed for at least 4 hours post-dose.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>4</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>One risedronate 35 mg IR tablet taken following an overnight fast, 30 minutes before ingesting a high-fat meal; no additional food will be allowed for at least 4 hours post-dose.</description>
  </arm_group>
