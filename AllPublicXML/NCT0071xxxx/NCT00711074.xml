  <primary_outcome>
    <measure>total radioactivity in plasma, whole blood, faeces and urine.</measure>
    <time_frame>Pre-dose, post dose, 0.25h, 0.5h, 0.75h, 1h, 1.5h, 2, 3, 4, 6, 9, 12, 24, 48, 72, 96, 120, 144, 168, 192, 216, 240h</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Additional metabolite identification</measure>
    <time_frame>0.5h, 3h, 6h, 12h, 24h</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>general safety assessments: AE reporting, physical examination, ECG, safety haematology and clinical chemistry assessment, vital signs</measure>
    <time_frame>Screening, pre-dose, Visit 2 (residential period) and visit 3 follow up</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Anticipated">4</enrollment>
