  <brief_title>Effects of Qigong, Tai Chi and Yoga Practice in Older Adults</brief_title>
  <official_title>Effects of Qigong, Tai Chi and Yog Practice on Indicators of Health and Quality of Life in Older Adults</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Bastyr University</agency>
      <agency_class>Other</agency_class>
