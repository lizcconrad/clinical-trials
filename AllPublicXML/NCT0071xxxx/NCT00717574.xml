  <brief_title>Addition of Nitrous Oxide and Its Effects on Depth of Anesthesia</brief_title>
  <acronym>N20</acronym>
  <official_title>Addition of Nitrous Oxide to a Sevoflurane or Propofol Based Anesthetic and Its Effects on Depth of Anesthesia Indices (N20)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Oklahoma</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
