  <brief_title>Collection of Patient-Reported Symptoms and Performance Status Via the Internet</brief_title>
  <official_title>Collection of Patient-Reported Symptoms and Performance Status Via the Internet</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alliance for Clinical Trials in Oncology</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
