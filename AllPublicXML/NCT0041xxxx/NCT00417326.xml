  <primary_outcome>
    <measure>pulmonary function</measure>
    <time_frame>14 days</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>mortality</measure>
    <time_frame>28 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>ventilator status</measure>
    <time_frame>28 days</time_frame>
  </secondary_outcome>
