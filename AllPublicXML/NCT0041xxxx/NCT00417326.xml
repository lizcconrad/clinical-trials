    <arm_group_label>P</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>ONO-5046Na</intervention_name>
    <description>twenty four hour intravenous infusion at 0.2 mg/kg/hr for 14 days</description>
    <arm_group_label>E</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Patients with community-acquired pneumonia

          -  Patients with acute respiratory failure

        Exclusion Criteria:

          -  Patients with nosocomial pneumonia

          -  Patients on mechanical ventilation
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>20 Years</minimum_age>
    <maximum_age>79 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
