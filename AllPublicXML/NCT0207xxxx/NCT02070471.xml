  <primary_outcome>
    <measure>AUC of CK-MB for 72 hours post PCI</measure>
    <time_frame>pre PCI, PCI 0h, 6h, 12h, 18h, 24h, 48h, 72h</time_frame>
  </primary_outcome>
  <secondary_outcome>
