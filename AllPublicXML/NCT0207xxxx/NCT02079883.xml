    <measure>Full thickness macular hole (FTMH) closure by SD-OCT as determined by the treating physician</measure>
    <time_frame>12 months</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Mean change in visual acuity (VA) from baseline across time</measure>
    <time_frame>12 months</time_frame>
  </primary_outcome>
