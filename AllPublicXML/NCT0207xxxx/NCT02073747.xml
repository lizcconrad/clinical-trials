  <brief_title>The Various Effects of Gaseous Albuterol on Serum Lactate</brief_title>
  <official_title>The Various Effects of Gaseous Albuterol on Serum Lactate</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University Medical Center of Southern Nevada</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
