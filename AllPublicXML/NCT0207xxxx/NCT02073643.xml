  <brief_title>Study of Household Purchasing Patterns, Eating and Recreation</brief_title>
  <acronym>SHoPPER</acronym>
  <official_title>Delay Discounting and Household Food Choices</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Rush University Medical Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
