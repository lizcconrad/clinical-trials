  <brief_title>C-Reactive Protein (CRP) in Obese Diabetic Women</brief_title>
  <official_title>Interventions Study for the Control of Diabetes Mellitus Type 2 in Obese Women</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AstraZeneca</agency>
      <agency_class>Industry</agency_class>
