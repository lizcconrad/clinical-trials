  <brief_title>Impact of Body Weight on the Immediate Health of the Pediatric Population</brief_title>
  <official_title>The Influence of Childhood Obesity on Presentation to a Pediatric Emergency Department</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Antonios Likourezos</agency>
      <agency_class>Other</agency_class>
