  <primary_outcome>
    <measure>The comparison between randomly assigned study arms in the mean area under the curve plasma HIV RNA at 48 weeks.</measure>
  </primary_outcome>
  <secondary_outcome>
    <measure>Proportion of patients with undetectable plasma HIV RNA, Changes in CD4+ cell count, use of resistance test result in selecting new ART regimen</measure>
  </secondary_outcome>
  <enrollment>300</enrollment>
  <condition>HIV Infection</condition>
  <intervention>
    <intervention_type>Device</intervention_type>
    <intervention_name>HIV drug resistance tests based upon genotype testing</intervention_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  HIV infected patients taking combination antiretroviral therapy, with plasma HIV RNA&gt;
             2000copies/mL, who were willing to change ARVs and who provided informed consent.

        Exclusion Criteria:

          -  Patients who were ARV na√Øve, who were experiencing an acute illness or judged to be
             unable to comply with the protocol requirements.
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
