    <allocation>Randomized</allocation>
    <intervention_model>Crossover Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Double</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Mean of total sleep time over 2 nights.</measure>
  </primary_outcome>
  <secondary_outcome>
    <measure>Mean of other PSG sleep parameters over 2 nights, mean of subjective sleep parameters, and exploratory residual effect.</measure>
  </secondary_outcome>
  <enrollment>50</enrollment>
  <condition>Primary Insomnia</condition>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>MK0928, gaboxadol / Duration of Treatment: 6 days for treatment periods (2 days/dose)</intervention_name>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Placebo / Duration of Treatment: 2 days for screening period</intervention_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Patients with a diagnosis of primary insomnia

        Exclusion Criteria:

          -  Patients with an active psychiatric disorder other than primary insomnia
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>20 Years</minimum_age>
    <maximum_age>64 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
