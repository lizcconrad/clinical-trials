  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
  <primary_outcome>
    <measure>Change in Fibromyalgia Impact Questionnaire (FIQ) total score</measure>
    <time_frame>throughout study</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Effect of GW679769 vs placebo in health-related quality of life outcomes</measure>
    <time_frame>throughout study</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Relationship between PK of GW679769 and clinical outcome in patients</measure>
    <time_frame>throughout study</time_frame>
  </secondary_outcome>
  <secondary_outcome>
