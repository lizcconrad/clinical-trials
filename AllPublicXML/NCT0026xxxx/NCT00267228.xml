  <brief_title>Funen Anorexia Nervosa Study - Retrospective Cohort Study and 10 Year Follow-up Study</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Odense University Hospital</agency>
      <agency_class>Other</agency_class>
