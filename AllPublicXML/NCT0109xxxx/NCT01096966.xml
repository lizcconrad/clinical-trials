  </study_design_info>
  <primary_outcome>
    <measure>Mean change from baseline to 12 weeks in pain in the last 24 hours (daily scores averaged over 14 days)</measure>
    <time_frame>baseline to 12 weeks after baseline</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Subjects achieving 20% pain relief at 12 weeks compared to baseline</measure>
    <time_frame>baseline to 12 weeks after baseline</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Subjects 'much improved' or 'very much improved' at 12 weeks</measure>
    <time_frame>12 weeks after baseline</time_frame>
  </secondary_outcome>
