  <brief_title>Named Patient Program With Rotigotine Transdermal System</brief_title>
  <official_title>Named Patient Program With Rotigotine Transdermal System</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>UCB Pharma</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
