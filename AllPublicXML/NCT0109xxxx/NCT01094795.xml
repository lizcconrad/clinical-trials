  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The primary purpose of this study is to assess incidence rates of targeted infections,
      malignancies and mortality among patients with rheumatoid arthritis who are treated with
      abatacept.
    </textblock>
  </brief_summary>
  <overall_status>Active, not recruiting</overall_status>
  <start_date type="Actual">March 31, 2011</start_date>
  <completion_date type="Anticipated">June 24, 2019</completion_date>
  <primary_completion_date type="Anticipated">June 24, 2019</primary_completion_date>
