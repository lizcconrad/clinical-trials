  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Cohort</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Hospitalized infection</measure>
    <time_frame>Every 6 months throughout the study</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Malignancy (total, lymphoma, lung cancer, breast cancer, colorectal cancer, prostate cancer)</measure>
    <time_frame>Every 6 months throughout the study</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Total mortality</measure>
    <time_frame>Every 6 months throughout the study</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Autoimmune disorders (lupus, psoriasis, multiple sclerosis)</measure>
    <time_frame>Every 6 months throughout the study</time_frame>
  </secondary_outcome>
  <number_of_groups>5</number_of_groups>
  <enrollment type="Anticipated">100000</enrollment>
