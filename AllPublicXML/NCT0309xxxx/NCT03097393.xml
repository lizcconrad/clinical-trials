  <brief_title>Serum Procalcitonin Level May Predict Acute Kidney Injury in ICU Patients With Sepsis</brief_title>
  <official_title>Serum Procalcitonin Level May Predict Acute Kidney Injury in ICU Patients With Sepsis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Benha University</agency>
      <agency_class>Other</agency_class>
