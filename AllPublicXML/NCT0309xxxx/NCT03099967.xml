  <brief_title>Cardiac Troponin T After Transcatheter Closure of the Interatrial Septum</brief_title>
  <official_title>Cardiac Troponin T After Transcatheter Closure of the Interatrial Septum</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Skane University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
