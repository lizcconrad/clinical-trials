  <brief_title>Efficacy of Buprenorphine on Postoperative Endodontic Analgesia</brief_title>
  <acronym>EBPEA</acronym>
  <official_title>Efficacy of Buprenorphine on Postoperative Endodontic Analgesia</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University Hospitals Cleveland Medical Center</agency>
      <agency_class>Other</agency_class>
