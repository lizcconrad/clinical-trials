  <brief_title>Effect of Hyaluronidase on Intraocular Pressure</brief_title>
  <official_title>Effect of Hyaluronidase on Intraocular Pressure After Retrobulbar Anaesthesia During Cataract Surgery</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Assiut University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
