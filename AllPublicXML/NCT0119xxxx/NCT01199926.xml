  <brief_title>Effect of Vitamin D Supplementation on Muscle Mass and Function</brief_title>
  <official_title>Impact of Vitamin D Supplementation on Strength and Lean Mass Accumulation During an Exercise Intervention</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Purdue University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
