  <primary_outcome>
    <measure>Fold change in AUC (0-4h) relative to no treatment baseline for plasma glucose, plasma insulin, and plasma C-peptide</measure>
    <time_frame>4 hours</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pharmacodynamic effects on plasma glucose, plasma insulin, and plasma c-peptide</measure>
    <time_frame>3 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Safety: adverse events, vital sign measurements, physical examination</measure>
    <time_frame>3 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetics: sparse plasma samples will be collected for characterization of PF-0251802 pharmacokinetics (PK)</measure>
    <time_frame>3 days</time_frame>
  </secondary_outcome>
