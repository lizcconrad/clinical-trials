  <brief_title>Trial on Profermin and Fresubin in Ulcerative Colitis</brief_title>
  <acronym>CUPE2</acronym>
  <official_title>A Randomized Trial on Profermin and Fresubin for Dietetic Treatment of Active Ulcerative Colitis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Nordisk Rebalance A/S</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>Hvidovre University Hospital</agency>
      <agency_class>Other</agency_class>
    </collaborator>
  </sponsors>
  <source>Nordisk Rebalance A/S</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      This study aim to investigate the effect of Profermin versus Fresubin in the dietetic
      treatment of active ulcerative colitis.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>August 2010</start_date>
  <completion_date type="Actual">December 2012</completion_date>
  <primary_completion_date type="Actual">September 2012</primary_completion_date>
  <phase>Phase 2/Phase 3</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Crossover Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Development in Simple Clinical Colitis Activity Index (SCCAI) score</measure>
    <time_frame>8 and 16 weeks</time_frame>
    <description>Clinical significant difference (&gt;1.5) in course of SCCAI score between the two intervention groups</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Reduction in SCCAI score</measure>
    <time_frame>8 and 16 weeks</time_frame>
    <description>Proportion of patients with a reduction in SCCAI above 1.5, 3.0 and 4.5</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Clinical remission</measure>
    <time_frame>8 and 16 weeks</time_frame>
    <description>Proportion of patients in clinical remission (SCCAI=&lt;2.5)</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Safety - Number of Participants with Adverse Events as a Measure of Safety and Tolerability</measure>
    <time_frame>20 weeks</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">73</enrollment>
  <condition>Ulcerative Colitis</condition>
