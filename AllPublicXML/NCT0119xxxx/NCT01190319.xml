  <brief_title>Study: Effects of Strawberries on Blood Pressure</brief_title>
  <acronym>CSCBP</acronym>
  <official_title>Effect of Strawberry Powder on Blood Pressure in Individuals With Pre-Hypertension</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of California, Davis</agency>
      <agency_class>Other</agency_class>
