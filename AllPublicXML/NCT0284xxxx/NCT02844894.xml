  <brief_title>Hemodynamic Response to Intubation</brief_title>
  <official_title>The Effects of Preintubation Esmolol and and Dexmedotimidine on Ischmeia Modified Albumin Levels</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ufuk University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
