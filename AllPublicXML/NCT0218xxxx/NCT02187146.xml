  <brief_title>The Effects of Serum Vitamin D and IVF Outcome</brief_title>
  <acronym>VIDIO</acronym>
  <official_title>The Effects of Serum Vitamin D and IVF Outcome: a Prospective Cohort Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Birmingham</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
