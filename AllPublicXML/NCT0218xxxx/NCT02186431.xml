  <arm_group>
    <arm_group_label>history of corneal infiltrative events</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>To quantify and compare baseline tear proteins and ocular response in contact lens wearers with a history of corneal infiltrative events</description>
  </arm_group>
  <arm_group>
    <arm_group_label>without a history of corneal infiltrative events</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
