  <brief_title>Ankle Athletic Taping Fatigue After a Standard Tennis Warm up</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>CEU San Pablo University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>CEU San Pablo University</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      Identify, quantify and compare the fatigue caused in two ankle athletic tapes after the
      completion of a tennis warm-up.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>June 2014</start_date>
  <completion_date type="Actual">November 2014</completion_date>
  <primary_completion_date type="Actual">September 2014</primary_completion_date>
  <phase>N/A</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
