  <brief_title>Safety and Efficacy of Saxagliptin for Glycemic Control in Non-Critically Ill Hospitalized Patient</brief_title>
  <official_title>Safety and Efficacy of Saxagliptin for Glycemic Control in Non-Critically Ill Hospitalized Patient</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Brigham and Women's Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Brigham and Women's Hospital</source>
