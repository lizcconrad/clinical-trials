  <brief_title>Prospective Study on Embolization of Intracranial Aneurysms With Pipeline™ Embolization Device</brief_title>
  <acronym>PREMIER</acronym>
  <official_title>PREMIER PROSPECTIVE STUDY ON EMBOLIZATION OF INTRACRANIAL ANEURYSMS WITH PIPELINE™ EMBOLIZATION DEVICE</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Medtronic Neurovascular Clinical Affairs</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
