  <primary_outcome>
    <measure>Pharmacokinetics of vildagliptin</measure>
    <time_frame>Up to 12 hours post-dose</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pharmacodynamic parameters (dipeptidyl peptidase IV (DPP-4) activity, glucagon-like peptide-1(GLP-1), glucose, insulin, glucagon)</measure>
    <time_frame>Up to 4 hours or 12 hours post-dose</time_frame>
  </secondary_outcome>
