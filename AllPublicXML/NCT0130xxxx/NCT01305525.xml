  <brief_title>A Prospective Clinical Outcomes Registry</brief_title>
  <acronym>PAIN Registry</acronym>
  <official_title>Partnership for Advancement In Neuromodulation (PAIN): A Prospective Clinical Outcomes Registry</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>St. Jude Medical</agency>
