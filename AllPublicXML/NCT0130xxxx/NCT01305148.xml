    <arm_group_label>Randomized - Genetic</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Subjects who are randomized to 90 days of follow-up and whose warfarin dose was determined using the genetic information from the GenoSTAT test and clinical information through the warfarindosing.org website</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Randomized - Clinical</arm_group_label>
    <arm_group_type>No Intervention</arm_group_type>
