  <primary_outcome>
    <measure>Pharmacokinetics of ASP1941 to evaluate metabolism and excretion</measure>
    <time_frame>up to 216 hours</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Identification of metabolic profile of ASP1941</measure>
    <time_frame>up to 216 hours</time_frame>
  </secondary_outcome>
