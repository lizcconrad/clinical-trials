  <brief_title>Use of Glucose and Saline for Fetal Movement Perception</brief_title>
  <official_title>Impact of Glucose Administration on Perception of Fetal Movements</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Tel-Aviv Sourasky Medical Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
