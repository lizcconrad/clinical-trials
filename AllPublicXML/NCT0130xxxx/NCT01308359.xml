  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to investigate the hypothesis that glucose administration
      increases fetal movement perception by the pregnant woman.
    </textblock>
  </brief_summary>
  <overall_status>Unknown status</overall_status>
  <last_known_status>Recruiting</last_known_status>
