  </study_design_info>
  <primary_outcome>
    <measure>Asthma-related health-care utilization as measured by total amount of asthma medication purchased and number of contacts with health-care facilities</measure>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number of days patients or assistant persons are absent from work due to patient's asthma</measure>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of exacerbations and treatment failures</measure>
  </secondary_outcome>
  <enrollment>1970</enrollment>
  <condition>Asthma</condition>
