  <secondary_outcome>
    <measure>Change in PANSS positive, negative and general psychopathology symptom subscales</measure>
    <time_frame>change from baseline to visit 2, from baseline to end of CRT and from baseline to last visit</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in cognition as measured by a Cognitive Battery of Tests</measure>
    <time_frame>change from baseline to end of CRT and from baseline to last visit</time_frame>
  </secondary_outcome>
