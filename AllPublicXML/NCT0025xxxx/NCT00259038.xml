  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Is hospitalized with congestive heart failure (CHF)

          -  Has a Swan-Ganz catheter inserted for CHF management and has pulmonary capillary wedge
             pressure 18 mmHg or higher

        Exclusion Criteria:

          -  Has had a heart transplant

          -  Requires mechanical ventilation or mechanical circulatory support
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
