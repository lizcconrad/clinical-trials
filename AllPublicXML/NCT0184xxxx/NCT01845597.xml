  <brief_title>Survivorship and Outcomes of Robot Assisted Medial Partial Knee Replacement</brief_title>
  <official_title>Survivorship and Outcomes of Robotically Assisted Medial UKA</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Northwest Surgical Specialists, Vancouver</agency>
      <agency_class>Other</agency_class>
