  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Creatine</intervention_name>
    <description>200 mg per kg per day with a daily maximum of 5 grams divided in two daily doses</description>
    <arm_group_label>1</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Metafolin</intervention_name>
    <description>0.5 mg per kg per day by mouth with a maximum of 8 milligrams divided in two daily doses</description>
    <arm_group_label>1</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Vitamin B12</intervention_name>
    <description>1 mg by mouth per day for all weights and ages</description>
    <arm_group_label>1</arm_group_label>
