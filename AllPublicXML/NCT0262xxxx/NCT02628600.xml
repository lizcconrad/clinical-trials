    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Frequency of treatment-emergent adverse events (TEAEs)</measure>
    <time_frame>13 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Proportion of subjects achieving culture conversion at Month 6</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Anticipated">200</enrollment>
  <condition>NTM Lung Infection Due to MAC</condition>
