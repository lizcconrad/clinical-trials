  <brief_title>Fundus Autofluorescence After Vitrectomy for Epiretinal Membrane</brief_title>
  <official_title>Correlation of Postoperative Vision and Fundus Autofluorescence After Vitrectomy for Epiretinal Membrane</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Kyorin University</agency>
      <agency_class>Other</agency_class>
