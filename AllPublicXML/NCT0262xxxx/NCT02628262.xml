  <brief_title>Fundus Autofluorescence After Macular Hole Surgery</brief_title>
  <official_title>Fundus Autofluorescence Images in Gas-filled Eyes Immediately After Macular Hole Surgery</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Makoto Inoue</agency>
      <agency_class>Other</agency_class>
