  <brief_title>A Clinical Pharmacology Study of Saxagliptin in Patients With Type 2 Diabetes Mellitus</brief_title>
  <official_title>A Clinical Pharmacology Study of Saxagliptin in Patients With Type 2 Diabetes Mellitus</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Kyowa Hakko Kirin Co., Ltd</agency>
      <agency_class>Industry</agency_class>
