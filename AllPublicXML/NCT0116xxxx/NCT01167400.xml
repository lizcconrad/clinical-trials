  <brief_title>Cybercycling for Cognitive Health</brief_title>
  <official_title>Cybercycling for Older Adults: Neuropsychological, Physiological and Behavioral Effects</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Union College, New York</agency>
      <agency_class>Other</agency_class>
