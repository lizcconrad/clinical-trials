  <brief_title>Cyberknife Radiosurgery and Quality of Life</brief_title>
  <official_title>Cyberknife Radiosurgery and Quality of Life</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Stanford University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
